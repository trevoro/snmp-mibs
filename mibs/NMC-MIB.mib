nmcTeNtpSrvrLoss OBJECT-TYPE
SYNTAX INTEGER{
  enable(1),
  disable(2)
}
ACCESS read-write
STATUS mandatory
DESCRIPTION
"Enable/disable for the trap generated when communication 
with all NTP servers has failed."
::= { nmcTe 15 }

nmcTeNtpSrvrRestore OBJECT-TYPE
SYNTAX INTEGER{
  enable(1),
  disable(2)
}
ACCESS read-write
STATUS mandatory
DESCRIPTION
"Enable/disable for the trap generated when communication 
with the NTP server has been restored."
::= { nmcTe 16 }

nmcTeNtpSrvrDegraded OBJECT-TYPE
SYNTAX INTEGER{
  enable(1),
  disable(2)
}
ACCESS read-write
STATUS mandatory
DESCRIPTION
"Enable/disable for the trap generated when communication 
with the primary NTP server has failed, but the secondary 
has been restored."
::= { nmcTe 17 }

nmcTeDnsSrvrRestore OBJECT-TYPE
SYNTAX INTEGER{
  enable(1),
  disable(2)
}
ACCESS read-write
STATUS mandatory
DESCRIPTION
"Enable/disable for the trap generated when communication 
with the primary DNS server has been 
restored.
Default=disable."
::= { nmcTe 18 }

nmcTeDnsSrvrDegraded OBJECT-TYPE
SYNTAX INTEGER{
  enable(1),
  disable(2)
}
ACCESS read-write
STATUS mandatory
DESCRIPTION
"Enable/disable for the trap generated when communication 
with the primary DNS server has failed, but the secondary 
DNS server is operational.
Default=disabled."
::= { nmcTe 19 }

nmcTeLogSrvrRestore OBJECT-TYPE
SYNTAX INTEGER{
  enable(1),
  disable(2)
}
ACCESS read-write
STATUS mandatory
DESCRIPTION
"Enable/disable for the trap generated when communication 
with a logging server has been restored."
