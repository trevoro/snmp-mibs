    ::= { sonusSs7NodeEntry 53 }

sonusSs7NodeTimerT34 OBJECT-TYPE
    SYNTAX          Integer32 (2..15)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The Timer T34 is started when Loop Back Acknowledgement is sent under ANSI.
             The Timer T34 is started when segmentation is received for IAM, ACM, CPG,
             ANM or CON under ITU. The valid range for ANSI is 10 to 15 seconds. The valid
             range for ITU is 2 to 4 seconds."
    ::= { sonusSs7NodeEntry 54 }

sonusSs7NodeTimerT35 OBJECT-TYPE
    SYNTAX          Integer32 (15..20)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The Timer T35 is started when the last digit recieved is not an ST digit or
             the fixed number of digits have been recieved. This procedure is only valid
             for overlap hence ITU. This timer has no relevence in ANSI."
    DEFVAL          { 18 }
    ::= { sonusSs7NodeEntry 55 }

sonusSs7NodeTimerT36 OBJECT-TYPE
    SYNTAX          Integer32 (2..15)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The Timer T36 is started when IAM is recived with a segmentation indication for ANSI.
             This timer is started when transit or incoming international exchange recieves Continuity check
             request message. The valid range for ANSI is 2 to 4 seconds and 10 to 15 
             seconds for ITU. "
    ::= { sonusSs7NodeEntry 56 }

sonusSs7NodeTimerT37 OBJECT-TYPE
    SYNTAX          Integer32 (2..30)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The Timer T37 is started when User Part Availability test is started under ANSI. This
             timer is started when a request for and Echo control device is sent to another exchange
             under ITU. The valid value for ANSI is 30 seconds and for ITU is 2 to 4 seconds."
    ::= { sonusSs7NodeEntry 57 }

sonusSs7NodeTimerT38 OBJECT-TYPE
    SYNTAX          Integer32 (10..60)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The Timer T38 is started when a suspend message is sent to the preceding switch. This timer
             is only relevent under ITU."
    DEFVAL          { 30 }
    ::= { sonusSs7NodeEntry 58 }

sonusSs7NodeTimerT39 OBJECT-TYPE
    SYNTAX          Integer32 (1..65535)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The Timer T39 is started when MCID request is sent. It is only relevent in the ITU."
    ::= { sonusSs7NodeEntry 59 }

sonusSs7NodeTimerT40 OBJECT-TYPE
    SYNTAX          Integer32 (1..65535)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The Timer T40 could be changed from its default using this entry."
    ::= { sonusSs7NodeEntry 60 }

sonusSs7NodeTimerTAcc OBJECT-TYPE
    SYNTAX          Integer32 (3..8)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "Automatic Congestion Control Timer as per ANSI T1.113.4 - 1995. The value
             of the timer is 5 Seconds."
    DEFVAL          { 5 }
    ::= { sonusSs7NodeEntry 61 }

sonusSs7NodeTimerTCcr OBJECT-TYPE
    SYNTAX          Integer32 (1..3)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "Continuity Check Request Timer. The value As per ANSI T1.113.4 - 1995 is 2 Seconds."
    DEFVAL          { 2 }
    ::= { sonusSs7NodeEntry 62 }

sonusSs7NodeTimerTCcrr OBJECT-TYPE
    SYNTAX          Integer32 (16..31)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "Continuity Check Failed Recived. The value as per ANSI T1.113.4 - 1995 is 20 Seconds."
    DEFVAL          { 20 }
    ::= { sonusSs7NodeEntry 63 }

sonusSs7NodeTimerTCgb OBJECT-TYPE
    SYNTAX          Integer32 (3..8)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "Timer started on recipt of a Circuit Group Block Message. The value as per
             ANSI T1.113.4 - 1995 is 5 Seconds."
    DEFVAL          { 5 }
    ::= { sonusSs7NodeEntry 64 }

sonusSs7NodeTimerTCra OBJECT-TYPE
    SYNTAX          Integer32 (16..31)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "Timer started when Circuit Reservation Acknowledgment is sent. The time value
             as per ANSI T1.113.4 - 1995 is 20 seconds."
    DEFVAL          { 20 }
    ::= { sonusSs7NodeEntry 65 }

sonusSs7NodeTimerTCrm OBJECT-TYPE
    SYNTAX          Integer32 (3..4)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "This timer is started when the circuit reservation message is sent. The value 
             as per ANSI T1.113.4 - 1995 is 3 - 4 Seconds. The default is set to 4 seconds."
    DEFVAL          { 4 }
    ::= { sonusSs7NodeEntry 66 }

sonusSs7NodeTimerTCvt OBJECT-TYPE
    SYNTAX          Integer32 (5..12)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The Timer T CVT is started when the Circuit Validation Test message
             is sent. This message is only valid for ANSI."
    DEFVAL          { 10 }
    ::= { sonusSs7NodeEntry 67 }

sonusSs7NodeTimerTExm OBJECT-TYPE
    SYNTAX          Integer32 (1..65535)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The Timer Exm,d is started when IAM is sent to the succeeding network. This
             timer has no relevence in the ITU. This timer is in milliseconds"
    ::= { sonusSs7NodeEntry 68 }

sonusSs7NodeTimerTGrs OBJECT-TYPE
    SYNTAX          Integer32 (3..8)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "This timer is started when a circuit group reset message is received. 
             This timer is only relevent in the ANSI network. "
    DEFVAL          { 5 }
    ::= { sonusSs7NodeEntry 69 }

sonusSs7NodeTimerTHga OBJECT-TYPE
    SYNTAX          Integer32 (0..300)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "This timer is started on recognition of carrier loss. It is only relevent in ANSI
             network. The valid range is 0 to 300 seconds."
    DEFVAL          { 150 }
    ::= { sonusSs7NodeEntry 70 }

sonusSs7NodeTimerTScga OBJECT-TYPE
    SYNTAX          Integer32 (0..120)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "This timer is started when initial demand continuity check fails in the
