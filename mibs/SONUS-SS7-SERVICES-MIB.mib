    DEFVAL          { 60 }
    ::= { sonusSs7NodeEntry 71 }

sonusSs7NodeTimerTScgad OBJECT-TYPE
    SYNTAX          Integer32 (5..120)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "This timer is started failure of demand continuity check in the SCGA group."
    DEFVAL          { 65 }
    ::= { sonusSs7NodeEntry 72 }

sonusSs7NodeApplProfileName OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..23))
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The SS7 Node Profile to be applied to the node. The values here are
             a snap shot only and might have changed since they were set."
    ::= { sonusSs7NodeEntry 73 }

sonusSs7NodeNetworkInd OBJECT-TYPE
    SYNTAX          INTEGER { intl0(1), intl1(2), natl0(3), natl1(4) }
