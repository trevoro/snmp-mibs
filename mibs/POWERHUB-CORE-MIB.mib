                SYNTAX  INTEGER
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                    "The power supply number"
                ::= { alPSEntry 1 }

            alPSStatus OBJECT-TYPE
                SYNTAX  INTEGER {
                            powerGood (1),
                            powerPresentButBad(2),
                            powerNotPresent(3),
                            powerIntermittant(4),
                            powerUnknown(5)
                        }
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                    "The status of the power supply identified by the
                    instance of alPSNumber."
                ::= { alPSEntry 2 }

