		 saVrErNumber,
		 saVrNumber,
		 saVrTransmissionPriority }
     ::= { saVrTable 1 }

SaVrEntry ::= SEQUENCE {
     saVrT5nodeIndex
	  INTEGER,
     saVrErNumber
	  INTEGER (0..15),
     saVrNumber
	  INTEGER (1..15),
     saVrTransmissionPriority
	  INTEGER (0..2),
     saVrWindowSize
