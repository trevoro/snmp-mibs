saVrMinWindowSize        OBJECT-TYPE
     SYNTAX INTEGER (0..255)
     ACCESS read-only
     STATUS mandatory
     DESCRIPTION
	  "The minimum window size for this virtual route."
     ::= { saVrEntry 6 }

saVrMaxWindowSize        OBJECT-TYPE
     SYNTAX INTEGER (0..255)
     ACCESS read-only
     STATUS mandatory
     DESCRIPTION
	  "The maximum window size for this virtual route."
     ::= { saVrEntry 7 }

saVrPacingCount     OBJECT-TYPE
     SYNTAX INTEGER (0..255)
     ACCESS read-only
     STATUS mandatory
     DESCRIPTION
