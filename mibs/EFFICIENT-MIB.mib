    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
        "NVRAM configuration for ATM."
    INDEX { vccIndex }
    ::= { nvatmVccTable 1 }

NvatmVccEntry ::=
    SEQUENCE {  
       vccIndex    INTEGER,
       nvatmVccVpi INTEGER,
       nvatmVccVci INTEGER,
       nvatmVccAalType INTEGER,
       nvatmVccEncType INTEGER                    
    }
       
vccIndex OBJECT-TYPE
    SYNTAX INTEGER (0..7)
    ACCESS not-accessible
    STATUS current
    DESCRIPTION
        " "
    ::= { nvatmVccEntry 1 }
	
nvatmVccVpi OBJECT-TYPE
    SYNTAX INTEGER (0..4095)
    ACCESS read-write
    STATUS optional
    DESCRIPTION
        "The VPI value of the VCL.  The maximum VPI value
