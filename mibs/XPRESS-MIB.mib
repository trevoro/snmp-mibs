     }


rBChanPhoneNum OBJECT-TYPE
     SYNTAX  DisplayString (SIZE(0..16))
     ACCESS  read-write
     STATUS  mandatory
     DESCRIPTION
	     "Enter the telephone number that this B Channel responds to."
     ::= { rlanBriChanConfigEntry 1 }


rBChanSPID OBJECT-TYPE
     SYNTAX  DisplayString (SIZE(0..20))
