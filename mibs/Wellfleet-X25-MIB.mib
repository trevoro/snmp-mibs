    		Counter,
    	    wfX25VcSegmentRxs
    		Counter,
    	    wfX25VcBytesRxs
    		Counter,
    	    wfX25VcIntBytesRxs
    		Counter,
    	    wfX25VcApPktsDropped
    		Counter,
    	    wfX25VcRemoteX121Addr
    		DisplayString,
    	    wfX25VcBurstQClippedPkts
