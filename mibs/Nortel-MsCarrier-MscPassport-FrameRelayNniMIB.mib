      mscFrNniCa component."
   INDEX {
      mscFrNniIndex,
      mscFrNniCaIndex }
::= { mscFrNniCaRowStatusTable 1 }

MscFrNniCaRowStatusEntry ::= SEQUENCE {
   mscFrNniCaRowStatus
