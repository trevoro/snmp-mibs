   INDEX {
      mscFrNniIndex,
      mscFrNniDlciIndex }
::= { mscFrNniDlciStateTable 1 }

MscFrNniDlciStateEntry ::= SEQUENCE {
   mscFrNniDlciAdminState
      INTEGER,
   mscFrNniDlciOperationalState
      INTEGER,
   mscFrNniDlciUsageState
      INTEGER,
