     SEQUENCE {
	 sc800t3LocalAlarmConfigIndex
	     SCinstance,
	 sc800t3OOF
	     INTEGER,
	 sc800t3AISLOCAL
	     INTEGER,
	 sc800t3TIMLOS
	     INTEGER,
	 sc800t3TXDLOS
	     INTEGER,
	 sc800t3RXDLOS
	     INTEGER,
	 sc800t3LOS
	     INTEGER,
	 sc800t3TPSFAIL
