    ::= { sc800t3Maintenance 1 }


sc800t3MaintenanceEntry OBJECT-TYPE
    SYNTAX Sc800t3MaintenanceEntry
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
    "An entry in the GDC maintenance Table"
    INDEX { sc800t3MaintenanceIndex }
    ::= { sc800t3MaintenanceTable 1 }


Sc800t3MaintenanceEntry ::=
SEQUENCE {
	sc800t3MaintenanceIndex      	SCinstance,
	sc800t3LedStatus		OCTET STRING,
        sc800t3SoftReset		INTEGER,
	sc800t3DefaultInit		INTEGER,
        sc800t3ValidNearEndIntervals 	INTEGER,
        sc800t3ValidFarEndIntervals  	INTEGER,	
	sc800t3ShelfType		INTEGER,
	sc800t3TwinPackPowerSupply	INTEGER,
	sc800t3TestAllLeds		INTEGER,
        sc800t3NearEndResetStats        INTEGER,
        sc800t3FarEndResetStats         INTEGER,
        sc800t3NearEndStatLastInitial   INTEGER,
