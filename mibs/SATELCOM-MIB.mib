              "RLAY"
          ::= { chanParamEntry 60 }

chanParamDlciBe OBJECT-TYPE
          SYNTAX      INTEGER (0..32767)
          ACCESS      read-write
          STATUS      mandatory
          DESCRIPTION
              "Be(Burst Excess) is the excess above the Bc that will be
transmitted to the line by the MegaPAC in any given Tc.
The units are 80bits."
          REFERENCE
              "RLAY"
          ::= { chanParamEntry 61 }

chanParamDlciDe OBJECT-TYPE
             SYNTAX      INTEGER {
                                off(1),
                                on(2),
                                all(129)
                                }
          ACCESS      read-write
          STATUS      mandatory
          DESCRIPTION
              "DE(Discard Eligibility) when set will indicate that in
 a congested situation this frame should be discarded first."

          REFERENCE
              "RLAY"
          ::= { chanParamEntry 62 }

chanParamDlciProtocol OBJECT-TYPE
               SYNTAX      INTEGER {
                                none(1),
                                ip(2),
                                ipx(4),
                                iso(8),
                                fragment(16),
                                sna(32)
                                }

          ACCESS      read-write
          STATUS      mandatory
          DESCRIPTION
              "Specifies header in RFC 1480/1490 connections"
          REFERENCE
              "RLAY"
          ::= { chanParamEntry 63 }

chanParamDlciDsap OBJECT-TYPE
          SYNTAX      INTEGER (0..254)
          ACCESS      read-write
          STATUS      mandatory
          DESCRIPTION
              "Used in RFC 1490 SNA connections - each SAP identifies a specific
PU, hence by using different SAPs, multiple PUs can be multiplexed on the same
Frame Relay PVC"
