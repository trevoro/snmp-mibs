     chanStatEcho OBJECT-TYPE
          SYNTAX      INTEGER {
                              enable(2),
                              disable(1)
                              }
          ACCESS      read-only
          STATUS      mandatory
          DESCRIPTION
              "Echo.
           Determines whether 'echo' is enabled or disabled. If
              set to controlled, the echo facility is controlled by
              the operator."
          REFERENCE
              "TERM"
          ::= { chanStatEntry  53 }

     chanStatDataForwarding OBJECT-TYPE
          SYNTAX      INTEGER
          ACCESS      read-only
          STATUS      mandatory
          DESCRIPTION
              "Data Forwarding.
           This parameter controls which character or
              characters cause the accumulated data to be
              forwarded. The possible values may be added together
              to forward on one of a number of conditions. The
                  allowable values are:

              0       Forward only on buffer full.
              128     Forward on graphics characters.
              64      Forward on all characters in columns 0 and 1
                              of the International Alphabet No. 5.
              1       Forward on A-Z, a-z and 0-9.
              2       Forward on Carriage Return.
              4       Forward on ESC, BEL, ENQ, ACK.
              8       Forward on DEL, CAN, DC2.
              16      Forward on EXT, EOT.
              32      Forward on HT, LF, VT, FF."
          REFERENCE
              "TERM"
          ::= { chanStatEntry  54 }

     chanStatDelay OBJECT-TYPE
          SYNTAX      INTEGER
          ACCESS      read-only
          STATUS      mandatory
          DESCRIPTION
              "Delay.
           Specifies the idle time in tenths of a second,
              allowable between successive characters before the
              accumulated data is forwarded. A value of 0 disables
                  the timeout facility."
          REFERENCE
              "TERM"
          ::= { chanStatEntry  55 }

     chanStatDRI OBJECT-TYPE
          SYNTAX      INTEGER {
                                enabled(2),
                                rts(33),
                                cts(129),
                                init-enable(65),
                                disabled(1)
                              }
          ACCESS      read-only
          STATUS      mandatory
          DESCRIPTION
              "DRI.
           Flow control of terminal by the MegaPAC. Valid
                  settings are:
              enabled         MegaPAC flow controls terminal using
                                        XON/XOFF.
              cts             MegaPAC flow controls terminal using
                                        CTS.
              init+enable     MegaPAC sends XON automatically on
                              initialisation.
            disable         No flow control."
          REFERENCE
              "TERM"
          ::= { chanStatEntry  56 }

     chanStatPadSignals OBJECT-TYPE
          SYNTAX      INTEGER {
                              enable(2),
                              disable(1)
                              }
          ACCESS      read-only
          STATUS      mandatory
          DESCRIPTION
              "Pad Signals.
           Enable X.28 prompts and/or messages to be displayed
              on the users terminal."
          REFERENCE
              "TERM"
          ::= { chanStatEntry  57 }

     chanStatBreak OBJECT-TYPE
          SYNTAX      INTEGER
          ACCESS      read-only
          STATUS      mandatory
          DESCRIPTION
              "Break.
           The Action the MegaPAC will take on receipt of a
              Break Signal. The valid values are:

              0       When the MegaPAC receives a Break Signal from
                          the terminal no action is taken.
              1       When the MegaPAC receives a Break Signal from
                          the terminal it will transmit an INTERRUPT
                              packet.
              2       When the MegaPAC receives a Break Signal from
                          the terminal it will transmit an RESET
                              packet.
              5       When the MegaPAC receives a Break Signal from
                          the terminal it will transmit an INTERRUPT
                          packet and an indication of Break PAD
                              message.
              8       When the MegaPAC receives a Break Signal from
                          the terminal it will escape from data
                              transfer state.
              21      When the MegaPAC receives a Break Signal from
                          the terminal it will transmit an INTERRUPT
                          packet and an indication of Break PAD
                              message, while setting (X3) parameter (8)
                          to the value 1 (discard any data
                          received from the network)."
          REFERENCE
              "TERM"
          ::= { chanStatEntry  58 }

     chanStatPadding OBJECT-TYPE
          SYNTAX      INTEGER
          ACCESS      read-only
          STATUS      mandatory
        DESCRIPTION
              "Padding.
           The number of NULL characters inserted after a
              carriage return."
          REFERENCE
              "TERM"
          ::= { chanStatEntry  59 }

     chanStatFold OBJECT-TYPE
          SYNTAX      INTEGER
          ACCESS      read-only
          STATUS      mandatory
          DESCRIPTION
              "Fold
           The number of characters output before line folding
              occurs. Setting to 0 disables line folding."
          REFERENCE
              "TERM"
          ::= { chanStatEntry  60 }

     chanStatDRO OBJECT-TYPE
          SYNTAX      INTEGER {
                                enabled(2),
                                cts(129),
                                init-enable(65),
                                disabled(1)
                              }
          ACCESS      read-only
          STATUS      mandatory
          DESCRIPTION
              "Data Restraint Out."
          REFERENCE
              "TERM"
          ::= { chanStatEntry  61 }

     chanStatLFI OBJECT-TYPE
          SYNTAX      INTEGER
          ACCESS      read-only
          STATUS      mandatory
          DESCRIPTION
              "LFI.
           This parameter controls linefeed insertion after a
              carriage return. The valid seetings are:

              0       Line feed insertion disabled.
              1       A linefeed is inserted after a carriage
                          return, in the data forwarded to the
                              terminal.
              4       A linefeed is sent to the terminal after echo
                          of a carriage return to the terminal.
              5       As defined by values 1 + 4.
              6       Linefeed inserted after carriage return from
                          terminal and each carriage return echoed to
                              the terminal.
              7       As defined by values 1 + 6."
          REFERENCE
              "TERM"
          ::= { chanStatEntry  62 }

     chanStatLFPad OBJECT-TYPE
          SYNTAX      INTEGER
          ACCESS      read-only
          STATUS      mandatory
          DESCRIPTION
              "LFPad.
           The number of NULL padding characters sent to a
              device following a linefeed character."
          REFERENCE
              "TERM"
          ::= { chanStatEntry  63 }

     chanStatEdit OBJECT-TYPE
          SYNTAX      INTEGER {
                              enable(1),
