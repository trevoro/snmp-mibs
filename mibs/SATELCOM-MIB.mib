            ::= { base 5 }

        baseBufferPoolLowest OBJECT-TYPE
            SYNTAX      INTEGER
            ACCESS      read-only
            STATUS      mandatory
            DESCRIPTION
                "Node Buffer Pool Lowest.
                The lowest value the buffer pool has ever reached."
            ::= { base 6 }

        baseBufferPoolEmpty OBJECT-TYPE
            SYNTAX      INTEGER
            ACCESS      read-only
            STATUS      mandatory
            DESCRIPTION
                "Node Buffer Pool Empty.
                The number of times that the free buffer pool has fallen below
                the minimum buffer threshold 'baseBuffThreshold'."
            ::= { base 7 }

        baseStatBufferThreshold OBJECT-TYPE
            SYNTAX      INTEGER (5..1000)
            ACCESS      read-only
            STATUS      mandatory
            DESCRIPTION
                "Node Buffer Threshold.
                When the number of buffers in the free buffer pool reaches
                this threshold, flow control will occur."
            ::= { base 8 }

