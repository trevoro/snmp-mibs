                "CONS DLC ILAN XLAN"
            ::= { ctrlParamEntry 36 }

            ctrlParamDSAP OBJECT-TYPE
            SYNTAX      INTEGER (2..254)
            ACCESS      read-write
            STATUS      mandatory
            DESCRIPTION
                "DSAP.
                The Destination Service Access Point."
            REFERENCE
                "CONS DLC ILAN XLAN"
            ::= { ctrlParamEntry 37 }

            ctrlParamSSAP OBJECT-TYPE
            SYNTAX      INTEGER (2..254)
            ACCESS      read-write
            STATUS      mandatory
