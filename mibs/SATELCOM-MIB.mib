-- The Controller Group (Statistics Section)

-- The Controller Group described below relates to MegaPAC protocol
-- interfaces that support a physical or virtual interface
-- connection.  The controller Group defines all parameters available
-- in "flat table format", and as such, certain object types may or
-- may not be presented depending on the controller type being used.

        ctrlNumberConfigured OBJECT-TYPE
            SYNTAX      INTEGER
            ACCESS      read-only
            STATUS      mandatory
            DESCRIPTION
                "Total Number of Controllers Configured.
                The number of controllers in the MegaPAC which have been
                configured."
            ::= { controller 1 }

        ctrlTotalNumber OBJECT-TYPE
            SYNTAX      INTEGER
            ACCESS      read-only
            STATUS      mandatory
            DESCRIPTION
                "Total Number of Controllers.
                The TOTAL number of controllers in the MegaPAC including
                unconfigured (NULL) controllers."
            ::= { controller 2 }

        ctrlStatTable OBJECT-TYPE
            SYNTAX      SEQUENCE OF CtrlStatEntry
            ACCESS      not-accessible
            STATUS      mandatory
--          DESCRIPTION
--              ""
            ::= { controller 3 }

        ctrlStatEntry OBJECT-TYPE
            SYNTAX      CtrlStatEntry
            ACCESS      not-accessible
            STATUS      mandatory
            DESCRIPTION
                ""
            INDEX       { ctrlStatIndex }
            ::= { ctrlStatTable 1 }

        CtrlStatEntry ::= SEQUENCE {
            ctrlStatIndex
                INTEGER,
            ctrlStatName
                DisplayString,
            ctrlStatType
                INTEGER,
            ctrlStatSubType
                INTEGER,
            ctrlStatNumberChannels
                INTEGER,
            ctrlStatFirstChannelIndex
                INTEGER,
            ctrlStatCRCErrors
                Counter,
            ctrlStatTotalCallSetUps
                Counter,
            ctrlStatCurrentCalls
                INTEGER,
            ctrlStatDataIn
                Counter,
            ctrlStatDataOut
                Counter,
            ctrlStatDataFramesIn
                Counter,
            ctrlStatDataFramesOut
                Counter,
            ctrlStatDataFrameRateIn
                INTEGER,
            ctrlStatDataFrameRateOut
                INTEGER,
            ctrlStatCallsReceived
                Counter,
            ctrlStatCallsSent
                Counter,
            ctrlStatClearsReceived
                Counter,
            ctrlStatRetransmissions
                Counter,
            ctrlStatRejects
                Counter,
            ctrlStatState
                INTEGER,
            ctrlStatSubState
                INTEGER,
            ctrlStatNoTries
                Counter,
            ctrlStatOptions
                INTEGER,
            ctrlStatLCGN
                INTEGER,
            ctrlStatResetsReceived
                Counter,
            ctrlStatInterruptsReceived
                Counter,
            ctrlStatOuputQueueLength
                INTEGER,
            ctrlStatStationAddress
                DisplayString,
            ctrlStatDestinationAddress
                DisplayString,
            ctrlStatEIA
                DisplayString,
            ctrlStatSdlcOptions
                INTEGER,
            ctrlStatArpRequestRec
                INTEGER,
            ctrlStatArpRequestSent
                INTEGER,
            ctrlStatArpResponsesRec
                INTEGER,
            ctrlStatTestType
                INTEGER,
            ctrlStatTestSet
                INTEGER,
            ctrlStatTest
               INTEGER,
            ctrlStatLstate
               INTEGER,
            ctrlStatRstate
               INTEGER 
        }

        ctrlStatIndex OBJECT-TYPE
