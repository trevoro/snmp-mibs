--
-- Hewlett Packard Company
-- MIB Definition for the OC traps (from logs)
-- 
HP-OPENCALL-SS7SNMPAGENT-BASE-MIB DEFINITIONS ::= BEGIN

   IMPORTS
 	MODULE-IDENTITY, OBJECT-TYPE, NOTIFICATION-TYPE,
        enterprises
                FROM SNMPv2-SMI

        DisplayString
                FROM SNMPv2-TC ;


-- deplacement de MODULE-IDENTITY en debut de module
-- ajout de CONTACT-INFO dans le MODULE-IDENTITY
   openCall MODULE-IDENTITY
       LAST-UPDATED "200409090000Z"
       ORGANIZATION "Hewlett Packard Company,
		     OpenCall Business Unit"

       CONTACT-INFO "Raymond Mercier"
	   DESCRIPTION
               "This is the MIB module that describes an HP private mib
		for the management of OpenCall traps. "
    REVISION    "200409090000Z"
    DESCRIPTION "text"
    REVISION    "200309090000Z"
    DESCRIPTION "text"

       ::= { enterprises hp(11) nm(2) inNetElem(29) 2 }

--   hp                OBJECT IDENTIFIER ::= { enterprises 11 }
--   nm                OBJECT IDENTIFIER ::= { hp 2 }
--   inNetElem         OBJECT IDENTIFIER ::= { nm 29 }


-- Hereunder, in order to freeze the ObjectID in the mib tree, only object 
-- Traps has been defined

-- First level tree -----------------------------------------
   openCallTraps     OBJECT IDENTIFIER ::= { openCall 0 }  


-- Traps variables -----------------------------------------
-- Trap: (sourceName, sourceOID, eventType, probableCause, severity, additionalText)

--   sourcePlatformEntity       OBJECT IDENTIFIER ::= { openCallTraps 1 }  
--   sourceObjectID	        	OBJECT IDENTIFIER ::= { openCallTraps 2 }
--   perceveidSeverity          OBJECT IDENTIFIER ::= { openCallTraps 3 }  
--   alarmText                  OBJECT IDENTIFIER ::= { openCallTraps 4 }  
--   probableCause              OBJECT IDENTIFIER ::= { openCallTraps 5 }  
--   correctiveAction           OBJECT IDENTIFIER ::= { openCallTraps 6 }  
--   timeStamp                  OBJECT IDENTIFIER ::= { openCallTraps 7 }  

-- remplacer not-accessible par read-only
   sourcePlatformEntity OBJECT-TYPE
       SYNTAX     DisplayString 
       MAX-ACCESS read-only
       STATUS     current
       DESCRIPTION
               " This is the source entity that sends the trap, typically this
			   is a combination of platform and machine name. (ex: oc_platform, nta2225.india.hp.com"
       ::= { openCallTraps 1 }

   sourceObjectID OBJECT-TYPE
       SYNTAX     DisplayString 
       MAX-ACCESS read-only
       STATUS     current
       DESCRIPTION
               " This identifies the objectID of the instance that sends the trap. (ex: Process Name)"
       ::= { openCallTraps 2 }

   perceveidSeverity OBJECT-TYPE
       SYNTAX     DisplayString 
       MAX-ACCESS read-only
       STATUS     current
       DESCRIPTION
               " This identifies the severity of the trap. Possible severity are
			   1. Major
			   2. Minor
			   3. Warning
			   4. Informative
			   5. Critical"
       ::= { openCallTraps 3 }

   alarmText OBJECT-TYPE
       SYNTAX     DisplayString 
       MAX-ACCESS read-only
       STATUS     current
       DESCRIPTION
               " This describes the alarm generating the trap."
       ::= { openCallTraps 4 }

   probableCause OBJECT-TYPE
       SYNTAX     DisplayString 
       MAX-ACCESS read-only
       STATUS     current
       DESCRIPTION
               " This identifies the probable cause of the trap."
       ::= { openCallTraps 5 }

   correctiveAction OBJECT-TYPE
       SYNTAX     DisplayString 
       MAX-ACCESS read-only
       STATUS     current
       DESCRIPTION
               " This describes the actions to apply for correcting the
               problem that generated this alarm."
       ::= { openCallTraps 6 }

-- renommer timeStamp en htimeStamp
   htimeStamp OBJECT-TYPE
       SYNTAX     DisplayString 
       MAX-ACCESS read-only
       STATUS     current
       DESCRIPTION
               " Time-Stamp recorded in the log generated by this alarm."
       ::= { openCallTraps 7 }


