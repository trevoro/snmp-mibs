      cabhCdpWanDataAddrClientId, 
      cabhCdpLanTransThreshold, 
      cabhCdpLanTransCurCount      FROM CABH-IETF-CDP-MIB 
    
      ZeroBasedCounter32           FROM RMON2-MIB; 
    
    
