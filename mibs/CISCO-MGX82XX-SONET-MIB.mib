		   cwSonetLineCurrentLineNum   INTEGER,
                   cwSonetLineCurrentStatus    INTEGER,
		   cwSonetLineCurrentValidFlag 	INTEGER,
                   cwSonetLineCurrent15minTimeElapsed    	INTEGER,
                   cwSonetLineCurrentESs       Gauge,
