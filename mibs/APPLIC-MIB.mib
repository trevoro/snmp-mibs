scPortHistoryGoodPktsSent OBJECT-TYPE
	SYNTAX Counter
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
	"The total number of good packets sent by the port."
	::= { scPortHistoryEntry 9 }

portHistoryMemoryAvailability OBJECT-TYPE
	SYNTAX  INTEGER         { 
		standard(1), 
		limited1(2), 
		limited2(3), 
		notSupported(255)
		} 
	ACCESS  read-only 
	STATUS  mandatory 
