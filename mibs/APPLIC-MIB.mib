	::= { lsMonitor 8 }


lsMatrixFilterEntry OBJECT-TYPE
	SYNTAX LsMatrixFilterEntry
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION
"An entry in the filter table."
	INDEX { lsMatrixFilterAddress }
	::= { lsMatrixFilterTable 1 }

LsMatrixFilterEntry ::= SEQUENCE {
