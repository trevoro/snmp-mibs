		STATUS	mandatory
		DESCRIPTION " "
		::= { recfTranslation 1 }

recfLink2TraEntry 	OBJECT-TYPE
		SYNTAX	RecfLink2TraEntry
		ACCESS 	not-accessible
		STATUS	mandatory
		DESCRIPTION "INTRODUCED 2.0.0"
		INDEX { recfLink2TraIndex }
		::= { recfLink2TraTable  1 }

RecfLink2TraEntry      		::= SEQUENCE 	{
			recfLink2TraIndex
		  		Link2Index,
 			recfLink2TraStartMode
