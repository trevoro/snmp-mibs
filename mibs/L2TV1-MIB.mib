-- ----------------------------------------------------------------------------
-- The L2T Tunnel Connectivity Test Table
--
-- This table permits the testing of connectively for protential
-- L2T Tunnels.
-- ----------------------------------------------------------------------------

   l2tTunnelConnectTestTable OBJECT-TYPE
      SYNTAX SEQUENCE OF L2tTunnelConnectTestEntry
      ACCESS not-accessible
      STATUS mandatory
      DESCRIPTION
         "The L2T tunnel connectivity test table."
      ::= { l2tTests 1 }

   l2tTunnelConnectTestEntry OBJECT-TYPE
      SYNTAX L2tTunnelConnectTestEntry
      ACCESS not-accessible
      STATUS mandatory
      DESCRIPTION
         "An L2T tunnel connectivity test entry."
      INDEX { l2tTunnelConnectTestIndex }
      ::= { l2tTunnelConnectTestTable 1 }

   L2tTunnelConnectTestEntry ::= SEQUENCE {
      l2tTunnelConnectTestIndex       INTEGER,
      l2tTunnelConnectTestHostName    DisplayString,
      l2tTunnelConnectTestResult      INTEGER,
      l2tTunnelConnectTestType        INTEGER,
      l2tTunnelConnectTestStatus      INTEGER
   }

   l2tTunnelConnectTestIndex OBJECT-TYPE
      SYNTAX INTEGER(1..65535)
      ACCESS read-only
      STATUS mandatory
      DESCRIPTION
         "The index of the tunnel connectivity test table.
          The value of the index is a number which was used
          during row creation."
      ::= { l2tTunnelConnectTestEntry 1 }

   l2tTunnelConnectTestHostName OBJECT-TYPE
      SYNTAX DisplayString
      ACCESS read-write
      STATUS mandatory
      DESCRIPTION
         "The host name used to establish a Layer-2 tunnel.
      
          This object is used to trigger a connectivity test
          to a specific host.  A new test is triggered by 
          SETTING this object using a non-existent index    
          and SETTING l2tTunnelConnectTestStatus to createAndGo.
          For example to trigger a new connectivity test 
          when the table currently contains row 1 through 4,
          one would send a SET as follows:                    
           SNMP SET l2tTunnelConnectHostName.5   = Host-To-Test
                    l2tTunnelConnectTestStatus.5 = createAndGo
          See l2tHostName for the possible host names which
          may be specified.
          
          Existing entries may NOT be modified."             
      ::= { l2tTunnelConnectTestEntry 2 }

   l2tTunnelConnectTestResult OBJECT-TYPE
      SYNTAX INTEGER {
                 inProgress(1),
                 successful(2),
                 tunnelAlreadyUp(3),
                 hostNotResponding(4),
                 localAuthFailure(5),
                 remoteAuthFailure(6)
             }
      ACCESS read-only
      STATUS mandatory
      DESCRIPTION
         "The result of the tunnel connectivity test.           
          1 = test is in progress
          2 = test ended successfully
          3 = tunnel is already active
          4 = host did not respond
          5 = local authentication failure
          6 = remote authentication failure."
      ::= { l2tTunnelConnectTestEntry 3 }

   l2tTunnelConnectTestType OBJECT-TYPE
      SYNTAX INTEGER {
                l2tpTunnel(1),
                l2fTunnel(2),
                pptpTunnel(3),
                none(99)
             }
      ACCESS read-only
      STATUS mandatory
      DESCRIPTION
         "The type of Layer-2 Tunnel.  Possible values are:
          1  = Layer-2 Tunnel Protocol Tunnel
          2  = Layer-2 Forwarding Tunnel
          3  = Point-to-Point Tunnel Protocol Tunnel
          99 = no tunnel created."                    
      ::= { l2tTunnelConnectTestEntry 4 }

   l2tTunnelConnectTestStatus OBJECT-TYPE
      SYNTAX INTEGER {
                active(1),     
                createAndGo(4) 
             }
      ACCESS read-write
      STATUS mandatory
      DESCRIPTION
         "The status of the test entry. Possible values are:
          1  = active entry                    
          4  = createAndGo              
         
          This object is used to trigger a connectivity test
          to a specific host.  A new test is triggered by
          SETTING this object to createAndGo using a non-existent
          index and SETTING l2tTunnelConnectTestHostName to a 
          specific host name.  For example to trigger a new 
          connectivity test when the table currently contains 
          row 1 through 4, one would send a SET as follows:
           SNMP SET l2tTunnelConnectHostName.5   = Host-To-Test
                    l2tTunnelConnectTestStatus.5 = createAndGo
          See l2tHostName for the possible host names which
          may be specified.

          Existing entries may NOT be modified."
      ::= { l2tTunnelConnectTestEntry 5 }

-- ----------------------------------------------------------------------------
-- The L2T Tunnel Response Time Test Table
--
-- This table permits the testing the response time of an existing
-- L2T Tunnel.
-- ----------------------------------------------------------------------------

   l2tTunnelRspTimeTestTable OBJECT-TYPE
      SYNTAX SEQUENCE OF L2tTunnelRspTimeTestEntry
      ACCESS not-accessible
      STATUS mandatory
      DESCRIPTION
         "The L2T tunnel response time test table."
      ::= { l2tTests 2 }

   l2tTunnelRspTimeTestEntry OBJECT-TYPE
      SYNTAX L2tTunnelRspTimeTestEntry
      ACCESS not-accessible
      STATUS mandatory
      DESCRIPTION
         "An L2T tunnel response time test entry."
      INDEX { l2tTunnelRspTimeTestIndex }
      ::= { l2tTunnelRspTimeTestTable 1 }

   L2tTunnelRspTimeTestEntry ::= SEQUENCE {
      l2tTunnelRspTimeTestIndex       INTEGER,
      l2tTunnelRspTimeTestHostName    DisplayString,
      l2tTunnelRspTimeTestResult      INTEGER,
      l2tTunnelRspTimeTestRtripTime   INTEGER,
      l2tTunnelRspTimeTestStatus      INTEGER
   }

   l2tTunnelRspTimeTestIndex OBJECT-TYPE
      SYNTAX INTEGER(1..65535)
      ACCESS read-only
      STATUS mandatory
      DESCRIPTION
         "The index of the tunnel connectivity test table.
          The value of the index is a number which was used
          during row creation."
      ::= { l2tTunnelRspTimeTestEntry 1 }

   l2tTunnelRspTimeTestHostName OBJECT-TYPE
      SYNTAX DisplayString
      ACCESS read-write
      STATUS mandatory
      DESCRIPTION
         "The host name used to test the round-trip response
          time of an existing Layer-2 Tunnel.

          This object is used to trigger a response time test
          of an existing Layer-2 to a specific host.  A new test
          is triggered by SETTING this object using a non-existent
          index and SETTING l2tTunnelRspTimeTestStatus to createAndGo.
          For example to trigger a new connectivity test
          when the table currently contains row 1 through 4,
          one would send a SET as follows:
           SNMP SET l2tTunnelRspTimeHostName.5   = Host-To-Test
                    l2tTunnelRspTimeTestStatus.5 = createAndGo
          See l2tHistTunnelRemoteHostName for the possible host
          names which may be specified.

          Existing entries may NOT be modified."
      ::= { l2tTunnelRspTimeTestEntry 2 }

   l2tTunnelRspTimeTestResult OBJECT-TYPE
      SYNTAX INTEGER {
                 inProgress(1),
                 successful(2),
                 hostNotResponding(3),
                 tunnelDown(4)
             }
      ACCESS read-only
      STATUS mandatory
      DESCRIPTION
         "The result of the tunnel connectivity test.
          1 = test is in progress
          2 = test ended successfully
          3 = host did not respond
          4 = a tunnel to this host is NOT available."
      ::= { l2tTunnelRspTimeTestEntry 3 }

   l2tTunnelRspTimeTestRtripTime OBJECT-TYPE
      SYNTAX INTEGER
      ACCESS read-only
      STATUS mandatory
      DESCRIPTION
         "The round time in milliseconds of response time test."
      ::= { l2tTunnelRspTimeTestEntry 4 }

   l2tTunnelRspTimeTestStatus OBJECT-TYPE
      SYNTAX INTEGER {
                active(1),
                createAndGo(4)
             }
      ACCESS read-write
      STATUS mandatory
      DESCRIPTION
         "The status of the test entry. Possible values are:
          1  = active entry
          4  = createAndGo

          This object is used to trigger a response time test
          of an existing Layer-2 to a specific host.  A new test
          is triggered by SETTING this object using a non-existent
          index to createAndGo and SETTING l2tTunnelRspTimeHostName
          to a specific host.  For example to trigger a new response
          time test when the table currently contains row 1 through 4,
          one would send a SET as follows:
           SNMP SET l2tTunnelRspTimeHostName.5   = Host-To-Test
                    l2tTunnelRspTimeTestStatus.5 = createAndGo
          See l2tHistTunnelRemoteHostName for the possible host
          names which may be specified.

          Existing entries may NOT be modified."
      ::= { l2tTunnelRspTimeTestEntry 5 }

 
-- ----------------------------------------------------------------------------
-- The L2T TRAP Control Group         
--
-- This group of objects controls the sending L2T TRAPs.   
-- ----------------------------------------------------------------------------

   l2tTrapCntlTunnelStart OBJECT-TYPE
      SYNTAX INTEGER {
                enabled(1),
                disabled(2)
             }
      ACCESS read-write
      STATUS mandatory
      DESCRIPTION
         "This object defines the administrative state of sending the L2T
          Tunnel Start TRAP "
      DEFVAL { enabled }
      ::= { l2tTrapCntl 1 }      

   l2tTrapCntlTunnelStop OBJECT-TYPE
      SYNTAX INTEGER {
                enabled(1),
                disabled(2)
             }
      ACCESS read-write
      STATUS mandatory
      DESCRIPTION
         "This object defines the administrative state of sending the L2T
          Tunnel Stop TRAP "
      DEFVAL { enabled }
      ::= { l2tTrapCntl 2 }      

   l2tTrapCntlAuthFailTunnel OBJECT-TYPE
      SYNTAX INTEGER {
                enabled(1),
                disabled(2)
             }
      ACCESS read-write
      STATUS mandatory
      DESCRIPTION
         "This object defines the administrative state of sending the L2T
          Tunnel Authentication Failure TRAP "
      DEFVAL { enabled }
      ::= { l2tTrapCntl 3 }      

   l2tTrapCntlAuthFailUser OBJECT-TYPE
      SYNTAX INTEGER {
                enabled(1),
                disabled(2)
             }
      ACCESS read-write
      STATUS mandatory
      DESCRIPTION
         "This object defines the administrative state of sending the L2T
          User Authentication Failure TRAP "
      DEFVAL { enabled }
      ::= { l2tTrapCntl 4 }      

-- ----------------------------------------------------------------------------
-- The L2T TRAP Group
-- Describes TRAPs which can be generated by the Layer Two Transport Protocol
-- ----------------------------------------------------------------------------

l2tTunnelStart TRAP-TYPE
    ENTERPRISE ibmIROCroutingl2t
    VARIABLES  { l2tTunnelLocalTunnelControlId,
                 l2tTunnelType }
    DESCRIPTION
       "This TRAP is generated when a Layer-2 Tunnel 
        is created."
    ::= 1

l2tTunnelStop TRAP-TYPE
    ENTERPRISE ibmIROCroutingl2t
    VARIABLES  { l2tTunnelLocalTunnelControlId,
                 l2tTunnelType,
                 l2tTunnelUpTime }
    DESCRIPTION
       "This TRAP is generated when a Layer-2 Tunnel 
        ends."       
    ::= 2

l2tAuthFailTunnel TRAP-TYPE
    ENTERPRISE ibmIROCroutingl2t
    VARIABLES  { l2tAuthFailTunnelHostName,
                 l2tAuthFailTunnelIpAddress }
    DESCRIPTION
       "This TRAP is generated when a Layer-2 Tunnel 
        is NOT created due to an authentication failure."
    ::= 3

l2tAuthFailUser TRAP-TYPE
    ENTERPRISE ibmIROCroutingl2t
    VARIABLES  { l2tAuthFailUserId }
    DESCRIPTION
       "This TRAP is generated when a Session for a   
        Layer-2 Tunnel is NOT created due to an 
        authentication failure."
    ::= 4

END
