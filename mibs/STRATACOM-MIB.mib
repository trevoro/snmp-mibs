        ::= { atmTrkEntry 20 }

atmTrkSvcLcnHigh OBJECT-TYPE
    SYNTAX      INTEGER
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION "The Upper limit of the LCN value which can be used on this trunk 
