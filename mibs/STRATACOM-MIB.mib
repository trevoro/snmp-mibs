		atmEndPtSlot		INTEGER,
		atmEndPtPort		INTEGER,
		atmEndPtVpi			INTEGER,
		atmEndPtVci			INTEGER,
		atmEndPtConnIndex	INTEGER,
    
		atmEndPtAdminStatus	INTEGER,
		atmEndPtOpStatus	INTEGER,
		atmEndPtRowStatus	INTEGER,
    
		atmEndPtCIR			INTEGER,
		atmEndPtMIR			INTEGER,
		atmEndPtPIR			INTEGER,
		atmEndPtQIR			INTEGER,
		atmEndPtPercUtil	INTEGER,
