-- The ncm csu miscellaneous table

        ncmcsuMiscellaneousTable OBJECT-TYPE
        SYNTAX SEQUENCE OF NcmcsuMiscellaneousEntry
        ACCESS not-accessible
        STATUS mandatory
		DESCRIPTION
               "The ncmcsu miscellaneous table."


	 --DEFAULT test-function csucommon-test
        --DEFAULT next-function csucommon-next

        ::= { ncm-csu 6016 }


        ncmcsuMiscellaneousEntry OBJECT-TYPE
        SYNTAX NcmcsuMiscellaneousEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION
               "An entry in the ncmcsu miscellaneous table."
        INDEX   { ncmcsuNIDMiscellaneousIndex, ncmcsuMiscellaneousIndex }
        ::= { ncmcsuMiscellaneousTable 1 }


        NcmcsuMiscellaneousEntry ::=
        SEQUENCE {

             ncmcsuNIDMiscellaneousIndex
                 INTEGER,
             ncmcsuMiscellaneousIndex
                 INTEGER,
	     ncmcsuResetESFErrCounter
		 INTEGER,
	     ncmcsuRetrieveNetESFErrCounter
		 INTEGER,
	     ncmcsuRetrieveEqESFErrCounter
		 INTEGER,
	     ncmcsuSendBOPmessage
		 INTEGER,
	     ncmcsuResetNetEqBERAlarm
		 INTEGER
	     }


        ncmcsuNIDMiscellaneousIndex  OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
        "This variable specifies the node id of the node."
        ::= { ncmcsuMiscellaneousEntry 1 }


        ncmcsuMiscellaneousIndex  OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
        "This variable specifies the interface index ."
         ::= { ncmcsuMiscellaneousEntry 2 }


        ncmcsuResetESFErrCounter  OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
        "The ncmcsu reset esf error counter message(44)."
        ::= { ncmcsuMiscellaneousEntry 3 }
 

        ncmcsuRetrieveNetESFErrCounter  OBJECT-TYPE
        SYNTAX INTEGER 
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "The ncmcsu retrieval net esf error counter from 
         the Retrieve Event Counters message(18 01 0F)."
        ::= { ncmcsuMiscellaneousEntry 4 } 


        ncmcsuRetrieveEqESFErrCounter  OBJECT-TYPE
        SYNTAX INTEGER  
        ACCESS read-only 
        STATUS mandatory 
        DESCRIPTION 
        "The ncmcsu retrieval eq esf error counter from 
         the Retrieve Event Counters message(18 01 0F)." 
        ::= { ncmcsuMiscellaneousEntry 5 }  
 

        ncmcsuSendBOPmessage  OBJECT-TYPE
        SYNTAX INTEGER  {
                plb-activate(1),
                plb-deactivate(2),
               llb-activate(3),
                llb-deactivate(4)
           }
        ACCESS read-write
        STATUS mandatory  
        DESCRIPTION  
        "The ncmcsu send messages(18 01 38,39,3A,3B) to 
	 activate or deactivate the plb or llb respectively."  
        ::= { ncmcsuMiscellaneousEntry 6 }   
 

        ncmcsuResetNetEqBERAlarm  OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
        "The ncmcsu reset Net and Eq BER Alarms message(18 01 3D)."
        ::= { ncmcsuMiscellaneousEntry 7 }


