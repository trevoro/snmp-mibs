                nb30PermDeleteEntry     INTEGER,
                nb30PermFilterType      OCTET STRING,
                nb30PermFilterAction    INTEGER,
                nb30PermSourceAddress   OCTET STRING,
                nb30PermDBMacAddress    OCTET STRING
             }

nb30PermCreateFilter        OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.6.2.4.1.1
    SYNTAX      INTEGER
                {
                    createPermanentFilter(1)
                }
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION
                "Add an entry to the permanent database to filter packets
                entering port 1 if those packets are destined for the
                specified address."
::= { nb30PermDBEntry 1 }

nb30PermCreateForward   OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.6.2.4.1.2
    SYNTAX      INTEGER
                {
                    createPermanentForward(1)
                }
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION
                "Add an entry to the permanent database to forward
                packets entering port 1 to the remote port if those
                packets are destined for the specified address."
::= { nb30PermDBEntry 2 }

nb30PermDeleteEntry     OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.6.2.4.1.3
    SYNTAX      INTEGER
                {
                    deletePermanentEntry(1)
                }
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION
                "Delete an entry from the permanent database."
::= { nb30PermDBEntry 3 }

nb30PermFilterType      OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.6.2.4.1.4
    SYNTAX      OCTET STRING
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION
                "Display the entry type, STATIC ."
::= { nb30PermDBEntry 4 }

nb30PermFilterAction        OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.6.2.4.1.5
    SYNTAX      INTEGER
                {
                    filter(1),
                    relay(2)
                }
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION
                "The action taken on packets entering port 1."
::= { nb30PermDBEntry 5 }

nb30PermSourceAddress   OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.6.2.4.1.6
    SYNTAX      OCTET STRING
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION
                "The source address in this permanent database entry, i.e.
                the instance."
::= { nb30PermDBEntry 6 }

nb30PermDBMacAddress	OBJECT-TYPE
    SYNTAX	OCTET STRING (SIZE (6))
    ACCESS	read-only
    STATUS	mandatory
    DESCRIPTION
	"The specific MAC address for this entry."
::= { nb30PermDBEntry 7 }


-- Special Database Statistical Object Definitions
--
-- This section defines the objects under the Special Database
-- Statistical node.
--
-- Instance Identifiers for Special Database Statistical Objects
--
--      There is only one instance of a Special Database Statistical object,
--      thus the Instance Identifier is 0.
--
-- Special Database Statistical Objects

nb30SpecNumEntries      OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.6.3.1
    SYNTAX      INTEGER
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION
                "The number of filters in the special database."
::= { nb30SpecDB 1 }

nb30SpecMaxEntries      OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.6.3.2
    SYNTAX      INTEGER
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION
                "The maximum number of filters the special database can
                contain."
::= { nb30SpecDB 2 }

nb30SpecNextFilterNum   OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.6.3.3
    SYNTAX      INTEGER
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION
                "The number of the next available filter that can be added
                to the special database. This OID will return a 255 if the
                database is full"
::= { nb30SpecDB 3 }

-- Special Database Filter Object Definitions
--
-- This section defines the objects under the Special Database
-- Filter node.
--
-- Instance Identifiers for Special Database Filter Objects
--
--      The instance identifier of the special database filter objects
--      is the filter number, values from 1 through 10 are allowed.
--
-- Special Database Filter Objects

nb30SpecDBTable     OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.6.3.4
    SYNTAX      SEQUENCE OF Nb30SpecDBEntry
    ACCESS      not-accessible
    STATUS      mandatory
    DESCRIPTION
                ""
::= { nb30SpecDB 4 }

nb30SpecDBEntry     OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.6.3.4.1
    SYNTAX      Nb30SpecDBEntry
    ACCESS      not-accessible
    STATUS      mandatory
    DESCRIPTION
                ""
    INDEX       { nb30SpecDbFilterNumber }

::= { nb30SpecDBTable 1 }

Nb30SpecDBEntry ::=
    SEQUENCE {
                nb30SpecEnable          INTEGER,
                nb30SpecPort1           INTEGER,
                nb30SpecDestAddress     OCTET STRING,
                nb30SpecSrcAddress      OCTET STRING,
                nb30SpecType            OCTET STRING,
                nb30SpecDataField       OCTET STRING,
                nb30SpecDeleteFilter    INTEGER,
                nb30SpecDbFilterNumber  INTEGER
             }

nb30SpecEnable      OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.6.3.4.1.1
    SYNTAX      INTEGER
                {
                    disableFilter(1),
                    enableFilter(2)
                }
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION
                "Enable or disable the filter."
::= { nb30SpecDBEntry 1 }

nb30SpecPort1       OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.6.3.4.1.2
    SYNTAX      INTEGER
                {
                    filter(1),
                    relay(2)
                }
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION
                "Set port 1 to filter or forward packets."
::= { nb30SpecDBEntry 2 }

nb30SpecDestAddress OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.6.3.4.1.3
    SYNTAX      OCTET STRING
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION
                "Set the special database to filter packets with a specific
                destination address."
::= { nb30SpecDBEntry 3 }

nb30SpecSrcAddress      OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.6.3.4.1.4
    SYNTAX      OCTET STRING
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION
                "Set the special database to filter packets with a specific
                source address."
::= { nb30SpecDBEntry 4 }

nb30SpecType        OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.6.3.4.1.5
    SYNTAX      OCTET STRING
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION
                "Set the special database to filter packets with a specific
                type field."
::= { nb30SpecDBEntry 5 }

nb30SpecDataField       OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.6.3.4.1.6
    SYNTAX      OCTET STRING
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION
                "Set the special database to filter packets based on the
                first sixteen bytes of data in the packet."
::= { nb30SpecDBEntry 6 }

nb30SpecDeleteFilter        OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.6.3.4.1.7
    SYNTAX      INTEGER
                {
                    deleteFilter(1)
                }
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION
                "Delete a filter from the special database."
::= { nb30SpecDBEntry 7 }

nb30SpecDbFilterNumber	OBJECT-TYPE
    SYNTAX	INTEGER
    ACCESS	read-only
    STATUS	mandatory
    DESCRIPTION
	"The specific special data base filter number."
::= { nb30SpecDBEntry 8 }

END
