nb30RemPortStatus       OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.5.1.1.3
    SYNTAX      OCTET STRING
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION
                "The status in which the remote port is operating. The status
                messages for the Remote Port are:

                NO EXT CLOCK
                DCE NOT READY / NO DM
                DCE NOT READY / NO CTS
                LINK OK / DCE READY
                NET LOOP / RED ALARM
                REM LOOP / RED ALARM
                NORMAL / RED ALARM
                NET LOOP / YELLOW ALARM
                REM LOOP / YELLOW ALARM
                NORMAL / YELLOW ALARM
                NET LOOP / LINK OK
                REM LOOP / LINK OK
                NORMAL / LINK OK
                NET LOOP / LINK LOST
                REM LOOP / LINK LOST
                NORMAL / LINK LOST"
::= { nb30RemPortEntry 3 }

nb30RemPortNetName  OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.5.1.1.4
    SYNTAX      OCTET STRING
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION
                "The user-defined name assigned to a wide area segment
                connected to the remote port. The default network name
                for Remote Port is REMOTE LINK."
::= { nb30RemPortEntry 4 }

nb30RemPortFrRxs    OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.5.1.1.5
    SYNTAX      Counter
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION
                "The number of frames received at the remote port."
::= { nb30RemPortEntry 5}

nb30RemPortFwdOutbs OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.5.1.1.6
    SYNTAX      Counter
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION
                "The number of frames forwarded successfully to the
                remote port."
::= { nb30RemPortEntry 6 }

nb30RemPortDisLOBs      OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.5.1.1.7
    SYNTAX      Counter
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION
                "The number of packets that were discarded by the bridge
                because of a lack of buffer space to maintain the data."
::= { nb30RemPortEntry 7 }

nb30RemPortDisTDEs      OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.5.1.1.8
    SYNTAX      Counter
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION
                "The number of frames that were intended for forwarding
                but were discarded when the maximum time period set for
                transmission was exceeded before forwarding was possible."
::= { nb30RemPortEntry 8 }

nb30RemPortCRCErrs      OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.5.1.1.9
    SYNTAX      Counter
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION
                "The number of packets detected with Cyclical Redundancy
                Check failures."
::= { nb30RemPortEntry 9 }

nb30RemPortFrAlErrs     OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.5.1.1.10
    SYNTAX      Counter
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION
                "The number of errors due to misaligned packets."
::= { nb30RemPortEntry 10 }

nb30RemPortId	OBJECT-TYPE
    SYNTAX	INTEGER
    ACCESS	read-only
    STATUS	mandatory
    DESCRIPTION
	"The remote port ID, there is only a single port which is
	identified as 0."
::= { nb30RemPortEntry 11 }

-- Acquired Database Statistical Object Definitions
--
-- This section defines the objects under the Acquired Database
-- Statistics node.
--
-- Instance Identifiers for Acquired Database Statistical Objects
--
--      There is only one instance of an Acquired Database Statistical object,
--      thus the Instance Identifier is 0.
--
--  Acquired Database Statistical Objects

nb30AcqTotalEntries     OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.6.1.1
    SYNTAX      INTEGER
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION
                "The total number of entries in the acquired database."
::= { nb30AcqDB 1 }

nb30AcqMaxEntries       OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.6.1.2
    SYNTAX      INTEGER
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION
                "The maximum number of entries allowed in the acquired
                database 8191 entries."
::= { nb30AcqDB 2 }

nb30AcqStaticEntries        OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.6.1.3
    SYNTAX      INTEGER
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION
                "The number of addresses added to the Acquired database
                by the user or network manager."
::= { nb30AcqDB 3 }

nb30AcqDynamicEntries   OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.6.1.4
    SYNTAX      INTEGER
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION
                "The number of entries that have been accumulated in
                the Acquired database through the bridge's learning
                process."
::= { nb30AcqDB 4 }

nb30AcqDynAgeTime       OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.6.1.5
    SYNTAX      INTEGER
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION
                "The length of time allowed for a dynamic entry in the
