	ejbJRunServerName OBJECT-TYPE
        SYNTAX DisplayString (SIZE (1..255))
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "The name of this JRun Server to which the EJB belongs."
        ::= { jrunEjbEntry 2 }


    ejbName OBJECT-TYPE
        SYNTAX DisplayString (SIZE (1..255))
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
             "The name of the EJB."
