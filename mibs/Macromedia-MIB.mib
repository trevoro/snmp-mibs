Macromedia-MIB DEFINITIONS ::= BEGIN

        --
        --  MIB Definitions for Macromedia software products.

		-- Cluster Membership Table is supported in NT and Windows 2000 servers, not in UNIX servers.


    IMPORTS
	TRAP-TYPE FROM RFC-1215
        enterprises
                                FROM RFC1155-SMI
        OBJECT-TYPE
                                FROM RFC-1212
        DisplayString
                                FROM RFC1213-MIB;

    allaire				OBJECT IDENTIFIER ::= { enterprises 7138 }
    software            OBJECT IDENTIFIER ::= { allaire  1 }
    coldfusion			OBJECT IDENTIFIER ::= { software 1 } 
	jrun				OBJECT IDENTIFIER ::= { software 2 } 
	jrunServers			OBJECT IDENTIFIER ::= { software 3 } 	
	jrunJDBCs			OBJECT IDENTIFIER ::= { software 4 }
	jrunWebApps			OBJECT IDENTIFIER ::= { software 5 }
	jrunEJBs			OBJECT IDENTIFIER ::= { software 6 }
	probes				OBJECT IDENTIFIER ::= { software 7 }
	haaManagement		OBJECT IDENTIFIER ::= { software 8 }
    serverManagement	OBJECT IDENTIFIER ::= { software 9 }
    clusterManagement	OBJECT IDENTIFIER ::= { software 10 }
    clusterMembership	OBJECT IDENTIFIER ::= { software 11 }
    trapRecord			OBJECT IDENTIFIER ::= { software 12 }


-----------  ColdFusion Information ----------------


    cfOSName OBJECT-TYPE
        SYNTAX DisplayString (SIZE (1..128))
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "The name of the operation system."
        ::= { coldfusion 1 }


    cfOSVersion OBJECT-TYPE
        SYNTAX DisplayString (SIZE (1..128))
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "The version of the operation system."
        ::= { coldfusion 2 }


    cfOSBuildNumber OBJECT-TYPE
        SYNTAX DisplayString (SIZE (1..128))
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "The build number of the operation system."
        ::= { coldfusion 3 }


    cfOSAdditionalInfo OBJECT-TYPE
        SYNTAX DisplayString (SIZE (1..128))
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "Additional information concerning the operation system."
        ::= { coldfusion 4 }

    cfProductName OBJECT-TYPE
        SYNTAX DisplayString (SIZE (1..128))
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "The name of the ColdFusion product."
        ::= { coldfusion 5 }

    cfProductLevel OBJECT-TYPE
        SYNTAX DisplayString (SIZE (1..128))
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "The level of the ColdFusion product."
        ::= { coldfusion 6 }

    cfProductVersion OBJECT-TYPE
        SYNTAX DisplayString (SIZE (1..128))
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "The version of the ColdFusion product."
        ::= { coldfusion 7 }

	cfPerformanceMonitorOn OBJECT-TYPE
        SYNTAX INTEGER {
                        no(0),
                        yes(1)
                        }
		ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "Indicator specifying whether ColdFusion Enable Performance Monitoring is turned on.  
			 If off, the next 11 runtime parameters will have the value 0."
        ::= { coldfusion 8 }

-----------  ColdFusion Performance Information Begin----------------

    cfPageHits OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "The number of ColdFusion page hits per seconds."
        ::= { coldfusion 9 }

    cfRequestsQueued OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "The number of ColdFusion requests currently being queued."
        ::= { coldfusion 10 }

    cfDatabaseHits OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "The number of database hits by ColdFusion pages per seconds."
        ::= { coldfusion 11 }

    cfRequestsRunning OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "The number of requests currently being processed by ColdFusion."
        ::= { coldfusion 12 }

    cfReqestsTimedOut OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "The number of ColdFusion requests that have been timed out."
        ::= { coldfusion 13 }

    cfBytesIn OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "The total number of bytes that have been read by ColdFusion per second."
        ::= { coldfusion 14 }

    cfBytesOut OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "The total number of bytes that have been outputted by ColdFusion per second."
        ::= { coldfusion 15 }

    cfAvgQueueTimes OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "The average amount of time spent by a ColdFusion page request 
			in the queue before being processed."
        ::= { coldfusion 16 }

    cfAvgRequestTime OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "The average amount of process time spent on a ColdFusion request 
			(queuing time not included)."
        ::= { coldfusion 17 }

    cfAvgDBTime OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "The average amount of process time spent by ColdFusion for a database requests."
        ::= { coldfusion 18 }

    cfCachePops OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "The number of cache pops performed by ColdFusion per seconds."
        ::= { coldfusion 19 }

-----------  ColdFusion Performance Information End----------------


    cfMaxRequests OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "Maximum number of concurrent requests ColdFusion may process."
        ::= { coldfusion 20 }


    cfLimitTime OBJECT-TYPE
        SYNTAX INTEGER {
                        no(0),
                        yes(1)
                        }
		ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "Indicator specifying whether ColdFusion should limit the time a request may take."
        ::= { coldfusion 21 }

    cfMaxSeconds OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "Maximum number of seconds a ColdFusion request may take."
        ::= { coldfusion 22 }

    cfTrustCache OBJECT-TYPE
        SYNTAX INTEGER {
                        no(0),
                        yes(1)
                        }
		ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "Indicator specifying whether ColdFusion trusts the content of the cache."
        ::= { coldfusion 23 }

    cfTemplateCacheSize OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "The size of the ColdFusion template cache."
        ::= { coldfusion 24 }

    cfRunningRDS OBJECT-TYPE
        SYNTAX INTEGER {
                        no(0),
                        yes(1)
                        }
		ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "Indicator specifying whether ColdFusion is running."
        ::= { coldfusion 25 }

	cfDebuggingOn OBJECT-TYPE
        SYNTAX INTEGER {
                        no(0),
                        yes(1)
                        }
		ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "Indicator specifying whether one of the debug settings in the ColdFusion Admin
			 Debug Settings page is turned on (Enable Performance Monitoring excluded)."
        ::= { coldfusion 26 }

    cfEnforceStrictAttributeValidation OBJECT-TYPE
        SYNTAX INTEGER {
                        no(0),
                        yes(1)
                        }
		ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "Indicator specifying whether ColdFusion strictly enforces attribute validation."
        ::= { coldfusion 27 }

    cfRestartThreshold OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "The maximum number of unresponsive requests allowed before restarting ColdFusion."
        ::= { coldfusion 28 }

    cfMaxCachedQueries OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "The maximum number of cached queries for ColdFusion."
        ::= { coldfusion 29 }

    cfMailServerName OBJECT-TYPE
        SYNTAX DisplayString (SIZE (1..128))
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "The name of the Mail Server used by ColdFusion."
        ::= { coldfusion 30 }

    cfMailServerPortNumber OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "The port number of the ColdFusion Mail Server."
        ::= { coldfusion 31 }

    cfMailServerConnectTimeout OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "The time ColdFusion should wait for a response from the mail server."
        ::= { coldfusion 32 }

