		dsucsuConnType INTEGER,
                dsucsuConnNumDs0s INTEGER (1..24),
                dsucsuConnSetStatus INTEGER,
                dsucsuConnConnect INTEGER,
                dsucsuConnUpdateRequired INTEGER,
                dsucsuConnUpdateCmd INTEGER,
                dsucsuConnClearEditBuff INTEGER
		}
					    
