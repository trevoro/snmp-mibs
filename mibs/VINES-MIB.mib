				}


vlogsFileIndex		OBJECT-TYPE
			SYNTAX  INTEGER {
				patch(1),
				backup(2)
			}
			ACCESS  read-only
			STATUS  mandatory
			DESCRIPTION
				"The index value into the vlogsTable."
			::= { vlogsEntry 1 }

vlogsDescr		OBJECT-TYPE
			SYNTAX  OCTET STRING
			ACCESS  read-only
			STATUS  mandatory
			DESCRIPTION
				"The record entry for the log."
			::= { vlogsEntry 2 }



