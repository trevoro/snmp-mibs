vrtpNonSeqTotOut  OBJECT-TYPE
	SYNTAX  Counter
	ACCESS  read-only
	STATUS  optional
	DESCRIPTION
		"The number of non-sequenced RTP packets sent."
	::= { vrtp 48 }

vrtpNonSeqErrsIn  OBJECT-TYPE
	SYNTAX  Counter
	ACCESS  read-only
	STATUS  optional
	DESCRIPTION
		"The number of errors while receiving non-sequenced RTP
		packets."
