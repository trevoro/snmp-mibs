mlMBTableMBOwner        OBJECT-TYPE
			SYNTAX  OCTET STRING
			ACCESS  read-only
			STATUS  mandatory
			DESCRIPTION
				"The StreetTalk name of the user to whom
				 this mailbox relates."

			::= { mlMailBoxTableEntry 2 }


mlMBTableUnreadMsgs     OBJECT-TYPE
			SYNTAX  INTEGER
			ACCESS  read-only
			STATUS  mandatory
			DESCRIPTION
				"The number of unread messages in this
				 mailbox."

			::= { mlMailBoxTableEntry 3 }


mlMBTableTotalMsgs      OBJECT-TYPE
			SYNTAX  INTEGER
			ACCESS  read-only
			STATUS  mandatory
			DESCRIPTION
				"The total number of messages in this
				 mailbox."

			::= { mlMailBoxTableEntry 4 }

-- logs group

vlogsTable	OBJECT-TYPE
		SYNTAX  SEQUENCE OF VlogsEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			"The Table containing entries for each log."
		::= { vlogs 1 }

vlogsEntry             OBJECT-TYPE
			SYNTAX  VlogsEntry
			ACCESS  not-accessible
			STATUS  mandatory
			DESCRIPTION
				"Entry into vlogsEntry."
			INDEX { vlogsFileIndex }
			::= { vlogsTable 1 }


VlogsEntry          ::= SEQUENCE {
				vlogsFileIndex
					INTEGER,
				vlogsDescr
