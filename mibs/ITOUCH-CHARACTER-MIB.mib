                                    FROM RFC1317-MIB;
            
            characterDep    OBJECT IDENTIFIER ::= { iTouch 2 }
            xCharacter      OBJECT IDENTIFIER ::= { iTouch 13 }
            
            -- ITouch Character subgroups
            
            xBasic          OBJECT IDENTIFIER ::= { xCharacter 1 }
            xQueue          OBJECT IDENTIFIER ::= { xCharacter 2 }
