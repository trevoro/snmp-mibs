--  Planned Configuration and Control Table

--  The table describing:
--      parameters used to configure each BOP/COP port.
--
--      parameters required to configure traps at BOP/COP port.
--
--      parameters used to effect specific and immediate actions at each 
--      BOP/COP port.


bopSapAdmTable OBJECT-TYPE
   SYNTAX  SEQUENCE OF BopSapAdmEntry
   ACCESS  not-accessible
   STATUS  mandatory
   DESCRIPTION
"A table containing the planned configuration information about each BOP/COP 
 port."
   ::= { cxBOPIO 2 }


   bopSapAdmEntry OBJECT-TYPE
      SYNTAX  BopSapAdmEntry
      ACCESS  not-accessible
      STATUS  mandatory
      DESCRIPTION
"The planned configuration information for a specific BOP/COP port."
      INDEX { bopSapAdmNumber }
      ::= { bopSapAdmTable 1 }


   BopSapAdmEntry ::=
      SEQUENCE {
         bopSapAdmNumber
            INTEGER,
         bopSapAdmAlias
            Alias,
         bopSapAdmProtocol
            INTEGER,
         bopSapAdmPortClockSource
            INTEGER,
         bopSapAdmPortSpeed
            INTEGER,
         bopSapAdmPortDuplex
            INTEGER,
         bopSapAdmPortMaxFrameSize
            INTEGER,
         bopSapAdmPortDataEncoding
            INTEGER,
         bopSapAdmPortIdleCondition
            INTEGER,
         bopSapAdmPortPreAmbleLength
            INTEGER,
   		 bopSapAdmPortFlagSharing
            INTEGER, 
         bopSapAdmPortFilterMask
            INTEGER,
         bopSapAdmPortFilterAddress1
            INTEGER,
         bopSapAdmPortFilterAddress2
            INTEGER,
         bopSapAdmPortFilterAddress3
