chanQosIsAAL5 OBJECT-TYPE
	SYNTAX	INTEGER {
			on(1),
			off(2)
		}
	MAX-ACCESS	read-only
	STATUS	current
	DESCRIPTION
	    "Indicates if this connection is AAL 5"
	::= { channelEntry 22 }

chanQosPerPacketPolicing OBJECT-TYPE
	SYNTAX  INTEGER {
			on(1),
			off(2)
		}
	MAX-ACCESS  read-only
	STATUS  current
	DESCRIPTION
