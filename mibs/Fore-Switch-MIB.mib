hwPortModel OBJECT-TYPE
	SYNTAX	INTEGER {
			model-TAXI-100(1),
			model-TAXI-140(2),
			model-DS3(3),
			model-OC3(4),
			model-8B10B(5),
			model-6port-TAXI-100(6),
			model-ASX-CTL(7),
			model-OC12(8),
			model-OC48(9),
			model-DS3-PDH(16),
			model-E3(17),
			model-J2(18),
			model-DS1(19),
			model-E1(20),
			model-TP25(21),
			model-ASX-BP(22),
			model-CESDS1(23),
			model-CESE1(24),
			model-FRAMDS1(25),
			model-FRAME1(26),
			model-IMADS1(27),
