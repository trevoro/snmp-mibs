		portAAL5PacketDiscardCBR INTEGER,
		portAAL5PacketDiscardVBR INTEGER,
		portAAL5PacketDiscardUBR INTEGER,
		portPPPolicingCBR       INTEGER,
		portPPPolicingVBR	INTEGER,
		portUBRTagging		INTEGER,
		portCACStatus		INTEGER,
