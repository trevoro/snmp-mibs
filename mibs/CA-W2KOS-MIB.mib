	SYNTAX INTEGER {
		disabled(1),
		poll-interval-only(2),
		poll-interval-and-query(3),
		query-only(4)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.378>If set to 'disabled', the agent no more retrieves the registry entry information (not by polling, nor by get request) and sets all statuses to ok. If set to 'poll-interval-only', the agent only retrieves the information by polling, but not by get request. If set to 'poll-interval-and-query', the agent retrieves the information in both cases (polling and get request). If set to 'query-only', the agent retrieves the information for get requests only."
	DEFVAL { 2 }
	::= { w2kConfigRegGroup 2 }


w2kConfigRegName OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.379>Name of the watcher to be added/removed, monitoring a registry entry with this name. The name is the complete registry path ending with the registry leaf name. If the leaf name contains a backslash, the leaf name must be quoted with double quotes. Important: This name is case insensitive."
	::= { w2kConfigRegGroup 3 }


w2kConfigRegDescription OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.136>Any description for this watcher."
	::= { w2kConfigRegGroup 4 }


w2kConfigRegAggLag OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.137>The number of consecutive poll intervals on which any status is not in the Ok or Up state, before the aggregate status changes. Valid values are greater than 0."
	DEFVAL { 1 }
	::= { w2kConfigRegGroup 5 }


w2kConfigRegExist OBJECT-TYPE
	SYNTAX INTEGER {
		should-exist(1),
		should-not-exist(2)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.382>Flag indicating whether to raise an alert if a registry entry is existent or if it is not-existent."
	DEFVAL { 1 }
	::= { w2kConfigRegGroup 6 }


w2kConfigRegExistMonitor OBJECT-TYPE
	SYNTAX INTEGER {
		do-not-monitor(1),
		down-warning(2),
		down-critical(3)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.383>Flag indicating whether the existence of the registry entry should be monitored and if so, the level of alert to raise."
	DEFVAL { 3 }
	::= { w2kConfigRegGroup 7 }


w2kConfigRegValueRef OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.384>This attribute is used as the reference value the registry entry is compared with. If the condition is one of 'should-be-equal', 'should-not-be-equal', 'should-be-below' or 'should-be-above', this reference value must be set by the user. In case of 'should-not-change', by setting the status button attribute to 'reset-value', the reference value is set to the current value of the registry entry."
	DEFVAL { "0" }
	::= { w2kConfigRegGroup 8 }


w2kConfigRegValueCond OBJECT-TYPE
	SYNTAX INTEGER {
		should-be-equal(1),
		should-not-be-equal(2),
		should-be-below(3),
		should-be-above(4),
		should-not-change(5)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.385>Flag indicating whether to raise an alert if a registry entry does not fullfill the given condition compared with the reference value attribute. 'should-be-below' and 'should-be-above' is only allowed for entries with type DWORD. For entries with type Binary 'should-not-change' is the only possible value."
	DEFVAL { 5 }
	::= { w2kConfigRegGroup 9 }


w2kConfigRegValueMonitor OBJECT-TYPE
	SYNTAX INTEGER {
		do-not-monitor(1),
		down-warning(2),
		down-critical(3)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.386>Flag indicating whether the value of the registry entry should be monitored and if so, the level of alert to raise."
	DEFVAL { 2 }
	::= { w2kConfigRegGroup 10 }


w2kConfigRegButton OBJECT-TYPE
	SYNTAX INTEGER {
		none(1),
		add(2),
		remove(3)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.226>This attribute is used to add or remove a watcher with the current value of the watcher name attribute in this group."
	::= { w2kConfigRegGroup 11 }




