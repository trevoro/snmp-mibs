-- *****************************************************************************
-- Network Interfaces Config Group
-- *****************************************************************************


w2kConfigNetPollInterval OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.361>The time (in seconds) between the polling of the network interface information. Valid values are greater than or equal to 30."
	DEFVAL { 120 }
	::= { w2kConfigNetGroup 1 }


w2kConfigNetPollMethod OBJECT-TYPE
	SYNTAX INTEGER {
		disabled(1),
		poll-interval-only(2),
		poll-interval-and-query(3),
		query-only(4)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.362>If set to 'disabled', the agent no more retrieves the network interface information (not by polling, nor by get request) and sets all statuses to ok. If set to 'poll-interval-only', the agent only retrieves the information by polling, but not by get request. If set to 'poll-interval-and-query', the agent retrieves the information in both cases (polling and get request). If set to 'query-only', the agent retrieves the information for get requests only."
	DEFVAL { 2 }
	::= { w2kConfigNetGroup 2 }


w2kConfigNetAggLag OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.137>The number of consecutive poll intervals on which any status is not in the Ok or Up state, before the aggregate status changes. Valid values are greater than 0."
	DEFVAL { 1 }
	::= { w2kConfigNetGroup 3 }


w2kConfigNetInPktWarn OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.364>Warning threshold for the rate of received packets over TCP/IP connections (packets per second). Valid values are greater than 0 and smaller than the critical threshold."
	DEFVAL { 200 }
	::= { w2kConfigNetGroup 4 }


w2kConfigNetInPktCrit OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.365>Critical threshold for the rate of received packets over TCP/IP connections (packets per second). Valid values are greater than 1 and greater than the warning threshold."
	DEFVAL { 400 }
	::= { w2kConfigNetGroup 5 }


w2kConfigNetInPktMonitor OBJECT-TYPE
	SYNTAX INTEGER {
		do-not-monitor(1),
		warning-only(2),
		critical-only(3),
		both(4)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.366>Flag indicating whether the received packets rate should be monitored and if so, which threshold should be compared with."
	DEFVAL { 4 }
	::= { w2kConfigNetGroup 6 }


w2kConfigNetOutPktWarn OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.367>Warning threshold for the rate of sent packets over TCP/IP connections (packets per second). Valid values are greater than 0 and smaller than the critical threshold."
	DEFVAL { 200 }
	::= { w2kConfigNetGroup 7 }


w2kConfigNetOutPktCrit OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.368>Critical threshold for the rate of sent packets over TCP/IP connections (packets per second). Valid values are greater than 1 and greater than the warning threshold."
	DEFVAL { 400 }
	::= { w2kConfigNetGroup 8 }


w2kConfigNetOutPktMonitor OBJECT-TYPE
	SYNTAX INTEGER {
		do-not-monitor(1),
		warning-only(2),
		critical-only(3),
		both(4)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.369>Flag indicating whether the sent packets rate should be monitored and if so, which threshold should be compared with."
	DEFVAL { 4 }
	::= { w2kConfigNetGroup 9 }


w2kConfigNetInErrDWarn OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.370>Warning threshold for the rate of received errors over TCP/IP connections (errors per poll interval). Valid values are greater than 0 and smaller than the critical threshold."
	DEFVAL { 20 }
	::= { w2kConfigNetGroup 10 }


w2kConfigNetInErrDCrit OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.371>Critical threshold for the rate of received errors over TCP/IP connections (errors per poll interval). Valid values are greater than 1 and greater than the warning threshold."
