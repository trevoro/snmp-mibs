	FLOWMGREXT-MIB

   --FORCE-INCLUDE <mib.h>
   --FORCE-INCLUDE <snmpdefs.h>
   --FORCE-INCLUDE <snmpstat.h>
   --FORCE-INCLUDE "mibproto.h"
 
	DEFINITIONS ::= BEGIN

	IMPORTS
	MODULE-IDENTITY, OBJECT-TYPE, Integer32, Counter64, IpAddress
			FROM SNMPv2-SMI
		DisplayString
			FROM SNMPv2-TC
		TRAP-TYPE
			FROM RFC-1215
		flowMgrExt
			FROM APENT-MIB;

--  flowMgrExt MIB

flowMgrExtMib MODULE-IDENTITY
	LAST-UPDATED "9707202000Z" -- Wed July 20 20:00:00 EST 1997
	ORGANIZATION "ArrowPoint Communications Inc."
	CONTACT-INFO
	"                       Ken Siegel
	Postal:               ArrowPoint Communications Inc.
				                 50 Nagog Park
				                 Acton, Massachusetts 01720
	   Tel:                   +1 978-206-3000  option 1
	   E-Mail:                support@arrowpoint.com"
	DESCRIPTION
		"This MIB module describes the ArrowPoint Communications
		enterprise MIB for the flow manager module."
	::= { flowMgrExt 1 }


-- apFlowMgrExt Scalars

apFlowMgrExtFlowTraceOptions OBJECT-TYPE
	SYNTAX DisplayString(SIZE( 1..10))
	MAX-ACCESS read-write
	STATUS current
	DESCRIPTION
		"This object specifies a bitmask of the current flow 
		trace settings."
	::= { flowMgrExt 2 }

apFlowMgrExtFlowTraceState OBJECT-TYPE
	SYNTAX INTEGER {
			enabled(1)                             ,
			disabled(2)                              
	}
	MAX-ACCESS read-write
	STATUS current
	DESCRIPTION
		"This object specifies the current state of flow tracing."
	::= { flowMgrExt 3 }

apFlowMgrExtFlowTraceFilename OBJECT-TYPE
	SYNTAX DisplayString( SIZE( 0..16 ) )
	MAX-ACCESS read-write
	STATUS current
	DESCRIPTION
		"This object specifies the file name of the log file on disk."
	::= { flowMgrExt 4 }

apFlowMgrExtFlowTraceMaxFileSize OBJECT-TYPE
	SYNTAX INTEGER( 1..100 )
	MAX-ACCESS read-write
	STATUS current
	DESCRIPTION
		"This object specifies the maximum file length allowed to be
		saved to the disk."
	DEFVAL { 100 }
	::= { flowMgrExt 5 }

apFlowMgrExtFlowTraceDuration OBJECT-TYPE
	SYNTAX INTEGER( 1..60 )
	MAX-ACCESS read-write
	STATUS current
	DESCRIPTION
		"This object specifies the duration of the Flow Tracing logging
		session in seconds."
	DEFVAL { 30 }
	::= { flowMgrExt 6 }

apFlowMgrExtPermanentPort1 OBJECT-TYPE
	SYNTAX INTEGER( 0..65535 )
	MAX-ACCESS read-write
	STATUS current
	DESCRIPTION
		"This object specifies the first destination tcp port number
		for flows that the flow manager will not reclaim."
	DEFVAL { 0 }
	::= { flowMgrExt 7 }

apFlowMgrExtPermanentPort2 OBJECT-TYPE
	SYNTAX INTEGER( 0..65535 )
	MAX-ACCESS read-write
	STATUS current
	DESCRIPTION
		"This object specifies the second destination tcp port number
		for flows that the flow manager will not reclaim."
	DEFVAL { 0 }
	::= { flowMgrExt 8 }

apFlowMgrExtPermanentPort3 OBJECT-TYPE
	SYNTAX INTEGER( 0..65535 )
	MAX-ACCESS read-write
	STATUS current
	DESCRIPTION
		"This object specifies the third destination tcp port number
		for flows that the flow manager will not reclaim."
	DEFVAL { 0 }
	::= { flowMgrExt 9 }

apFlowMgrExtPermanentPort4 OBJECT-TYPE
	SYNTAX INTEGER( 0..65535 )
	MAX-ACCESS read-write
	STATUS current
	DESCRIPTION
		"This object specifies the forth destination tcp port number
		for flows that the flow manager will not reclaim."
	DEFVAL { 0 }
	::= { flowMgrExt 10 }

apFlowMgrExtReserveCleanTimer OBJECT-TYPE
	SYNTAX INTEGER( 0..100 )
	MAX-ACCESS read-write
	STATUS current
	DESCRIPTION
		"This object specifies the time (in seconds) at which the 
         flow manager scans flows with a source or destination port
         of less than 25 and may attempt to clean up that flow."
	DEFVAL { 10 }
	::= { flowMgrExt 11 }

apFlowMgrExtPortIdleTable OBJECT-TYPE
	SYNTAX SEQUENCE OF APFlowMgrExtPortIdleEntry
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION
		"This is the table of port idle timer values."
	::= { flowMgrExt 12 }

apFlowMgrExtPortIdleEntry OBJECT-TYPE
    SYNTAX  APFlowMgrExtPortIdleEntry
    MAX-ACCESS  not-accessible
    STATUS  current
    DESCRIPTION
            "A table containing the port number and associated
             idle timer."
    INDEX  { apFlowMgrExtPortIdle }
    ::= { apFlowMgrExtPortIdleTable 1 } 

APFlowMgrExtPortIdleEntry ::=
    SEQUENCE {
		apFlowMgrExtPortIdle
			INTEGER,
		apFlowMgrExtPortIdleValue
			INTEGER,
		apFlowMgrExtPortLowWaterFCB
			INTEGER
		}

apFlowMgrExtPortIdle OBJECT-TYPE
	SYNTAX  INTEGER
	MAX-ACCESS  read-only
	STATUS  current
	DESCRIPTION
		"This is the port number of the port for which this entry
         contains the flow manager port configuration."
    ::= { apFlowMgrExtPortIdleEntry 2 } 

apFlowMgrExtPortIdleValue OBJECT-TYPE
	SYNTAX INTEGER ( 0..65535 )
	MAX-ACCESS  read-write
	STATUS  current
	DESCRIPTION
		"This is the flow manager reset idle value for this port."
    DEFVAL { 0 } 
    ::= { apFlowMgrExtPortIdleEntry 3 } 

apFlowMgrExtPortLowWaterFCB OBJECT-TYPE
	SYNTAX INTEGER ( 1..100 )
	MAX-ACCESS  read-write
	STATUS  current
	DESCRIPTION
		"This is the configured flow manager FCB low water mark 
		 for this port.  When free FCBs constitute this percentage
         of total FCBs, expedited flow recovery procedures are invoked."
    DEFVAL { 25 } 
    ::= { apFlowMgrExtPortIdleEntry 4 } 

apFlowMgrExtIdleTimer OBJECT-TYPE
	SYNTAX INTEGER( 0..600 )
	MAX-ACCESS read-write
	STATUS current
	DESCRIPTION
		"This object specifies the time (in seconds) which the 
         flow manager will use to time out flows. A value of 0
         indicates that internal values will be used."
	DEFVAL { 1 }
	::= { flowMgrExt 13 }


