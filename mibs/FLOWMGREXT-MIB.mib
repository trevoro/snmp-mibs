			apFlowMgrStatIfTCPAvg               Integer32,
			apFlowMgrStatIfCurrentActiveTCP		Integer32,
			apFlowMgrStatIfCurrentActiveUDP		Integer32,
			apFlowMgrStatIfAccountingReportsRx	Integer32,
			apFlowMgrStatIfResets				Integer32,
			apFlowMgrStatIfFcbCount				Integer32
		}

apFlowMgrStatIfIndex OBJECT-TYPE
