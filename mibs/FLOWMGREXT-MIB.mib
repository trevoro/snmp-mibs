-- ********************** apFlowMgrExtDOSAttackTable BEGIN *****************

apFlowMgrExtDOSAttackTable OBJECT-TYPE
	SYNTAX	SEQUENCE OF APFlowMgrExtDOSAttackEntry
	MAX-ACCESS  not-accessible
	STATUS	current
	DESCRIPTION
		"This is the table of Denial of Service (DOS) attack
         events."
	::= { flowMgrExt 28 }

apFlowMgrExtDOSAttackEntry OBJECT-TYPE
    SYNTAX  APFlowMgrExtDOSAttackEntry
    MAX-ACCESS  not-accessible
    STATUS  current
    DESCRIPTION
		"This is the table of Denial of Service (DOS) attack
         events."
    INDEX  { apFlowMgrExtDOSAttackIndex }
    ::= { apFlowMgrExtDOSAttackTable 1 } 

APFlowMgrExtDOSAttackEntry ::=
    SEQUENCE {
		apFlowMgrExtDOSAttackIndex
			INTEGER,
        apFlowMgrExtDOSAttackSrcAddress
            IpAddress,
        apFlowMgrExtDOSAttackDestAddress
            IpAddress,
        apFlowMgrExtDOSAttackFirstEvent
            DisplayString,
        apFlowMgrExtDOSAttackLastEvent
            DisplayString,
        apFlowMgrExtDOSAttackEventCount
            Integer32,
        apFlowMgrExtDOSAttackEventType
            INTEGER,
        apFlowMgrExtDOSAttackEventString
            DisplayString
		}
 
apFlowMgrExtDOSAttackIndex OBJECT-TYPE
	SYNTAX  INTEGER
	MAX-ACCESS  read-only
	STATUS  current
	DESCRIPTION
		"This is the index for the DOS event."
    ::= { apFlowMgrExtDOSAttackEntry 1 } 

apFlowMgrExtDOSAttackSrcAddress OBJECT-TYPE
    SYNTAX IpAddress
	MAX-ACCESS  read-only
	STATUS  current
	DESCRIPTION
		"This is the source ip address for this
         DOS attack event."
    ::= { apFlowMgrExtDOSAttackEntry 2 } 

apFlowMgrExtDOSAttackDestAddress OBJECT-TYPE
    SYNTAX IpAddress
	MAX-ACCESS  read-only
	STATUS  current
	DESCRIPTION
		"This is the destination ip address for this
         DOS attack event."
    ::= { apFlowMgrExtDOSAttackEntry 3 } 

apFlowMgrExtDOSAttackFirstEvent OBJECT-TYPE
    SYNTAX DisplayString
	MAX-ACCESS  read-only
	STATUS  current
	DESCRIPTION
		"This is the first time this DOS attack occurred."
    ::= { apFlowMgrExtDOSAttackEntry 4 } 


apFlowMgrExtDOSAttackLastEvent OBJECT-TYPE
    SYNTAX DisplayString
	MAX-ACCESS  read-only
	STATUS  current
	DESCRIPTION
		"This is the last time this DOS attack occurred."
    ::= { apFlowMgrExtDOSAttackEntry 5 } 

apFlowMgrExtDOSAttackEventCount OBJECT-TYPE
    SYNTAX Integer32
	MAX-ACCESS  read-only
	STATUS  current
	DESCRIPTION
		"This is the number of times this DOS attack
         had occurred."
    ::= { apFlowMgrExtDOSAttackEntry 6 } 

apFlowMgrExtDOSAttackEventType OBJECT-TYPE
    SYNTAX INTEGER {
             syn(1),
             land(2),
             zeroport(3),
             illegalsrc(4),
             pingofdeath(5),
             smurf(6),
             illegaldst(7)
           }
	MAX-ACCESS  read-only
	STATUS  current
	DESCRIPTION
		"This is the type of DOS attack which has occurred."
    ::= { apFlowMgrExtDOSAttackEntry 7 } 

apFlowMgrExtDOSAttackEventString  OBJECT-TYPE
    SYNTAX DisplayString( SIZE( 0..16 ) )
	MAX-ACCESS  read-only
	STATUS  current
	DESCRIPTION
		"This is the ascii text for the type of DOS attack 
         which has occurred."
    ::= { apFlowMgrExtDOSAttackEntry 8 } 

-- ********************** apFlowMgrExtDOSAttackEntry END ********************

