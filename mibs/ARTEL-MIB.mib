          SYNTAX  INTEGER {
		     enabled(1),
		     disabled(2)
		  }
	  ACCESS  read-write
          STATUS  mandatory
          DESCRIPTION
	     "Administratively enables and disables the receipt of
	     broadcasts on this port." 
	  ::= { sbPortEntry 8 }



	sbPortName  OBJECT-TYPE
		SYNTAX  DisplayString (SIZE (0..255))
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
