	    STATUS  mandatory
	    ::= { kip 1 }

    kipEntry OBJECT-TYPE
	    SYNTAX  KipEntry
	    ACCESS  read-write
	    STATUS  mandatory
	    ::= { kipTable 1 }

    KipEntry ::= SEQUENCE {
	    kipNet		OCTET STRING,
	    kipNextHop		IpAddress,
	    kipHopCount		INTEGER,
	    kipBCastAddr	IpAddress,
	    kipCore		INTEGER,
	    kipKfps		INTEGER
    }

