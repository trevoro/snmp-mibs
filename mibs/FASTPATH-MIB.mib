	    atifIndex		INTEGER,
	    atifDescr		OCTET STRING,
	    atifType		INTEGER,
	    atifNetStart	OCTET STRING,
	    atifNetEnd		OCTET STRING,
	    atifNetAddress	OCTET STRING,
	    atifStatus		INTEGER,
	    atifNetConfig	INTEGER,
	    atifZoneConfig	INTEGER,
	    atifZone		OCTET STRING,
	    atifIfIndex		INTEGER
    }

