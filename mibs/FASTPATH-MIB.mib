    rtmpTable OBJECT-TYPE
	    SYNTAX  SEQUENCE OF RtmpEntry
	    ACCESS  read-write
	    STATUS  mandatory
	    ::= { rtmp 1 }

    rtmpEntry OBJECT-TYPE
	    SYNTAX  RtmpEntry
	    ACCESS  read-write
	    STATUS  mandatory
	    ::= { rtmpTable 1 }

    RtmpEntry ::= SEQUENCE {
	    rtmpRangeStart	OCTET STRING,
	    rtmpRangeEnd	OCTET STRING,
	    rtmpNextHop		OCTET STRING,
	    rtmpInterface	INTEGER,
	    rtmpHops		INTEGER,
	    rtmpState		INTEGER
    }

    rtmpRangeStart OBJECT-TYPE
	    SYNTAX  OCTET STRING
	    ACCESS  read-write
	    STATUS  mandatory
	    ::= { rtmpEntry 1 }

    rtmpRangeEnd OBJECT-TYPE
	    SYNTAX  OCTET STRING
	    ACCESS  read-write
	    STATUS  mandatory
	    ::= { rtmpEntry 2 }

