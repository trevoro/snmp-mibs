    AarpEntry ::= SEQUENCE {
	    aarpIfIndex	INTEGER,
	    aarpPhysAddress	OCTET STRING,
	    aarpNetAddress	OCTET STRING
    }

    aarpIfIndex OBJECT-TYPE
	    SYNTAX  INTEGER
	    ACCESS  read-only
