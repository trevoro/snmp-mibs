	    SYNTAX  OCTET STRING
	    ACCESS  read-write
	    STATUS  mandatory
	    ::= { kipEntry 1 }

    kipNextHop OBJECT-TYPE
	    SYNTAX  IpAddress
	    ACCESS  read-write
	    STATUS  mandatory
	    ::= { kipEntry 2 }

    kipHopCount OBJECT-TYPE
	    SYNTAX  INTEGER
	    ACCESS  read-write
	    STATUS  mandatory
	    ::= { kipEntry 3 }

    kipBCastAddr OBJECT-TYPE
	    SYNTAX  IpAddress
	    ACCESS  read-write
	    STATUS  mandatory
	    ::= { kipEntry 4 }

    kipCore OBJECT-TYPE
	    SYNTAX  INTEGER {
		core(1),
		notcore(2)
	    }
	    ACCESS  read-write
	    STATUS  mandatory
	    ::= { kipEntry 5 }

    kipKfps OBJECT-TYPE
	    SYNTAX  INTEGER
	    ACCESS  read-write
	    STATUS  mandatory
	    ::= { kipEntry 6 }

