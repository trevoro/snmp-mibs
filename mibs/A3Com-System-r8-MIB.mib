               ::= { a3ComFW 2 }

           a3sysFwSelfTest OBJECT-TYPE
               SYNTAX  INTEGER {
                    skip              (1),
                    testOnce          (2),
                    other             (3),
                    loop              (4)
               } 
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
                       "This parameter determines how the system handles
