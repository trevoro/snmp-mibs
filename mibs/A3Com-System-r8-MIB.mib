           a3sysFwSerBaudAux OBJECT-TYPE
               SYNTAX  INTEGER {
                    unknown  (1),
                    bps110   (2),
                    bps300   (3),
                    bps600   (4),
                    bps1200  (5),
                    bps2400  (6),
