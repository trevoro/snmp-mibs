-- This MIB describes objects, to monitor BS2000 systems via SNMP.
-- These objects are realized by a subagent within the BS2000 system.
-- The subagent assumes that the subsystem SM2 is running always.
-- This MIB has two parts:
--    - sniSM2Parms:   static informations
--    - sniSM2Mpoints: samples structured by SM2 buffers


SNIBS2PERF-MIB DEFINITIONS ::= BEGIN

-- DateAndTime ::= OCTET STRING (SIZE (8 | 11))
--        Definition copied from RFC 1514
--        A date-time specification for the local time of day.
--        This data type is intended to provide a consistent
--        method of reporting date information.
--
--            field  octets  contents                  range
--            =====  ======  ========                  =====
--              1      1-2   year                      0..65536
--                           (in network byte order)
--              2       3    month                     1..12
--              3       4    day                       1..31
--              4       5    hour                      0..23
--              5       6    minutes                   0..59
--              6       7    seconds                   0..60
--                           (use 60 for leap-second)
--              7       8    deci-seconds              0..9
--              8       9    direction from UTC        "+" / "-"
--                           (in ascii notation)
--              9      10    hours from UTC            0..11
--             10      11    minutes from UTC          0..59
--
--            Note that if only local time is known, then
--            timezone information (fields 8-10) is not present.
--
--
--
IMPORTS
		DateAndTime, DisplayString FROM SNMPv2-TC
	OBJECT-TYPE, enterprises FROM RFC1155-SMI;

sni                  OBJECT IDENTIFIER ::= { enterprises 231 }
sniProductMibs       OBJECT IDENTIFIER ::= { sni 2 }
sniSar               OBJECT IDENTIFIER ::= { sniProductMibs  13 }
sniSarParms          OBJECT IDENTIFIER ::= { sniSar 1 }
sniSarMpoints        OBJECT IDENTIFIER ::= { sniSar 2 }
sniSarHist           OBJECT IDENTIFIER ::= { sniSar 3 }

-- reserved set of OID's { sniSar.x | x = 10,11, ..,19 } for performance
-- MIB of BS2000

sniSM2Parms          OBJECT IDENTIFIER ::= { sniSar 10 }
sniSM2Mpoints        OBJECT IDENTIFIER ::= { sniSar 11 }
-- sniSM2Hist           OBJECT IDENTIFIER ::= { sniSar 12 }

--
--
-- BS2000 specific definitions
--
--

sm2Status OBJECT-TYPE
	SYNTAX  INTEGER
		{
		    running     (1),  -- subsystem SM2 running
		    not-running (2),  -- subsystem SM2 not running
		    unkown      (3)   -- program interface error
		     -- not-running (1), subsystem SM2 not running
		     -- created (2),     subsystem SM2 created
		     -- in-hold (3),     subsystem SM2 in hold/held
		     -- in-delete (4),   subsystem SM2 in delete/deleted
		     -- down (5),        SM2G task down
		     -- unkown (6)       program interface error
		}
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	     "Status of the measurement subsystem SM2.
	      The subsystem SM2 is assumed running always."
	::= { sniSM2Parms 1 }

sm2Version OBJECT-TYPE
	SYNTAX  DisplayString (SIZE (8))
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	     "Version of the measurement subsystem SM2: format Vnn.0Ann"
	::= { sniSM2Parms 2 }

sm2Interval OBJECT-TYPE
	SYNTAX  INTEGER (10..3600)
		-- a negative value (-1) stands for:
		--   at the moment no data available
		--   or error conditions have occured
	ACCESS  read-write
	STATUS  mandatory
	DESCRIPTION
	     "online cycle of the measurement subsystem SM2 in units of a
	      second, also called measurement interval (default value: 120).
	      Valid integer range for setting: 10..3600 "
	::= { sniSM2Parms 3 }

sm2SamplingCycle OBJECT-TYPE
	SYNTAX  INTEGER (200..10000)
		-- a negative value (-1) stands for:
		--   at the moment no data available
		--   or error conditions have occured
	ACCESS  read-write
	STATUS  mandatory
	DESCRIPTION
	     "sampling cycle of the measurement subsystem SM2
	      in units of a millisecond (default value: 800).
	      Valid integer range for setting: 200..10000 "
	::= { sniSM2Parms 4 }

