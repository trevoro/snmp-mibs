	::= { cxMcPrtBsyEntry 8 }

cxMcPrtBsyUseRtsDcdCndRem         OBJECT-TYPE
	SYNTAX INTEGER { no (1), yes (2) }
	ACCESS  read-write
	STATUS  mandatory
	DESCRIPTION
	"Use the state of the RTS or DCD modem control when the port 
	selected is conditioned by the remote DCD or RTS."
