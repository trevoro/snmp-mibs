	-----------------------------------------------------------------------
	-- Star CES Config Table
	-----------------------------------------------------------------------
	starDS1E1CESConfTable 	OBJECT-TYPE
 		SYNTAX  SEQUENCE OF StarDS1E1CESConfEntry
 		ACCESS  not-accessible
 		STATUS  mandatory
 		DESCRIPTION
			"The CES configuration table. This includes mapping channels
	 		 from ATM Port to CBR interfaces. There is one AtmfDS1E1CESConfEntry
	 		 per CES Entity"
 		::= { starCesInfo 1 }

	starDS1E1CESConfEntry	 OBJECT-TYPE
 		SYNTAX  StarDS1E1CESConfEntry
 		ACCESS  not-accessible
 		STATUS  mandatory
 		DESCRIPTION
		"An entry in the CES table. For each entry there is a corresponding
		 entry in the stack table"
 		INDEX   { starDS1E1CESSlotId, starDS1E1CESPortId,
				  starDS1E1CESChannelId }
 		::= { starDS1E1CESConfTable 1 }

	StarDS1E1CESConfEntry ::=
		SEQUENCE {
			starDS1E1CESSlotId				INTEGER,
			starDS1E1CESPortId	 			INTEGER,
			starDS1E1CESChannelId			INTEGER,
			starDS1E1CESMapATMIndex			INTEGER,
			starDS1E1CESMapVPI				INTEGER,
			starDS1E1CESMapVCI				INTEGER,
			starDS1E1CESCBRService			INTEGER,
			starDS1E1CESCBRClockMode		INTEGER,
			starDS1E1CESCas					INTEGER,
			starDS1E1CESPartialFill			INTEGER,
			starDS1E1CESBufMaxSize			INTEGER,
			starDS1E1CESCDVRxT				INTEGER,
			starDS1E1CESCellLossIntegPrd	INTEGER
 		}
	
	starDS1E1CESSlotId	OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object indicates the slot id."
		::= { starDS1E1CESConfEntry 1 }
 
	 starDS1E1CESPortId		 OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object indicates the port number
			 whose info is needed."
		::= { starDS1E1CESConfEntry  2 }
 
	starDS1E1CESChannelId	OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object indicates the channel number
			 whose info is needed."
		::= { starDS1E1CESConfEntry  3 }

 	starDS1E1CESMapATMIndex	 OBJECT-TYPE
	 	SYNTAX  INTEGER
	 	ACCESS  read-only
	 	STATUS  mandatory
	 	DESCRIPTION
		 	"The value of this object is equal to MIB II's ifIndex
			 value of the ATM Port interface mapped through this 
			 CES to a CBR interface."
	 	::= { starDS1E1CESConfEntry   4 }

 	starDS1E1CESMapVPI	OBJECT-TYPE
	 	SYNTAX  INTEGER
	 	ACCESS  read-only
	 	STATUS  mandatory 
	 	DESCRIPTION
		 	"The value of this object is equal to the VPI used 
			for the emulated circuit represented by  this entry
		 	in the ifTable. If there is no connection, this
		 	object is meaningless and will have the value zero."
	 	::= { starDS1E1CESConfEntry 5 }

 	starDS1E1CESMapVCI	OBJECT-TYPE
	 	SYNTAX  INTEGER
	 	ACCESS  read-only
	 	STATUS  mandatory
	 	DESCRIPTION
		 	"The value of this object is equal to the VCI used 
			for the emulated circuit represented by  this entry
			in the ifTable. If there is no connection, this
			object is meaningless and will have the value zero."
	 	::= { starDS1E1CESConfEntry 6 }

	starDS1E1CESCBRService	OBJECT-TYPE
	 	SYNTAX  INTEGER {
			unstructured ( 1),
			structured ( 2)
		}
	 	ACCESS  read-write
	 	STATUS  mandatory
	 	DESCRIPTION
			"Define if DS1/E1 service as structured or not. A
			structured (2) interface is some n * 64kbps. An unstrucutred (1)
			interface is 1.544Mbps or 2.048Mbps. Unstructured(1) passes all
			bits through the ATM network. Structured(2) passes data bits through
			the ATM network, an may also pass signalling bits."
	 	::= { starDS1E1CESConfEntry 7 }

 	starDS1E1CESCBRClockMode	OBJECT-TYPE
	 	SYNTAX  INTEGER {
			synchronous ( 1),
			srts ( 2),
			adaptive ( 3)
		}
	 	ACCESS  read-write
	 	STATUS  mandatory
	 	DESCRIPTION
			"Define if DS1/E1 service clocking mode. This maps into transmit
	 	 	clock source of CBR interface."
	 	::= { starDS1E1CESConfEntry 8 }

 	starDS1E1CESCas	OBJECT-TYPE
	 	SYNTAX  INTEGER {
			basic (1),
			e1Cas (2),
			ds1SfCas (3),
			ds1EsfCas (4)
		}
	 	ACCESS  read-write
	 	STATUS  mandatory
	 	DESCRIPTION
			"This parameter selects which AAL1 format should be used
			:- Basic does not carry CAS bits, and uses a single 125 usec
			frame. e1Cas,ds1SfCas and ds1EsfCas carry CAS bits in a multi
			frame structure for E1,DS1 SF and DS1 ESF respectively. This
			applies to structured interfaces only.Default is basic (1)."
	 	::= { starDS1E1CESConfEntry 9 }

 	starDS1E1CESPartialFill	OBJECT-TYPE
	 	SYNTAX  INTEGER ( 0..47)
	 	ACCESS  read-write
	 	STATUS  mandatory
	 	DESCRIPTION
		""
	 	::= { starDS1E1CESConfEntry  10 }

	starDS1E1CESBufMaxSize	OBJECT-TYPE
	 	SYNTAX  INTEGER ( 1..65536 )
	 	ACCESS  read-write
	 	STATUS  mandatory
	 	DESCRIPTION
		 	""
	 	::= { starDS1E1CESConfEntry  11 }

 	starDS1E1CESCDVRxT	OBJECT-TYPE
	 	SYNTAX  INTEGER ( 1..65535 )
	 	ACCESS  read-write
	 	STATUS  mandatory
	 	DESCRIPTION
		 	""
	 	::= { starDS1E1CESConfEntry  12 }

	starDS1E1CESCellLossIntegPrd	OBJECT-TYPE
