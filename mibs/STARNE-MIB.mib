STARNE-MIB DEFINITIONS ::= BEGIN

	IMPORTS
		IpAddress, enterprises, TimeTicks, Counter, Gauge 
			FROM RFC1155-SMI
		OBJECT-TYPE	
			FROM RFC-1212
		DisplayString	
			FROM RFC1213-MIB;

	-- Star Network Element Proprietary MIB definition

	samsung			OBJECT IDENTIFIER ::= { enterprises 236 }

	starNeMib		OBJECT IDENTIFIER ::= { samsung 5 }

	starSystem		OBJECT IDENTIFIER ::= { starNeMib 1 }
	starSlot		OBJECT IDENTIFIER ::= { starNeMib 2 }
	starPort		OBJECT IDENTIFIER ::= { starNeMib 3 }
	starConn		OBJECT IDENTIFIER ::= { starNeMib 4 }
	starBackbone	OBJECT IDENTIFIER ::= { starNeMib 5 }
	starTrap		OBJECT IDENTIFIER ::= { starNeMib 6 }

	----------------------------------------------------------------------
	-- System Generic Group
	----------------------------------------------------------------------
	starSysGeneric	OBJECT IDENTIFIER ::= { starSystem 1 }

	starNeId	OBJECT-TYPE
		SYNTAX	INTEGER (1..1023)
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Star Node Identifier"
		::= { starSysGeneric 1 }

	starNeDescr	OBJECT-TYPE
		SYNTAX	DisplayString
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Star Node Description"
		::= { starSysGeneric 2 }

	starNeMipAddress	OBJECT-TYPE
		SYNTAX	IpAddress
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"IP address that is used for inter-slot communication"
		::= { starSysGeneric 3 }

	starNeTimeofDay		OBJECT-TYPE
		SYNTAX	TimeTicks
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			"Time of day in seconds since the epoch"
		::= { starSysGeneric 4 }

	starNeOperStatus	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Operational status of this Node"
		::= { starSysGeneric 5 }

	starNeReset	OBJECT-TYPE
		SYNTAX	INTEGER {
			notreset(1),
			reset(2)
		}
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			"This object can be used to reset this Node"
		::= { starSysGeneric 6 }

	starNeSlotReset	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	write-only
		STATUS	mandatory
		DESCRIPTION
			"This object can be used to reset a slot"
		::= { starSysGeneric 7 }

	starNeSwDownload	OBJECT-TYPE
--		SYNTAX	INTEGER {
--			notdownloaded(1),
--			downloaded(2)
--		}
--		SYNTAX	OCTET STRING
-- STAda02930 HYOUNG 2000.9.22
		SYNTAX  DisplayString
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			"This object can be used to initiate a software 
			 download"
		::= { starSysGeneric 8 }

	starNeHwVersion	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Specifies the hardware version for this Node"
		::= { starSysGeneric 9 }

	starNeBootSwVersion	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Specifies the boot software version for this Node"
		::= { starSysGeneric 10 }

	starNeSwVersion	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Specifies the running software version for this Node"
		::= { starSysGeneric 11 }

	starNeXCoordinate	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			"Specifies the geographic latitudinal coordinate for 
			 this Node.  Must be initially set by the NMS operator"
		::= { starSysGeneric 12 }

	starNeYCoordinate	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			"Specifies the geographic longitudinal coordinate for 
			 this Node.  Must be initially set by the NMS operator"
		::= { starSysGeneric 13 }

	starNeNodeManagerIP	OBJECT-TYPE
		SYNTAX	IpAddress
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Specifies the IP address for the Node Manager"
		::= { starSysGeneric 14 }

	starNeMasterManagerIP	OBJECT-TYPE
		SYNTAX	IpAddress
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Specifies the IP address for the Master Manager"
		::= { starSysGeneric 15 }

	starNeStandbyMasterManagerIP	OBJECT-TYPE
		SYNTAX	IpAddress
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Specifies the IP address for the standby Master 
			 Manager"
		::= { starSysGeneric 16 }

	starNeAlive	OBJECT-TYPE
		SYNTAX	INTEGER {
			onespu(1),
			twospu(2)
		}
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Star Node Alive Value"
		::= { starSysGeneric 17 }

	starMibRegister OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	write-only
		STATUS	mandatory
		DESCRIPTION
			"The set value will be SLOT #. If a slot # is set,
			his MIB will be registered."
		::= { starSysGeneric 18 }

	starNeCurrentAlarmTftp OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	write-only
		STATUS	mandatory
		DESCRIPTION
			""
		::= { starSysGeneric 19 }

	starNeCurrentTimeDescr OBJECT-TYPE
		SYNTAX	DisplayString
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			"Time & date in string for NMS easy-looking
			19980601151000 when set
			1998.06.01 15:10:00 when get"
		::= { starSysGeneric 20 }

	-- added by sunah 99.8.13 --
    starNeSoftwareVersion OBJECT-TYPE
        SYNTAX  DisplayString
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
            "The Version of STAR Software"
        ::= { starSysGeneric 22 }
 
    starNeSoftwareType OBJECT-TYPE
        SYNTAX  INTEGER{
                star1(1)
        }
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
            "This value means the type of STAR Software"
        ::= { starSysGeneric 23 }
 
    starNeSwUpgradeDownload OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  write-only
        STATUS  mandatory
        DESCRIPTION
            "If set, start download files to be upgraded"
        ::= { starSysGeneric 24 }
 
    starNeSwUpgradeCancel OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  write-only
        STATUS  mandatory
        DESCRIPTION
            "If set, cancel downloading files to be upgraded"
        ::= { starSysGeneric 25 }
 
    starNeSwUpgradeActionTrigger OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  write-only
        STATUS  mandatory
        DESCRIPTION
            "If set, trigger the renaming of *.NEW to *.Z or *.CFG and reset"
        ::= { starSysGeneric 26 }
 
    starNeSwUpgradedFiles OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
            "The value of the downloaded files by this time for upgrading"
        ::= { starSysGeneric 27 }
 
    starNeSwUpgradeCurrentFile OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
            "The value of the currently downloading file for upgrading"
        ::= { starSysGeneric 28 }
 
    starNeSwUpgradeStatus OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
            "The value of the status for upgrading"
        ::= { starSysGeneric 29 }
 

	-----------------------------------------------------------------------
	-- Star System Slot Redundency Info Group
	-----------------------------------------------------------------------
	starSysSlotRedundencyInfo	OBJECT IDENTIFIER ::= { starSystem 2 }

	starActiveSPUSlotNo	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS  read-only		
		STATUS	mandatory
		DESCRIPTION
			"Slot # of active SPU"
		::= { starSysSlotRedundencyInfo 1 }

	starStandbySPUSlotNo	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS  read-only		
		STATUS	mandatory
		DESCRIPTION
			"Slot # of standby SPU"
		::= { starSysSlotRedundencyInfo 2 }

	starActiveSCUSlotNo	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS  read-only		
		STATUS	mandatory
		DESCRIPTION 
			"Slot # of active SCU"
		::= { starSysSlotRedundencyInfo 3 }

	starStandbySCUSlotNo	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS  read-only		
		STATUS	mandatory
		DESCRIPTION 
			"Slot # of standby SSU"
		::= { starSysSlotRedundencyInfo 4 }

	starActiveSSUSlotNo	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS  read-only		
		STATUS	mandatory
		DESCRIPTION 
			"Slot # of active SSU"
		::= { starSysSlotRedundencyInfo 5 }

	starStandbySSUSlotNo	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS  read-only		
		STATUS	mandatory
		DESCRIPTION 
			"Slot # of standby SSU"
		::= { starSysSlotRedundencyInfo 6 }

	-----------------------------------------------------------------------
	-- Star System Config Manager Group
	-----------------------------------------------------------------------
	starConfigManager	OBJECT IDENTIFIER ::= { starSystem 3 }

	starCfAllChanged	OBJECT-TYPE
		SYNTAX	INTEGER {
			notchanged(1),
			changed(2)
		}
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			"This object can be used to inform the Configuration
			 Manager that all configuration files have changed and
			 they should be re-loaded"
		::= { starConfigManager 1 }

	starCfFileChanged	OBJECT-TYPE
--		SYNTAX	OCTET STRING
-- STAda02930 HYOUNG 2000.9.22
		SYNTAX  DisplayString
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			"This object can be used to inform the Configuration
			 Manager that one particular configuration file has 
			 been changed and should be re-loaded
			 Flag + FileId + IP address"
		::= { starConfigManager 2 }

	starCfBootFileName	OBJECT-TYPE
		SYNTAX	DisplayString
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Specifies the filename of the Star Node boot image"
		::= { starConfigManager 3 }

	starCfShelf	OBJECT-TYPE
		SYNTAX	INTEGER {
			lower(1),
			upper(2)
		}
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			"Specifies the Star shelf location within the rack.
			 This object must be set initially by the NMS 
			 operator"
		::= { starConfigManager 4 }

	starCfMaxSlots	OBJECT-TYPE
		SYNTAX	INTEGER (1..14)
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Specifies the maximum number of slots in the Star 
			 chassis"
		::= { starConfigManager 5 }

	starCfBootSwitch	OBJECT-TYPE
		SYNTAX	INTEGER {
			flash(1),
			nodemgr(2),
			mastermgr(3)
		}
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			"This object is used to specify the download source
			 at bootup time"
		::= { starConfigManager 6 }

	starCfInvalidateFlash	OBJECT-TYPE
		SYNTAX	INTEGER {
			valid(1),
			invalid(2)
		}
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			"This object is used to inform the Star software at
			 bootup time to boot using the NMS, flash memory does
			 not contain valid information anymore"
		::= { starConfigManager 7 }

	starCfActivePSUNo	OBJECT-TYPE
		SYNTAX	INTEGER (1..4)
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Number of current active power supply units"
		::= { starConfigManager 8 }

	starCfPSUOperStatus	OBJECT-TYPE
		SYNTAX	INTEGER {
			psu1on(1),
			psu1installed(2),
			psu2on(4),
			psu2installed(8),
			psu3on(16),
			psu3installed(32),
			psu4on(64),
			psu4installed(128)
		}
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Operational and installed status of power supply units"
		::= { starConfigManager 9 }

	starCfFanStatus	OBJECT-TYPE
		SYNTAX	INTEGER {
				fan1on(1),
				fan2on(2),
				fan3on(4),
				fan4on(8)
		}
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Operational status of Fan units"
		::= { starConfigManager 10 }

	starCfAluLedStatus	OBJECT-TYPE
		SYNTAX	INTEGER {
				critical(1),
				major(2),
				minor(4),
				history(8),
				aco(16),
				fanfail(32),
				buzzer(64)
		}
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Alarm unit LED display status"
		::= { starConfigManager 11 }

	starCfAluCharacterDisplay	OBJECT-TYPE
		SYNTAX	DisplayString
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Alarm unit character display"
		::= { starConfigManager 12 }

	starCfAluBrightness	OBJECT-TYPE
		SYNTAX	INTEGER (1..4)
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			"Alarm unit LED brightness indicator, 1=0%, 2=25%,
			 3=50% and 4=70%"
		::= { starConfigManager 13 }

	starCfSlotInstalled	OBJECT-TYPE
		SYNTAX	INTEGER {
			slot1installed(1),
			slot2installed(2),
			slot3installed(4),
			slot4installed(8),
			slot5installed(16),
			slot6installed(32),
			slot7installed(64),
			slot8installed(128),
			slot9installed(256),
			slot10installed(512),
			slot11installed(1024),
			slot12installed(2048),
			slot13installed(4096),
			slot14installed(8192),
			slot15installed(16384)
		}
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Slot installed status for each board in the Star Node"
		::= { starConfigManager 14 }

	-----------------------------------------------------------------------
	-- Star System Account Manager Group
	-----------------------------------------------------------------------
	starAccountManager	OBJECT IDENTIFIER ::= { starSystem 4 }

	starAmCDRFileGetCompleted	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			"This object can be used to indicate that NMS has
			 gotton AM file"
		::= { starAccountManager 1 }

	starAmCDRThresholdCount	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			"This object is the maximum count of 
			 AM file"
		::= { starAccountManager 2 }

	starAmCDRThresholdTime	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			"This object is the maximum time of 
			 AM file"
		::= { starAccountManager 3 }

	starAmCDRFileGetRequest	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	write-only
		STATUS	mandatory
		DESCRIPTION
			"CDR request from NMS"
		::= { starAccountManager 4 }

	starAmCDRFileCount	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"CDR split file number"
		::= { starAccountManager 5 }

	starAmCDRMaxCount	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			"File Split count"
		::= { starAccountManager 6 }

	starAmCacheTimer	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			"CDR cache timer default is 5 sec"
		::= { starAccountManager 7 }

	starAmCacheCounter	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			"CDR cache counter default is 12"
		::= { starAccountManager 8 }

