MICOM-MPANL-SIGNALING-MIB DEFINITIONS ::= BEGIN
	IMPORTS
		Counter 
                   		 	FROM RFC1155-SMI
		OBJECT-TYPE
                   			FROM RFC-1212
		DisplayString
							FROM RFC1213-MIB
		micom-oscar
							FROM MICOM-OSCAR-MIB
        TRAP-TYPE
                            FROM RFC-1215
        mcmSysAsciiTimeOfDay
                            FROM MICOM-SYS-MIB;


-- group OBJECT IDENTIFIERs
	

	micom-msm		OBJECT IDENTIFIER ::= {micom-oscar 19}
	configuration	OBJECT IDENTIFIER ::= {micom-msm 1}
	control			OBJECT IDENTIFIER ::= {micom-msm 2}
	statistics		OBJECT IDENTIFIER ::= {micom-msm 3}
	status			OBJECT IDENTIFIER ::= {micom-msm 4}


-- Note: The following group contains 2 related tables for all configuration
--       functionality. 
--	 1) The Standard or Operational Parameters (Operational) provide the
--	 SNMP management interface with the Frame Relay Access Device 
--	 operational modules.
--	 Objects defined for this interface begin with the 'mcm' prefix.
--	 2) The Configuration Parameters (Configuration) provide the SNMP 
--	 management interface with a stored Frame Relay Access Device configuration. 
--	 Objects defined for this interface begin with the 'nvm' prefix.
--	


--  Operational Parameters (Operational) Group


--  the MPANL Signaling Profile Parameters Group (Standard/Operational Parameters)

--  This group provides the parameters to view the profile of the access device.  

mcmMSMProfileCfgGroup  OBJECT IDENTIFIER ::= { configuration 1 }


mcmMSMProfileCfgGroupNodeID OBJECT-TYPE
	SYNTAX	INTEGER (1..65535)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object represents a unique \
		  identifier for an access device. (Operational);
	   HELP = ;
	   CAPABILITIES = NET_DISP, VPN_DISP;"
	::= { mcmMSMProfileCfgGroup 1 }


mcmMSMProfileCfgGroupCustomerID OBJECT-TYPE
	SYNTAX	INTEGER (0..65535)
   	ACCESS  read-only
     	STATUS  mandatory
     	DESCRIPTION
          "NAME = ;
	   DESC = The value of this object uniquely identifies \
		  a customer to which a given access device belongs. \
		  (Operational);
	   HELP = ;
	   CAPABILITIES = NET_DISP, VPN_DISP;"
    	::= { mcmMSMProfileCfgGroup 2 }

mcmMSMProfileCfgGroupDNAPrefix OBJECT-TYPE
	SYNTAX	DisplayString (SIZE(1..31))
	ACCESS  read-only
     	STATUS  mandatory
     	DESCRIPTION
       	  "NAME = ; 
	   DESC = The value of this object specifies the prefix of the \
    	  Directory Network Address which uniquely identifies \
		  the unit within the network topology. (Operational);
	   HELP = ;
	   CAPABILITIES = NET_DISP, VPN_DISP;"
	::= { mcmMSMProfileCfgGroup 3 }


--  "Parallel" Configuration Parameters (Configuration)Group


--  the MPANL Signaling Profile Parameters Group (Configuration Parameters)

--  This table provides the parameters to define the profile of the Frame 
--  Relay Access Device. (Configuration)

nvmMSMProfileCfgGroup OBJECT IDENTIFIER ::= { configuration 2 }


nvmMSMProfileCfgGroupNodeID OBJECT-TYPE
	SYNTAX	INTEGER (1..65535)
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object represents a unique \
		  identifier for an access device. (Configuration);
	   HELP = ;
	   CAPABILITIES = NET_CFG, VPN_DISP;"
	::= { nvmMSMProfileCfgGroup 1 }


nvmMSMProfileCfgGroupCustomerID OBJECT-TYPE
	SYNTAX	INTEGER (0..65535)
    	ACCESS  read-write
     	STATUS  mandatory
     	DESCRIPTION
          "NAME = ;
	   DESC = The value of this object uniquely identifies \
		  a customer to which a given access device belongs. \
		  Also, this is used by the NAC in registering the \
		  Egress table. (Configuration);
	   HELP = ;
	   CAPABILITIES = NET_CFG, VPN_DISP;"
     	::= { nvmMSMProfileCfgGroup 2}

nvmMSMProfileCfgGroupDNAPrefix OBJECT-TYPE
	SYNTAX	DisplayString (SIZE(1..31))
	ACCESS  read-write
     	STATUS  mandatory
     	DESCRIPTION
       	  "NAME = ; 
	   DESC = The value of this object specifies the prefix of the \
		  Directory Network Address which uniquely identifies \
		  the unit within the network topology. (Configuration);
	   HELP = ;
	   CAPABILITIES = NET_CFG, VPN_DISP;"
	::= { nvmMSMProfileCfgGroup 3 }


--  Operational Parameters (Operational) Table


--  the MPANL DTE Signaling Line Parameters Group (Standard/Operational Parameters)

--  This group provides the parameters to view the DTE line configuration
--  of the access device.  


mcmMSMDTELinkCfgTable OBJECT-TYPE
	SYNTAX  SEQUENCE OF McmMSMDTELinkCfgEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
 	  "NAME = ; 
    	   DESC = The MPANL DTE Signaling Line Parameters \
		  table. (Operational);
   	   HELP = ;
    	   CAPABILITIES = NET_CFG, VPN_DISP;"
 	::= { configuration 3 }

mcmMSMDTELinkCfgEntry OBJECT-TYPE
	SYNTAX  McmMSMDTELinkCfgEntry 
	ACCESS  not-accessible
	STATUS  mandatory
 	DESCRIPTION
   	  "NAME = ; 
    	   DESC = An entry in the MPANL DTE Signaling Line Parameters \
            		  table. (Operational);
    	   HELP = ;
    	   CAPABILITIES = ;"
 	INDEX   { mcmMSMDTELinkCfgIfIndex }
	::= { mcmMSMDTELinkCfgTable 1 }

McmMSMDTELinkCfgEntry ::= SEQUENCE {
    mcmMSMDTELinkCfgIfIndex                 INTEGER,
	mcmMSMDTELinkCfgMaxSubChannelRange	INTEGER,
	mcmMSMDTELinkCfgDTEReceiverBW		INTEGER,
	mcmMSMDTELinkCfgDCEReceiverBW		INTEGER,
	mcmMSMDTELinkCfgDTEMaxFrameSize		INTEGER,
	mcmMSMDTELinkCfgDCEMaxFrameSize		INTEGER

	}

mcmMSMDTELinkCfgIfIndex OBJECT-TYPE
    SYNTAX  INTEGER (1..255)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "NAME = ;
       DESC = Ifindex associated with the MPANL DTE line  \
              parameters.;
              
       HELP = ;
       CAPABILITIES = ;"
    ::= { mcmMSMDTELinkCfgEntry 1 }


mcmMSMDTELinkCfgMaxSubChannelRange OBJECT-TYPE
	SYNTAX	INTEGER (17..255)
    	ACCESS  read-only
     	STATUS  mandatory
     	DESCRIPTION
          "NAME = ;
	   DESC = The value of this object represents the highest \
		  DLCI value to be used for sub-channel (CID) allocation. \
		  (Note that the current default value, 63, may change \
		  depending on the VoFR agreements.)  Note that user \
		  DLCIs on the MPANL start at DLCI-17.  This \
		  information is signaled from the DTE to the DCE. \
		  (Operational);
	   HELP = ;
	   CAPABILITIES = ;"
	DEFVAL { 63 }
     	::= { mcmMSMDTELinkCfgEntry 2}

mcmMSMDTELinkCfgDTEReceiverBW OBJECT-TYPE
	SYNTAX	INTEGER (16000..2048000)
	ACCESS  read-only
     	STATUS  mandatory
     	DESCRIPTION
       	  "NAME = ; 
	   DESC = The value of this object represents the maximum \
		  throughput (in bits/sec) that the MPANL DCE is \
		  capable of transmitting(not less than 16Kbs \
                  and not exceeding the Access Link bandwidth). \
                  Several SVCs inside the netlink may use this \
                  bandwidth. Each SVC will consume a part of \
                  this bandwidth.\
		  (Operational);
	   HELP = ;
	   CAPABILITIES = ;"
	::= { mcmMSMDTELinkCfgEntry 3}

mcmMSMDTELinkCfgDCEReceiverBW OBJECT-TYPE
	SYNTAX	INTEGER (16000..2048000)
	ACCESS  read-only
     	STATUS  mandatory
     	DESCRIPTION
       	  "NAME = ; 
	   DESC = The value of this object represents the maximum \
		  throughput (in bits/sec) that the MPANL DTE is \
		  capable of transmitting(not less than 16Kbs \
                  and not exceeding the Access Link bandwidth). \
                  Several SVCs inside the netlink may use this \
                  bandwidth. Each SVC will consume a part of this \
                  bandwidth.\
		  (Operational);
	   HELP = ;
	   CAPABILITIES = ;"
	::= { mcmMSMDTELinkCfgEntry 4}

mcmMSMDTELinkCfgDTEMaxFrameSize OBJECT-TYPE
	SYNTAX	INTEGER (50..4100)
	ACCESS  read-only
     	STATUS   mandatory
     	DESCRIPTION
       	  "NAME = ; 
	   DESC = The value of this object represents the maximum \
		  frame size (in bytes) that the MPANL DCE is capable \
		  of transmitting. This value is signaled from the \
                  DTE to the DCE where it is used by the VoFR procedure \
                  in the wan driver to manage frame delay by fragmenting \
                  VoFR frames. Multiple VoFR frames can constitute a frame \
                  relay frame.\
		  (Operational);
	   HELP = ;
	   CAPABILITIES = ;"
	::= { mcmMSMDTELinkCfgEntry 5}

mcmMSMDTELinkCfgDCEMaxFrameSize OBJECT-TYPE
	SYNTAX	INTEGER (50..4100)
	ACCESS  read-only
     	STATUS  mandatory
     	DESCRIPTION
       	  "NAME = ; 
	   DESC = The value of this object represents the maximum \
		  frame size (in bytes) that the MPANL DTE is capable \
		  of transmitting. This value is signaled from the \
                  DTE to the DCE where it is used by the VoFR procedure \
                  in the wan driver to manage frame delay by fragmenting \
                  VoFR frames. Multiple VoFR frames can constitute a frame \
                  relay frame.\
		  (Operational);
	   HELP = ;
	   CAPABILITIES = ;"
	::= { mcmMSMDTELinkCfgEntry 6}


--  the MPANL DTE Signaling Line Parameters Table (Configuration Parameters)

--  This group provides the parameters to define the DTE line configuration
--  of the access device.  

nvmMSMDTELinkCfgTable OBJECT-TYPE
	SYNTAX  SEQUENCE OF NvmMSMDTELinkCfgEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
 	  "NAME = ; 
    	   DESC = The MPANL DTE Signaling Line Parameters \
		  table. (Configuration);
   	   HELP = ;
    	   CAPABILITIES = NET_CFG, VPN_DISP;"
 	::= { configuration 4 }

nvmMSMDTELinkCfgEntry OBJECT-TYPE
	SYNTAX  NvmMSMDTELinkCfgEntry 
	ACCESS  not-accessible
	STATUS  mandatory
 	DESCRIPTION
   	  "NAME = ; 
    	   DESC = An entry in the MPANL DTE Signaling Line Parameters \
            		  table. (Configuration);
    	   HELP = ;
    	   CAPABILITIES = ;"
 	INDEX   { nvmMSMDTELinkCfgIfIndex }
	::= { nvmMSMDTELinkCfgTable 1 }

NvmMSMDTELinkCfgEntry ::= SEQUENCE {
        nvmMSMDTELinkCfgIfIndex         INTEGER,
	nvmMSMDTELinkCfgMaxSubChannelRange	INTEGER,
	nvmMSMDTELinkCfgDTEReceiverBW		INTEGER,
	nvmMSMDTELinkCfgDCEReceiverBW		INTEGER,
	nvmMSMDTELinkCfgDTEMaxFrameSize		INTEGER,
	nvmMSMDTELinkCfgDCEMaxFrameSize		INTEGER,
    nvmMSMDTELinkCfgEntryStatus         INTEGER
	}

nvmMSMDTELinkCfgIfIndex OBJECT-TYPE
    SYNTAX  INTEGER (1..255)
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
      "NAME = ;
       DESC = Ifindex associated with the MPANL DTE line  \
              parameters.;
       HELP = ;
       CAPABILITIES = ;"
    ::= { nvmMSMDTELinkCfgEntry 1 }


nvmMSMDTELinkCfgMaxSubChannelRange OBJECT-TYPE
	SYNTAX	INTEGER (17..255)
    	ACCESS  read-write
     	STATUS  mandatory
     	DESCRIPTION
          "NAME = ;
	   DESC = The value of this object represents the highest \
		  DLCI value to be used for sub-channel allocation. \
		  (Note that the current default value, 63, may change \
		  depending on the VoFR agreements.)  Note that user \
