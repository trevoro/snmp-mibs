nvmMSMDTELinkCfgEntryStatus OBJECT-TYPE
    SYNTAX  INTEGER {
        modify (1)
        }
        ACCESS  write-only
        STATUS 	obsolete 
        DESCRIPTION
          "NAME = ;
       DESC = This object was obsoleted as of release \
	      3.0; and is included here only to support \
	      backwards compatibility for software \
	      versions prior to release 3.0. \
	      This object is used in the MSM profile table \
              to add new rows, modify existing rows, \
              and delete existing rows. (Configuration);
       HELP = ;
       CAPABILITIES = ;"
    ::= { nvmMSMDTELinkCfgEntry 7  }



--  the MPANL Signaling Statistics Table (Operational Parameters - Counters)

--  This table provides counters to be used for monitoring during debugging and
--  performance characterization.  These counters are maintained as one per LAP-F
--  connection.

mcmMSMStatsLAPFConnTable OBJECT-TYPE
	SYNTAX  SEQUENCE OF McmMSMStatsLAPFConnEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ; 
	   DESC = The MPANL Signaling Statistics for LAP-F connections \
		  table.; \
	   HELP = ;
	   CAPABILITIES = NET_CFG, VPN_DISP;"
	::= { statistics 1 }

mcmMSMStatsLAPFConnEntry OBJECT-TYPE
	SYNTAX  McmMSMStatsLAPFConnEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ; 
	   DESC = An entry in the MPANL Signaling Statistics for LAP-F \
		  connections table.; \

	   HELP = ;
	   CAPABILITIES = ;"
	INDEX   { mcmMSMStatsLAPFConnIfIndex }
	::= { mcmMSMStatsLAPFConnTable 1 }

McmMSMStatsLAPFConnEntry ::= SEQUENCE {
	mcmMSMStatsLAPFConnIfIndex			INTEGER,
	mcmMSMStatsLAPFConnReestablished	Counter,
	mcmMSMStatsLAPFConnEstablished		Counter,
	mcmMSMStatsLAPFConnDisconnects		Counter,
	mcmMSMStatsProfileTxCnt				Counter,
	mcmMSMStatsProfileRxCnt				Counter,
	mcmMSMStatsRestartReqTxCnt			Counter,
	mcmMSMStatsRestartReqRxCnt			Counter,
	mcmMSMStatsDnaAssociationCnt		Counter,
	mcmMSMStatsDnaDeassociationCnt		Counter,
	mcmMSMStatsPANLInfoElementsTxCnt	Counter,
	mcmMSMStatsPANLInfoElementsRxCnt	Counter
	}

mcmMSMStatsLAPFConnIfIndex OBJECT-TYPE
	SYNTAX	INTEGER (1..255)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object corresponds to a value \
		  of IfIndex, and is used as the index into the \
		  MSM Statistics for LAP-F connections table.;
	   HELP = ;
	   CAPABILITIES = ;"
	::= { mcmMSMStatsLAPFConnEntry 1 }

mcmMSMStatsLAPFConnReestablished OBJECT-TYPE
	SYNTAX	Counter
    	ACCESS  read-only
     	STATUS  obsolete
     	DESCRIPTION
          "NAME = ;
           DESC = This object was obsoleted as of release \
	      	  3.0; and is included here only to support \
	          backwards compatibility for software \
	          versions prior to release 3.0. \
		  Counter representing the number of times the LAP-F \
		  connection was attempted to be re-established.  This counter \
		  indicates the number of CONNECT_REQ messages \
		  sent to the LAP-F module.;
	   HELP = ;
	   CAPABILITIES = ;"
     	::= { mcmMSMStatsLAPFConnEntry 2 }

mcmMSMStatsLAPFConnEstablished OBJECT-TYPE
	SYNTAX	Counter
	ACCESS  read-only
     	STATUS  mandatory
     	DESCRIPTION
       	  "NAME = ; 
	   DESC = Counter representing the number of times the LAP-F \
		  connection was established.  This counter indicates \
		  the number of CONNECT_IND messages sent by \
		 the LAP-F module.;
	   HELP = ;
	   CAPABILITIES = ;"
	::= { mcmMSMStatsLAPFConnEntry 3 }

mcmMSMStatsLAPFConnDisconnects OBJECT-TYPE
	SYNTAX	Counter
    	ACCESS  read-only
     	STATUS  mandatory
     	DESCRIPTION
          "NAME = ;
	   DESC = Counter representing the number of times a LAP-F \
		  connection was broken.  This counter indicates \
		  the number of DISCONNECT_IND messages sent by \
		 the LAP-F module.;
	   HELP = ;
	   CAPABILITIES = ;"
     	::= { mcmMSMStatsLAPFConnEntry 4 }

mcmMSMStatsProfileTxCnt OBJECT-TYPE
	SYNTAX	Counter
    	ACCESS  read-only
     	STATUS  mandatory
     	DESCRIPTION
          "NAME = ;
	   DESC = Counter represents the number of profiles \
		  sent without the occurrence of a restart request.;
	   HELP = ;
	   CAPABILITIES = ;"
     	::= { mcmMSMStatsLAPFConnEntry 5 }

mcmMSMStatsProfileRxCnt OBJECT-TYPE
	SYNTAX	Counter
    	ACCESS  read-only
     	STATUS  mandatory
     	DESCRIPTION
          "NAME = ;
	   DESC = Counter represents the number of profiles \
		  received without the occurrence of a restart request.;
	   HELP = ;
	   CAPABILITIES = ;"
     	::= { mcmMSMStatsLAPFConnEntry 6 }

mcmMSMStatsRestartReqTxCnt OBJECT-TYPE
	SYNTAX	Counter
    	ACCESS  read-only
     	STATUS  mandatory
     	DESCRIPTION
          "NAME = ;
	   DESC = Counter representing the number \
		  of restart requests sent.;
	   HELP = ;
	   CAPABILITIES = ;"
     	::= { mcmMSMStatsLAPFConnEntry 7 }

mcmMSMStatsRestartReqRxCnt OBJECT-TYPE
	SYNTAX	Counter
    	ACCESS  read-only
     	STATUS  mandatory
     	DESCRIPTION
          "NAME = ;
	   DESC = Counter representing the number \
		  of restart requests received.;
	   HELP = ;
	   CAPABILITIES = ;"
     	::= { mcmMSMStatsLAPFConnEntry 8 }

mcmMSMStatsDnaAssociationCnt OBJECT-TYPE
	SYNTAX	Counter
    	ACCESS  read-only
     	STATUS  mandatory
     	DESCRIPTION
          "NAME = ;
	   DESC = Counter representing the number \
		  of DNA prefix associations.;
	   HELP = ;
	   CAPABILITIES = ;"
     	::= { mcmMSMStatsLAPFConnEntry 9 }

mcmMSMStatsDnaDeassociationCnt OBJECT-TYPE
	SYNTAX	Counter
    	ACCESS  read-only
     	STATUS  mandatory
     	DESCRIPTION
          "NAME = ;
	   DESC = Counter representing the number \
		  of DNA prefix de-associations.;
	   HELP = ;
	   CAPABILITIES = ;"
