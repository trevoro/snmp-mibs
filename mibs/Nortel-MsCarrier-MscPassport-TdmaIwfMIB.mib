                 noCause(0),
                 unassignedNumber(1),
                 noRouteToDest(3),
                 normalClearing(16),
                 userBusy(17),
                 noResponse(18),
                 userNoAnswer(19),
                 invalidNumberFormat(28),
                 unspecNormal(31),
                 temporaryFailure(41),
                 channelUnavailable(44),
                 resourceUnavailable(47),
                 qosUnavailable(49),
                 serviceUnavailable(63),
                 unimplementedOption(79),
                 invalidCallRefValue(81),
                 invalidChannel(82),
                 unspecInvalidMessage(95),
