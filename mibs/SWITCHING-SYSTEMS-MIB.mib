			INTEGER,
		swSysEthernetPortRateByteTransmitRate
			INTEGER,
		swSysEthernetPortRatePeakByteTransmitRate
			INTEGER,
		swSysEthernetPortRateFrameTransmitRate
			INTEGER,
		swSysEthernetPortRatePeakFrameTransmitRate
			INTEGER
}

swSysEthernetPortRateIndex OBJECT-TYPE
	SYNTAX	INTEGER
