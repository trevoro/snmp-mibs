        -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- --
        -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- --
        --
        -- com21HcxOc3VlanStatsGroup
        --
        -- This group provides the capability to retrieve Statistics
        -- collected on the WAN side for vlans on the OC3 Card.
        --
        -- Refer to 'com21HcxVlanCtrlGroup' for Vlan Configuration
        --
        -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- --
        -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- --
 
 
        com21HcxOc3VlanStatsTable OBJECT-TYPE
                SYNTAX SEQUENCE OF HcxOc3VlanStatsEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "."
                ::= { com21HcxOc3VlanStatsGroup 1 }
 
        com21HcxOc3VlanStatsEntry OBJECT-TYPE
                SYNTAX HcxOc3VlanStatsEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "."
                INDEX { hcxOc3VlanStatsVlanId }
                ::= { com21HcxOc3VlanStatsTable 1 }
 
        HcxOc3VlanStatsEntry ::= SEQUENCE {
                hcxOc3VlanStatsVlanId                 INTEGER,
                hcxOc3VlanStatsCurrTxPkts                  Integer32,
                hcxOc3VlanStatsCurrRxPkts                  Integer32,
                hcxOc3VlanStatsCurrCrcPkts                 Integer32,
                hcxOc3VlanStatsPrevTxPkts                  Integer32,
                hcxOc3VlanStatsPrevRxPkts                  Integer32,
                hcxOc3VlanStatsPrevCrcPkts                 Integer32,
                hcxOc3VlanStatsClear                   INTEGER }
 
        hcxOc3VlanStatsVlanId OBJECT-TYPE
                SYNTAX INTEGER (1..64)
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "
                        Specifies the index into this table. An entry 
			exists for each vlan which is on the Oc3 Port."
                ::= { com21HcxOc3VlanStatsEntry 1 }
 
        hcxOc3VlanStatsCurrTxPkts OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "
                        Specifies the number of Pkts. Transmitted on
                        the VPI/VCI value associated with this entry in
                        the current 15-min time period."
                ::= { com21HcxOc3VlanStatsEntry 2 }
 
 
        hcxOc3VlanStatsCurrRxPkts OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "
                        Specifies the number of Pkts. Received on
                        the VPI/VCI value associated with this entry in
                        the current 15-min time period."
                ::= { com21HcxOc3VlanStatsEntry 3 }
 
        hcxOc3VlanStatsCurrCrcPkts OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "
                        Specifies the number of Pkts. Received with
                        CRC error on the VPI/VCI value associated
                        with this entry in the current 15-min time period."
                ::= { com21HcxOc3VlanStatsEntry 4 }
 
        hcxOc3VlanStatsPrevTxPkts OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "
                        Specifies the number of Pkts. Transmitted on
                        the VPI/VCI value associated with this entry in
                        the previous 15-min time period."
                ::= { com21HcxOc3VlanStatsEntry 5 }
 
 
        hcxOc3VlanStatsPrevRxPkts OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "
                        Specifies the number of Pkts. Received on
                        the VPI/VCI value associated with this entry in
                        the previous 15-min time period."
                ::= { com21HcxOc3VlanStatsEntry 6 }
 
        hcxOc3VlanStatsPrevCrcPkts OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "
                        Specifies the number of Pkts. Received with
                        CRC error on the VPI/VCI value associated
                        with this entry in the previous 15-min time period."
                ::= { com21HcxOc3VlanStatsEntry 7 }
 
        hcxOc3VlanStatsClear OBJECT-TYPE
                SYNTAX INTEGER { nil (1), clear(2) }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "
                        Provides the capability to clear the
                        statistics. Clearing of Statistics is allowed
                        only if  hcxStatsControl attribute is set
                        to wrapCurr."
                ::= { com21HcxOc3VlanStatsEntry 8 }
 


