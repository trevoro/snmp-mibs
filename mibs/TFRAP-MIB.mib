tfrapCfgDteConnStartDs0            OBJECT-TYPE
     SYNTAX        INTEGER
     {
         ds0-01        (1),
         ds0-02        (2),
         ds0-03        (3),
         ds0-04        (4),
         ds0-05        (5),
         ds0-06        (6),
         ds0-07        (7),
         ds0-08        (8),
         ds0-09        (9),
         ds0-10        (10),
         ds0-11        (11),
         ds0-12        (12),
         ds0-13        (13),
         ds0-14        (14),
         ds0-15        (15),
         ds0-16        (16),
         ds0-17        (17),
         ds0-18        (18),
         ds0-19        (19),
         ds0-20        (20),
