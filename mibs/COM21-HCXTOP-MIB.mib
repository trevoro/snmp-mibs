        -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- --
        --
        -- com21HcxShelfGroup
        --
        -- defines a shelf in the ComController shelf. One instance for each 
	-- shelf.
        --
        -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- --
        -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- --
 
        com21HcxShelfTable OBJECT-TYPE
                SYNTAX  SEQUENCE OF HcxShelfEntry
                MAX-ACCESS  not-accessible
                STATUS  current
                DESCRIPTION   
                        "TBD."
                ::= { com21HcxShelfGroup 1 }
 
        com21HcxShelfEntry OBJECT-TYPE
                SYNTAX  HcxShelfEntry
                MAX-ACCESS  not-accessible 
                STATUS  current
                DESCRIPTION "."
                INDEX { hcxShelfId }
                ::= { com21HcxShelfTable 1 }
 
        HcxShelfEntry ::= SEQUENCE {
                hcxShelfId              INTEGER,
                hcxShelfNoSlots		Integer32}
 
        hcxShelfId OBJECT-TYPE
                SYNTAX INTEGER (1..2)
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "
                        contains ComController shelf id."
                ::= { com21HcxShelfEntry 1 }
 
        hcxShelfNoSlots OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "
                        contains number of slots in this shelf."
                ::= { com21HcxShelfEntry 2 }
 


        -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- --
        -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- --
        --
        -- com21HcxSlotGroup
        --
        -- defines a slot in the ComController shelf. One instance for each 
	-- slot.
	--
        -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- --
        -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- --
 
        com21HcxSlotTable OBJECT-TYPE
                SYNTAX  SEQUENCE OF HcxSlotEntry
                MAX-ACCESS  not-accessible
                STATUS  current
                DESCRIPTION   
                        "TBD."
                ::= { com21HcxSlotGroup 1 }  

        com21HcxSlotEntry OBJECT-TYPE
                SYNTAX  HcxSlotEntry
                MAX-ACCESS  not-accessible 
                STATUS  current
                DESCRIPTION "."
                INDEX { hcxShelfNo, hcxSlotId }
                ::= { com21HcxSlotTable 1 }

        HcxSlotEntry ::= SEQUENCE {
		hcxShelfNo		INTEGER,
		hcxSlotId		INTEGER,
		hcxSlotSupportedTypes	INTEGER,
		hcxSlotConfigCardType	HcxCardType,
		hcxSlotInsertCardType	HcxCardType }

        hcxShelfNo OBJECT-TYPE
                SYNTAX INTEGER (1..2)
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "
                        contains ComController shelf id."
                ::= { com21HcxSlotEntry 1 }

        hcxSlotId OBJECT-TYPE
                SYNTAX INTEGER (1..11)
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "
                        contains ComController slot id."
                ::= { com21HcxSlotEntry 2 }

        hcxSlotSupportedTypes OBJECT-TYPE
                SYNTAX INTEGER {
			rx(1),
			tx(2),
			cc(3),
			com21Exp(4),
			txDig(5),
			intConn(6),
			rxIntConn(7),
			noCard(8),
			rxCom21Exp(9) }
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "
                        contains card types supported for this slot. com21Exp
                        specifies 10baseT or 100baseT or oc3.
                        intConn is the interconnect card used on the
                        expansion shelf (Shelf2).

                        rxIntConn is used to indicate a slot that can be
                        used for an RX, 100bT or 10bT card in the single shelf
                        configuration or for the interconnect card when an
                        expansion shelf is present.

			rx indicates either RX or RXM cards.

                        On the expansion shelf there is support of up to
                        8 rx cards.

                        rxCom21Exp defines RX, oc3, 10bT or 100bT card for
			slots 3 and 4. For all other slots it defines 
			Rx, 10bT or 100bT cards (This implies that 
			oc3 card is supported only in slots 2,3 and 4).
			Note that there is a maximum of 5x10bT/100bT and 
			12xRX in the system.
			maximum number of oc3 cards supported is 2
			

                        Release 2.3 software shall support the following slot
                        designations. 

			if hcxPhyConfigType = singleShelf:

			  Shelf 1
			    Slot 1  - cc
			    Slot 2  - com21Exp
			    Slot 3  - rxCom21Exp
			    Slot 4  - rxCom21Exp
			    Slot 5  - rxIntConn
			    Slot 6  - rxCom21Exp
			    Slot 7  - rxCom21Exp
			    Slot 8  - rx
			    Slot 9  - rx
		   	    Slot 10 - txDig
			    Slot 11 - tx

			if hcxPhyConfigType = fullShelf:

			  Shelf 1		Shelf2
			    Slot 1  - cc	  Slot 1  - intConn
			    Slot 2  - com21Exp	  Slot 2  - rx
			    Slot 3  - rxCom21Exp  Slot 3  - rx
			    Slot 4  - rxCom21Exp  Slot 4  - rx
			    Slot 5  - rxIntConn   Slot 5  - rx
			    Slot 6  - rxCom21Exp  Slot 6  - rx
			    Slot 7  - rxCom21Exp  Slot 7  - rx
			    Slot 8  - rx	  Slot 8  - rx
			    Slot 9  - rx	  Slot 9  - rx
		   	    Slot 10 - txDig	  Slot 10 - noCard
			    Slot 11 - tx	  Slot 11 - noCard

			if hcxPhyConfigType = compactShelf:

			  Shelf 1
			    Slot 1  - cc
			    Slot 2  - com21Exp
			    Slot 3  - rxCom21Exp
			    Slot 4  - rx
			    Slot 5  - txDig
			    Slot 6  - tx"
                ::= { com21HcxSlotEntry 3 }

        hcxSlotConfigCardType OBJECT-TYPE
                SYNTAX HcxCardType
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "
                        contains card type configured for this slot. This
			attribute will be checked against the set of 
			supported card types and rejected if not present.
			Note that the cardtype of ethDual10bT can be used
                        to specify either the 4k or 8k CAM versions of the
                        10bT card. Note that a 10bT or 100bT or oc3 card 
			CANNOT be unconfigured if an STU on an associated 
			VLAN is acquired."
                ::= { com21HcxSlotEntry 4 }

        hcxSlotInsertCardType OBJECT-TYPE
                SYNTAX HcxCardType
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "
                        contains card type inserted in the ComController slot. 
			A value of empty shall be stored if there is no
			card."
                ::= { com21HcxSlotEntry 5 }

        hcxInsertChange NOTIFICATION-TYPE
                OBJECTS { hcxAlmSeverity, hcxEventLogTime,
                            hcxSlotInsertCardType, hcxShelfNo, hcxSlotId }
                STATUS current
                DESCRIPTION "
			generated whenever a card is inserted for the
			first time, or during restart of the ComController.
			Only severity is WARNING."
                ::= { com21Traps 52 }

        hcxCardRemoved NOTIFICATION-TYPE
                OBJECTS { hcxAlmSeverity, hcxEventLogTime, hcxShelfNo, 
		            hcxSlotId }
                STATUS current
                DESCRIPTION "
                        generated whenever an inserted and correctly
			configured card is removed. Default severity is MAJOR."
                ::= { com21Traps 120 }

        hcxCardRemClear NOTIFICATION-TYPE 
                OBJECTS { hcxAlmSeverity, hcxEventLogTime, hcxShelfNo, 
                            hcxSlotId }
                STATUS current
                DESCRIPTION " 
                        generated whenever a card is reinserted in a slot
			or when a slot is configured to empty or when a
			slot is reassigned, and that slot has a previously 
			generated hcxCardRemoved trap. If the inserted card 
			is of the incorrect type, then a subsequent 
			hcxMismatchDetect trap will be generated. Severity 
			is CLEARED."
                ::= { com21Traps 121 }

        hcxMismatchDetect NOTIFICATION-TYPE
                OBJECTS { hcxAlmSeverity, hcxEventLogTime,
                            hcxSlotInsertCardType, hcxShelfNo, hcxSlotId }
                STATUS current
                DESCRIPTION "
			generated when the inserted card does not match
			the configured card type. Default severity is MAJOR."
                ::= { com21Traps 53 }

        hcxMismatchClear NOTIFICATION-TYPE
                OBJECTS { hcxAlmSeverity, hcxEventLogTime,
                            hcxSlotInsertCardType, hcxShelfNo, hcxSlotId }
                STATUS current
                DESCRIPTION "
			this mismatch clear is submitted when an incorrect
			card is removed from a slot. Severity is CLEARED."
                ::= { com21Traps 54 }

	hcxCardNoRespond NOTIFICATION-TYPE
                OBJECTS { hcxAlmSeverity, hcxEventLogTime,
                            hcxSlotInsertCardType, hcxShelfNo, hcxSlotId }
                STATUS current
                DESCRIPTION "
                        generated when an inserted card fails to respond to
			a CC generated message. This an event with no clear 
			condition. Only severity is WARNING."
                ::= { com21Traps 55 }

	hcxCardPollFail NOTIFICATION-TYPE
                OBJECTS { hcxAlmSeverity, hcxEventLogTime,
                            hcxSlotInsertCardType, hcxShelfNo, hcxSlotId }
                STATUS current
                DESCRIPTION "
                        generated when an inserted card fails to respond to
                        a CC polling. Default severity is MAJOR."
                ::= { com21Traps 56 }

        hcxCardPollFailClear NOTIFICATION-TYPE 
                OBJECTS { hcxAlmSeverity, hcxEventLogTime, 
                            hcxSlotInsertCardType, hcxShelfNo, hcxSlotId } 
                STATUS current
                DESCRIPTION " 
                        generated when a card successfully responses to
                        CC polling after a previous failure. Only severity 
			is CLEARED." 
                ::= { com21Traps 57 }

        hcxCardDnldFailNoImage NOTIFICATION-TYPE 
                OBJECTS { hcxAlmSeverity, hcxEventLogTime, 
                            hcxSlotInsertCardType, hcxShelfNo, hcxSlotId } 
                STATUS current
                DESCRIPTION " 
                        generated when a card cannot be download due to a
			lack of the correct image. Only severity is WARNING."
                ::= { com21Traps 58 }

 

        -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- --
        -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- --
        --
        -- com21HcxImageListGroup
        --
        -- defines all the images loaded on to the Common Controller.
	-- This list includes both ComPort and ComController card images. The 
	-- NMAPS shall add an Image List entry and the ComController shall 
	-- respond by downloading the specified image via FTP.
        --
        -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- --
        -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- --

        com21HcxImageListTable OBJECT-TYPE
                SYNTAX  SEQUENCE OF HcxImageListEntry
                MAX-ACCESS  not-accessible
                STATUS  current
                DESCRIPTION
                        "table of all ComController and ComPort images loaded 
			 on the CC."
                ::= { com21HcxImageListGroup 1 }
 
        com21HcxImageListEntry OBJECT-TYPE
                SYNTAX  HcxImageListEntry
                MAX-ACCESS  not-accessible
                STATUS  current
                DESCRIPTION "."
                INDEX { hcxImageId }
                ::= { com21HcxImageListTable 1 }
 
        HcxImageListEntry ::= SEQUENCE {
		hcxImageId		INTEGER,
                hcxImageType            INTEGER,
		hcxImageFilename	DisplayString,
                hcxSwRevision	        DisplayString,
                hcxHwRevision           DisplayString,
                hcxFwRevision           DisplayString,
		hcxImageState		INTEGER }

        hcxImageId OBJECT-TYPE
                SYNTAX INTEGER (1..4294967295)
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "
                        contains image list reference."
                ::= { com21HcxImageListEntry 1 }

        hcxImageType OBJECT-TYPE
                SYNTAX INTEGER {
				hcxCC (1),
				hcxTx (2),
				hcxRx (3),
				hcxEth10baseT (4),
				hcxEth100baseT (5),
				hcxTele (6),
				stu (7),
				stu3com (8),
				hcxRxm (9),
				atmlSwitch (10),
				compCfg (11),
				compSwitch (12),
				hcxOc3 (13)  }
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "
                        contains image type."
                ::= { com21HcxImageListEntry 2 }

        hcxImageState OBJECT-TYPE
                SYNTAX INTEGER { active(1), alternate(2) }
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "
                        this attribute specifies the state of an image.
			An image is either part of the active set of
			ComController and ComPort images, or is a part
			of the alternate set."
                ::= { com21HcxImageListEntry 7 }

        hcxImageFilename OBJECT-TYPE
                SYNTAX DisplayString (SIZE(64))
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "
                        contains image filename. This shall be used to 
			retrieve the image from the boot server."
                ::= { com21HcxImageListEntry 3 }

        hcxSwRevision OBJECT-TYPE
                SYNTAX DisplayString (SIZE(32))
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "
                        contains image software revision."
                ::= { com21HcxImageListEntry 4 }

        hcxHwRevision OBJECT-TYPE
                SYNTAX DisplayString (SIZE(32))
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "
                        contains hardware revision supported by this image.
			Note that in the future, we may define a range of 
			hardware revisions supported."
                ::= { com21HcxImageListEntry 5 }

        hcxFwRevision OBJECT-TYPE
                SYNTAX DisplayString (SIZE(32))
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "
                        contains firmware boot revision supported by this
			image. Note that in the future, we may define a 
			range of hardware revisions supported."
                ::= { com21HcxImageListEntry 6 }



        -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- --
        -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- --
        --
        -- com21HcxDbControlGroup
        --
        -- used to manage the database revisions.
        --
        -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- --
        -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- --
 
        hcxDbAction OBJECT-TYPE
                SYNTAX INTEGER {
				nil(1),
				saveDb(2),
				deleteSavedDb(3),
				deletePeriodicDb(4),
				revertToPeriodicDb(5),
				revertToSavedDb(6) }
                MAX-ACCESS read-write
                STATUS current 
                DESCRIPTION "  
                        This attribute is used to control various database
			actions. saveDb causes the present active database
			to be saved to disk. This will overwrite any previously
			saved database. revertToPeriodicDb causes the
			periodically saved database to become the active
			database. revertToSavedDb causes the user saved
			database to become the active database. deleteSavedDb
			and deletePeriodicDb simply delete the current
			saved and periodic database respectively. In the case
			of these last two options, the CC shall reboot."
                ::= { com21HcxDbControlGroup 1 }

        hcxDbActionResult OBJECT-TYPE
                SYNTAX INTEGER {
                                inprogress(1),
                                success(2),
                                diskFailure(3),
                                noDatabaseFound(4),
                                invalidDatabase(5) }
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "
                        This attribute contains the result of the last
                        hcxDbAction."
                ::= { com21HcxDbControlGroup 2 }

        hcxDbActionComplete NOTIFICATION-TYPE
                OBJECTS { hcxAlmSeverity, hcxEventLogTime,
                            hcxDbActionResult }
                STATUS current
                DESCRIPTION "
                        this notification is generated whenever the CC
                        completes the database action. This an event with
                        no clear condition. Only severity is WARNING."
                ::= { com21Traps 19 }
 
        hcxDbBackupPeriod OBJECT-TYPE
                SYNTAX INTEGER  (0..24)
                MAX-ACCESS read-write 
                STATUS current  
                DESCRIPTION "   
                        the period, in hours, at which the database will 
			be automatically saved on the disk. 0 will
			disable the auto backup. Default 24." 
                ::= { com21HcxDbControlGroup 3 }

 

        -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- --
        -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- --
        --
        -- com21HcxServTypeGroup
        --
        -- defines all Service Types. These define the various categories
        -- of upstream and downstream bandwidth service available to a
	-- user.
        --
        -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- --
        -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- --
 
        com21HcxServTypeTable OBJECT-TYPE
                SYNTAX  SEQUENCE OF HcxServTypeEntry
                MAX-ACCESS  not-accessible
                STATUS  current
                DESCRIPTION
                        "TBD."
                ::= { com21HcxServTypeGroup 1 }
 
        com21HcxServTypeEntry OBJECT-TYPE
                SYNTAX  HcxServTypeEntry
                MAX-ACCESS  not-accessible
                STATUS  current
                DESCRIPTION "."
                INDEX { hcxServiceTypeId }
                ::= { com21HcxServTypeTable 1 }
 
        HcxServTypeEntry ::= SEQUENCE {
                hcxServiceTypeId	Integer32,
                hcxServTypeDesc         DisplayString,
                hcxDnStrmType           INTEGER,
                hcxDnStrmRate           INTEGER,
		hcxUpStrmType           INTEGER,
                hcxUpStrmMinRate	INTEGER,
		hcxUpStrmMaxRate        INTEGER,
		hcxServTypeDelete	INTEGER }
 
        hcxServiceTypeId OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "
                        contains service type id."
                ::= { com21HcxServTypeEntry 1 }

        hcxServTypeDesc OBJECT-TYPE 
                SYNTAX DisplayString (SIZE (32))
                MAX-ACCESS read-write
                STATUS current 
                DESCRIPTION "  
                        contains service type description string. This
                        string must be unique."
                ::= { com21HcxServTypeEntry 2 } 

	hcxDnStrmType OBJECT-TYPE
                SYNTAX INTEGER { cbr (1), ondemand (2) }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "
                        defines CBR or ondemand (UBR) traffic in the
			downstream direction. Default is ondemand."
                ::= { com21HcxServTypeEntry 3 }

	hcxDnStrmRate OBJECT-TYPE
                SYNTAX INTEGER (0..11560000)
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "
                        peak bits per second in the downstream direction. 
			Policing/shaping of this attribute is the 
			responsibility of the appropriate Ethernet
                        card. Setting this value to 0 will have the
                        effect of stopping downstream traffic."
                ::= { com21HcxServTypeEntry 4 }

        hcxUpStrmType OBJECT-TYPE
                SYNTAX INTEGER { cbr (1), ondemand (2) }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "
                        defines CBR or ondemand in the upstream direction. 
			This attribute controls the type of minimum
			cell traffic. Default is ondemand."
                ::= { com21HcxServTypeEntry 5 }
 
        hcxUpStrmMinRate OBJECT-TYPE
                SYNTAX INTEGER (0..1650000)
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "
                        bits per second in the upstream direction. 
                        Policing/shaping of this attribute is the 
                        responsibility of the appropriate RX card."
                ::= { com21HcxServTypeEntry 6 }
 
        hcxUpStrmMaxRate OBJECT-TYPE
                SYNTAX INTEGER (0..1650000)
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "
                        peak bits per second in the upstream direction. 
			This is a best-effort rate. Policing/shaping 
			of this attribute is the responsibility of the 
			appropriate RX card."
                ::= { com21HcxServTypeEntry 7 }
 
	hcxServTypeDelete OBJECT-TYPE 
                SYNTAX INTEGER { nil (1), execute (2) }
                MAX-ACCESS read-create
                STATUS current
                DESCRIPTION "
                        The ComController Service Type Delete is an
                        active-value. The setting of this value to execute(2)
                        causes the multicast list entry to be deleted."
                ::= { com21HcxServTypeEntry 8 }



        -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- --
        -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- --
        --
        -- com21HcxBootControlGroup
        --
        -- tbd
        --
        -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- --
        -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- --
 
        hcxFileTransProt OBJECT-TYPE
                SYNTAX INTEGER { ftp (1), tftp (2) }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "
                        specifies the type of protocol used for file
                        transfer. i.e. for config_db and images. This
                        information is stored in NVRAM. Note that a
                        upon boot, if there is no valid CC image, an
                        TFTP will be attempted regardless of the setting
                        of this attribute. Default is ftp."
                ::= { com21HcxBootControlGroup 1 }
 
        hcxFileServerCntrl OBJECT-TYPE
                SYNTAX INTEGER { bootp (1), mibSpecified (2) }
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "
                        specifies how the file server address is
                        obtained. This parameter is controlled by CC
                        Boot shell line. Can elect to use bootp or allow
                        the operator to explicitly specify. Note that if
                        this attribute is set to mibSpecified then no
                        bootp should be generated by the boot or runtime
                        code. This information is stored in NVRAM. Default
                        is bootp."
                ::= { com21HcxBootControlGroup 2 }
 
        hcxImageServerIpAddr OBJECT-TYPE
                SYNTAX IpAddress
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "
                        IP address of the image file server. This attribute
                        is writeable only if hcxFileServerCntrl is set to
                        mibSpecified. Otherwise it is a read-only attribute
                        detailing the IP address returned by bootp. This
                        attribute is held in NVRAM. Default is 0.0.0.0."
                ::= { com21HcxBootControlGroup 3 }
 
        hcxImageServerPath OBJECT-TYPE
                SYNTAX DisplayString (SIZE(32))
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "
                        specifies path to the image files. This attribute
                        is writeable only  if hcxFileServerCntrl is set to
                        mibSpecified. Otherwise it is a read-only attribute
                        detailing the path returned by bootp. This attribute 
			is held in NVRAM. Default is null string."
                ::= { com21HcxBootControlGroup 4 }

         hcxImageServerLogin OBJECT-TYPE
                SYNTAX DisplayString (SIZE(16))
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "
                        specifies a login to the image server. This attribute
                        is only applicable if hcxFileServerCntrl is set to
                        mibSpecified and hcxFileTransProt is set to ftp. This
                        attribute is held in NVRAM. Default is ftp."
                ::= { com21HcxBootControlGroup 5 }

         hcxImageServerPassword OBJECT-TYPE
                SYNTAX DisplayString (SIZE(16))
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "
                        specifies a password to the image server. This
                        attribute is only applicable if hcxFileServerCntrl
                        is set to mibSpecified and hcxFileTransProt is set
                        to ftp. This attribute is held in NVRAM. Default is
                        anonymous."
                ::= { com21HcxBootControlGroup 6 }

         hcxDatabaseServerIpAddr OBJECT-TYPE
                SYNTAX IpAddress
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "
                        IP address of the server on which to save and
                        retrieve the config database. This attribute is
                        only applicable if hcxFileServerCntrl is set to
                        mibSpecified. Default is 0.0.0.0."
                ::= { com21HcxBootControlGroup 7 }

        hcxDatabaseServerPath OBJECT-TYPE
                SYNTAX DisplayString (SIZE(32))
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "
                        specifies path and the name of the config
                        database file. This attribute is only applicable
                        if hcxFileServerCntrl is set to mibSpecified.
                        Default is null string."
                ::= { com21HcxBootControlGroup 8 }

         hcxDatabaseServerLogin OBJECT-TYPE
                SYNTAX DisplayString (SIZE(16))
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "
                        specifies a login to the Database server. This
                        attribute is only applicable if hcxFileServerCntrl
                        is set to mibSpecified and hcxFileTransProt is set
                        to ftp. Default is ftp."
                ::= { com21HcxBootControlGroup 9 }

         hcxDatabaseServerPassword OBJECT-TYPE
                SYNTAX DisplayString (SIZE(16))
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "
                        specifies a password to the Database server. This
                        attribute is only applicable if hcxFileServerCntrl
                        is set to mibSpecified and hcxFileTransProt is set
                        to ftp. Default is anonymous."
                ::= { com21HcxBootControlGroup 10 }


 
END

