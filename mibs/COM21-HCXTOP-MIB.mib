COM21-HCXTOP-MIB DEFINITIONS ::= BEGIN
        IMPORTS MODULE-IDENTITY, OBJECT-TYPE, NOTIFICATION-TYPE, Integer32,
        IpAddress, Gauge32, TimeTicks
        FROM SNMPv2-SMI
        DisplayString, MacAddress
        FROM SNMPv2-TC
        com21, com21Hcx, com21Traps, com21Reg
        FROM COM21-HCX-MIB
		  hcxAlmSeverity, hcxEventLogTime
        FROM COM21-HCXALM-MIB;

        com21HcxCtrl MODULE-IDENTITY
                LAST-UPDATED "9701080000Z"
                ORGANIZATION "Com21, Inc."
                CONTACT-INFO
                "       Network Management
                                Postal: Paul Gordon
                                Com21, Inc.
                                750 Tasman Drive
                                Milpitas, California 95035
                                USA
                                Tel: +1 408 953 9100
                                Fax: +1 408 953 9299
                                E-mail: pgordon@com21.com"

                DESCRIPTION
                        "This is the Com21 ComController Unit Control MIB 
			   module. It provides for overall, high-level 
			   control of the ComController and comUNITY system. 
			   COM21 Part# 005-0025-00"
                ::= { com21Hcx 1 }

        com21HcxControlGroup            OBJECT IDENTIFIER ::= {com21Hcx  2}
	com21HcxAcqCnfgGroup		OBJECT IDENTIFIER ::= {com21Hcx  3}
	com21HcxAlmOverGroup		OBJECT IDENTIFIER ::= {com21Hcx  4}
	com21HcxStuOverGroup		OBJECT IDENTIFIER ::= {com21Hcx  5}
        com21HcxTrapRcvrGroup           OBJECT IDENTIFIER ::= {com21Hcx  6}
        com21HcxShelfGroup              OBJECT IDENTIFIER ::= {com21Hcx  7}
        com21HcxSlotGroup               OBJECT IDENTIFIER ::= {com21Hcx  8}
        com21HcxImageListGroup          OBJECT IDENTIFIER ::= {com21Hcx  9}
        com21HcxServTypeGroup           OBJECT IDENTIFIER ::= {com21Hcx 10}
	com21HcxArpFiltTrapGroup	OBJECT IDENTIFIER ::= {com21Hcx 12}
	com21HcxStuDefaultGroup		OBJECT IDENTIFIER ::= {com21Hcx 13}
	com21HcxDbControlGroup		OBJECT IDENTIFIER ::= {com21Hcx 14}
	com21HcxBootControlGroup	OBJECT IDENTIFIER ::= {com21Hcx 18}

	com21HcxPhase30			OBJECT IDENTIFIER ::= {com21Reg  1}
	com21HcxCompact			OBJECT IDENTIFIER ::= {com21Reg  2}

	UpstrmFreqKhz    ::= INTEGER (5000..40000)
        StuGain          ::= INTEGER (18..58)
        EpochTime        ::= INTEGER (0..2147483647)
        PrimServiceState ::= INTEGER { is(1), oos(2) }
	Com21RowStatus   ::= INTEGER { active(1), create(2), destroy(3),
				       deactive(4) }

        HcxCardType      ::= INTEGER { rx(1), tx(2), ethDual10bT(3), 
				       ethQuad10bT(4), eth100bT(5), tele(6), 
				       cc(7), oc3(8), atm25(9), empty(10),
				       txRf(11), txDig(12), eth10bT8k(13),
				       intConn(14), rxm(15), atmlSw(16),
				       compactSw(17) }


        -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- --
        -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- --
        --
        -- COM21 ComController Enterprise MIB
        --
        -- This MIB contains SNMP Groups and Attributes definitions for COM21
        -- specific management.
        -- Also supported by the ComController shall be the MIB II for the 
	-- SNMP interface and the MIB II Interfaces Group for both the 
	-- ComController and ComPort Ethernet interfaces.
        -- This Enterprise MIB contains many elements of the ATM UNI 2.1 ILMI
        -- MIB. This full MIB is not presently supported by the COM21 equipment
        -- as there are no external UNI interfaces. When this changes then the
        -- full MIB should be supported and the related Enterprise MIB entries
        -- removed.
        --
        -- The com21HcxCtrl MIB contains the following groups :
        --
        --   com21HcxControlGroup       - this group supports the overall
        --                                control of the ComController as a 
	--				  whole.
        --
	--   com21HcxAcqCnfgGroup       - this group provides for configuration
	--				  of the ComPort parameters passed 
	--				  during acquisition.
	--
	--   com21HcxAlmOverGroup	- this group contains an overview of
	--				  the alarm state and alarm config
	--				  of the ComController.
	--
	--   com21HcxStuOverGroup	- this group provides an overview of
	--				  of the ComPort connectivity.
	--
        --   com21HcxTrapRcvrGroup      - this group provides each management
        --                                entry with the ability to control the
        --                                reception of ComController and 
	--				  ComPort traps.
        --
	--   com21HcxShelfGroup		- this group specifies the shelf
	--				  type, the number of slots and
	--				  any other specific information.
	--
        --   com21HcxSlotGroup          - this group provides a table with a
        --                                single entry for each physical card
        --                                slot in the ComController system. 
	--				  The user may use this group to 
	--				  preconfigure the ComController.
        --
        --   com21HcxImageListGroup     - table of stored ComController and 
	--				  ComPort images.
        --
        -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- --
        -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- --

        -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- --
        -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- --
        --
        -- com21HcxControlGroup
        --
        -- defines the attributes related to the management of overall 
        -- ComController functionality. 
        --
        -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- --
        -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- --
  
	hcxMacAddress OBJECT-TYPE
	        SYNTAX MacAddress
		MAX-ACCESS read-only
		STATUS current
		DESCRIPTION "
			Contains IEEE 802 medium access control address of the 
			ComController device."
		::= { com21HcxControlGroup 1 }

        hcxEncryptionEnable OBJECT-TYPE
                SYNTAX INTEGER { true (1), false (2) }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "
                        Used to enable or disable encryption on a system 
			basis. Default is true."
                ::= { com21HcxControlGroup 2 }
 
	hcxPartNumber OBJECT-TYPE
		SYNTAX DisplayString (SIZE(32))
		MAX-ACCESS read-only
		STATUS current
		DESCRIPTION "
			The ComController Part Number is a display string ."
		::= { com21HcxControlGroup 3 }

	hcxEpochTime OBJECT-TYPE
		SYNTAX EpochTime 
		MAX-ACCESS read-write
		STATUS current
		DESCRIPTION "
			The Epoch Time is a 31 bit signed positive integer
			containing the number of seconds since 1970."
		::= { com21HcxControlGroup 4 }

	hcxUserText OBJECT-TYPE
		SYNTAX DisplayString (SIZE(32))
		MAX-ACCESS read-write
		STATUS current
		DESCRIPTION "
			The User Text is a non-volatile string which is set
			by the management system with an arbitrary value. This
			value does not have to be unique. It's purpose is solely
			to assist the carrier personnel with identifying the
			ComController device."
		::= { com21HcxControlGroup 5 }

	hcxSerialNumber OBJECT-TYPE
		SYNTAX DisplayString (SIZE(32))
		MAX-ACCESS read-only
		STATUS current
		DESCRIPTION "
			The Serial Number is a 32 character number used
			by COM21 to uniquely identify the device."
		::= { com21HcxControlGroup 6 }

	hcxIpAddress OBJECT-TYPE
                SYNTAX IpAddress
        	MAX-ACCESS read-only
        	STATUS current
        	DESCRIPTION "
			IP address of the ComController SNMP Agent."
        	::= { com21HcxControlGroup 7 }

	hcxIpMask OBJECT-TYPE
                SYNTAX IpAddress
        	MAX-ACCESS read-write
        	STATUS current
        	DESCRIPTION "
			IP mask of the ComController SNMP Agent."
        	::= { com21HcxControlGroup 27 }

	hcxGatewayAddress OBJECT-TYPE
                SYNTAX IpAddress
        	MAX-ACCESS read-write
        	STATUS current
        	DESCRIPTION "
			IP address of any IP Gateway. Default is 0.0.0.0."
        	::= { com21HcxControlGroup 8 }

	hcxRestartAction OBJECT-TYPE
		SYNTAX INTEGER {
				nil(1),
				warmStart(2),
				coldStart(3) }
		MAX-ACCESS read-write
		STATUS current
		DESCRIPTION "
			The Restart Action is an active-value. The setting
			of this value to warmStart(2) causes the 
			ComController to restart utilizing the present 
			active software load. The setting of this value to 
			coldStart(3) causes the ComController to restart, 
			refreshing the current active firmware images and 
			the active configuration file from the NMAPS. This 
			completion of the reboot shall be indicated by the 
			standard SNMP warmStart or coldStart trap."
		::= { com21HcxControlGroup 9 }

        hcxRestartNotify NOTIFICATION-TYPE       
                OBJECTS { hcxAlmSeverity, hcxEventLogTime }
                STATUS current
                DESCRIPTION "
                        this trap is generated upon HCX initialization
                        after a restart. This trap indicates to a management
			system that the HCX SNMP Agent, and the system
			generally, is ready.  Only severity is WARNING."
                ::= { com21Traps 59 }
 
	hcxMajorAlarmStatusLed OBJECT-TYPE
		SYNTAX INTEGER { on (1), off (2) }
		MAX-ACCESS read-only
		STATUS current
		DESCRIPTION "
			The Major Alarm Status LED is a read-only 
			variable which describes the condition of the
			ComController Major Alarm Status light emitting 
			diode located on the CC board. A value of on(1) 
			indicates that a Major alarm exists and that the 
			LED is illuminated. A value of off(2) indicates 
			that no major alarm condition is detected."
		::= { com21HcxControlGroup 10 }

        hcxMajorStatusLedChange NOTIFICATION-TYPE
                OBJECTS { hcxAlmSeverity, hcxEventLogTime,
		            hcxMajorAlarmStatusLed }
                STATUS current
                DESCRIPTION "
			generated whenever there is a change in major alarm 
			LED status. This an event with no clear condition. 
			Only severity is WARNING."
                ::= { com21Traps 40 }

        hcxMinorAlarmStatusLed OBJECT-TYPE
                SYNTAX INTEGER { on (1), off (2) }
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "
                        The Minor Alarm Status LED is a read-only 
                        variable which describes the condition of the 
                        ComController Minor Alarm Status light emitting 
			diode located on the CC board. A value of on(1) 
			indicates that a Minor alarm exists and that the 
			LED is illuminated. A value of off(2) indicates 
			that no minor alarm condition is detected."
                ::= { com21HcxControlGroup 11 }
 
        hcxMinorStatusLedChange NOTIFICATION-TYPE
                OBJECTS { hcxAlmSeverity, hcxEventLogTime,
		            hcxMinorAlarmStatusLed }
                STATUS current
                DESCRIPTION "
			generated whenever there is a change in minor alarm 
                        LED status. This an event with no clear condition. 
			Only severity is WARNING."
                ::= { com21Traps 41 }

        hcxAutoLoadBalancing OBJECT-TYPE
                SYNTAX INTEGER { on (1), off (2) }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "
                        This attribute controls whether the ComController
                        shall perform automatic load balancing after an
			RX resource is added (or restored to the system). 
			Default is on."
                ::= { com21HcxControlGroup 12 }

        hcxAutoFreqHopping OBJECT-TYPE 
                SYNTAX INTEGER { on (1), off (2) } 
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION " 
                        This attribute controls whether the ComController
			shall perform automatic frequency hopping. Note that
			the auto frequency hopping can also be controlled
			on a specific RX port basis. Turning this attribute 
			off will override the individual ComPort 
			configurations. Default will be on."
                ::= { com21HcxControlGroup 13 }
 
	hcxKeyRenewalPeriod OBJECT-TYPE
                SYNTAX INTEGER (0..8784)
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "
                        Contains the period at which encryption keys should
			be renewed. This attribute is expressed in hours.
			i.e. value of 48 indicates a 48hour renewal frequency.
			Default is 24. 0 disables renewal."
                ::= { com21HcxControlGroup 14 }

