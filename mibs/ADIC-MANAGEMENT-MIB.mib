	--
	-- Begin Physical Library Drive definitions
	--
    phMediaBarCode  OBJECT-TYPE
       SYNTAX AdicBarCode 
       ACCESS read-only
       STATUS mandatory
       DESCRIPTION
            ""
       ::= { phMediaEntry 1 }

