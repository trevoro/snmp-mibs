utmPtermTabNum    OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "The number of entries in the table utmPtermTable"
        ::= {utmPtermData  1}
 
utmPtermTable   OBJECT-TYPE
        SYNTAX  SEQUENCE OF UtmPtermEntry
        ACCESS  not-accessible
        STATUS  mandatory
        DESCRIPTION "Table of all physical terminals"
        ::= {utmPtermData  2}
 
utmPtermEntry   OBJECT-TYPE
        SYNTAX  UtmPtermEntry
        ACCESS  not-accessible
        STATUS  mandatory
        DESCRIPTION "An entry in the table"
        INDEX {utmPtermIndex}
        ::= {utmPtermTable 1}
 
UtmPtermEntry ::=
        SEQUENCE
        {
          utmPtermIndex         INTEGER,
          utmPtermName          DisplayString,
          utmPtermProname       DisplayString,
          utmPtermLterm         DisplayString,
          utmPtermBCAMAppl      DisplayString,
          utmPtermPtyp          DisplayString,
          utmPtermStatus        INTEGER, 
          utmPtermConnected     INTEGER,
          utmPtermConnectStatus INTEGER,
 
 
-- begin of objects for BS2000
          utmPtermConnectForced INTEGER,
          utmPtermConnectMultiplexed INTEGER,
-- end of objects for BS2000
 
 
          utmPtermConTime       INTEGER,
          utmPtermLett          INTEGER,
          utmPtermConb          INTEGER
        }
 
utmPtermIndex   OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "A unique value for each entry, its value
                     ranges between 1 and the value of utmPtermTabNum"
        ::= {utmPtermEntry 1}
 
utmPtermName   OBJECT-TYPE
        SYNTAX  DisplayString
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "Name of the chosen physical terminal"
        ::= {utmPtermEntry 2}
 
utmPtermProname   OBJECT-TYPE
        SYNTAX  DisplayString
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION "Processor name of the physical terminal (or blank
                     for local devices)"
        ::= {utmPtermEntry 3}
 
utmPtermLterm   OBJECT-TYPE
        SYNTAX  DisplayString
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION "Name of the logical UTM terminal that belongs to
                     the physical terminal"
        ::= {utmPtermEntry 4}
 
utmPtermBCAMAppl   OBJECT-TYPE
        SYNTAX  DisplayString
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION "BCAM name of the UTM application"
        ::= {utmPtermEntry 5}
 
utmPtermPtyp   OBJECT-TYPE
        SYNTAX  DisplayString
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "Partner type of the UTM terminal"
        ::= {utmPtermEntry 6}
 
utmPtermStatus   OBJECT-TYPE
        SYNTAX  INTEGER   {
                            on  (1),
                            off (2)
                          }
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION "The UTM terminal is either locked (off) or unlocked (on)"
        ::= {utmPtermEntry 7}
 
utmPtermConnected   OBJECT-TYPE
        SYNTAX  INTEGER   {
                            yes (1),
			    no (2),
			    waiting (3)
                          }
        ACCESS  read-write
        STATUS  mandatory
	DESCRIPTION "The UTM terminal is either connected (yes) or
		     not connected (no) or waiting for connection"
        ::= {utmPtermEntry 8}
 
utmPtermConnectStatus    OBJECT-TYPE
        SYNTAX  INTEGER  {
                            automaticCon (1),
                            terminalPool (2),
                            na           (3)
                         }
        ACCESS  read-write
        STATUS  mandatory
	DESCRIPTION "Status = 'A' means automatic connection at application start,
		     status = 'P' means a terminalpool connection"
        ::= {utmPtermEntry 9}
 
 
-- begin of objects for BS2000
utmPtermConnectForced    OBJECT-TYPE
        SYNTAX  INTEGER  {
                            yes (1),
                            no  (2)
                         }
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "Like automatic connection but also forces a existing 
