UTM-MIB DEFINITIONS ::= BEGIN
--  MIB-Version: 3
 
--
-- Title:   MIB for UTM V3.3 (and above)
--
-- Version: 3
-- Date:    Feb. 1998
-- Author:  Birgit Lamers        BS2000 DC3
-- Reason:  Access of utmMainApplHomeDir is set to 'read-only'
--
-- Version: 2
-- Date:    Dez. 1997
-- Author:  Birgit Lamers        BS2000 DC3
-- Reason:  New values for PtermConnected, TacStatus and  ConConnected
--          due to UTM V4.0
--
-- Date:    since March 1996
-- Author:  Birgit Lamers        BS2000 OS45
--          Manfred Mayr         MR PD 43
--                
 
 IMPORTS
        Gauge, enterprises     FROM RFC1155-SMI
        DisplayString   FROM RFC1213-MIB
        OBJECT-TYPE     FROM RFC-1212
      DateAndTime     FROM SNMPv2-TC
		TRAP-TYPE FROM RFC-1215;
 
--      copied from RFC1514:
--   DateAndTime ::= OCTET STRING (SIZE (8 | 11))
   --        A date-time specification for the local time of day.
   --        This data type is intended to provide a consistent
   --        method of reporting date information.
   --
   --            field  octets  contents                  range
   --            _____  ______  ________                  _____
   --              1      1-2   year                      0..65536
   --                           (in network byte order)
   --              2       3    month                     1..12
   --              3       4    day                       1..31
   --              4       5    hour                      0..23
   --              5       6    minutes                   0..59
   --              6       7    seconds                   0..60
   --                           (use 60 for leap-second)
   --              7       8    deci-seconds              0..9
   --              8       9    direction from UTC        "+" / "-"
   --                           (in ascii notation)
   --              9      10    hours from UTC            0..11
   --             10      11    minutes from UTC          0..59
   --
   --            Note that if only local time is known, then
   --            timezone information (fields 8-10) is not present.
 
 
sni                 OBJECT IDENTIFIER ::= { enterprises 231 }
sniProductMibs      OBJECT IDENTIFIER ::= { sni 2 }
sniUTM              OBJECT IDENTIFIER ::= { sniProductMibs 19 }
 
 
-- UTM  Application Table  ( Names of all applications)
--
 
 
 
-- begin of objects for UNIX
utmApplTabNum                 OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "The number of entries in the table utmApplTable"
        ::= {sniUTM 1}
 
utmApplTable           OBJECT-TYPE
        SYNTAX  SEQUENCE OF UtmApplEntry
        ACCESS  not-accessible
        STATUS  mandatory
        DESCRIPTION "Table of UTM applications"
        ::= {sniUTM 2}
 
utmApplEntry        OBJECT-TYPE
        SYNTAX  UtmApplEntry
        ACCESS  not-accessible
        STATUS  mandatory
        DESCRIPTION "An entry in the table"
	INDEX {utmApplIndex}
	::= {utmApplTable 1}
 
UtmApplEntry ::=
        SEQUENCE 
        {
            utmApplIndex                INTEGER,
            utmApplName                 DisplayString,
            utmApplStatus               INTEGER,
            utmApplSharedMemKey         Gauge,
            utmApplSharedMemSegSize     Gauge,
            utmApplSemId                Gauge,
            utmApplHomeDir              DisplayString
        }
 
utmApplIndex   OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "A unique value for each entry, its value
                     ranges between 1 and the value of utmApplTabNum"
        ::= {utmApplEntry 1}
 
utmApplName      OBJECT-TYPE
        SYNTAX  DisplayString
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "Name of the UTM application"
        ::= {utmApplEntry 2}
 
utmApplStatus    OBJECT-TYPE
        SYNTAX  INTEGER {
                          up   (1),
                          down (2)
                        }
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "Status of the UTM application is either up or down"
        ::= {utmApplEntry 3}
 
utmApplSharedMemKey      OBJECT-TYPE
        SYNTAX  Gauge
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "Shared memory key of the UTM application"
        ::= {utmApplEntry 4}
 
utmApplSharedMemSegSize      OBJECT-TYPE
        SYNTAX  Gauge
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "Size of the shared memory segement of the UTM application"
        ::= {utmApplEntry 5}
 
utmApplSemId      OBJECT-TYPE
        SYNTAX  Gauge
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "Semaphore ID number of the UTM application"
        ::= {utmApplEntry 6}
 
utmApplHomeDir      OBJECT-TYPE
        SYNTAX  DisplayString
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "Home directory of the UTM application"
        ::= {utmApplEntry 7}
-- end of objects for UNIX
 
 
 
 
