        ::= {utmPtermEntry 10}
 
utmPtermConnectMultiplexed    OBJECT-TYPE
        SYNTAX  INTEGER  {
                            yes (1),
                            no  (2)
                         }
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "The terminal uses either a multiplex connection or not"
        ::= {utmPtermEntry 11}
-- end of objects for BS2000
 
 
utmPtermConTime   OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "Duration of existing connection in minutes"
        ::= {utmPtermEntry 12}
 
utmPtermLett   OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "Number of messages input and output at the terminal
                     since the start of the application"
        ::= {utmPtermEntry 13}
 
utmPtermConb   OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "Number of breakdowns of the physical or virtual connection
                     between this terminal and the application since the start of
                     the application"
        ::= {utmPtermEntry 14}
 
 
 
 
