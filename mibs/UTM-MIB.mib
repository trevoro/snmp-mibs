utmConData          OBJECT IDENTIFIER ::= { utmApplicationData  9 }
utmLpapData         OBJECT IDENTIFIER ::= { utmApplicationData 10 }
utmPoolData         OBJECT IDENTIFIER ::= { utmApplicationData 11 }
utmLsesData         OBJECT IDENTIFIER ::= { utmApplicationData 12 }
utmLtacData         OBJECT IDENTIFIER ::= { utmApplicationData 13 }
