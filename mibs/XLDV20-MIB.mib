	TEXTUAL-CONVENTION FROM SNMPv2-TC
                 IpAddress, TimeTicks
                       FROM RFC1155-SMI
                 OBJECT-TYPE
                       FROM RFC-1212
                 TRAP-TYPE
                       FROM RFC-1215
                 onu 
                    FROM AN-MIB ;


