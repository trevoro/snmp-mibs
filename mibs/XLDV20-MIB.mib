             -- **************************************************************
             --             TEXTUAL-CONVENTIONs for XLDV20-MIB
             --
             -- use of textual convention syntax necessary for the exported
             -- types Xldv20ControlStatus, Xldv20AdminState, Unsigned16 and
             -- Xldv20FlowDirection in order to create the file snmpinfo.dat
             -- For load production the textual convention definitions are
             -- hidden by comment signs.
             --
             -- ATTENTION  use TCON: after comment sign for mosy tools
             --            to prepare the snmpinfo.dat file           

             -- **************************************************************
             --
             -- --------------------------------------------------------------
             --                            States
             -- --------------------------------------------------------------

             -- The control status of an entity.
             --
             --TCON: Xldv20ControlStatus ::= TEXTUAL-CONVENTION
             --TCON:      STATUS current
             --TCON:      DESCRIPTION
             --TCON:        "This data type is used as the syntax for the control status 
             --TCON:         attributes."
             --TCON:      SYNTAX INTEGER {
             --TCON:        requestIdle(1),
             --TCON:        requestBusy(2),
             --TCON:        requestPassed(3),
             --TCON:        requestFailed(4)
             --TCON:      }

             Xldv20ControlStatus ::=
                 INTEGER {
                     requestIdle(1),
                     requestBusy(2),
                     requestPassed(3),
                     requestFailed(4)
                 }

             -- The operating state of an entity.
             --
             Xldv20OperState ::=
                 INTEGER {
                     enabled(1),
                     disabled(2)
                 }

             -- Indicates whether a slot is empty or not.
             --
             Xldv20SlotStatus ::=
                 INTEGER {
                     unknown(0),
                     used(1),
                     empty(2)
                 }

             -- The administrative state of an entity.
             --
             --TCON: Xldv20AdminState ::= TEXTUAL-CONVENTION
             --TCON:      STATUS current
             --TCON:      DESCRIPTION
             --TCON:        "This data type is used as the syntax for the admin state 
             --TCON:         attributes. "
             --TCON:      SYNTAX INTEGER {
             --TCON:        unlocked(1),
             --TCON:        locked(2)
             --TCON:      }

             Xldv20AdminState ::=
                 INTEGER {
                     unlocked(1),
                     locked(2)
                 }

             -- The availability status of a hardware unit
             --
             Xldv20AvailStatus ::=
                 INTEGER {
                         available(1),
                         failed(2),
                         degraded(3),
                         offline(4),
                         notInstalled(5),
                         dependency(7)
            -- only internal states
            --               noDependency(6),
            --               noRestart(8),
            --               warmRestart(9),
            --               coldRestart(10),
            --               reloadRestart(11),
            --               noUpgrade(12),
            --               upgrade(13)
                }

             -- The attribute reflects upon the internal states of the
             -- Central Office Modem.
             --
             Xldv20LinkState ::=
                 INTEGER {
                     quiet(0),
                     idle(1),
                     train(2),
                     active(3),
                     download(4),
                     remoteDownload(6),
                     sleepMode(7),
                     unknown(255)
                 }

             -- The log read status.
             --
             Xldv20DbuReadStatus ::=
                 INTEGER {
                    dbuReadComplete                     (7081),
                    dbuReadFailed                       (7082),
                    dbuFileCreationFailed               (7083),
                    dbuFileTransmissionFailed           (7084)
                 }
     
             -- The access mode of a telnet session
             --
             Xldv20TelnetAccess ::=
                 INTEGER {
                     read(1),
                     trace(2),
                     write(3),
                     noAccess(4)
                 }

             -- The status of a telnet session
             --
             Xldv20TvTelnetSessionStatus ::=
                 INTEGER {
                     running(1),
                     stopped(2)
                 }

             -- The type of endpoint for continuity check
             --
             Xldv20EndpointType ::=
                 INTEGER { 
                     source  (1),
                     sink    (2),
                     both    (3)
                     }

             -- The type of reset result
             --
             Xldv20RstResult ::=
                 INTEGER { 
                     requestFailed (4)
                     }

             -- State of IMA group
             --
             Xldv20ImaGroupState ::=
                 INTEGER {
                   notConfigured(1),
                   startUp(2),
                   startUpAck(3),
                   configAbortUnsupported(4),
                   configAbortIncompatibleSymmetry(5),
                   configAbortOther(6),
                   insufficientLinks(7),
                   blocked(8),
                   operational(9),
                   configAbortUnsupportedImaVersion(10) }
 
              -- Failure Reason of an IMA group
              -- ATM Forum IMA v1.1, Section 10.2.1, page 55
              --
              Xldv20ImaGroupFailureStatus ::=
                  INTEGER {
                    noFailure(1), -- unit is up
                    startUpNe(2),
                    startUpFe(3),
                    invalidValueNe(4),
                    invalidValueFe(5),
                    failedAssymetricNe(6),
                    failedAssymetricFe(7),
                    insufficientLinksNe(8),
                    insufficientLinksFe(9),
                    blockedNe(10),
                    blockedFe(11),
                    otherFailure(12),
                    invalidImaVersionNe(13),
                    invalidImaVersionFe(14) }
 
              -- Failure Reason of an IMA group
              -- Indicate the transmit clock mode of the IMA group.
