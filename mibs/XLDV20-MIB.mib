        INDEX         { xldv20IfIndex }
        ::= { xldv20Stm1NePPTPTable 1 }

     Xldv20Stm1NePPTPEntry ::=
        SEQUENCE { xldv20S1nLineType                 Xldv20LineType,
                   xldv20S1nPathTraceSend            OCTET STRING,
                   xldv20S1nPathTraceSendDefault     Xldv20OperState,
