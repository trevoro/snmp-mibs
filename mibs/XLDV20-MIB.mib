        STATUS        mandatory
        DESCRIPTION
             " The received payload type. "
        ::= { xldv20E3NePPTPEntry 9 }

        xldv20E3nBandwidthUsage OBJECT-TYPE
        -- info-VALUE:   0..100, default 0
        -- info-ROU:     GET db
        -- info-IMPL:    V2.0->
        -- info-VERCTRL: -
