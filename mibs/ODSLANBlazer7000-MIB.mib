-- COMMUNITY TABLE

   lanBlazer7000AgentCommunity OBJECT IDENTIFIER ::= { lanBlazer7000Agent 2 }

--
-- The community table is the database used by the agent to authenticate SNMP requests
-- and to generate SNMP traps.  This table is only accessible for users with security clearance.
--

   lanBlazer7000CommunityTable OBJECT-TYPE
      SYNTAX  SEQUENCE OF LanBlazer7000CommunityEntry
      ACCESS  not-accessible
      STATUS  mandatory
      DESCRIPTION
              ""
      ::= { lanBlazer7000AgentCommunity 1 }

   lanBlazer7000CommunityEntry OBJECT-TYPE
      SYNTAX  LanBlazer7000CommunityEntry
      ACCESS  not-accessible
      STATUS  mandatory
      DESCRIPTION
              ""
      INDEX   { lanBlazer7000CommunityIndex }
      ::= { lanBlazer7000CommunityTable 1 }

   LanBlazer7000CommunityEntry ::=
      SEQUENCE {
         lanBlazer7000CommunityIndex
            INTEGER,
         lanBlazer7000CommunityString
            DisplayString,
         lanBlazer7000CommunityAddressType
            INTEGER,
         lanBlazer7000CommunityAddress
            NetworkAddress,
         lanBlazer7000CommunityAccess
            INTEGER,
         lanBlazer7000CommunityTrapReceiver
            INTEGER,
         lanBlazer7000CommunitySecurityLevel
            INTEGER,
         lanBlazer7000CommunityStatus
            RowStatus
      }

   lanBlazer7000CommunityIndex OBJECT-TYPE
      SYNTAX  INTEGER
      ACCESS  read-only
      STATUS  mandatory
      DESCRIPTION
              "An index that uniquely identifies this entry."
      ::= { lanBlazer7000CommunityEntry 1 }

   lanBlazer7000CommunityString OBJECT-TYPE
      SYNTAX  DisplayString (SIZE(0..32))
      ACCESS  read-write
      STATUS  mandatory
      DESCRIPTION
              "The community string included in the SNMP PDU used for authentication
               purposes."
      ::= { lanBlazer7000CommunityEntry 2 }

   lanBlazer7000CommunityAddressType OBJECT-TYPE
      SYNTAX  INTEGER {
                 any(1),
                 ipv4(2)
              }
      ACCESS  read-write
      STATUS  mandatory
      DESCRIPTION
              "The type of address associated with this community.  If set to any(1), only
              the community string is authenticated."
      ::= { lanBlazer7000CommunityEntry 3 }

   lanBlazer7000CommunityAddress OBJECT-TYPE
      SYNTAX  NetworkAddress
      ACCESS  read-write
      STATUS  mandatory
      DESCRIPTION
              "If the address type is any, then the value of this object is a null string.  If
              the type is ipv4(2), then this value represents a 4 byte IP address."
      ::= { lanBlazer7000CommunityEntry 4 }

   lanBlazer7000CommunityAccess OBJECT-TYPE
      SYNTAX  INTEGER {
                 none(1),
                 readOnly(2),
                 readWrite(3),
                 moreSpecific(4)
              }
      ACCESS  read-write
      STATUS  mandatory
      DESCRIPTION
              "The MIB access supported for this community entry.

              A Get or GetNext SNMP PDU is authenticated if the value of this object
              is read-only(2) or read-write(3).  A Set request will be honored if the value
              of this object is read-write(3).

              If more granular access control is desired, then the value of this object is set
              to more-specific(4), and the view table should be consulted.  This enables
              the capability to set different access rights to different branches of the MIB
              for a particular community. "
      ::= { lanBlazer7000CommunityEntry 5 }

   lanBlazer7000CommunityTrapReceiver OBJECT-TYPE
      SYNTAX  INTEGER {
                 disable(1),
                 enable(2)
              }
      ACCESS  read-write
      STATUS  mandatory
      DESCRIPTION
              "If set to enable(1), this community entry is considered a trap receiver.
              When the agent generates an SNMP trap, a copy will be sent to this host
              using this community string."
      ::= { lanBlazer7000CommunityEntry 6 }

   lanBlazer7000CommunitySecurityLevel OBJECT-TYPE
      SYNTAX  INTEGER {
                 normal(1),
                 administrator(2)
              }
      ACCESS  read-write
      STATUS  mandatory
      DESCRIPTION
              "Secure tables are only accessible from users with security clearance.  For
              example, this table (the community table) is only accessible by parties that
              have the security clearance."
      ::= { lanBlazer7000CommunityEntry 7 }

   lanBlazer7000CommunityStatus OBJECT-TYPE
      SYNTAX  RowStatus
      ACCESS  read-write
      STATUS  mandatory
      DESCRIPTION
              ""
      ::= { lanBlazer7000CommunityEntry 8 }


-- WEB SERVER

   lanBlazer7000AgentWeb OBJECT IDENTIFIER ::= { lanBlazer7000Agent 3 }

   lanBlazer7000AgentWebServerURL OBJECT-TYPE
      SYNTAX  DisplayString (SIZE(0..31))
      ACCESS  read-write
      STATUS  mandatory
      DESCRIPTION
              "The URL of where the document server software is
              installed. The switch uses this location to find online
              help and bimapped graphics.  Enter the host name or IP
              address of the HTTP server at the HTTP Server Location
              prompt, followed by ':2010'. For example, for a host
              named 'phantom,', enter 'http://phantom:2010'.  If no
              server is desired or installed, set this object to the
              empty string."  
      ::= { lanBlazer7000AgentWeb 1 }

   lanBlazer7000AgentWebServerHelpDirectory OBJECT-TYPE
      SYNTAX  DisplayString (SIZE(0..31))
      ACCESS  read-write
      STATUS  mandatory
      DESCRIPTION
              "The subdirectory that contains the help files on the
              document server.  Typically, this directory is 'help'."  
      ::= { lanBlazer7000AgentWeb 2 }
   
   
