-- VLAN EXCHANGE 

   lanBlazer7000VlanExchange OBJECT IDENTIFIER ::= { odsLANBlazer7000Mib 14 }

-- VLAN EXCHANGE SWITCH 
-- General switch-wide objects

   lanBlazer7000VlanExchangeSwitch OBJECT IDENTIFIER ::= { lanBlazer7000VlanExchange 1 }

   lanBlazer7000VlanExchangeState OBJECT-TYPE
      SYNTAX  INTEGER {
                 enable(1),
                 disable(2)
              }
      ACCESS  read-write
      STATUS  mandatory
      DESCRIPTION
              "When this object is set to enable(1), the switch attempts to
              learn VLANs from a major networking equipment vendor on switch
              ports that have their VLAN Exchange parameter set to enable(1).
              trunking mode is IEEE 802.1Q Format or Multi-level Format.

              The factory default value for this object is disable(2)."
      ::= { lanBlazer7000VlanExchangeSwitch 1 }

   lanBlazer7000VlanExchangeDomainName OBJECT-TYPE
      SYNTAX  DisplayString (SIZE(0..32))
      ACCESS  read-write
      STATUS  mandatory
      DESCRIPTION
              "The VLAN Exchange Domain Name of the switch.  A switch may
              only belong to one domain."
      ::= { lanBlazer7000VlanExchangeSwitch 2 }

   lanBlazer7000VlanExchangeUpdaterId OBJECT-TYPE
      SYNTAX  NetworkAddress
      ACCESS  read-only
      STATUS  mandatory
      DESCRIPTION
              "The IP address of the switch from which the VLAN Exchange 
              configuration was learned."
      ::= { lanBlazer7000VlanExchangeSwitch 3 }

   lanBlazer7000VlanExchangeUpdateTimeStamp OBJECT-TYPE
      SYNTAX  DisplayString (SIZE(0..12))
      ACCESS  read-only
      STATUS  mandatory
      DESCRIPTION
              "The time at which the VLAN Exchange configuration changed
              on the initiating switch."
      ::= { lanBlazer7000VlanExchangeSwitch 4 }

   lanBlazer7000VlanExchangeConfigRevNum OBJECT-TYPE
      SYNTAX  INTEGER
      ACCESS  read-only
      STATUS  mandatory
      DESCRIPTION
               "Configuration Revision Number of VLAN Exchange Configuration on
               the switch that initiated the VLAN Exchange."
      ::= { lanBlazer7000VlanExchangeSwitch 5 }
   

