	REFERENCE "'DMTF|System Resources 2|001' 9"
	::= { dmtfSystemResources2Entry 9 }

resourceSize OBJECT-TYPE
	SYNTAX		DmiInteger
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION	"jNumber of consecutive bytes that a memory or I/O resource
