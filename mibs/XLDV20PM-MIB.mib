     -- --------------------------------------------------------------
     --           Thresholding Table for PM on Physical layer
     --           (all Interfaces)
     -- --------------------------------------------------------------

     xldv20pmThresholdLimitTable OBJECT-TYPE
     -- info-IMPL:      V2.0-> 
     -- info-VERCTRL:   -
        SYNTAX        SEQUENCE OF Xldv20pmThresholdLimitEntry
        ACCESS        not-accessible
        STATUS        mandatory
        DESCRIPTION
             " The threshold limit table for all interface. 
               This table associates threshold limit index/PM counter
               pairs with threshold limit values for a 15 minutes
               interval. Entries with same threshold limit index
               belong to the same threshold limit profile.
               Each of the following TC-adaptor types are available
               and can be configured: adc, s1n, s3n, d3n, e3n.
               A crossing of a threshold limit leads to a 
               threshold crossing alert. "
        --DEFAULT test-function-async     fsnPmThlEntryTest
        --DEFAULT next-function-async     fsnPmThlEntryNext
        --DEFAULT set-function-async      fsnPmThlEntrySet
        --DEFAULT get-function-async      fsnPmThlEntryGet
        ::= { xldv20pmThresholding 1 }

     xldv20pmThresholdLimitEntry OBJECT-TYPE
     -- info-IMPL:      V2.0-> 
     -- info-VERCTRL:   -
        SYNTAX        Xldv20pmThresholdLimitEntry
        ACCESS        not-accessible
        STATUS        mandatory
        DESCRIPTION
             " An entry describing the threshold limit information . "
        INDEX { xldv20pmThlThresholdLimitIndex }
        ::= { xldv20pmThresholdLimitTable 1 }

     Xldv20pmThresholdLimitEntry ::=
        SEQUENCE { xldv20pmThlThresholdLimitIndex          INTEGER,
            xldv20pmThlNeBackgroundBlockErrors             INTEGER, -- s1f, adc, e3n
            xldv20pmThlNeCodeViolations                    INTEGER, -- s3n
            xldv20pmThlNeCodeViolationsCP                  INTEGER, -- d3n
            xldv20pmThlNeCodeViolationsP                   INTEGER, -- d3n
            xldv20pmThlNeCodeViolationsPLCP                INTEGER, -- d3n
            xldv20pmThlNeErroredSeconds                    INTEGER, -- adc, e3n
            xldv20pmThlNeErroredSecondsCP                  INTEGER, -- d3n
            xldv20pmThlNeErroredSecondsPLCP                INTEGER, -- d3n
            xldv20pmThlNeSas                               INTEGER, -- d3n
            xldv20pmThlNeSeverelyErroredFrameSeconds       INTEGER, -- s3n
            xldv20pmThlNeSeverelyErroredSeconds            INTEGER, -- s1n, s3n, adc, e3n
            xldv20pmThlNeSeverelyErroredSecondsCP          INTEGER, -- d3n
            xldv20pmThlNeSeverelyErroredSecondsPLCP        INTEGER, -- d3n
            xldv20pmThlNeUnavailableSeconds                INTEGER, -- s1n, s3n, d3n, adc, e3n
            xldv20pmThlNeUnavailableSecondsPLCP            INTEGER, -- s3n
            xldv20pmThlNeAisSeconds                        INTEGER, -- s1n
            xldv20pmThlNeOfsSeconds                        INTEGER, -- s1n
            xldv20pmThlNeLosSeconds                        INTEGER, -- s1n
            xldv20pmThlFeBackgroundBlockErrors             INTEGER, -- s1f, adc, e3f
            xldv20pmThlFeCodeViolations                    INTEGER, -- s3f
            xldv20pmThlFeCodeViolationsCP                  INTEGER, -- d3f
            xldv20pmThlFeCodeViolationsPLCP                INTEGER, -- d3f
            xldv20pmThlFeErroredSeconds                    INTEGER, -- s1f, s3f, adr, e3f
            xldv20pmThlFeErroredSecondsCP                  INTEGER, -- d3f
            xldv20pmThlFeErroredSecondsPLCP                INTEGER, -- d3f
            xldv20pmThlFeSas                               INTEGER, -- d3f
            xldv20pmThlFeSeverelyErroredSeconds            INTEGER, -- s1f, s3f, adr, e3f
            xldv20pmThlFeSeverelyErroredSecondsCP          INTEGER, -- d3f
            xldv20pmThlFeSeverelyErroredSecondsPLCP        INTEGER, -- d3f
            xldv20pmThlFeUnavailableSeconds                INTEGER, -- s1f, s3f, adr, e3f
            xldv20pmThlFeUnavailableSecondsPLCP            INTEGER, -- d3f
            xldv20pmThlDiscardedCells                      INTEGER, -- tca
            xldv20pmThlImaGroupUnavailSecs                 INTEGER, -- IMA group
            xldv20pmThlImaGroupNeNumFailures               INTEGER, -- IMA group
            xldv20pmThlImaLinkImaViolations                INTEGER, -- IMA link
            xldv20pmThlImaLinkOifAnomalies                 INTEGER, -- IMA link
            xldv20pmThlImaLinkNeSevErroredSecs             INTEGER, -- IMA link
            xldv20pmThlImaLinkFeSevErroredSecs             INTEGER, -- IMA link
            xldv20pmThlImaLinkNeUnavailSecs                INTEGER, -- IMA link
            xldv20pmThlImaLinkFeUnavailSecs                INTEGER, -- IMA link
            xldv20pmThlImaLinkNeTxUnusableSecs             INTEGER, -- IMA link
            xldv20pmThlImaLinkNeRxUnusableSecs             INTEGER, -- IMA link
            xldv20pmThlImaLinkFeTxUnusableSecs             INTEGER, -- IMA link
            xldv20pmThlImaLinkFeRxUnusableSecs             INTEGER, -- IMA link
            xldv20pmThlImaLinkNeTxNumFailures              INTEGER, -- IMA link
            xldv20pmThlImaLinkNeRxNumFailures              INTEGER -- IMA link
