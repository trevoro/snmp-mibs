        --DEFAULT next-function-async     fsnTcEntryNext
        --DEFAULT set-function-async      fsnTcEntrySet
        --DEFAULT get-function-async      fsnTcEntryGet
        ::= { xldv20pmProtocolMonitoring 1 }

