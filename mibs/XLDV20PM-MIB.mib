     -- info-ROU:       GET  SET [db,gf,up,tm,hm,ut,so,sn,cp]
     -- info-FAILEDSRC: hwm, tlm, callp       ==>> for alarm traps only
     -- info-FAILEDOBJ: [hwm] LU155,LUDSX3,LUPON,CMU,SUAD,SUVD,LTAD,
     -- info-FAILEDOBJ: [hwm] LIETH,LI25,NTAD,NTVD,
     -- info-FAILEDOBJ: [tlm] S1n(= STM-1 NePPTP),S1f,S3n,E3n,D3n,S3f,
     -- info-FAILEDOBJ: [tlm] D3f,E3f,Pon,Adc(= Adc-PPTP),Adr,Vdc,Vdr,
     -- info-FAILEDOBJ: [tlm] Ibm(= Ibm-PPTP),Eth,Tca  
     -- info-FAILEDOBJ: [callp] VpcTP, VccTP  
     -- info-IMPL:      [V2.0,V2.x]:[rw,ro,*]
     -- info-IMPL:      e.g.: V2.0-> (= all vers. begin. with V1.1U)
     -- info-IMPL:      e.g.: V2.0:*->V2.1  V2.0:rw->2.1   V2.1:* 
     -- info-VERCTRL:   [GET,SET [db,asw,agent,blocked] or "-" 
     --                             (blocked == blocked by SNMP agent)
     --                             ("-"     == no special handling)
     -- **************************************************************


     xldv20Pm        OBJECT IDENTIFIER ::= { onu 5 }
     -- info-IMPL:      V2.0-> 
     -- info-VERCTRL:   -

     -- ==============================================================
     --       TEXTUAL-CONVENTIONs for  X L D V 2 0 P M - M I B
     -- ==============================================================

     -- --------------------------------------------------------------
     --                         T y p e s                
     -- --------------------------------------------------------------

     -- An Interval type identifier to select between the 15 minute
     -- counters and the day counter.
     --
     Xldv20pmIntervalType ::=
         INTEGER { min15(1),
                   day(2) }

     -- The PM Control request commands from the TMN-OS
     --
     Xldv20pmControlReq ::=
         INTEGER { noRequest                       (1),
                   resetIndividCurrent             (2),
                   setNrHistEntriesSize            (3),
                   paramFaulty                     (4),
                   nrHistEntriesSizeNotAcceptable  (5),
                   dataDaseMemoryNotAvailable      (6),
                   create                          (7),  -- PMo on Atm
                   delete                          (8),  -- PMo on Atm
                   addEndpoint                     (9),  -- PMo on Atm
                   removeEndpoint                  (10), -- PMo on Atm
                   indexLimitExceeded              (11), -- PMo on Atm
                   atmPmObjectActive               (12), -- PMo on Atm
                   atmPmObjectNotExists            (13), -- PMo on Atm
                   endpointExistsAlready           (14), -- PMo on Atm

                   requestBusy                     (15),-- user command handling
                   setDataFailed                   (16),-- user command handling
                   aswTimeout                      (17),-- user command handling
                   commandFailed                   (18),-- user command handling
                   commandSucceeded                (19) -- user command handling
                  }

                    
     -- A type identifier to select the overall PM event counter
     -- These counters define a threshold limit profile.
     --
     Xldv20pmCounterName ::=
         INTEGER { 
            noPmCounter(0),
            neBackgroundBlockErrors(1),          -- s1n, adc, e3n
            neCodeViolations(2),                 -- s3n
            neCodeViolationsCP(3),               -- d3n
            neCodeViolationsP(4),                -- d3n
            neCodeViolationsPLCP(5),             -- d3n
            neErroredSeconds(6),                 -- s1n, s3n, adc, e3n
            neErroredSecondsCP(7),               -- d3n
            neErroredSecondsPLCP(8),             -- d3n
            neSas(9),                            -- d3n
            neSeverelyErroredFrameSeconds(10),   -- s3n
            neSeverelyErroredSeconds(11),        -- s1n, s3n, adc, e3n
            neSeverelyErroredSecondsCP(12),      -- d3n
            neSeverelyErroredSecondsPLCP(13),    -- d3n
            neUnavailableSeconds(14),            -- s1n, s3n, adc, e3n
            neUnavailableSecondsPLCP(15),        -- s3n
            neAisSeconds(16),                    -- s1n
            neOfsSeconds(17),                    -- s1n
            neLosSeconds(18),                    -- s1n
            feBackgroundBlockErrors(19),         -- s1f, adr, e3f
            feCodeViolations(20),                -- s3f
            feCodeViolationsCP(21),              -- d3f
            feCodeViolationsPLCP(22),            -- d3f
            feErroredSeconds(23),                -- s1f, s3f, adr, e3f
            feErroredSecondsCP(24),              -- d3f
            feErroredSecondsPLCP(25),            -- d3f
            feSas(26),                           -- d3f
            feSeverelyErroredSeconds(27),        -- s1f, s3f, adr, e3f
            feSeverelyErroredSecondsCP(28),      -- d3f
            feSeverelyErroredSecondsPLCP(29),    -- d3f
            feUnavailableSeconds(30),            -- s1f, s3f, adr, e3f
            feUnavailableSecondsPLCP(31),        -- s3f
            discardedCells(32),                  -- tca
            imaGroupUnavailSecs(33),             -- IMA Group
            imaGroupNeNumFailures(34),           -- IMA Group
            imaLinkImaViolations(35),            -- IMA Link
            imaLinkOifAnomalies(36),             -- IMA Link
            imaLinkNeSevErroredSecs(37),         -- IMA Link
            imaLinkFeSevErroredSecs(38),         -- IMA Link
            imaLinkNeUnavailSecs(39),            -- IMA Link
            imaLinkFeUnavailSecs(40),            -- IMA Link
            imaLinkNeTxUnusableSecs(41),         -- IMA link
            imaLinkNeRxUnusableSecs(42),         -- IMA link
            imaLinkFeTxUnusableSecs(43),         -- IMA link
            imaLinkFeRxUnusableSecs(44),         -- IMA link
            imaLinkNeTxNumFailures(45),          -- IMA link
            imaLinkNeRxNumFailures(46),          -- IMA link
            maxPmCounter(47)
                 }     

     -- A type identifier to select valid combination of TLM interfaces and OAM
     -- levels. To be used witin trap xldv20pmTrThresholdCrossingAlert.
     --
     Xldv20pmType ::=
         INTEGER { 
            pmTypeUnknown(0),
            s1F1PMo(1),              -- for Performance Monitoring
            s1F2PMo(2),
            s1F3PMo(3),
            s3F1PMo(4),
            s3F2PMo(5),
            s3F3PMo(6),
            d3F2PMo(7),
            d3F3PMo(8),
            e3F3PMo(9),
            adcF3PMo(10),
            s1PrM(11),               -- for Protocol Monitoring
            s3PrM(12),
            e3PrM(13),
            d3PrM(14),
            adcPrM(15),
            e1ImaPrM(16),            -- for PrM on E1/Ds1
            d1ImaPrM(17),
            e1ImaPMo(18),            -- for PMo on E1/Ds1
            d1ImaPMo(19),
            atmImaPMo(20),
            maxPmType(21)
                 }     

     -- --------------------------------------------------------------
     --                         T y p e s  for PM on ATM layer
     -- --------------------------------------------------------------

     -- An flow type identifier to select between the segement or
     -- endToEnd flow on F4 (VPC) or F5 (VCC) level
     --
     Xldv20pmFlowType ::=
         INTEGER {
            undefined(0),
            f4Segment(1),
            f4EndToEnd(2),
            f5Segment(3),
            f5EndToEnd(4)
                    }

     -- A type identifier to estimate the validity of a PM event
     -- counter
     --
     Xldv20pmSuspectType ::=
         INTEGER { undefined(0),
                   reliable(1),
                   suspect(2) }

     -- A type which selects the type and termination point
     -- of cross connection index
     --
     Xldv20pmConnectionIndexType ::=
         INTEGER { 
                   undefined(0),
                   vplUni(1),
                   vplNni(2),
                   vclUni(3),
                   vclNni(4)
                   }
     
     -- A type which selects the role of endpoint
     --
     Xldv20pmRoleType ::=
          INTEGER { undefined(0),
                    analyseWithBackwardReporting(1),
                    analyseWithoutBackwardReporting(2),
                    generateWithCollect(3),
                    generateWithoutCollect(4)}

     -- A type which selects the type of endpoint
     --
    Xldv20pmEndpointType ::=
          INTEGER { source(1),
                    sink(2) }

     -- A type which selects the block size
     -- A number of user cells after which a Pmo cell is inserted.
     --
     Xldv20pmBlockSizeType ::=
          INTEGER { undefined(0),
                    blockSize128(128),
                    blockSize256(256),
                    blockSize512(512),
                    blockSize1024(1024) }

     Xldv20pmAtmCounterType ::=
         INTEGER { 
            transmittedUserCells(1),
            lostUserCells(2), 
            misinsertedUserCells(3) }


