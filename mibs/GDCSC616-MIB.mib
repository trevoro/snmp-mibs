     	SYNTAX SEQUENCE OF BqlIntervalMaintenanceEntry
     	ACCESS not-accessible
     	STATUS mandatory
    	DESCRIPTION
     		"The SC616, DC610, DC612 Loop Interval  Maintenance table. "
         ::= { bql 18 }
 	
