			}
	ACCESS    read-write
	STATUS    mandatory
	DESCRIPTION     "Object to select loop and channel RTS/CTS delay
			 for DC610 and DC612 only."
	::= { bqlConfigEntry 10 }

bqlAntiStream OBJECT-TYPE
	SYNTAX	INTEGER {
			noAntistream(1),
			antistream(2)
			}
	ACCESS    read-write
	STATUS    mandatory
	DESCRIPTION     "Object to select loop and channel Anti-streaming
