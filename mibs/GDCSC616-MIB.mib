	::= { bqlConfigEntry 11 }

bqlTXDataElasticBufr OBJECT-TYPE
	SYNTAX	INTEGER {
			enable(1),
			disable(2)
			}
	ACCESS    read-write
	STATUS    mandatory
	DESCRIPTION     "Object to select loop and channel transmit data
			 elastic buffer	for DC610 and DC612 only."
	::= { bqlConfigEntry 12 }

bqlRTSCTSDelayTime OBJECT-TYPE
	SYNTAX	INTEGER {
			ms0(1),
			ms5(2),
			ms10(3),
			ms15(4),
			ms20(5),
			ms25(6),
			ms30(7),
			ms35(8),
			ms40(9),
			ms45(10),
			ms50(11),
