	--	1.4.3
	--	. rip-accept-filter
	--	Modeled as a table with filter number as the index.

	    alRipAccptFilTable OBJECT-TYPE
		SYNTAX  SEQUENCE OF RipAccptFilEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
		    "RIP Accept filter table" 
		::= { alrip 41 }

	    ripAccptFilEntry OBJECT-TYPE
	        SYNTAX  RipAccptFilEntry
	        ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
		    "RIP Accept filter entry "
		INDEX   { ripAccptFilNumber }
		::= { alRipAccptFilTable 1 }

	    RipAccptFilEntry ::= 
		SEQUENCE {
		    ripAccptFilNumber
			INTEGER(1..64),
        	    ripAccptFilAddr
            		IpAddress,
	            ripAccptFilMask
    		        IpAddress,
		    ripAccptFilPort
			INTEGER(1..14)
		}

	    ripAccptFilNumber OBJECT-TYPE
		SYNTAX  INTEGER(1..64)
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
		    "Filter Number; Range 1-64"
		::= { ripAccptFilEntry 1 }

	    ripAccptFilAddr OBJECT-TYPE 
		SYNTAX  IpAddress 
		ACCESS  read-write 
		STATUS  mandatory 
		DESCRIPTION 
		    "Valid network or subnet address" 
		::= { ripAccptFilEntry 2 }

	    ripAccptFilMask OBJECT-TYPE 
		SYNTAX  IpAddress 
		ACCESS  read-write 
		STATUS  mandatory 
		DESCRIPTION 
		    "Accept filter Mask" 
		::= { ripAccptFilEntry 3 }

	    ripAccptFilPort OBJECT-TYPE 
		SYNTAX  INTEGER(1..14) 
		ACCESS  read-write 
		STATUS  mandatory 
		DESCRIPTION 
		    "Port number on which network should be 
		    accepted or not. Range 1-14." 
		::= { ripAccptFilEntry 4 }

