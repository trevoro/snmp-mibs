	    tcpFilSrcMask OBJECT-TYPE
		SYNTAX  IpAddress
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
		    "Mask"
		::= { tcpFilEntry 3 }

	    tcpFilProtocol OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
		    "tcp(1) or udp(2)"
		::= { tcpFilEntry 4 }

	    tcpFilDstPort OBJECT-TYPE
