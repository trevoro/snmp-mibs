		ipDiagAlInternUdpProt    	INTEGER,

		ipDiagAlInternUdpPort     	INTEGER,

		ipDiagAlInternIcmpProt    	INTEGER,

		ipDiagAlInternAhProt  		INTEGER,

		ipDiagAlInternEspProt   	INTEGER

		}



	ipDiagAlInternIndex   OBJECT-TYPE

			SYNTAX  INTEGER

			ACCESS  read-only

			STATUS  mandatory

		DESCRIPTION

