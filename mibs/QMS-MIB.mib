qmsPtrBinEntry OBJECT-TYPE
	SYNTAX	QMSPtrBinEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION
			"A paper tray entry containing its currently selected
			default paper size and paper tray name."
	INDEX	{ qmsPtrBinIndex }
	::= { qmsPtrBinTable 1 }

QMSPtrBinEntry ::=
	SEQUENCE {
		qmsPtrBinIndex		INTEGER,
		qmsPtrBinType		INTEGER,
		qmsPtrBinId			INTEGER,
