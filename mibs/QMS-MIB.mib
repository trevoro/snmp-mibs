------------------------------------------------------------------------------
-- QMS CGM Emulation Option Handles
------------------------------------------------------------------------------

qmsCGMSuppressScale OBJECT-TYPE
	SYNTAX INTEGER {
		yes(2),
		no(1)
	}
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
        "Specifies whether the file should be scaled.

        Choices
            No:  The file is scaled.
            Yes: The file is NOT scaled. 

        Default
            No

        Notes
            If yes is Selected and a file with metric scaling is printed, the file 
            will not be scaled."
        ::= { qmsCGM 1 }

qmsCGMMono OBJECT-TYPE
	SYNTAX INTEGER {
		on(2),
		off(1)
	}
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
        "Specifies whether the file should be printed in monochrome or 
        color.

        Choices
            On:  The file prints in monochrome.
            Off: The file prints in color.

        Default
            Off

        Notes
            Off (color printing) is not available on monochrome printers."
        ::= { qmsCGM 2 }

qmsCGMIgnoreBorder OBJECT-TYPE
	SYNTAX INTEGER {
		yes(2),
		no(1)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"Causes the non-printable area of a page to be ignored.

                Choices
                    Yes, No

                Default
                    No

                Notes
                    If the non-printable area is ignored, the image is shifted up and to 
                    the right."
	::= { qmsCGM 3 }

qmsCGMOriginX OBJECT-TYPE
	SYNTAX INTEGER (0..1700)
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
        "The viewport origin, along with the viewport size, determines the paper size selected.
        The image is scaled to fit within the specified viewport.
        Specifies the X origin of the CGM viewport in .01'' increments.

        Choices
            0-1700 (0''-17.00'')

        Default
            100 (1'')"
        ::= { qmsCGM 4 }

qmsCGMOriginY OBJECT-TYPE
	SYNTAX INTEGER (0..1700)
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
        "The viewport origin, along with the viewport size, determines the paper size selected.
        The image is scaled to fit within the specified viewport.
        Specifies the Y origin of the CGM viewport in .01'' increments.

        Choices
            0-1700 (0''-17.00'')

        Default
            100 (1'')"
        ::= { qmsCGM 8 }

qmsCGMPrintErrors OBJECT-TYPE
	SYNTAX INTEGER {
		yes(2),
		no(1)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
	"Causes an additional page, listing errors encountered, to print if 
        a CGM job contains errors.

        Choices
            Yes, No

        Default
            No"
	::= { qmsCGM 12 }

