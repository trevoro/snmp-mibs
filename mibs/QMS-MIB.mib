	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
			"The input/output tray designator (input=1, output=0)."
	::= { qmsPtrBinEntry 2 }

qmsPtrBinId OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
