qmsPtrJobsSheet OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
			"The number of sheets defined for this job.  Note: Sheets
			are defined as the physical sheet of paper as opposed to
			the number of logical page images appearing on the physical
			sheet of paper."
	::= { qmsPtrJobsEntry 6 }

qmsPtrJobsChannel OBJECT-TYPE
	SYNTAX	INTEGER {
				other(1),	-- was "other(0)"
