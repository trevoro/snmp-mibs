                matrixPortGateway OBJECT-TYPE
                        SYNTAX  IpAddress
                        ACCESS  read-write
                        STATUS  mandatory
                        DESCRIPTION
                                "IP gateway of the device connected
                                 to a specific matrix port."
                        ::= { matrixPortEntry 10 }

        matrixLastChange OBJECT-TYPE
                SYNTAX  TimeTicks
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                        "Timestamp for last change in matrix tables on any
                        Matrix module"
