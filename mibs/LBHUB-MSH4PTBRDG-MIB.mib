    --

    cPenr           OBJECT IDENTIFIER ::= { cProducts 7 }
    cPenril         OBJECT IDENTIFIER ::= { --! POINTER Penril_OID
                                                cPenr 1 }
    --
    --
    -- This subtree is allocated for the use of the 3com
