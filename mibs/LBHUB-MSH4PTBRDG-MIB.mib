		cOperIndex				INTEGER,
		cOperID					OCTET STRING,
		cOperPriv				INTEGER,
		cOperPasswd				OCTET STRING,
		cOperStatus				INTEGER
	}

	cOperIndex OBJECT-TYPE
		SYNTAX  INTEGER (1..60)
		ACCESS	read-write
		STATUS	mandatory

		DESCRIPTION
			"This variable indexes the table of operator accounts."
