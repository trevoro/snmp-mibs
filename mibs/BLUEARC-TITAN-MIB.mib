--
-- StorageTitan / Virtual Volumes
--
--------------------------------------------------------------------------------

    virtualVolumeTitanNumber OBJECT-TYPE
        SYNTAX Integer32 (1..2147483647)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The number of virtual volumes which are currently accessible.
             NOTE: a virtual volume is only accessible if its associated
                   parent volume is mounted."
        ::= { virtualVolumesTitan 1 }

    virtualVolumeTitanTable OBJECT-TYPE
        SYNTAX SEQUENCE OF VirtualVolumeTitanEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "A table containing information about each virtual volume.

             The number of entries in this table is given by the value of
             virtualVolumeTitanNumber."
        ::= { virtualVolumesTitan 2 }

    virtualVolumeTitanEntry OBJECT-TYPE
        SYNTAX VirtualVolumeTitanEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "Information about a virtual volume."
        INDEX { virtualVolumeTitanSpanId, virtualVolumeTitanName }
        ::= { virtualVolumeTitanTable 1 }

    VirtualVolumeTitanEntry ::=
        SEQUENCE {
            virtualVolumeTitanSpanId
                Unsigned32,
            virtualVolumeTitanName
                DisplayString,
            virtualVolumeTitanPath
                DisplayString,
            virtualVolumeTitanEmailNumber
                Integer32,
            virtualVolumeTitanQuotaNumber
                Integer32
        }

    virtualVolumeTitanSpanId OBJECT-TYPE
        SYNTAX Unsigned32 (1..4294967295)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The parent device of the virtual volume.
             The device identified by a particular value of this index is
             the span underlying the volume hosting the Virtual Volume."
        ::= { virtualVolumeTitanEntry 1 }

    virtualVolumeTitanName OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The name assigned to the virtual volume by a user for
             identification purposes."
        ::= { virtualVolumeTitanEntry 2 }

    virtualVolumeTitanPath OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The path to the root of this Virtual Volume."
        ::= { virtualVolumeTitanEntry 3 }

    virtualVolumeTitanEmailNumber OBJECT-TYPE
        SYNTAX Integer32 (1..2147483647)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The number of email addresses associated with this
            Virtual Volume."
        ::= { virtualVolumeTitanEntry 4 }

    virtualVolumeTitanQuotaNumber OBJECT-TYPE
        SYNTAX Integer32 (1..2147483647)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The number of quotas associated with this
            Virtual Volume."
        ::= { virtualVolumeTitanEntry 5 }

    virtualVolumeTitanEmailTable OBJECT-TYPE
        SYNTAX SEQUENCE OF VirtualVolumeTitanEmailEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "A table containing the email address associated with
            this Virtual Volume.

             The number of entries in this table is given by the value of
             virtualVolumeTitanEmailNumber, in virtualVolumeTitanTable."
        ::= { virtualVolumesTitan 3 }

    virtualVolumeTitanEmailEntry OBJECT-TYPE
        SYNTAX VirtualVolumeTitanEmailEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "An email address associated with a Virtual Volume,
            which is uniquely identified by virtualVolumeTitanSpanId
            and virtualVolumeTitanName in virtualVolumeTitanTable."
        INDEX { virtualVolumeTitanSpanId, virtualVolumeTitanName, virtualVolumeTitanEmailAddress }
        ::= { virtualVolumeTitanEmailTable 1 }

    VirtualVolumeTitanEmailEntry ::=
        SEQUENCE {
            virtualVolumeTitanEmailAddress
                DisplayString
        }

    virtualVolumeTitanEmailAddress OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "An email address associated with a Virtual Volume."
        ::= { virtualVolumeTitanEmailEntry 1 }

    virtualVolumeTitanQuotaTemplateTable OBJECT-TYPE
        SYNTAX SEQUENCE OF VirtualVolumeTitanQuotaTemplateEntry
        MAX-ACCESS not-accessible
        STATUS obsolete
        DESCRIPTION
            "A table containing quota templates associated with
            Virtual Volumes. The template is indexed by the values
            virtualVolumeTitanSpanId and virtualVolumeTitanTarget in
            virtualVolumeTitanTable, and the
            virtualVolumeQuotaTemplateTargetType.

            The two types of quota template are: user quota template,
            and group quota template. The templates define the
            default quota settings which will be used to create a
            Default Quota when a user or group begins to use the Virtual
            Volume. Once the default quota has been created, a user
            or group's quota may be modified, whence it will become an
            Explicit Quota.

            This object is now obsolete."
        ::= { virtualVolumesTitan 4 }

    virtualVolumeTitanQuotaTemplateEntry OBJECT-TYPE
        SYNTAX VirtualVolumeTitanQuotaTemplateEntry
        MAX-ACCESS not-accessible
        STATUS obsolete
        DESCRIPTION
            "A quota template associated with a Virtual Volume.
            The template is indexed by the values virtualVolumeTitanSpanId
            and virtualVolumeTitanTarget in virtualVolumeTitanTable, and 
            the virtualVolumeQuotaTemplateTargetType.

            This object is now obsolete."
        INDEX { virtualVolumeTitanSpanId, virtualVolumeTitanName, virtualVolumeQuotaTemplateTargetType }
        ::= { virtualVolumeTitanQuotaTemplateTable 1 }

    VirtualVolumeTitanQuotaTemplateEntry ::=
        SEQUENCE {
            virtualVolumeQuotaTemplateTargetType
