                STATUS mandatory
                DESCRIPTION
               "The status of the entry.  Set to 'disabled' to
               disable the entry."
                ::= { bootRecordEntry 21 }

            bootRecordMopFile OBJECT-TYPE
                SYNTAX DisplayString (SIZE (0..15))
                ACCESS read-write
                STATUS mandatory
                DESCRIPTION
                    "The software file name to request for down-line 
                    load of the server via MOP or XMOP."
                ::= { bootRecordEntry 22 }

