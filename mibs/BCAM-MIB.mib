    bcamTsapTabNeaTsel OCTET STRING (SIZE(8)),
    bcamTsapTabHost DisplayString (SIZE(8)),
    bcamTsapTabDiagnostic OCTET STRING (SIZE(4)),
    bcamTsapTabTsduSends Counter ,
    bcamTsapTabByteSends Counter ,
    bcamTsapTabTsduReceiveds Counter ,
    bcamTsapTabByteReceiveds Counter ,
    bcamTsapTabSendCallOverMaxs Counter ,
    bcamTsapTabLetterTimeouts Counter ,
    bcamTsapTabInWaitBuck1Hist INTEGER (0..65535),
    bcamTsapTabInWaitBuck2Hist INTEGER (0..65535),
    bcamTsapTabInWaitBuck3Hist INTEGER (0..65535),
    bcamTsapTabInWaitBuck4Hist INTEGER (0..65535),
    bcamTsapTabInWaitBuck5Hist INTEGER (0..65535),
    bcamTsapTabReactBuck1Hist INTEGER (0..65535),
    bcamTsapTabReactBuck2Hist INTEGER (0..65535),
    bcamTsapTabReactBuck3Hist INTEGER (0..65535),
    bcamTsapTabReactBuck4Hist INTEGER (0..65535),
    bcamTsapTabReactBuck5Hist INTEGER (0..65535),
    bcamTsapTabInWaitBuck1 INTEGER (0..65535),
    bcamTsapTabInWaitBuck2 INTEGER (0..65535),
    bcamTsapTabInWaitBuck3 INTEGER (0..65535),
    bcamTsapTabInWaitBuck4 INTEGER (0..65535),
    bcamTsapTabInWaitBuck5 INTEGER (0..65535),
    bcamTsapTabReactBuck1 INTEGER (0..65535),
    bcamTsapTabReactBuck2 INTEGER (0..65535),
    bcamTsapTabReactBuck3 INTEGER (0..65535),
    bcamTsapTabReactBuck4 INTEGER (0..65535),
    bcamTsapTabReactBuck5 INTEGER (0..65535),
    bcamTsapTabTsduSendHists Counter ,
    bcamTsapTabByteSendHists Counter ,
    bcamTsapTabTsduReceivedHists Counter ,
    bcamTsapTabByteReceivedHists Counter ,
    bcamTsapTabSendCallOverMaxHists Counter ,
    bcamTsapTabLetterTimeoutHists Counter ,
    bcamTsapTabFunction INTEGER ,
    bcamTsapTabCurrConn Gauge ,
    bcamTsapTabCloseConns Counter ,
    bcamTsapTabClessSendBytes Counter ,
    bcamTsapTabClessRecvBytes Counter ,
