        sCritCurCnt      INTEGER
 }

sCritIndex OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
                "Location in the critical process table."
        ::= { sCritEntry 1 }

sCritName OBJECT-TYPE
        SYNTAX  DisplayString (SIZE (0..15))
