	   HELP = ;
	   CAPABILITIES = ;"
	DEFVAL { disable }
	::= { mcmT1E1CsuCfgEntry 16 }

mcmT1E1CsuCfgTransmitLoopDown OBJECT-TYPE
	SYNTAX	INTEGER {
		disable (1),
		enable (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object selects the T1 CSU \
		  transmit loop down. \
		  Note that for an E1 CSU interface, this object is not used.;
	   HELP = ;
	   CAPABILITIES = ;"
	DEFVAL { disable }
	::= { mcmT1E1CsuCfgEntry 17 }

mcmT1E1CsuCfgTxRxHDB3	 OBJECT-TYPE
	SYNTAX	INTEGER {
		disable (1),
		enable  (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object selects the E1 CSU HDB3. \
		  Note that for a T1 CSU interface, this object is not used.;
	   HELP = ;
	   CAPABILITIES = ;"
	DEFVAL { enable }
	::= { mcmT1E1CsuCfgEntry 18 }

mcmT1E1CsuCfgTxRxCRC4	 OBJECT-TYPE
	SYNTAX	INTEGER {
		disable (1),
		enable  (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object selects the E1 CSU CRC4. \
		  Note that for a T1 CSU interface, this object is not used.;
	   HELP = ;
	   CAPABILITIES = ;"
	DEFVAL { disable }
	::= { mcmT1E1CsuCfgEntry 19 }

mcmT1E1CsuCfgDS0Connection OBJECT-TYPE
	SYNTAX	DisplayString (SIZE (0..83))
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object selects the DS0 \
		  channels for the T1/E1 CSU line connection. \
		  Valid characters are decimal digits, comma \
		  (','), and hyphen ('-').;
	   HELP = Comma (',') is used as a delimiter between \
		  individual channel selections. \
		  e.g., 13,16 means DS0 channels 13 and 16. \
		  Hyphen ('-') is used only in Set operations \
		  to indicate a range of DS0 channels. \
		  e.g., 2,5-8 means DS0 channels 2,5,6,7, and 8.;
	   CAPABILITIES = ;"
	DEFVAL { "" }
	::= { mcmT1E1CsuCfgEntry 20 }



--  the 56K CSU Configuration Table 
--  This table provides the 56K CSU configuration parameters.

mcmCsuCfgTable  OBJECT-TYPE
    SYNTAX  SEQUENCE OF McmCsuCfgEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
      "NAME = ;
           DESC = The 56K CSU Configuration table.;
       HELP = ;
           CAPABILITIES = NET_CFG, VPN_CFG;"
    ::= { csu-configuration 2 }

mcmCsuCfgEntry OBJECT-TYPE
    SYNTAX  McmCsuCfgEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
      "NAME = ;
           DESC = An entry in the 56K CSU Configuration \
                  table.);
           HELP = ;
           CAPABILITIES = ;"
    INDEX   { mcmCsuCfgifIndex }
    ::= { mcmCsuCfgTable 1 }

McmCsuCfgEntry ::= SEQUENCE {
	mcmCsuCfgifIndex				INTEGER,
    mcmCsuCfgOperatingMode		INTEGER,
    mcmCsuCfgClockingSource		INTEGER,
    mcmCsuCfgTxOutOfFrame		INTEGER,
    mcmCsuCfgTxOutOfService		INTEGER,
    mcmCsuCfgTxControlModeIdle  	INTEGER,
    mcmCsuCfgZeroSuppressDisable 	INTEGER,
    mcmCsuCfgTxIdle			INTEGER,
    mcmCsuCfgCSULoopback		INTEGER,
    mcmCsuCfgFilterForceEnable  	INTEGER,
    mcmCsuCfgFilterForcingCntl   	INTEGER
    }

mcmCsuCfgifIndex OBJECT-TYPE
	SYNTAX	INTEGER (1..255)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The 56k Csu Cfg interface index \
				value of the specifc entry.;
	   HELP = ;
	   CAPABILITIES = ;"
	::= { mcmCsuCfgEntry 1 }

mcmCsuCfgOperatingMode OBJECT-TYPE
	SYNTAX	INTEGER {
		dds-pri-4Wire-56k (1),
		dds-sc-WithSecondaryChannel-72k (2),
		cc-64k-ClearChannel-64k (3),
		dds-pri-2Wire-56k (4)
		}
	ACCESS	read-write
	STATUS	mandatory	
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object selects the 56K CSU \
		  operating mode.;
	   HELP = ;
	   CAPABILITIES = ;"
	DEFVAL { dds-pri-4Wire-56k }
	::= { mcmCsuCfgEntry 2 }

mcmCsuCfgClockingSource OBJECT-TYPE
	SYNTAX	INTEGER {
		externalClock (1),
		internalClock (2)
		}
	ACCESS	read-write
	STATUS	mandatory	
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object selects the 56K CSU \
		  clocking source.;
	   HELP = ;
	   CAPABILITIES = ;"
	DEFVAL { externalClock }
	::= { mcmCsuCfgEntry 3 }

mcmCsuCfgTxOutOfFrame OBJECT-TYPE
	SYNTAX	INTEGER {
		normalTransmitCondition (1),
		transmitOutOfFrame (2)
		}
	ACCESS	read-write
	STATUS	mandatory
