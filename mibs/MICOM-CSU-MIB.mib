	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object indicates the number \
		  of E1 CSU FAS error counts.  FAS error counts \
		  record word errors in the frame alignment \
		  signal in timeslot 0. \
		  Note that this is a 12-bit counter.;
	   HELP = ;
	   CAPABILITIES = ;"
 	::= { mcmE1CsuGenStatusEntry 13 }

mcmE1CsuGenStatusCRC4Errors OBJECT-TYPE
	SYNTAX	INTEGER (0..1023)	
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object indicates the number \
		  of E1 CSU CRC4 error counts.  This object \
		  is only applicable when CRC4 is enabled. \
		  Note that this is a 10-bit counter.;
	   HELP = ;
	   CAPABILITIES = ;"
	::= { mcmE1CsuGenStatusEntry 14 }



--  the 56K CSU Status and Statistics Table 
--  This table provides the 56K CSU status and statistics parameters.

mcmCsuStatusTable  OBJECT-TYPE
    SYNTAX  SEQUENCE OF McmCsuStatusEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
      "NAME = ;
           DESC = The 56K CSU Status and Statistics table.;
       HELP = ;
           CAPABILITIES = NET_DISP, VPN_DISP;"
    ::= { csu-status 4 }

mcmCsuStatusEntry OBJECT-TYPE
    SYNTAX  McmCsuStatusEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
      "NAME = ;
           DESC = An entry in the 56K CSU Status \
                  and Statistics table.;
           HELP = ;
           CAPABILITIES = ;"
    INDEX   { mcmCsuStatusifIndex }
    ::= { mcmCsuStatusTable 1 }

McmCsuStatusEntry ::= SEQUENCE {
	mcmCsuStatusifIndex				INTEGER,
    mcmCsuStatusLineStatus			INTEGER,
    mcmCsuStatusRxLossOfSignal		INTEGER,
    mcmCsuStatusFAWSync			INTEGER,
    mcmCsuStatusLoopPresent			INTEGER,
    mcmCsuStatusInsertLossLineLength    	INTEGER,
    mcmCsuStatusRxSignalMag			INTEGER,
    mcmCsuStatusInvalidBPVcounts		INTEGER	
    }

mcmCsuStatusifIndex OBJECT-TYPE
	SYNTAX	INTEGER (1..255)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The 56k CSU Status interface index \
				value of the specifc entry.;
	   HELP = ;
	   CAPABILITIES = ;"
	::= { mcmCsuStatusEntry 1 }

mcmCsuStatusLineStatus OBJECT-TYPE
	SYNTAX	INTEGER {
		operational (1),
		linkDown (2),
		testMode (3)
		}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object indicates the 56K \
		  CSU line status.;
	   HELP = ;
	   CAPABILITIES = ;"
	::= { mcmCsuStatusEntry 2 }

mcmCsuStatusRxLossOfSignal OBJECT-TYPE
	SYNTAX	INTEGER {
		yes (1),
		no (2)
		}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object indicates if the 56K \
		  CSU receiver has lost signal.;
	   HELP = ;
	   CAPABILITIES = ;"
	::= { mcmCsuStatusEntry 3 }

mcmCsuStatusFAWSync OBJECT-TYPE
	SYNTAX	INTEGER {
		yes (1),
		no (2)
		}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object indicates the 56K \
		  CSU DDS FAW Sync in DDS-SC or CC-64K.;
	   HELP = ;
	   CAPABILITIES = ;"
	::= { mcmCsuStatusEntry 4 }

mcmCsuStatusLoopPresent OBJECT-TYPE
	SYNTAX	INTEGER {
		yes (1),
		no (2)
		}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object indicates if the 56K \
		  CSU loop is currently present.;
	   HELP = ;
	   CAPABILITIES = ;"
	::= { mcmCsuStatusEntry 5 }

mcmCsuStatusInsertLossLineLength OBJECT-TYPE
	SYNTAX	INTEGER {
		iL50pt5db-LL8pt5km (7),
		iL44pt4db-LL7pt5km (8),
		iL38pt4db-LL6pt4km (9),
		iL32pt3db-LL5pt3km (10),
		iL26pt0db-LL4pt2km (11),
		iL19pt7db-LL3pt2km (12),
		iL13pt6db-LL2pt1km (13),
		iL7pt3db-LL1pt1km (14),
		iL0db-LL0km (15)
		}	
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object indicates the 56K \
		  CSU receiver Insertion loss (iL) in db, and \
		  Line Length (LL) in kilometers.;
	   HELP = ;
	   CAPABILITIES = ;"
	::= { mcmCsuStatusEntry 6 }

mcmCsuStatusRxSignalMag OBJECT-TYPE
	SYNTAX	INTEGER (0..255)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object indicates the 56K \
		  CSU receive signal magnitude expressed as \
		  the 'receiver slicer level'.  This value \
		  is used in conjunction with the db Insertion \
		  Loss ('iL' from InsertLossLineLength object) \
		  to compute the signal magnitude as follows: \
		  Nyquist frequency loop attn (in db) = \
		    [(iL)**2 + 20log10(51/RxSignalMag)].;
	   HELP = ;
	   CAPABILITIES = ;"
	::= { mcmCsuStatusEntry 7 }

mcmCsuStatusInvalidBPVcounts OBJECT-TYPE
	SYNTAX	INTEGER (0..255)	
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object indicates the 56K \
		  CSU invalid BPV (Bipolar Violation) count. \
		  Note that this is an 8-bit counter.;
	   HELP = ;
	   CAPABILITIES = ;"
	::= { mcmCsuStatusEntry 8 }



-- Trap Messages


-- T1 CSU Trap Messages 

mcmT1CsuRedAlarmSet TRAP-TYPE
    ENTERPRISE  micom-csu
    VARIABLES   { mcmSysAsciiTimeOfDay, mcmT1E1CsuCfgifIndex }
    DESCRIPTION
      "NAME = ;
       DESC =  When a downstream node detects a loss of \
               synchronization in its receive pair of a \
               T1 line, it is in red alarm condition. \
               Severity Level: CRITICAL.;
       HELP = ;"
    ::= 1

mcmT1CsuRedAlarmClear TRAP-TYPE
    ENTERPRISE  micom-csu
    VARIABLES   { mcmSysAsciiTimeOfDay, mcmT1E1CsuCfgifIndex }
    DESCRIPTION
      "NAME = ;
       DESC =  When a downstream node is back in \
               synchronization with its receive pair \
               of a T1 line. This trap clears the node \
               that has the Red Alarm condition. \
               Severity Level : INFORMATIVE.;
       HELP = ;"
    ::= 2

mcmT1CsuYellowAlarmSet TRAP-TYPE
    ENTERPRISE  micom-csu
    VARIABLES   { mcmSysAsciiTimeOfDay, mcmT1E1CsuCfgifIndex }
    DESCRIPTION
      "NAME = ;
       DESC =  When a downstream node detects a loss of \
               synchronization in its receive pair of a \
               T1 line, it is in red alarm condition. \
               In turn, it transmits a yellow alarm \
               signal upstream to the previous node. \
               Severity Level: MAJOR.;
       HELP = ;"
    ::= 3

mcmT1CsuYellowAlarmClear TRAP-TYPE
    ENTERPRISE  micom-csu
    VARIABLES   { mcmSysAsciiTimeOfDay, mcmT1E1CsuCfgifIndex }
    DESCRIPTION
      "NAME = ;
       DESC =  When a downstream node detects a loss of \
               synchronization in its receive pair of a \
               T1 line, it is in red alarm condition. \
               In turn, it transmits a yellow alarm \
               signal upstream to the previous node. \
               This trap clears the node that has the \
               Yellow Alarm condition.;
               Severity Level: INFORMATIVE.;
       HELP = ;"
    ::= 4

mcmT1CsuBlueAlarmSet TRAP-TYPE
    ENTERPRISE  micom-csu
    VARIABLES   { mcmSysAsciiTimeOfDay, mcmT1E1CsuCfgifIndex }
    DESCRIPTION
      "NAME = ;
       DESC =  When a downstream node detects a loss of \
               synchronization in its receive pair of a \
               T1 line, it is in red alarm condition. \
               In turn, it transmits a yellow alarm \
               signal upstream to the previous node \
               and a blue alarm signal downstream to \
               the next node.\
               Severity Level: MINOR.;
       HELP = ;"
    ::= 5

mcmT1CsuBlueAlarmClear TRAP-TYPE
    ENTERPRISE  micom-csu
    VARIABLES   { mcmSysAsciiTimeOfDay, mcmT1E1CsuCfgifIndex }
    DESCRIPTION
      "NAME = ;
       DESC =  When a downstream node detects a loss of \
               synchronization in its receive pair of a \
               T1 line, it is in red alarm condition. \
               In turn, it transmits a yellow alarm \
               signal upstream to the previous node \
               and a blue alarm signal downstream to \
               the next node. This trap clears the \
               node that has the Blue Alarm condition. \
               Severity Level: INFORMATIVE.;
       HELP = ;"
    ::= 6


-- E1 CSU Trap Messages 

mcmE1CsuRedAlarmSet TRAP-TYPE
    ENTERPRISE  micom-csu
    VARIABLES   { mcmSysAsciiTimeOfDay, mcmT1E1CsuCfgifIndex }
    DESCRIPTION
      "NAME = ;
       DESC =  This trap is generated upon the occurrence \
	  of an E1 CSU Red Alarm. A red alarm indicates that \
	  an out-of-frame condition existed for the last 100 \
	  milliseconds or longer. \
	  Severity Level: CRITICAL.;
       HELP = ;"
    ::= 7

mcmE1CsuRedAlarmClear TRAP-TYPE
    ENTERPRISE  micom-csu
    VARIABLES   { mcmSysAsciiTimeOfDay, mcmT1E1CsuCfgifIndex }
    DESCRIPTION
      "NAME = ;
       DESC = This trap is generated upon the clearance \
	  of an E1 CSU Red Alarm. A red alarm is cleared \
	  when the out-of-frame condition has been absent \
	  for 100 milliseconds. \
	  Severity Level : INFORMATIVE.;
       HELP = ;"
    ::= 8

mcmE1CsuRemoteAlarmSet TRAP-TYPE
    ENTERPRISE  micom-csu
    VARIABLES   { mcmSysAsciiTimeOfDay, mcmT1E1CsuCfgifIndex }
    DESCRIPTION
      "NAME = ;
       DESC = This trap is generated upon the occurrence \
	  of an E1 CSU Remote Alarm. \
	  Severity Level: MAJOR.;
       HELP = ;"
    ::= 9

mcmE1CsuRemoteAlarmClear TRAP-TYPE
    ENTERPRISE  micom-csu
    VARIABLES   { mcmSysAsciiTimeOfDay, mcmT1E1CsuCfgifIndex }
    DESCRIPTION
      "NAME = ;
       DESC = This trap is generated upon the clearance \
	  of an E1 CSU Remote Alarm. \
	  Severity Level: INFORMATIVE.;
       HELP = ;"
    ::= 10

mcmE1CsuAISAlarmSet TRAP-TYPE
    ENTERPRISE  micom-csu
    VARIABLES   { mcmSysAsciiTimeOfDay, mcmT1E1CsuCfgifIndex }
    DESCRIPTION
      "NAME = ;
       DESC = This trap is generated upon the occurrence \
	  of an E1 CSU AIS Alarm. The AIS (alarm indication \
	  signal) alarm is detected as an all ones condition \
	  on the E1 line persisting for 100ms or longer. \
	  This indicates the E1 line is unavailable. \
	  Severity Level: MINOR.;
       HELP = ;"
    ::= 11

mcmE1CsuAISAlarmClear TRAP-TYPE
    ENTERPRISE  micom-csu
    VARIABLES   { mcmSysAsciiTimeOfDay, mcmT1E1CsuCfgifIndex }
    DESCRIPTION
      "NAME = ;
       DESC = This trap is generated upon the clearance \
	  of an E1 CSU AIS Alarm. \
	  Severity Level: INFORMATIVE.;
       HELP = ;"
    ::= 12


END

