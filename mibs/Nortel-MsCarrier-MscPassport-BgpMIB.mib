      mscVrIpBgpPeer component."
   INDEX {
      mscVrIndex,
      mscVrIpIndex,
      mscVrIpBgpIndex,
      mscVrIpBgpPeerPeerAddressIndex }
::= { mscVrIpBgpPeerRowStatusTable 1 }

