      mscVrIpBgpExportIndex }
::= { mscVrIpBgpExportRowStatusTable 1 }

MscVrIpBgpExportRowStatusEntry ::= SEQUENCE {
   mscVrIpBgpExportRowStatus
      RowStatus,
   mscVrIpBgpExportComponentName
      DisplayString,
   mscVrIpBgpExportStorageType
      StorageType,
