   mscVrIpBgpPeerRowStatus
      RowStatus,
   mscVrIpBgpPeerComponentName
      DisplayString,
   mscVrIpBgpPeerStorageType
      StorageType,
   mscVrIpBgpPeerPeerAddressIndex
      IpAddress }

