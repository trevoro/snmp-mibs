DATASMART-MIB DEFINITIONS ::= BEGIN

IMPORTS
    enterprises, Gauge, IpAddress, Counter, TimeTicks
        FROM RFC1155-SMI
    OBJECT-TYPE
        FROM RFC-1212
--	DisplayString
--		FROM RFC1213-MIB
--    ifIndex, ifDescr
--        FROM RFC1213-MIB
--    dsx1LineStatus, dsx1CurrentESs, dsx1CurrentUASs
--        FROM RFC1406-MIB
    TRAP-TYPE
        FROM RFC-1215;

-- Internal Datatypes
-- 0..DLCINumber from RFC1315
   DLCI ::= INTEGER (1..1023)

-- From SNMPv2-SMI (SimpleTest can't compile if mix v1 & v2)
   Counter32 ::= Counter


    -- This type was added so that sunnet manager would display strings
    -- as strings instead of their ascii value.
    DisplayString ::=
        OCTET STRING
