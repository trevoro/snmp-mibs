      mscAppnIndex }
::= { mscAppnRowStatusTable 1 }

MscAppnRowStatusEntry ::= SEQUENCE {
   mscAppnRowStatus
      RowStatus,
   mscAppnComponentName
      DisplayString,
   mscAppnStorageType
