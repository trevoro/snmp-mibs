   mscAppnDnaDefaultSendToNetworkThruputClass
      Unsigned32,
   mscAppnDnaDefaultRecvFrmNetworkWindowSize
      Unsigned32,
   mscAppnDnaDefaultSendToNetworkWindowSize
      Unsigned32,
   mscAppnDnaAccountClass
      Unsigned32,
