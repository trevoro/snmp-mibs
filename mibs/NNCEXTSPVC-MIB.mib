     SYNTAX     INTEGER { disabled(1),       -- Disabled
                          enabledUniSide(2), -- Enabled UNI side
                          enabledNniSide(3)  -- Enabled NNI side
                        }
     MAX-ACCESS read-create
     STATUS     current
     DESCRIPTION
          "This object is used to configure the 'reroute on loss of continuity' 
           feature using OAM-CC cells.  It can be configured for either the UNI 
           side or the NNI side, or disabled."

     DEFVAL { disabled }

     ::= { nncFrSpvcTableEntry 12 }


   nncFrSpvcSrcBillingFlag   OBJECT-TYPE           
     SYNTAX     INTEGER { disabled(0), enabled(1) }
     MAX-ACCESS read-create
     STATUS     current
     DESCRIPTION  "Billing Configuration for the source endpoint."     

     DEFVAL { disabled }
         
     ::= { nncFrSpvcTableEntry 13 }

   nncFrSpvcFrPriority   OBJECT-TYPE            
     SYNTAX     INTEGER { bestEffort(1), committedThroughput(2), 
                          lowLatency(3), realTime(4) }
     MAX-ACCESS read-create
     STATUS     current
     DESCRIPTION   "Frame Relay Priority."    

     DEFVAL { bestEffort }
        
     ::= { nncFrSpvcTableEntry 14 }
          
   nncFrSpvcFrVsvdCongestionControl    OBJECT-TYPE               
     SYNTAX     INTEGER {  disabled(0), enabled(1) }
     MAX-ACCESS read-create
     STATUS     current
