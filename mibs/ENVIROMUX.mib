	::= { ipLinkEntry 1 }

ipLinkAddress           OBJECT-TYPE
	SYNTAX				DisplayString
	MAX-ACCESS  		read-write
	STATUS				current
	DESCRIPTION	        "IP Link URL"
	::= { ipLinkEntry 2 }
    
ipLinkDescription       OBJECT-TYPE
	SYNTAX				DisplayString
	MAX-ACCESS  		read-write
	STATUS				current
	DESCRIPTION	        "IP Link Description"
	::= { ipLinkEntry 3 }
    


    
END

	
    

