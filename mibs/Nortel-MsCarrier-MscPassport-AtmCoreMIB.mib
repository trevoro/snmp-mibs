   mscAtmIfVccOperStatus
      INTEGER,
   mscAtmIfVccLocalFailureCause
      INTEGER,
   mscAtmIfVccTroubled
      INTEGER,
   mscAtmIfVccSegLinkSideLoopbackState
      INTEGER,
   mscAtmIfVccSegSwitchSideLoopbackState
      INTEGER,
   mscAtmIfVccEndToEndLoopbackState
      INTEGER,
