mscAtmIfVpcVpdRxQosClass OBJECT-TYPE
   SYNTAX     INTEGER  {
                 unspecified(0),
                 constantBitRate(1),
                 variableBitRate(2),
                 connectionOriented(3),
                 connectionless(4),
                 sameAsTx(15) }
