   DEFVAL     { sameAsInterface }
::= { mscAtmIfVptVccVcdTrafficEntry 60 }

mscAtmIfVptVccVcdUsageParameterControl OBJECT-TYPE
   SYNTAX     INTEGER  {
                 disabled(1),
                 sameAsInterface(2) }
