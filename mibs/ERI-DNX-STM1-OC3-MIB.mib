	Counter32, NOTIFICATION-TYPE
		FROM SNMPv2-SMI
    DisplayString, TEXTUAL-CONVENTION
        FROM SNMPv2-TC
    eriMibs
        FROM ERI-ROOT-SMI
    devices, PortStatus, OneByteField, NestSlotAddress,
    LinkPortAddress, LinkCmdStatus, trapSequence
        FROM ERI-DNX-SMC-MIB;

eriDNXStm1Oc3MIB MODULE-IDENTITY 
      LAST-UPDATED    "200305050000Z"
      ORGANIZATION    "Eastern Research, Inc."
      CONTACT-INFO
                      "Customer Service
                Postal: Eastern Research, Inc.
                        225 Executive Drive
                        Moorestown, NJ 08057
                Phone:  +1-800-337-4374
                Email:  support@erinc.com"
      DESCRIPTION
              "The ERI Enterprise MIB Module for the DNX STM1 and OC3 devices."

      REVISION        "200305050000Z"
      DESCRIPTION
              "Nevio Poljak - eri_DnxNest MIB Rev 01.3 (SW Rel. 15.2)
               Added sts-3c(3) and au4-vc3-Seq(4) selections to optDevMultiplexMode 
               field from the Optical Device Config table to support new Link 
