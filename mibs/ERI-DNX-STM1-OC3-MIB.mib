-- *************************************************************************
-- This table is the Stm1Oc3 Mapper Status table

opticalMapperStatusTable OBJECT-TYPE
        SYNTAX SEQUENCE OF OpticalMapperStatusEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "This is the Stm1Oc3 Mapper Status table which consists
            of an entry for each of the 3 STM1/OC3 Mapper Payloads. The 
            total number of entries depends on the number of STM1/OC3 cards 
            in the Node."
        ::= { opticalDiag 2 }

opticalMapperStatusEntry OBJECT-TYPE
        SYNTAX OpticalMapperStatusEntry
        MAX-ACCESS not-accessible
        STATUS current                          
        DESCRIPTION
            "The conceptual row of the Stm1Oc3 Mapper Status table. 
            A row in this table cannot be added or deleted, only modified."
        INDEX { optMapperStatusAddr }
        ::= { opticalMapperStatusTable 1 }

OpticalMapperStatusEntry ::=
        SEQUENCE {
                optMapperStatusAddr           LinkPortAddress,	  
                optMapperStatusResrcId        Integer32,            
                optMapperState  			  INTEGER,
                optMapperErrSecs	          Counter32,
                optMapperErrFreeSecs	      Counter32,
                optMapperLoop  			      INTEGER,  	    -- RW
                optMapperPathLOPErrs		  Counter32,
                optMapperPathAISErrs		  Counter32,
                optMapperPathRDIErrs		  Counter32,
                optMapperCmdStatus			  INTEGER           -- RW
}

optMapperStatusAddr OBJECT-TYPE
        SYNTAX LinkPortAddress
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "This number uniquely identifies a STM1/OC3 Mapper Payload. 
            This number will be used throughout the system to identify 
            a unique resource."
        ::= { opticalMapperStatusEntry 1 }

optMapperStatusResrcId OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "This number uniquely identifies a STM1/OC3 Mapper resource. 
            This number is provided as key to allow the manager to 
            map this entry to a corresponding Resource Table entry."
        ::= { opticalMapperStatusEntry 2 }

optMapperState OBJECT-TYPE
        SYNTAX INTEGER {
        	ok  (0),
        	yel (2),              -- bit 1
        	ais (8),              -- bit 3
        	lop (16),             -- bit 4
        	lof (32),             -- bit 5
        	los (64),             -- bit 6
        	rdi (128),            -- bit 7
        	red (2048),           -- bit 11
        	cfa (4096),           -- bit 12
        	oos (65535),          -- all bits
        	txSlip (2097152),     -- bit 21
        	rxSlip (4194304),     -- bit 22
        	sef (8388608),        -- bit 23
        	slip (6291456),       -- bits 21,22
        	underTest(1073741824),-- bit 30
        	errors(2147483647)    -- all
        	}
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
           "This number uniquely identifies the Mapper Payload State.
        	ok       (0)          - In Sync
        	yel      (2)          - Yellow Alarm
        	ais      (8)          - Sending All Ones
            lop      (16)         - Loss of Ptr
        	lof      (32)         - Loss of Frame
        	los      (64)         - Loss of Signal
        	rdi      (128)        - Receive Defect Indicator
        	red      (2048)       - Red Alarm
        	cfa      (4096)       - Carrier Frame Alignment
        	oos      (65535)	  - Out of Service
        	txSlip   (2097152)    - Transmit Slip
        	rxSlip   (4194304)    - Receive Slip
        	sef      (8388608)    - Severely Errored Frames
        	slip     (6291456)    - Transmit/Receive Slip
        	underTest(1073741824) - Running Bit Error Test (BERT) 
        	errors   (2147483647) - Multiple Errors displays raw bit field value
           "
        ::= { opticalMapperStatusEntry 3 }
        

optMapperErrSecs OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "This is the total number of Errored Seconds for this Mapper Payload."
        ::= { opticalMapperStatusEntry 4 }
        
optMapperErrFreeSecs OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "This is the total number of Mapper Error Free Seconds."
        ::= { opticalMapperStatusEntry 5 }

optMapperLoop OBJECT-TYPE
        SYNTAX INTEGER {
        	off (0),
        	local (1),
        	line (3)
        	}
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
            "Indicates the loopback status of the Stm1Oc3 Mapper for this device..
              off        (0)  - indicates loopback is disabled
        	  local      (1)  - indicates Mapper local data is looped back to itself
        	  line       (3)  - entire Mapper Frame is looped, framing is not regenerated
        	"
        ::= { opticalMapperStatusEntry 6 }

optMapperPathLOPErrs OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "This is the total number of Mapper Path Loss of Pointer Errored 
            Seconds."
        ::= { opticalMapperStatusEntry 7 }

optMapperPathAISErrs OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "This is the total number of Mapper Path Alarm Indication Signal 
            Errored Seconds."
        ::= { opticalMapperStatusEntry 8 }

optMapperPathRDIErrs OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "This is the total number of Mapper Path Remote Defect Indication 
            Errored Seconds."
        ::= { opticalMapperStatusEntry 9 }

optMapperCmdStatus OBJECT-TYPE
        SYNTAX INTEGER {
    -- Initial default status for a row
            ready-for-command (0),
    -- Action States used in SET Command (1..99)
            updateTest (1),
            clearErrors (14),
                                
    -- Response States used in GET RESPONSE Command (100..199)
            update-successful (101),
            clear-successful (114),
                                
    -- Mapper Error Codes used in GET RESPONSE Command (200..799)
            err-general-test-error (200),
            err-invalid-loop-type (202),
            err-field-cannot-be-set (206),
            err-snmp-parse-failed(500),
            err-invalid-snmp-type(501),
            err-invalid-snmp-var-size(502) 
            }
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
            "This is the command status for the Framer Row.
           The value used in a SET will be replaced by a response
           value in the GET RESPONSE indicating success or failure.

            Default Response State used in GET RESPONSE Command
                ready-for-command (0)
                    initial default status for a row           
                    
            Mapper Action Commands used in SET Command (1..99)            
                updateTest (1)
                    Can be used optionally when activating loops and berts
                    in order to get status code back in response
                clearErrors (14)
                    Resets all error counters and bert test time to zero
                                
            Response States used in GET RESPONSE Command (100..199)
                update-successful (101)
                    Test action has been successfully performed
                clear-successful (114)
                    Error counts have been successfully cleared
                                
            The Error Codes used in GET RESPONSE Command (200..799)
                err-general-test-error (200)
                    Unknown Test request error occurred.
                err-invalid-loop-type (202)
                    Unrecognized loop selection
                err-field-cannot-be-set (206)
                    Read-only field was included in SET request
                err-snmp-parse-failed (500)
                    Agent could not parse variable
                err-invalid-snmp-type (501)
                    Variable ASN type does not match Agent defined type 
                err-invalid-snmp-var-size (502)
                    Variable size is too big" 
        ::= { opticalMapperStatusEntry 10 } 
   

-- *************************************************************************
-- This table is the Stm1Oc3 Transport Status table

opticalTransportStatusTable OBJECT-TYPE
        SYNTAX SEQUENCE OF OpticalTransportStatusEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "This is the Stm1Oc3 Transport Status table which consists
            of an entry for each of the STM1/OC3 Transport Overhead. The 
            total number of entries depends on the number of Stm1Oc3 cards 
            in the Node."
        ::= { opticalDiag 3 }

opticalTransportStatusEntry OBJECT-TYPE
        SYNTAX OpticalTransportStatusEntry
        MAX-ACCESS not-accessible
        STATUS current                          
        DESCRIPTION
            "The conceptual row of the Stm1Oc3 Transport Status table. 
            A row in this table cannot be added or deleted, only modified."
        INDEX { optTransportStatusAddr }
        ::= { opticalTransportStatusTable 1 }

OpticalTransportStatusEntry ::=
        SEQUENCE {
                optTransportStatusAddr        NestSlotAddress,	  
                optTransportStatusResrcId     Integer32,            
                optTransportLaserState  	  INTEGER,          -- RW
                optTransportErrSecs	          Counter32,
                optTransportErrFreeSecs	      Counter32,
                optTransportLoop  			  INTEGER,  	    -- RW
                optTransportLineAISErrs		  Counter32,
                optTransportLineRDIErrs		  Counter32,
                optTransportLineOOFErrs		  Counter32,
                optTransportLineLOFErrs		  Counter32,
                optTransportLineLOSErrs		  Counter32,
                optTransportCmdStatus	      INTEGER           -- RW
                }

optTransportStatusAddr OBJECT-TYPE
        SYNTAX NestSlotAddress
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "This number uniquely identifies a STM1/OC3 Transport Overhead
            Nest and Slot number. This number format will be used throughout 
            the system to identify a resource."
        ::= { opticalTransportStatusEntry 1 }

optTransportStatusResrcId OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "This number uniquely identifies a STM1/OC3 Transport resource. 
            This number is provided as key to allow the manager to 
            map this entry to a corresponding Resource Table entry."
        ::= { opticalTransportStatusEntry 2 }

optTransportLaserState OBJECT-TYPE
        SYNTAX INTEGER {
        	off (0),
        	on (1)
        	}
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
           "This specifies the State of the Laser Interface."
        ::= { opticalTransportStatusEntry 3 }
        

optTransportErrSecs OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "This is the total number of Errored Seconds for this Transport Overhead."
        ::= { opticalTransportStatusEntry 4 }
        
optTransportErrFreeSecs OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "This is the total number of Transport Error Free Seconds."
        ::= { opticalTransportStatusEntry 5 }

optTransportLoop OBJECT-TYPE
        SYNTAX INTEGER {
        	off (0),
        	terminal (1),
        	facility (2),
        	pathFacility (3)
        	}
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
            "Indicates the loopback status of the Stm1Oc3 Transport for this device.
              off          (0)  - indicates Transport loopback is disabled
        	  terminal     (1)  - The optical line interface transmit driver is looped back 
        	                      to the optical receiver. Ultimately the payload is reflected 
        	                      back to the Switch Fabric and the PCM Highway of the DNX 
        	                      Backplane.
        	  facility     (2)  - The Fiber Optic path is looped back to the facility.
        	  pathFacility (3)  - The Stm1Oc3 incoming data (Rx) is transmitted back to its source 
        	                      via the Tx path. The bus that interfaces to the mappers is in 
        	                      loopback, and the entire payload is returned to its source with a 
        	                      refreshed frame structure.
        	"
        ::= { opticalTransportStatusEntry 6 }

optTransportLineAISErrs OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "This is the total number of Transport Line Alarm Indication Signal 
            Errored Seconds."
        ::= { opticalTransportStatusEntry 7 }

optTransportLineRDIErrs OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "This is the total number of Transport Line Remote Defect Indication 
            Errored Seconds."
        ::= { opticalTransportStatusEntry 8 }

optTransportLineOOFErrs OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "This is the total number of Transport Line Out of Frame Errored 
            Seconds."
        ::= { opticalTransportStatusEntry 9 }

optTransportLineLOFErrs OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "This is the total number of Transport Line Loss of Frame Errored 
            Seconds."
        ::= { opticalTransportStatusEntry 10 }

optTransportLineLOSErrs OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "This is the total number of Transport Line Loss of Signal Errored 
            Seconds."
        ::= { opticalTransportStatusEntry 11 }

optTransportCmdStatus OBJECT-TYPE
        SYNTAX INTEGER {
    -- Initial default status for a row
            ready-for-command (0),

    -- Action States used in SET Command (1..99)
            updateTest (1),
            clearErrors (14),
                                
    -- Response States used in GET RESPONSE Command (100..199)
            update-successful (101),
            clear-successful (114),
                                
    -- Transport Error Codes used in GET RESPONSE Command (400..799)
            err-general-test-error (400),
            err-invalid-loop-type (402),
            err-field-cannot-be-set (406),
            err-snmp-parse-failed(500),
            err-invalid-snmp-type(501),
            err-invalid-snmp-var-size(502) 
            }
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
            "This is the command status for the Framer Row.
           The value used in a SET will be replaced by a response
           value in the GET RESPONSE indicating success or failure.

            Default Response State used in GET RESPONSE Command
                ready-for-command (0)
                    initial default status for a row           
                    
            Transport Action Commands used in SET Command (1..99)            
                updateTest (1)
                    Can be used optionally when activating loops and berts
                    in order to get status code back in response
                clearErrors (14)
                    Resets all error counters and bert test time to zero
                                
            Response States used in GET RESPONSE Command (100..199)
                update-successful (101)
                    Test action has been successfully performed
                clear-successful (114)
                    Error counts have been successfully cleared
                                
            The Error Codes used in GET RESPONSE Command (400..799)
                err-general-test-error (400)
                    Unknown Test request error occurred.
                err-invalid-loop-type (402)
                    Unrecognized loop selection
                err-field-cannot-be-set (406)
                    Read-only field was included in SET request
                err-snmp-parse-failed (500)
                    Agent could not parse variable
                err-invalid-snmp-type (501)
                    Variable ASN type does not match Agent defined type 
                err-invalid-snmp-var-size (502)
                    Variable size is too big" 
        ::= { opticalTransportStatusEntry 12 } 

