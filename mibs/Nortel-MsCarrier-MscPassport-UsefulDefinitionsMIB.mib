--      backplane and higher density functional processes."

mscPassport15000 OBJECT IDENTIFIER ::= { mscProducts 4 }

-- arcs immediately subordinate to Passport:
mscComponents              OBJECT IDENTIFIER ::= { mscPassport 1 }
