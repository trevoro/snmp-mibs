		pbcCmtsRogueCmPtr OBJECT-TYPE
			SYNTAX Integer32
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"An row index into docsIfCmtsCmStatusTable. When queried with the correct instance value (e.g. a CM's MAC address), returns the index in docsIfCmtsCmStatusTable which represents that CM."
			::= { pbcCmtsRogueCmEntry 3 }
		
		-- 1.3.6.1.4.1.5987.2.2.1.5.2.1.4
		pbcCmtsRogueCmType OBJECT-TYPE
			SYNTAX INTEGER
				{
				unProvisioned(1),
				rigged(2),
				configured(3)
				}
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Type of the Rogue CM. Valid Values are unProvisioned (1), rigged (2), configured (3)."
			::= { pbcCmtsRogueCmEntry 4 }
		
		-- 1.3.6.1.4.1.5987.2.2.1.5.2.1.5
		pbcCmtsRogueCmFirstActive OBJECT-TYPE
			SYNTAX DateAndTime
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Date and Time when CM was seen by the system for the first time."
			::= { pbcCmtsRogueCmEntry 5 }
		
		-- 1.3.6.1.4.1.5987.2.2.1.5.2.1.6
		pbcCmtsRogueCmLastActive OBJECT-TYPE
			SYNTAX DateAndTime
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Date and Time when the last time an IM was seen from this CM. The LastActive tells the latest time when CM was detected as rogue and put to rest."
			::= { pbcCmtsRogueCmEntry 6 }
		
		-- 1.3.6.1.4.1.5987.2.2.2
		pbcCmtsCableSpectrumManagement OBJECT IDENTIFIER ::= { pbcCmts 2 }
		
		-- 1.3.6.1.4.1.5987.2.2.2.2
		pbcCmtsSpectrumAnalysisMgmt OBJECT IDENTIFIER ::= { pbcCmtsCableSpectrumManagement 2 }
		
		-- 1.3.6.1.4.1.5987.2.2.2.2.1
		pbcCmtsSpectrumAnalysisObjects OBJECT IDENTIFIER ::= { pbcCmtsSpectrumAnalysisMgmt 1 }
		
		-- 1.3.6.1.4.1.5987.2.2.2.2.1.1
		pbcSpectrumAnalysisRFInputTable OBJECT-TYPE
			SYNTAX SEQUENCE OF PbcSpectrumAnalysisRFInputEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"There is an entry in this table for each
				upstream input connector on each DOCSIS module
				in the CMTS.  Each port is uniquely identified within
				the CMTS by its pbcCardIfPortIndex value.
				
				This index object is defined under the generic
				chassis management section of the PBC private
				MIB tree."
			::= { pbcCmtsSpectrumAnalysisObjects 1 }
		
		-- 1.3.6.1.4.1.5987.2.2.2.2.1.1.1
		pbcSpectrumAnalysisRFInputEntry OBJECT-TYPE
			SYNTAX PbcSpectrumAnalysisRFInputEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"An entry in the RF monitoring input (port) table."
			INDEX { pbcCardIfPortIndex }
			::= { pbcSpectrumAnalysisRFInputTable 1 }
		
		PbcSpectrumAnalysisRFInputEntry ::=
			SEQUENCE { 
				pbcSpectrumAnalysisEnable
					TruthValue,
				pbcSpectrumAnalysisCmdStatus
					INTEGER,
				pbcSpectrumAnalysisIntegrationLen
					Unsigned32,
				pbcSpectrumAnalysisFreqStep
					INTEGER,
				pbcSpectrumAnalysisFreqMin
					Unsigned32,
				pbcSpectrumAnalysisFreqMax
					Unsigned32,
				pbcSpectrumAnalysisArraySize
					Unsigned32,
				pbcSpectrumAnalysisActiveArray
					Unsigned32
			 }

		-- 1.3.6.1.4.1.5987.2.2.2.2.1.1.1.1
		pbcSpectrumAnalysisEnable OBJECT-TYPE
			SYNTAX TruthValue
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"True ('1') enables spectrum estimation.  This
				should be set only after setting the other
				parameters affecting the scan.  After setting
				this 'true', pbcSpectrumAnalysisCmdStatus will
				reflect initial and, ultimately, final status
				of the sweep attempt."
			DEFVAL { false }
			::= { pbcSpectrumAnalysisRFInputEntry 1 }
		
		-- 1.3.6.1.4.1.5987.2.2.2.2.1.1.1.2
		pbcSpectrumAnalysisCmdStatus OBJECT-TYPE
			SYNTAX INTEGER
				{
				available(1),
				scanInProgress(2),
				complete(3),
				generalFailure(4),
				noResources(5)
				}
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Result of pbcSpectrumAnalysisEnable command."
			DEFVAL { available }
			::= { pbcSpectrumAnalysisRFInputEntry 2 }
		
		-- 1.3.6.1.4.1.5987.2.2.2.2.1.1.1.3
		pbcSpectrumAnalysisIntegrationLen OBJECT-TYPE
			SYNTAX Unsigned32
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"This specifies the number of estimates over
				which the peak of the noise power is
				calculated."
			DEFVAL { 1 }
			::= { pbcSpectrumAnalysisRFInputEntry 3 }
		
		-- 1.3.6.1.4.1.5987.2.2.2.2.1.1.1.4
		pbcSpectrumAnalysisFreqStep OBJECT-TYPE
			SYNTAX INTEGER
				{
				kHz20(1),
				kHz40(2),
				kHz80(3),
				kHz160(4)
				}
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"The frequency step for scanning which defines
				the frequency resolution.  The choices are:
				20KHz, 40KHz, 80KHz and 160KHz."
			DEFVAL { kHz160 }
			::= { pbcSpectrumAnalysisRFInputEntry 4 }
		
		-- 1.3.6.1.4.1.5987.2.2.2.2.1.1.1.5
		pbcSpectrumAnalysisFreqMin OBJECT-TYPE
			SYNTAX Unsigned32
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"The low end of the frequency range being
				scanned.  The lowest legal value, per both
				the DOCSIS and Euro-DOCSIS standards, is
				5MHz."
			DEFVAL { 5000000 }
			::= { pbcSpectrumAnalysisRFInputEntry 5 }
		
		-- 1.3.6.1.4.1.5987.2.2.2.2.1.1.1.6
		pbcSpectrumAnalysisFreqMax OBJECT-TYPE
			SYNTAX Unsigned32
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Top end of the frequency range being scanned.
				For DOCSIS, the maximum is 42MHz and for
				Euro-DOCSIS it is 65MHz."
			DEFVAL { 42000000 }
			::= { pbcSpectrumAnalysisRFInputEntry 6 }
		
		-- 1.3.6.1.4.1.5987.2.2.2.2.1.1.1.7
		pbcSpectrumAnalysisArraySize OBJECT-TYPE
			SYNTAX Unsigned32 (2..32)
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"This variable holds/specifies the number of scan
				data tables to be allocated to this particular
				scan instance.  The size of each table and,
				therefore, the total amount of buffer space
				required is determined by the other scan parameters.
				If the agent is unable to allocate that amount of
				space, a status of 'noResources' will be returned
				in pbcSpectrumAnalysisCmdStatus following the
				setting of pbcSpectrumAnalysisEnable.
				
				If successful, this becomes the total number of
				table entries in pbcSpectrumAnalysisArrayIndexTable.
				
				A size of '2' is the absolute minimum to permit the
				agent to fill one table with data points while the
				management application is 'draining' the other."
			::= { pbcSpectrumAnalysisRFInputEntry 7 }
		
		-- 1.3.6.1.4.1.5987.2.2.2.2.1.1.1.8
		pbcSpectrumAnalysisActiveArray OBJECT-TYPE
			SYNTAX Unsigned32
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Contains the pbcSpectrumAnalysisArrayIndex number
				of the data table currently being filled with
				spectral data points."
			DEFVAL { 0 }
			::= { pbcSpectrumAnalysisRFInputEntry 8 }
		
		-- 1.3.6.1.4.1.5987.2.2.2.2.1.2
		pbcSpectrumAnalysisArrayIndexTable OBJECT-TYPE
			SYNTAX SEQUENCE OF PbcSpectrumAnalysisArrayIndexEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"The table serves as an index, with status, into
				the pbcSpectrumAnalysisDataTable, thus uniquely
				identifying all of the data points which constitute
				one spectral sweep."
			::= { pbcCmtsSpectrumAnalysisObjects 2 }
		
		-- 1.3.6.1.4.1.5987.2.2.2.2.1.2.1
		pbcSpectrumAnalysisArrayIndexEntry OBJECT-TYPE
			SYNTAX PbcSpectrumAnalysisArrayIndexEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"An entry in the pbcSpectrumAnalysisArrayIndexTable."
			INDEX { pbcCardIfPortIndex, pbcSpectrumAnalysisArrayIndex }
			::= { pbcSpectrumAnalysisArrayIndexTable 1 }
		
		PbcSpectrumAnalysisArrayIndexEntry ::=
			SEQUENCE { 
				pbcSpectrumAnalysisArrayIndex
					Unsigned32,
				pbcSpectrumAnalysisArrayStatus
					INTEGER,
				pbcSpectrumAnalysisArrayScanStart
					DateAndTime,
				pbcSpectrumAnalysisArrayScanStop
					DateAndTime
			 }

		-- 1.3.6.1.4.1.5987.2.2.2.2.1.2.1.1
		pbcSpectrumAnalysisArrayIndex OBJECT-TYPE
			SYNTAX Unsigned32
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Index number of this data table."
			::= { pbcSpectrumAnalysisArrayIndexEntry 1 }
		
		-- 1.3.6.1.4.1.5987.2.2.2.2.1.2.1.2
		pbcSpectrumAnalysisArrayStatus OBJECT-TYPE
			SYNTAX INTEGER
				{
				available(1),
				scanning(2),
				scanComplete(3),
				abort(4)
				}
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"The operational status of this data table.  The
				management application which is reading the
				spectrum monitoring data should, after reading
				all the data entries associated with this array
				(table),set this variable back to 'available'."
			DEFVAL { available }
			::= { pbcSpectrumAnalysisArrayIndexEntry 2 }
		
		-- 1.3.6.1.4.1.5987.2.2.2.2.1.2.1.3
		pbcSpectrumAnalysisArrayScanStart OBJECT-TYPE
			SYNTAX DateAndTime
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The date and time when this table's spectral
				sweep began."
			::= { pbcSpectrumAnalysisArrayIndexEntry 3 }
		
		-- 1.3.6.1.4.1.5987.2.2.2.2.1.2.1.4
		pbcSpectrumAnalysisArrayScanStop OBJECT-TYPE
			SYNTAX DateAndTime
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The date and time when this table's spectral
				sweep was completed or stopped for some other
				reason."
			::= { pbcSpectrumAnalysisArrayIndexEntry 4 }
		
		-- 1.3.6.1.4.1.5987.2.2.2.2.1.3
		pbcSpectrumAnalysisDataTable OBJECT-TYPE
			SYNTAX SEQUENCE OF PbcSpectrumAnalysisDataEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"The Frequency-Power tuples are stored in
				this table.  The maximum number of elements
				is determined by the size of the frequency
				step and the RF range to be scanned and can
				therefore not exceed 1850."
			::= { pbcCmtsSpectrumAnalysisObjects 3 }
		
		-- 1.3.6.1.4.1.5987.2.2.2.2.1.3.1
		pbcSpectrumAnalysisDataEntry OBJECT-TYPE
			SYNTAX PbcSpectrumAnalysisDataEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"An entry in the spectrum analysis data table."
			INDEX { pbcCardIfPortIndex, pbcSpectrumAnalysisArrayIndex, pbcSpectrumAnalysisFrequency }
			::= { pbcSpectrumAnalysisDataTable 1 }
		
		PbcSpectrumAnalysisDataEntry ::=
			SEQUENCE { 
				pbcSpectrumAnalysisFrequency
					Unsigned32,
				pbcSpectrumAnalysisPower
					OneHundredthdBmVPerHz
			 }

		-- 1.3.6.1.4.1.5987.2.2.2.2.1.3.1.1
		pbcSpectrumAnalysisFrequency OBJECT-TYPE
			SYNTAX Unsigned32
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The frequency that was sampled."
			::= { pbcSpectrumAnalysisDataEntry 1 }
		
		-- 1.3.6.1.4.1.5987.2.2.2.2.1.3.1.2
		pbcSpectrumAnalysisPower OBJECT-TYPE
			SYNTAX OneHundredthdBmVPerHz
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Measured power in hundredths of a dBmV/Hz."
			::= { pbcSpectrumAnalysisDataEntry 2 }
		
		-- 1.3.6.1.4.1.5987.2.2.2.2.2
		pbcCmtsSpectrumAnalysisNotificationEnables OBJECT IDENTIFIER ::= { pbcCmtsSpectrumAnalysisMgmt 2 }
		
		-- 1.3.6.1.4.1.5987.2.2.2.2.2.1
		pbcCmtsSpectrumAnalysisNotificationTable OBJECT-TYPE
			SYNTAX SEQUENCE OF PbcCmtsSpectrumAnalysisNotificationEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Allows specific 'sweep complete' trap recipients to be
				specified on a per-port basis."
			::= { pbcCmtsSpectrumAnalysisNotificationEnables 1 }
		
		-- 1.3.6.1.4.1.5987.2.2.2.2.2.1.1
		pbcCmtsSpectrumAnalysisNotificationEntry OBJECT-TYPE
			SYNTAX PbcCmtsSpectrumAnalysisNotificationEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Contains the IP address of the desired trap recipient
				of 'sweep complete' notifications generated by active
				spectral monitoring on a specific US port."
			INDEX { pbcCardIfPortIndex }
			::= { pbcCmtsSpectrumAnalysisNotificationTable 1 }
		
		PbcCmtsSpectrumAnalysisNotificationEntry ::=
			SEQUENCE { 
				pbcCmtsSpectrumAnalysisEnableSweepNotification
					TruthValue,
				pbcSweepNotificationRecipient
					IpAddress,
				pbcSweepNotificationRecipientPort
					Integer32,
				pbcCmtsSpectrumAnalysisNotificationType
					INTEGER
			 }

		-- 1.3.6.1.4.1.5987.2.2.2.2.2.1.1.1
		pbcCmtsSpectrumAnalysisEnableSweepNotification OBJECT-TYPE
			SYNTAX TruthValue
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"True ('1') enables 'sweep complete' notifications
				so that the receiver knows it may/should retrieve
				the current spectral data points."
			DEFVAL { false }
			::= { pbcCmtsSpectrumAnalysisNotificationEntry 1 }
		
		-- 1.3.6.1.4.1.5987.2.2.2.2.2.1.1.2
		pbcSweepNotificationRecipient OBJECT-TYPE
			SYNTAX IpAddress
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"The IP address of the management station wishing to
				receive the 'sweep complete' notifications from this
				system for this monitored port."
			::= { pbcCmtsSpectrumAnalysisNotificationEntry 2 }
		
		-- 1.3.6.1.4.1.5987.2.2.2.2.2.1.1.3
		pbcSweepNotificationRecipientPort OBJECT-TYPE
			SYNTAX Integer32 (0..65535)
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Port that recipient is listening on for notifications."
			DEFVAL { 162 }
			::= { pbcCmtsSpectrumAnalysisNotificationEntry 3 }
		
		-- 1.3.6.1.4.1.5987.2.2.2.2.2.1.1.4
		pbcCmtsSpectrumAnalysisNotificationType OBJECT-TYPE
			SYNTAX INTEGER
				{
				snmpv2c(1),
				snmpv3(2)
				}
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Selects between SNMPv2c and SNMPv3 traps."
			::= { pbcCmtsSpectrumAnalysisNotificationEntry 4 }
		
		-- 1.3.6.1.4.1.5987.2.2.2.2.3
		pbcCmtsSpectrumAnalysisNotificationPrefix OBJECT IDENTIFIER ::= { pbcCmtsSpectrumAnalysisMgmt 3 }
		
		-- 1.3.6.1.4.1.5987.2.2.2.2.3.1
		pbcCmtsSpectrumAnalysisNotifications OBJECT IDENTIFIER ::= { pbcCmtsSpectrumAnalysisNotificationPrefix 1 }
		

-- 1.3.6.1.4.1.5987.2.2.2.2.3.1.2
		pbcCardIfPortIndexID OBJECT-TYPE
			SYNTAX Integer32
			MAX-ACCESS accessible-for-notify
