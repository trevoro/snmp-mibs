     -- atmxVccReceiveTrafficDescriptorType objects.

	AtmTrafficDescrTypes ::= INTEGER {
		none(1),
		peakrate(2),
		noClpNoScr(3),
		clpNoTaggingNoScr(4),
		clpTaggingNoScr(5),
		noClpScr(6),
		clpNoTaggingScr(7), 
   		clpTaggingScr(8)
 	}  		  
            
