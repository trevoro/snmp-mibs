    ::= { msvmTimeSyncCompSettingDataEntry 17 }

msvmtscsdPoolID OBJECT-TYPE
    SYNTAX     DisplayString
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
        "This property specifies which ResourcePool the
