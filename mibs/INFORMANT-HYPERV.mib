    SYNTAX     DisplayString
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
        "The managed element."
    ::= { msvmElementSettingDataEntry 7 }

msvmesdSettingData OBJECT-TYPE
    SYNTAX     DisplayString
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
        "The SettingData object that is associated with the
        element."
    ::= { msvmElementSettingDataEntry 8 }

