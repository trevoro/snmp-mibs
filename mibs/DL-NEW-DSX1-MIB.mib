  dlcDataConfigTransmitTiming OBJECT-TYPE
   SYNTAX INTEGER {
    scte  (1),
    sct  (2),
    sctInverted (3)
   }
   ACCESS read-write
   STATUS mandatory
   DESCRIPTION
    "Selects the Data DTE tramsmit timing source.
    Can be SCTE, SCT or inverted SCT."
   ::= { dlcDataDteConfigEntry 6 }

-- ============================================================================
--
-- Configuration items controlling the unit multiplexer.
--
-- There are two mux-buses in the Eclipse,
-- so we define a table with two indexes: primary/secondary and slot number.
--

 dlcUnitMuxConfigTable OBJECT-TYPE
  SYNTAX SEQUENCE OF DlcMuxConfigEntry
  ACCESS not-accessible
  STATUS mandatory
  DESCRIPTION
   "This is the timeslot map configuration."
  --DEFAULT next-function next_dlcUnitMuxConfigTable 
  ::= { dlcUnitConfig 23 }

 dlcMuxConfigEntry OBJECT-TYPE
  SYNTAX DlcMuxConfigEntry
  ACCESS not-accessible
  STATUS mandatory
  DESCRIPTION
   "An entry in the timeslot map."
  INDEX { dlcMuxConfigBusId, dlcMuxConfigSlotNumber }
  ::= { dlcUnitMuxConfigTable 1 }
 
 DlcMuxConfigEntry ::=
  SEQUENCE {
   dlcMuxConfigBusId INTEGER,
   dlcMuxConfigSlotNumber INTEGER,
   dlcMuxConfigPortId PortId
  }


  dlcMuxConfigBusId OBJECT-TYPE
   SYNTAX INTEGER {
    primary  (1),
    secondary (2)
   }
   ACCESS read-only
   STATUS mandatory
   DESCRIPTION
    "Identifies which muxBus this timeslot is on."
   --DEFAULT get-function get_compl_0
   ::= { dlcMuxConfigEntry 1 }

  dlcMuxConfigSlotNumber OBJECT-TYPE
   SYNTAX INTEGER (1..31)
   ACCESS read-only
   STATUS mandatory
   DESCRIPTION
    "Uniquely identifies this timeslot."
   --DEFAULT get-function get_compl_1
   ::= { dlcMuxConfigEntry 2 }

  dlcMuxConfigPortId OBJECT-TYPE
   SYNTAX PortId
   ACCESS read-write
   STATUS mandatory
   DESCRIPTION
    "Says which DTE port receives data
    from the indicated muxBus
    during this timeslot."
   ::= { dlcMuxConfigEntry 3 }

 
