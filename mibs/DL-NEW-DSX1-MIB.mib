-- ============================================================================
--
-- Configuration items controlling the configuration and operation
-- of all our Dsx1 ports.
--

 dlcUnitDsx1ConfigTable OBJECT-TYPE
  SYNTAX SEQUENCE OF DlcDsx1ConfigEntry
  ACCESS not-accessible
  STATUS mandatory
  DESCRIPTION
   "A table of configuration items for a Dsx1 port."
  --DEFAULT next-function next_dlcDsx1Table 
  ::= { dlcUnitConfig 21 }

 dlcDsx1ConfigEntry OBJECT-TYPE
  SYNTAX DlcDsx1ConfigEntry
  ACCESS not-accessible
  STATUS mandatory
  DESCRIPTION
   "An entry in the Dsx1 configuration table."
  INDEX { dlcDsx1ConfigPortId }
  ::= { dlcUnitDsx1ConfigTable 1 }

 DlcDsx1ConfigEntry ::=
  SEQUENCE {
   dlcDsx1ConfigPortId  PortId,
   dlcDsx1Framing   INTEGER,
   dlcDsx1LineCode   INTEGER,
   dlcDsx1LineMatching  INTEGER,
   dlcDsx1DacsMode   Boolean,
   dlcDsx1UseDlcFdlProtocol Boolean,
   dlcDsx1UseAnsiProtocol  Boolean,
   dlcDsx1Bit7Stuffing  Boolean,
   dlcDsx1InBandBit  INTEGER
  }


  dlcDsx1ConfigPortId OBJECT-TYPE
   SYNTAX PortId
   ACCESS read-only
   STATUS mandatory
   DESCRIPTION
    "Uniquely identifies this port.
    Valid values are `mainNet', `aux', `otherNet'."
   --DEFAULT get-function get_compl_0
   ::= { dlcDsx1ConfigEntry 1 }

  dlcDsx1Framing OBJECT-TYPE
   SYNTAX INTEGER {
    esfFraming (1),
    d4Framing (2),
    crc4Enabled (3),
    crc4Disabled (4),
    unstructured (5)
   }
   ACCESS read-write
   STATUS mandatory
   DESCRIPTION
    "Dsx1 framing type:
    T1 --> D4 or ESF;
    E1 --> CRC4 or not.
    Unstructured is E1 only,
    when in this mode, the E1 line acts like
    a 2048 Bps pipe with no framing information."
   ::= { dlcDsx1ConfigEntry 2 }

  dlcDsx1LineCode OBJECT-TYPE
   SYNTAX INTEGER {
    b8zsCode (1),
    amiCode  (2),
    hdb3Code (3)
   }
   ACCESS read-write
   STATUS mandatory
   DESCRIPTION
    "Dsx1 line code type, AMI or B8ZS.
    E1 lines are always HDB3."
   ::= { dlcDsx1ConfigEntry 3 }

  dlcDsx1LineMatching OBJECT-TYPE
   SYNTAX INTEGER {
    buildOut0 ( 1),
    buildOut7p5 ( 2),
    buildOut15 ( 3),
    length0  ( 4),
    length133 ( 5),
    length266 ( 6),
    length399 ( 7),
    length533 ( 8),
    ohms75  ( 9),
    ohms120  (10)
   }
   ACCESS read-write
   STATUS mandatory
   DESCRIPTION
    "Dsx1 line build out or equalization:
    For a T1 NET port, LBO is 0, -7.5 or -15dB.
    For a T1 AUX port, EQ is 0..533 feet.
    For any E1 port, impedance is 75 or 120 Ohms."
   ::= { dlcDsx1ConfigEntry 4 }

  dlcDsx1DacsMode OBJECT-TYPE
   SYNTAX Boolean
   ACCESS read-write
   STATUS mandatory
   DESCRIPTION
    "True if DACS mode is in effect for this port."
   --DEFAULT get-function get_bool_supplier
   --DEFAULT cookie (char *)&GDB.Dbms.config.unit.dacsMode
   ::= { dlcDsx1ConfigEntry 5 }

  dlcDsx1UseDlcFdlProtocol OBJECT-TYPE
   SYNTAX Boolean
   ACCESS read-write
   STATUS mandatory
   DESCRIPTION
    "True when DLC FDL protocol is enabled
    over this port's FDL."
   ::= { dlcDsx1ConfigEntry 6 }

  dlcDsx1UseAnsiProtocol OBJECT-TYPE
   SYNTAX Boolean
   ACCESS read-write
   STATUS mandatory
   DESCRIPTION
    "True if ANSI Dsx1.403 is enabled over the FDL."
   ::= { dlcDsx1ConfigEntry 7 }

  dlcDsx1Bit7Stuffing OBJECT-TYPE
   SYNTAX Boolean
   ACCESS read-write
   STATUS mandatory
   DESCRIPTION
    "When true, bit 7 stuffing
    is turned on for a T1 Aux port
    to provide pulse density protection.
    Valid only on a T1 AUX port."
   ::= { dlcDsx1ConfigEntry 8 }

  dlcDsx1InBandBit OBJECT-TYPE
   SYNTAX INTEGER (1..8)
   ACCESS read-write
   STATUS mandatory
   DESCRIPTION
    "Specifies which bit in Timeslot 1
    of an E1 link is used to pass message traffic.
    Not implemented for T1 ports."
   ::= { dlcDsx1ConfigEntry 9 }

