-- ============================================================================
--
-- Following are the user-settable configuration items
-- that affect the unit as a whole
-- (vs. configuration items that are specific to a port).
--
dlcUnitConfig OBJECT IDENTIFIER ::= { dl-new-t1 2 }

 dlcUnitId OBJECT-TYPE
  SYNTAX DisplayString (SIZE (1..6))
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION
   "A name that uniquely and globally identifies the unit.
   The Unit ID must consist of (upper-case) letters 'A'..'Z'
   and digits '0'..'9'.
   The first character of the Unit ID may not be a digit."
  --DEFAULT get-function get_string_supplier
  --DEFAULT cookie  (char *)&GDB.Dbms.config.unit.id
  ::= { dlcUnitConfig 1 }

 dlcUnitProtectMode OBJECT-TYPE
  SYNTAX Boolean
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION
   "When true, the unit configuration
   can not be changed from the front panel
   nor can tests be initiated or terminated
   from the front panel."
  --DEFAULT test-function test_bool_supplier
  --DEFAULT get-function get_bool_supplier
  --DEFAULT cookie  (char *)&GDB.Dbms.config.unit.protect
  ::= { dlcUnitConfig 2 }

 dlcUnitYellowEnable OBJECT-TYPE
  SYNTAX Boolean
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION
   "When false, yellow alarm conditions are ignored
   when received from a Dsx1 port,
   and the unit will never transmit a Yellow Alarm signal
   on a Dsx1 port.
   This applies to all ports: mainNet, aux, otherNet."
  --DEFAULT get-function get_bool_supplier
  --DEFAULT cookie  (char *)&GDB.Dbms.config.unit.yellowAlarm
  ::= { dlcUnitConfig 3 }

 dlcUnitNetPassFdl OBJECT-TYPE
  SYNTAX PortId
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION
   "Indicates which Dsx1 DTE port's FDL
   is connected to the `mainNet' FDL
   when this unit has no traffic of its own.
   Meaninigful values are `aux', `otherNet', and `noPort'."
  ::= { dlcUnitConfig 4 }


