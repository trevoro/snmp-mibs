adnUCNodeAdminCommand OBJECT-TYPE
    SYNTAX  INTEGER {
    null(1),
    install(2),
    activate(3),
    commit(4),
    deactivate(5),
    withdraw(6),
    confirm-link(7),
    qInstallActivate(8),
    qDeactivateWithdraw(9),
    qConfirmChange(10),
    qRebaseline(11)
}
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "The NMS sets a command value into this object,as follows:
	
		NULL       No action to take place.
		install	   Places newly-transferred files in a read-
			   only partition.
		activate   HSS sends code/configuration to 
			   subsystems.
		commit	   HSS declares the activated code/
			   configuration to be in committed state. 
		deactivate Changes the state of the HSS by reverting
			   to the committed code and configuration.
		withdraw   Deletes installed files.
                confirm-link  Set by the NMS following receipt of 
                              successful Activate trap.
		qInstallActivate 	Places newly-transferred Quick
					configuration change files 
					in a secured partition and 
					sends configuration to subsytems.
		qDeactivateWithdraw     Undoes Quick configuration changes
					and deletes installed files.
		qConfirmChange		Confirms a Quick configuration
					change and sets it as an extension 
					of the committed version.
		qRebaseline		Install and commit a new set of 
					configuration files to synchronize
					the NMS and HSS after several
					Quick transactions.
	Common Name: Administrative Command"
    ::= { adnUCNodeGroup  1 }

adnUCNodeOperCommand OBJECT-TYPE
    SYNTAX  INTEGER {
    other(1),
    install(2),
    activate(3),
    commit(4),
    deactivate(5),
    withdraw(6),
    none(7),
    qInstallActivate(8),
    qDeactivateWithdraw(9),
    qConfirmChange(10),
    qRebaseline(11)
}
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This object indicates the current or last command
	accepted and processed by the HSS.
        Common Name: Oper Command"
    ::= { adnUCNodeGroup  2 }

adnUCNodeOperStatus OBJECT-TYPE
    SYNTAX  INTEGER {
    in-progress(1),
    success(2),
    failed(3),
    warning(4),
    rollback-in-progress(5),
    initial(6)
}
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This object notifies the NMS of the results of the last 
	command, or if it is still in progress.
        Common Name: Oper Command Status"
    ::= { adnUCNodeGroup  3 }

adnUCNodeState OBJECT-TYPE
    SYNTAX  INTEGER {
    committed(0),
    activateInProgress(1),
    uncommitted(2),
    activateInProgressSuspended(3),
    loadingCommittedSuspended(4),
    loadingCommitted(5),
    qConfirmed(6),
    qActivateInProgress(7),
    qUnconfirmed(8),
    qLoadingConfirmed(9),
    qRebaselineInProgress(10)
}
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This object notifies the NMS of the `state' of code/
	configuration update processing. The values are:
        committed -- The node is running the committed 
                     software version.
        activateInProgress -- An Activate command is in 
                     progress.
        uncommitted -- The node is running the uncommitted
                     software version.
        activateInProgressSuspended -- An Activate command 
                     is in progress.  The processing of
                     the Activate is suspended while the
                     Control Unit board waits for a 
                     confirmation that any coupler boards 
                     that have been reset have gone down,
                     before Activate processing continues.
        loadingCommittedSuspended -- The node is in the 
                     process of loading the committed 
                     software version, due to a Deactivate
                     command or an automatic rollback.  
                     The processing of the deactivation is
                     suspended while the Control Unit board 
                     waits for a confirmation that any 
                     coupler boards that have been reset 
                     have gone down, before deactivation 
                     processing continues.
        loadingCommitted -- The node is in the process of
                     loading the committed software version,
                     due to a Deactivate command or an 
                     automatic rollback.
        qConfirmed -- The node is running a confirmed Quick
		     configuration change on top of the 
		     committed version.
        qActivateInProgress -- A Quick Install&Activate 
		     command is in progress.
        qUnconfirmed -- The node is running the unconfirmed
                     Quick configuration.
        qLoadingConfirmed -- The node is in the process of
                     loading the confirmed Quick configuration,
                     due to a Quick Deactivate&Withdraw command 
		     or an automatic rollback.
	qRebaselineInProgress -- A Quick Rebaseline command is
		     in progress.
	Common name: Update Control Status"
    ::= { adnUCNodeGroup  4 }

