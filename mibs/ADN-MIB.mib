adnRtsCritSmryTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF AdnRtsCritSmryEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "This table summarizes each supported criterion cost, giving
        the ID (unique identifier) and the order and IDs of
        the three cost metrics that comprise the criterion cost."
    ::= { adnRoutesMIB  21 }

adnRtsCritSmryEntry OBJECT-TYPE
    SYNTAX  AdnRtsCritSmryEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "An entry in the routing criteria summary table. Each entry
     represents a particular routing criterion.
 
     For a routing criterion consisting of the ordered set of
     metric types { metric1, metric2, metric3 } registered as
     { adnRoutingCriteria n }, the object adnRtsCritID
     will have the value 'n', adnRtsCritOID will have
     the value '{ adnRoutingCriteria n }',
     adnRtsCritMetricHigh will have the value 'metric1',
     adnRtsCritMetricMid will have the value 'metric2'
     and adnRtsCritMetricLow will have the value 'metric3'."
    INDEX   { adnRtsCritID }
    ::= { adnRtsCritSmryTable  1 }

AdnRtsCritSmryEntry ::=
    SEQUENCE {
        adnRtsCritID
            INTEGER (-2147483648..2147483647),

        adnRtsCritOID
            AdnRoutingCriteriaType,

        adnRtsCritMetricHigh
            AdnRoutingMetricType,

        adnRtsCritMetricMid
            AdnRoutingMetricType,

        adnRtsCritMetricLow
            AdnRoutingMetricType
    }

adnRtsCritID OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "The entry number for this entry in this criterion 
	cost summary table. This value ranges from 1 to the 
	value of adnRtsCritCount and is equal to the rightmost 
	sub-identifier of the corresponding adnRtsCritOID value."
    ::= { adnRtsCritSmryEntry  1 }

adnRtsCritOID OBJECT-TYPE
    SYNTAX  AdnRoutingCriteriaType
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The routing criterion object identifier corresponding to
     adnRtsCritID."
    ::= { adnRtsCritSmryEntry  2 }

adnRtsCritMetricHigh OBJECT-TYPE
    SYNTAX  AdnRoutingMetricType
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The highest priority routing metric type of this
     routing criterion cost type."
    ::= { adnRtsCritSmryEntry  3 }

adnRtsCritMetricMid OBJECT-TYPE
    SYNTAX  AdnRoutingMetricType
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The second highest priority routing metric type in
     this routing criterion cost type."
    ::= { adnRtsCritSmryEntry  4 }

adnRtsCritMetricLow OBJECT-TYPE
    SYNTAX  AdnRoutingMetricType
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The third highest priority routing metric type in
     this routing criterion cost type."
    ::= { adnRtsCritSmryEntry  5 }

adnCalcCtlTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF AdnCalcCtlEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "General parameters used by the routing
        services for node-to-path table calculation."
    ::= { adnRoutesMIB  22 }

adnCalcCtlEntry OBJECT-TYPE
    SYNTAX  AdnCalcCtlEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "Each entry (row) of the adnCalcCtlTable provides the
        general parameters used by the routing services for
        node-to-path table calculation for a specific type of
        service and criterion.

	Note that there are 3 metric types known in R 2.2.
	Additional metric types may be added at the end of the
	entry, as future requirements dictate."
    INDEX   { adnTosIndex,  adnRtsCritID }
    ::= { adnCalcCtlTable  1 }

AdnCalcCtlEntry ::=
    SEQUENCE {
        adnTosIndex
            INTEGER,

        adnMaxNumPath
            INTEGER (-2147483648..2147483647),

        adnMinTimeBtwCalc
            INTEGER (-2147483648..2147483647),

        adnMetricDefHigh
            INTEGER (-2147483648..2147483647),

        adnMetricMaxHigh
            INTEGER (-2147483648..2147483647),

        adnMetricDefMid
            INTEGER (-2147483648..2147483647),

        adnMetricMaxMid
            INTEGER (-2147483648..2147483647),

        adnMetricDefLow
            INTEGER (-2147483648..2147483647),

        adnMetricMaxLow
            INTEGER (-2147483648..2147483647)
    }

adnTosIndex OBJECT-TYPE
    SYNTAX  INTEGER {
    other(1),
    fr(2),
    atmVc(3),
    atmVp(4)
}
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "Type of service: frame relay, ATM virtual channel,
         ATM virtual path, etc."
    ::= { adnCalcCtlEntry  1 }

adnMaxNumPath OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Maximum number of paths to be presented in the
	node-to-path routing table, for a given destination node,
        for a specific type of service and criterion."
    ::= { adnCalcCtlEntry  2 }

adnMinTimeBtwCalc OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)	-- UNITS seconds

    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "Minimum time between successive route calculations
        for a specific type of service and criterion."
    ::= { adnCalcCtlEntry  3 }

adnMetricDefHigh OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The high priority default cost metric value used for the
	specified Type Of Service (TOS), criterion. The default 
	value is set up at initialization. Thus, initially, 
	'adnMetricMaxHigh' is equal to 'adnMetricDefHigh'. This 
	'adnMetricDefHigh' allows easy recall of exactly what the 
	default value is."
    ::= { adnCalcCtlEntry  4 }

adnMetricMaxHigh OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "The current maximum value for the high priority 
	 cost metric value. Initialized to a release 
	 specific default, SNMP changeable. The value of 
	 the object 'adnMetricMaxHigh' is what is actually 
	 used by the routing software. A key use is to 
	 identify calculated paths 'too costly' to be 
	 considered."
    ::= { adnCalcCtlEntry  5 }

adnMetricDefMid OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The mid-priority default cost metric value used for the
        specified Type Of Service (TOS), criterion. The default 
	value is set up at initialization. Thus, initially, 
	'adnMetricMaxMid' is equal to 'adnMetricDefMid'. This 
	'adnMetricDefMid' allows easy recall of exactly what the 
	default value is."
    ::= { adnCalcCtlEntry  6 }

adnMetricMaxMid OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "The current maximum value for the mid-priority 
	 cost metric value. Initialized to a release 
	 specific default, SNMP changeable. The value 
	 of the object 'adnMetricMaxMid' is what is 
 	 actually used by the routing software. A key 
	 use is to identify calculated paths 'too costly' 
	 to be considered."
    ::= { adnCalcCtlEntry  7 }

adnMetricDefLow OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The low-priority default cost metric value used for the
        specified Type Of Service, criterion. The default value 
	is set up at initialization. Thus, initially, 
	'adnMetricMaxLow' is equal to 'adnMetricDefLow'. This 
	'adnMetricDefLow' allows easy recall of exactly what the 
	default value is."
    ::= { adnCalcCtlEntry  8 }

adnMetricMaxLow OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "The current maximum value for the low-priority cost metric
         value. Initialized to a release specific default, SNMP 
	 changeable. The value of the object 'adnMetricMaxLow' is 
	 what is actually used by the routing software. A key use is 
	 to identify calculated paths 'too costly' to be considered."
    ::= { adnCalcCtlEntry  9 }

