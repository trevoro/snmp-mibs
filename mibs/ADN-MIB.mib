adnLapFlowChangeReason OBJECT-TYPE
    SYNTAX  INTEGER {
    notStarted(1),
    abmeEntered(2),
    abmeReset(3),
    dmReceived(4),
    dmSent(5),
    discReceived(6),
    frmrReceived(8),
    frmrSent(9),
    n200Timeout(10),
    other(11)
}
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The reason for the most recent incrementing
		of adnlapFlowStateChanges."
    ::= { adnLapFlowEntry  3 }

adnLapFlowCurrentMode OBJECT-TYPE
    SYNTAX  INTEGER {
    other(1),
    disconnected(2),
    linkSetup(3),
    frameReject(4),
    informationTransfer(6),
    rejFrameSent(7),
    waitingAcknowledgement(8),
    stationBusy(9),
    remoteStationBusy(10),
    bothStationsBusy(11),
    waitingAckStationBusy(12),
    waitingAckRemoteBusy(13),
    waitingAckBothBusy(14),
    rejFrameSentRemoteBusy(15),
    xidFrameSent(16),
    error(17)
}
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The current condition of the session."
    ::= { adnLapFlowEntry  4 }

adnLapFlowBusyDefers OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The number of times this device was unable
		to transmit a frame due to a perceived
		remote busy condition.  Busy conditions can
		result from the receipt of an RNR (receive not ready) 
		from the remote device, the lack of valid sequence
		number space (window saturation), or other
		conditions."
    ::= { adnLapFlowEntry  5 }

adnLapFlowRejOutPkts OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The number of REJ (reject) frames sent by
