    ::= { adnHscBoardTrMgtEntry  13 }

adnHscBoardTrMgtHOBObsTh OBJECT-TYPE
    SYNTAX  INTEGER	-- UNITS cells

    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "This indicates the observation threshold in high
	priority output buffer;
	when the observation threshold is overpassed,
	the adnHscBoardTrMgtHOBObsCounter value increases;
	the value range of the threshold is 
	0..(adnHscBoardTrMgtHOBSize - 1)(cells);
        this is not relevant for BBT1B and BBE1B boards."
    ::= { adnHscBoardTrMgtEntry  14 }

adnHscBoardTrMgtMOBObsTh OBJECT-TYPE
    SYNTAX  INTEGER	-- UNITS cells

    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "This indicates the observation threshold in 
	medium priority output buffer;
	when the observation threshold is overpassed,
	the adnHscBoardTrMgtMOBObsCounter value increases;
	the value range of the threshold is 
	0..(adnHscBoardTrMgtMOBSize - 1) (cells);
        this is not relevant for HSC boards without a
	medium priority output buffer (e.g. BBT1B and
	BBE1B).."
    ::= { adnHscBoardTrMgtEntry  15 }

adnHscBoardTrMgtLOBObsTh OBJECT-TYPE
