    ::= { adnsonetPath  1 }

adnsonetPathEntry OBJECT-TYPE
    SYNTAX  AdnSonetPathEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "An entry in the ADN SONET/SDH Path Current table."
    INDEX   { ifIndex }
    ::= { adnsonetPathTable  1 }

AdnSonetPathEntry ::=
    SEQUENCE {
        adnsonetPathWidth
            INTEGER,

        adnsonetPathStatus
            INTEGER (-2147483648..2147483647),

        adnsonetPathLOP
            Counter,

        adnsonetPathAIS
            Counter,

        adnsonetPathRcvRDI
            Counter,

        adnsonetPathBIP8
            Counter,

        adnsonetPathFEBE
            Counter,

        adnsonetPathConfigRDI
            INTEGER,

        adnsonetPathConfigAIS
            INTEGER
    }

adnsonetPathWidth OBJECT-TYPE
    SYNTAX  INTEGER {
    sts3cSTM1(1),
    sts12cSTM4(2)
}
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The type of Synchronous Optical 
                 Network/Synchronous Digital Hierarchy 
                 (SONET/SDH) path."
    ::= { adnsonetPathEntry  1 }

adnsonetPathStatus OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This variable indicates the status
                 of the interface.
                 It is a bit map represented as a sum;
                 therefore, it can represent multiple
                 defects simultaneously.
                 The adnsonetPathNoDefect bit should
                 be set only if no other bit is set.

                 The bit positions are:
                   1   adnsonetLineNoDefect
                   2   adnsonetPathSTSLOP
                   4   adnsonetPathSTSAIS
                   8   adnsonetPathSTSRDI
                  16   adnsonetPathUnequipped *
                  32   adnsonetPathSignalLabelMismatch *
                  64   adnsonetPathBIP8
                 128   adnsonetPathFEBE

		 * not provided by HSS, but the value is 
		   required to ensure compatibility with 
		   the RFC sonetPathStatus."
    ::= { adnsonetPathEntry  2 }

adnsonetPathLOP OBJECT-TYPE
    SYNTAX  Counter	-- UNITS milliseconds

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Duration of loss-of-pointer (LOP).
	         Counter #12 of the ATM core."
    ::= { adnsonetPathEntry  3 }

adnsonetPathAIS OBJECT-TYPE
    SYNTAX  Counter	-- UNITS milliseconds

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Duration of alarm indication signal
                   (AIS) at path level.
	           Counter #8 of the ATM core."
    ::= { adnsonetPathEntry  4 }

adnsonetPathRcvRDI OBJECT-TYPE
    SYNTAX  Counter	-- UNITS milliseconds

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Duration of remote defect indication
		   (RDI) received from the remote
		   equipment at path level.
		   For Synchronous Digital Hierarchy (SDH),
		   this signal is known as a path Far End 
		   Receive Failure (FERF) event.
	           Counter #10 of the ATM core."
    ::= { adnsonetPathEntry  5 }

adnsonetPathBIP8 OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Number of 8-bit interleaved parity
                   (BIP-8) errors.
	           Counter #11 of the ATM core."
    ::= { adnsonetPathEntry  6 }

adnsonetPathFEBE OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Number of far end block errors (FEBE)
                   received from the remote equipment at
                   path level.
	           Counter #9 of the ATM core."
    ::= { adnsonetPathEntry  7 }

adnsonetPathConfigRDI OBJECT-TYPE
    SYNTAX  INTEGER {
    adnsonetPathRDION(1),
    adnsonetPathRDIOFF(2)
}
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "This variable indicates if a permanent
                  remote defect indication (RDI) is sent 
                  on the path.
                  For Synchronous Digital Hierarchy (SDH),
                  it indicates if a permanent Far End Receive
                  Failure (FERF) can be sent on the path.
                  This is a configuration option that should
                  be used only for maintenance purposes.
		  Common Name: Forcing a Path Remote Defect
		  Indication"
    ::= { adnsonetPathEntry  8 }

adnsonetPathConfigAIS OBJECT-TYPE
    SYNTAX  INTEGER {
    adnsonetPathAISON(1),
    adnsonetPathAISOFF(2)
}
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "This variable indicates if a permanent
                  alarm indication signal (AIS) is sent 
                  on the path.
                  This is a configuration option that should
                  be used only for maintenance purposes.
		  Common Name: Forcing a Path Alarm Indication
		  Signal"
    ::= { adnsonetPathEntry  9 }

