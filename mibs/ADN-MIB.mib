adnsonetLineTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF AdnSonetLineEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "The ADN SONET/SDH Line Current table."
    ::= { adnsonetLine  1 }

adnsonetLineEntry OBJECT-TYPE
    SYNTAX  AdnSonetLineEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "An entry in the ADN SONET/SDH Line Current table."
    INDEX   { ifIndex }
    ::= { adnsonetLineTable  1 }

AdnSonetLineEntry ::=
    SEQUENCE {
        adnsonetLineStatus
            INTEGER (-2147483648..2147483647),

        adnsonetLineAIS
            Counter,

        adnsonetLineRcvRDI
            Counter,

        adnsonetLineBIP
            Counter,

        adnsonetLineConfigLoopback
            INTEGER,

        adnsonetLineConfigRDI
            INTEGER,

        adnsonetLineConfigAIS
            INTEGER
    }

adnsonetLineStatus OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This variable indicates the status
                 of the interface.
                 It is a bit map represented as a sum;
                 therefore, it can represent multiple
                 defects simultaneously.
                 The adnsonetLineNoDefect bit should
                 be set only if no other bit is set.

                 The bit positions are:
                       1   adnsonetLineNoDefect
                       2   adnsonetLineAIS
                       4   adnsonetLineRDI
	               8   adnsonetLineBIP  
                      16   adnsonetLineHighBIP  
		       
		 Common Name: Line State"
    ::= { adnsonetLineEntry  1 }

adnsonetLineAIS OBJECT-TYPE
    SYNTAX  Counter	-- UNITS milliseconds

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Duration of alarm indication signal (AIS).
		  Counter #2 of the ATM core."
    ::= { adnsonetLineEntry  2 }

adnsonetLineRcvRDI OBJECT-TYPE
    SYNTAX  Counter	-- UNITS milliseconds

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Duration of remote defect indication (RDI) 
                 received from the remote equipement.
	         For a Synchronous Digital Hierarchy (SDH)
                 line, the RDI signal is known as Multiplexing
                 Section Far End Receive Failure (MS-FERF).
		 Counter #3 of the ATM core."
    ::= { adnsonetLineEntry  3 }

adnsonetLineBIP OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Number of 24-bit interleaved parity
                  (BIP-24) errors for STS-3c or STM-1.
                  Number of 96-bit interleaved parity
                  (BIP-96) errors for STS-12c or STM-4.
		  Counter #28 of the ATM core."
    ::= { adnsonetLineEntry  4 }

adnsonetLineConfigLoopback OBJECT-TYPE
    SYNTAX  INTEGER {
    adnsonetLineLoopbackON(1),
    adnsonetLineLoopbackOFF(2)
}
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "This variable indicates if line loopback
                  is activated or deactivated. 
                  When activated, received frames are 
                  retransmitted on the line.  
		  Common Name: Line Loopback"
    ::= { adnsonetLineEntry  5 }

adnsonetLineConfigRDI OBJECT-TYPE
    SYNTAX  INTEGER {
    adnsonetLineRDION(1),
    adnsonetLineRDIOFF(2)
}
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "This variable indicates if a permanent 
                  remote defect indication (RDI) or Far End
                  Receive Failure (FERF) is sent on the line.
                  It is a configuration option that should
                  be used only for maintenance purposes.
 		  Common Name: Forcing a Line Remote Defect 
 		  Indication"
    ::= { adnsonetLineEntry  6 }

adnsonetLineConfigAIS OBJECT-TYPE
    SYNTAX  INTEGER {
    adnsonetLineAISON(1),
    adnsonetLineAISOFF(2)
}
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "This variable indicates if a permanent
                  alarm indication signal (AIS) can be sent
                  on the line.
                  For STM-1 interfaces, this option can't be
                  activated (AIS off).
                  For STS-3c, STS-12c, and STM-4 interfaces,
                  this option can be activated
                  (AIS off or AIS on).
                  It is a configuration option that should
                  be used only for maintenance purposes.
		  Common Name: Forcing a Line Alarm Indication
		  Signal"
    ::= { adnsonetLineEntry  7 }

adnsonetPathTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF AdnSonetPathEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
