	SYNTAX  DisplayString
	ACCESS  read-only
	STATUS  optional
	DESCRIPTION
		"The hostname of the device reporting the message."
	::= { syslogtrap 4 }

tscSltMessage OBJECT-TYPE
	SYNTAX  DisplayString
	ACCESS  read-only
	STATUS  optional
	DESCRIPTION
		"The message from log message (stripped of time
		stamp, facility, and hostname"
	::= { syslogtrap 5 }

--
-- Trap Definitions
--

tscSltTrap TRAP-TYPE
	ENTERPRISE  syslogtrap
	VARIABLES   { 
		tscSltLogMessage,
		tscSltFacility,
		tscSltPid,
		tscSltHostname,
		tscSltMessage
		}
	DESCRIPTION
		"Reporting a syslog event."
		--#SUMMARY "%s"
		--#ARGUMENTS { 0 }
		--#SEVERITY CRITICAL
		--#SLOTMAP origin $A
		--#SLOTMAP facility $V2
		--#SLOTMAP pid $V3
		--#SLOTMAP message  $V5
	::= 1

