-- ***************************************************************************
-- *                           scfObjects               	             *
-- ***************************************************************************

--
-- SCF Information
--
--	This group provides general information pertaining to the System
--   Control Facility(SCF).
--

scfAgentId OBJECT-TYPE
	SYNTAX      Integer32
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"SCF board ID which this SNMP agent is currently running on."
	::= { scfInfo 1 }

scfAgentNumber OBJECT-TYPE
	SYNTAX      Integer32
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"Total count of available SCF boards."
	::= { scfInfo 2 }

--
-- SCF Agent Table
--

scfAgentTable OBJECT-TYPE
	SYNTAX      SEQUENCE OF ScfAgentEntry
	MAX-ACCESS  not-accessible
	STATUS      current
	DESCRIPTION
		"Table of available SCF boards."
	::= { scfInfo 3 }

scfAgentEntry OBJECT-TYPE
	SYNTAX      ScfAgentEntry
	MAX-ACCESS  not-accessible
	STATUS      current
	DESCRIPTION
		"Available SCF board entry."
	INDEX   { scfAgentIndex }
	::= { scfAgentTable 1 }

ScfAgentEntry ::=
	SEQUENCE {
		scfAgentIndex ScfIndex,
		scfXcpVersion DisplayString
	}

scfAgentIndex OBJECT-TYPE
	SYNTAX      ScfIndex
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"SCF board ID."
	::= { scfAgentEntry 1 }

scfXcpVersion OBJECT-TYPE
	SYNTAX      DisplayString
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"SCF board XSCF Control Package(XCP) version."
	::= { scfAgentEntry 2 }

--
-- SCF IP Address Information
--
scfIpAddressPortNumber OBJECT-TYPE
	SYNTAX      Integer32
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"SNMP communication port for all SCF LAN IP Addresses."
	::= { scfInfo 4 }

scfIpAddressNumber OBJECT-TYPE
	SYNTAX      Integer32
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"Total count of IP Address table entries."
	::= { scfInfo 5 }

--
-- SCF IP Address Table
--

scfIpAddressTable OBJECT-TYPE
	SYNTAX      SEQUENCE OF ScfIpAddressEntry
	MAX-ACCESS  not-accessible
	STATUS      current
	DESCRIPTION
		"Table of available management LAN IPs on SCF."
	::= { scfInfo 6 }

scfIpAddressEntry OBJECT-TYPE
	SYNTAX      ScfIpAddressEntry
	MAX-ACCESS  not-accessible
	STATUS      current
	DESCRIPTION
		"SCF LAN IP Address entry."
	INDEX   { scfIpAddressIndex }
	::= { scfIpAddressTable 1 }

ScfIpAddressEntry ::=
	SEQUENCE {
		scfIpAddressIndex ScfIndex,
		scfIpAddress IpAddress
	}

scfIpAddressIndex OBJECT-TYPE
	SYNTAX      ScfIndex
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"SCF IP Address table entry index."
	::= { scfIpAddressEntry 1 }

scfIpAddress OBJECT-TYPE
	SYNTAX      IpAddress
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"IP Address of a management LAN port."
	::= { scfIpAddressEntry 2 }

