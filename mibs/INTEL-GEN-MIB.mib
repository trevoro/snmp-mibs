            moduleBackupSwTftpIp            IpAddress,
            moduleSwTftpIpAndSwName         OCTET STRING (SIZE (4..260)),
            moduleSwUpdateResult            INTEGER,
            moduleSpecificObject            OCTET STRING (SIZE (0..256)),
