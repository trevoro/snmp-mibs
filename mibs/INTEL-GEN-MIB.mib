    ::= { moduleEntry 7 }

moduleUserAssignedNumber   OBJECT-TYPE
    SYNTAX  DisplayString (SIZE (0..24))
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
            "User-definable information about the module."
    ::= { moduleEntry 8 }

moduleUserAssignedName   OBJECT-TYPE
    SYNTAX  DisplayString (SIZE (0..24))
