MscLpDS3TestResultsEntry ::= SEQUENCE {
   mscLpDS3TestElapsedTime
      Counter32,
   mscLpDS3TestTimeRemaining
      Unsigned32,
   mscLpDS3TestCauseOfTermination
      INTEGER,
   mscLpDS3TestBitsTx
      PassportCounter64,
   mscLpDS3TestBytesTx
      PassportCounter64,
   mscLpDS3TestFrmTx
      PassportCounter64,
