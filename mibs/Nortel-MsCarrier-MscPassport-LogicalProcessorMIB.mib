      mscLpSonet component."
   INDEX {
      mscLpIndex,
      mscLpSonetIndex }
::= { mscLpSonetRowStatusTable 1 }

MscLpSonetRowStatusEntry ::= SEQUENCE {
   mscLpSonetRowStatus
      RowStatus,
   mscLpSonetComponentName
      DisplayString,
   mscLpSonetStorageType
      StorageType,
   mscLpSonetIndex
      Integer32 }

