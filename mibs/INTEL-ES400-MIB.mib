            es400PortVlanDefaultDesc
                DisplayString,
            es400PortVlanDefaultPortList
                PortList,
            es400PortVlanDefaultPortNumber
                INTEGER
        }

    es400PortVlanDefaultPvid OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
