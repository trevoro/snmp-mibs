MICOM-4400-T1E1-MIB DEFINITIONS ::= BEGIN
	IMPORTS
		TRAP-TYPE
			FROM RFC-1215
		mcmSysAsciiTimeOfDay
			FROM MICOM-SYS-MIB
		Counter
			FROM RFC1155-SMI
		OBJECT-TYPE
			FROM RFC-1212
		DisplayString
			FROM SNMPv2-TC
		micom-oscar
			FROM MICOM-OSCAR-MIB;


-- group OBJECT IDENTIFIERs

	micom-2t1e1			OBJECT IDENTIFIER ::= {micom-oscar 22}
	t1e1-r2-configuration 		OBJECT IDENTIFIER ::= { micom-2t1e1 1}
	t1e1-r2-control	 		OBJECT IDENTIFIER ::= { micom-2t1e1 2}
	t1e1-r2-status			OBJECT IDENTIFIER ::= { micom-2t1e1 3}
	t1e1-r2-statistics 		OBJECT IDENTIFIER ::= { micom-2t1e1 4}


-- *** CONFIGURATION groups/tables ***

-- Note: The following groups contain 2 related groups/tables for all configuration
--       functionality. 
--	 1) The Standard or Operational Parameters (Operational) provide the
--	 SNMP management interface with the access device operational modules.
--	 Objects defined for this interface begin with the 'mcm' prefix.
--	 2) The Configuration Parameters (Configuration) provide the SNMP 
--	 management interface with a stored access device configuration. 
--	 Objects defined for this interface begin with the 'nvm' prefix.
--	


--  Operational Parameter ("mcm") Groups/Tables


--  the T1/E1 Line Configuration Table 
--  (Standard/Operational Parameters)

--  This table provides the T1/E1 line configuration parameters
--  for identifying a T1/E1 line with its corresponding profile. (Operational)

mcm4400t1e1LineCfgTable OBJECT-TYPE
	SYNTAX  SEQUENCE OF Mcm4400t1e1LineCfgEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ; 
	   DESC = The T1/E1 Line Configuration table. \
		  (Operational);
	   HELP = ;
	   CAPABILITIES = NET_CFG, VPN_CFG;"
	::= { t1e1-r2-configuration 1 }

mcm4400t1e1LineCfgEntry OBJECT-TYPE
	SYNTAX  Mcm4400t1e1LineCfgEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ; 
	   DESC = An entry in the T1/E1 Line Configuration \
		  table. (Operational);
	   HELP = ;
	   CAPABILITIES = ;"
	INDEX   { mcm4400t1e1LineCfgLineID }
	::= { mcm4400t1e1LineCfgTable 1 }

Mcm4400t1e1LineCfgEntry ::= SEQUENCE {
	mcm4400t1e1LineCfgLineID		INTEGER,
	mcm4400t1e1LineCfgProfileNum		INTEGER
	}

mcm4400t1e1LineCfgLineID OBJECT-TYPE
	SYNTAX	INTEGER {
		local (1),
		network (2)
		}
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The T1/E1 line identification specifies which line \
		  is to be configured.  Used as the index to the \
		  T1/E1 Line Configuration table. (Operational);
	   HELP = ;
	   CAPABILITIES = ;"
	::= { mcm4400t1e1LineCfgEntry 1 }

mcm4400t1e1LineCfgProfileNum OBJECT-TYPE
	SYNTAX	INTEGER (1..2)
	ACCESS  read-write
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ; 
	   DESC = The value of this object indicates the profile \
		  to be used to configure the specified T1/E1 \
		  line. (Operational);
	   HELP = 1: expected as default profile for local line. \
		  2: expected as default profile for network line.;
	   CAPABILITIES = ;"
	::= { mcm4400t1e1LineCfgEntry 2 }


--  the T1/E1 Profile Configuration Table 
--  (Standard/Operational Parameters)

--  This table provides the T1/E1 profile configuration parameters.
--  This table is used to configure a profile of T1/E1 line configuration
--  characteristics; of which, a subset may be used for a particular T1/E1
--  line (local or network) to define its configuration. (Operational)

mcm4400t1e1ProfCfgTable OBJECT-TYPE
	SYNTAX  SEQUENCE OF Mcm4400t1e1ProfCfgEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ; 
	   DESC = The T1/E1 Profile Configuration table. \
		  (Operational);
	   HELP = ;
	   CAPABILITIES = NET_CFG, VPN_CFG;"
	::= { t1e1-r2-configuration 2 }

mcm4400t1e1ProfCfgEntry OBJECT-TYPE
	SYNTAX  Mcm4400t1e1ProfCfgEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ; 
	   DESC = An entry in the T1/E1 Profile Configuration \
		  table. (Operational);
	   HELP = ;
	   CAPABILITIES = ;"
	INDEX   { mcm4400t1e1ProfCfgProfileNum }
	::= { mcm4400t1e1ProfCfgTable 1 }

Mcm4400t1e1ProfCfgEntry ::= SEQUENCE {
	mcm4400t1e1ProfCfgProfileNum		INTEGER,
	mcm4400t1e1ProfCfgT1FrameFmt		INTEGER,
	mcm4400t1e1ProfCfgT1LineCode		INTEGER,
	mcm4400t1e1ProfCfgE1LineCode		INTEGER,
	mcm4400t1e1ProfCfgIdleCode		INTEGER,
	mcm4400t1e1ProfCfgT1LineBuildOut	INTEGER,
	mcm4400t1e1ProfCfgT1Loopback		INTEGER,
	mcm4400t1e1ProfCfgE1CRC4		INTEGER,
	mcm4400t1e1ProfCfgE1TS16Conn		INTEGER
	}

mcm4400t1e1ProfCfgProfileNum OBJECT-TYPE
	SYNTAX	INTEGER (1..2)
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object indicates the profile \
		  number.  Used as an index to the T1/E1 \
		  Profile Configuration table. (Operational);
	   HELP = ;
	   CAPABILITIES = ;"
	::= { mcm4400t1e1ProfCfgEntry 1 }

mcm4400t1e1ProfCfgT1FrameFmt OBJECT-TYPE
	SYNTAX	INTEGER {
		superFrame (1),
		extendedSuperFrame (2)
		}
	ACCESS  read-write
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object specifies the T1 \
		  frame format used in the line. (Operational);
	   HELP = ;
	   CAPABILITIES = ;"
	DEFVAL { superFrame }
	::= { mcm4400t1e1ProfCfgEntry 2 }

mcm4400t1e1ProfCfgT1LineCode OBJECT-TYPE
	SYNTAX	INTEGER {
		aMI (1),
		b8ZS (2)
		}
	ACCESS  read-write
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object specifies the T1 \
		  coding format used in the line. (Operational);
	   HELP = ;
	   CAPABILITIES = ;"
	DEFVAL { aMI }
	::= { mcm4400t1e1ProfCfgEntry 3 }

mcm4400t1e1ProfCfgE1LineCode OBJECT-TYPE
	SYNTAX	INTEGER {
		aMI (1),
		hDB3 (2)
		}
	ACCESS  read-write
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ; 
	   DESC = The value of this object specifies the E1 \
		  coding format used in the line. (Operational);
	   HELP = ;
	   CAPABILITIES = ;"
	DEFVAL { aMI }
	::= { mcm4400t1e1ProfCfgEntry 4 }

mcm4400t1e1ProfCfgIdleCode OBJECT-TYPE
	SYNTAX	INTEGER (0..'ff'h)
	ACCESS  read-write
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ; 
	   DESC = The value of this object specifies the code \
		  to be filled in the line when idle. (Operational);
	   HELP = ;
	   CAPABILITIES = ;"
	DEFVAL {'7f'h }
	::= { mcm4400t1e1ProfCfgEntry 5 }

mcm4400t1e1ProfCfgT1LineBuildOut OBJECT-TYPE
	SYNTAX	INTEGER {
		lessThan110Feet (1),
		lessThan220Feet (2),
		lessThan330Feet (3),
		lessThan440Feet (4),
		externalLBO (5),
		greaterThan665Feet (6),
		lessThan550Feet (7),
		lessThan660Feet (8)
		}
	ACCESS  read-write
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object specifies the cable \
		  length attached to a local T1 line. (Operational);
	   HELP = ;
	   CAPABILITIES = ;"
	DEFVAL { lessThan110Feet }
	::= { mcm4400t1e1ProfCfgEntry 6 }

mcm4400t1e1ProfCfgT1Loopback OBJECT-TYPE
	SYNTAX	INTEGER {
		disable (1),
		enable (2)
		}
	ACCESS  read-write
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object specifies whether \
		  to enable or disable the T1 line loopback \
		  to be invoked. (Operational);
	   HELP = 1: expected as default for a local T1 line profile. \
		  2: expected as default for a network T1 line profile.;
	   CAPABILITIES = ;"
	::= { mcm4400t1e1ProfCfgEntry 7 }

mcm4400t1e1ProfCfgE1CRC4 OBJECT-TYPE
	SYNTAX	INTEGER {
		disable (1),
		enable (2)
		}
	ACCESS  read-write
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object specifies whether \
		  to enable or disable a T1 line CRC4. (Operational);
	   HELP = ;
	   CAPABILITIES = ;"
	DEFVAL { disable }
	::= { mcm4400t1e1ProfCfgEntry 8 }

mcm4400t1e1ProfCfgE1TS16Conn OBJECT-TYPE
	SYNTAX	INTEGER {
		toCAS (1),
		idle (2),
		bypass (3),
		toDataPort1 (4),
		toDataPort2 (5)
		}
	ACCESS  read-write
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object specifies where the \
		  TS16 goes (for a T1 line). (Operational);
	   HELP = ;
	   CAPABILITIES = ;"
	DEFVAL { toCAS }
	::= { mcm4400t1e1ProfCfgEntry 9 }


--  the T1/E1 DS0 Channel Connection Configuration Table 
--  (Standard/Operational Parameters)

--  This table provides the T1/E1 DS0 channel connection configuration 
--  parameters. (Operational)

mcm4400t1e1DS0ChConnTable OBJECT-TYPE
	SYNTAX  SEQUENCE OF Mcm4400t1e1DS0ChConnEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ; 
	   DESC = The T1/E1 DS0 Channel Connection \
		  Configuration table.(Operational);
	   HELP = ;
	   CAPABILITIES = NET_CFG, VPN_CFG;"
	::= { t1e1-r2-configuration 3 }

mcm4400t1e1DS0ChConnEntry OBJECT-TYPE
	SYNTAX  Mcm4400t1e1DS0ChConnEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ; 
	   DESC = An entry in the T1/E1 DS0 Channel \
		  Connection Configuration table. (Operational);
	   HELP = ;
	   CAPABILITIES = ;"
	INDEX   { mcm4400t1e1DS0ChConnLineID,
		mcm4400t1e1DS0ChConnDS0Index }
	::= { mcm4400t1e1DS0ChConnTable 1 }

Mcm4400t1e1DS0ChConnEntry ::= SEQUENCE {
	mcm4400t1e1DS0ChConnLineID		INTEGER,
	mcm4400t1e1DS0ChConnDS0Index		INTEGER,
	mcm4400t1e1DS0ChConnState		INTEGER
	}

mcm4400t1e1DS0ChConnLineID OBJECT-TYPE
	SYNTAX	INTEGER {
		local (1),
		network (2)
		}
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The T1/E1 line identification specifies which line \
		  is to be configured.  Used as an index to the \
		  T1/E1 DS0 Channel Connection Configuration \
		  table. (Operational);
	   HELP = ;
	   CAPABILITIES = ;"
	::= { mcm4400t1e1DS0ChConnEntry 1 }

mcm4400t1e1DS0ChConnDS0Index OBJECT-TYPE
	SYNTAX	INTEGER (1..30)
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ; 
	   DESC = The T1/E1 DS0 index specifies which time slot \
		  is to be connected.  Used as an index to the \
		  T1/E1 DS0 Channel Connection Configuration \
		  table. (Operational);
	   HELP = For T1, 24 is the maximum.  An error will be \
		  returned for a T1 DS0 index greater than 24.;
	   CAPABILITIES = ;"
	::= { mcm4400t1e1DS0ChConnEntry 2 }

mcm4400t1e1DS0ChConnState OBJECT-TYPE
	SYNTAX	INTEGER {
		idle (1),
		bypassVoice (2),
		bypassData (3),
		toWANPort1 (4),
		toWANPort2 (5),
		toDataPort1 (6),
		toDataPort2 (7),
		toDVMChannelB1 (8),
		toDVMChannelB2 (9),
		toDVMChannelB3 (10),
		toDVMChannelB4 (11),
		toDVMChannelB5 (12),
		toDVMChannelB6 (13),
		toDVMChannelC1 (14),
		toDVMChannelC2 (15),
		toDVMChannelC3 (16),
		toDVMChannelC4 (17),
		toDVMChannelC5 (18),
		toDVMChannelC6 (19),
		toDVMChannelC7 (20),
		toDVMChannelC8 (21),
		toDVMChannelC9 (22),
		toDVMChannelC10 (23),
		toDVMChannelC11 (24),
		toDVMChannelC12 (25),
		toDVMChannelD1 (26),
		toDVMChannelD2 (27),
		toDVMChannelD3 (28),
		toDVMChannelD4 (29),
		toDVMChannelD5 (30),
		toDVMChannelD6 (31),
		toDVMChannelD7 (32),
		toDVMChannelD8 (33),
		toDVMChannelD9 (34),
		toDVMChannelD10 (35),
		toDVMChannelD11 (36),
		toDVMChannelD12 (37)
		}
	ACCESS  read-write
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ; 
	   DESC = The value of this object specifies where the \
		  T1/E1 line channel slot (DS0) is to be connected \
		  to. (Operational);
	   HELP = Note 1: if 2-37 is selected, the previous setting \
		  must be idle, or an error will be returned. \
		  Note 2: if idle is specified and the previous setting \
		  was bypass (voice or data), the corresponding DS0 \
		  in the other line will also be set to idle automatically. \
		  Note 3: if bypass (voice or data) is specified, the \
		  corresponding DS0 in the other line will also be \
		  set to the same bypass mode automatically.;
	   CAPABILITIES = ;"
	::= { mcm4400t1e1DS0ChConnEntry 3 }


