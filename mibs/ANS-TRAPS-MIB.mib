ANS-TRAPS-MIB DEFINITIONS ::= BEGIN

IMPORTS
	TRAP-TYPE FROM RFC-1215
	OBJECT-TYPE FROM RFC1155-SMI
	DisplayString, sysName, sysDescr, sysLocation, sysContact
		FROM RFC1213-MIB
	RowPointer, RowStatus, DateAndTime, mlpmpR115, common
		FROM ANS-COMMON-MIB
	InstancePointer, AlarmSeverity, AlarmType
		FROM ANS-ALARM-MIB
	slot, subrack, board, systemNode
		FROM ANS-EQUIPMENT-MIB;

traps		OBJECT IDENTIFIER ::= {mlpmpR115 7}


ansIdentifier OBJECT-TYPE
	SYNTAX	RowPointer
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION
		"A reference to the affected entity. "

	::= { traps  1 }

alarmingObject OBJECT-TYPE
	SYNTAX	InstancePointer
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION
		"The alarming object. Identifies either a variable in 
		the group of variables belonging to the alarming object, 
		or a row in a table representing the alarming object."
	::= { traps  11 }

alarmType OBJECT-TYPE
	SYNTAX	AlarmType
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION
		"The type of the alarm."
	::= { traps  12 }

alarmSeverity OBJECT-TYPE
	SYNTAX	AlarmSeverity
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION
		"The perceived severity of the fault."
	::= { traps  13 }

alarmTime OBJECT-TYPE
	SYNTAX	DateAndTime
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION
		"The time the fault was detected."
	::= { traps  14 }

alarmInformation OBJECT-TYPE
	SYNTAX	DisplayString
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION
		"Additional information pin-pointing the problem. If
		not provided, this string shall be of zero length."
	::= { traps  15 }

eventTime OBJECT-TYPE
	SYNTAX	DateAndTime
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION
		"The time the event occured."
	::= { traps  17 }

ntId OBJECT-TYPE
	SYNTAX	DisplayString(SIZE(0..32))
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION
		"Customer id or network terminal id or radio terminal id"
	::= { traps  18 }


--***************************************
--
-- Alarm traps on a subrack
--


cellbusFailure TRAP-TYPE
        ENTERPRISE subrack
        VARIABLES {
                alarmingObject,
                alarmType,
                alarmSeverity,
                alarmTime,
                alarmInformation,
		ntId
                }
        DESCRIPTION
                "This is sent when a Cellbus has a defect
                 that is longer than 2.5+/-0.5 seconds."
        ::= 5

fanFailure TRAP-TYPE
        ENTERPRISE subrack
        VARIABLES {
                alarmingObject,
                alarmType,
                alarmSeverity,
                alarmTime,
                alarmInformation,
		ntId
                }
        DESCRIPTION
                "Is sent when a fan failure has been detected in the subrack (this could be based on detection of over_temperature in the subrack) An alarm ceasing report (alarm severity ='cleared') will be sent when an indication that the fan is back in operation has been detected."
        ::= 6

powerFailure TRAP-TYPE
        ENTERPRISE subrack
        VARIABLES {
                alarmingObject,
                alarmType,
                alarmSeverity,
                alarmTime,
                alarmInformation,
		ntId
                }
        DESCRIPTION
                "Is sent when a power failure has been detected in the subrack. An alarm ceasing report (alarm severity = 'cleared') will be sent when an indication that the power is back on has been detected."
        ::= 7


--*************************************
--
-- Event traps on a slot
--


communicationLost TRAP-TYPE
	ENTERPRISE slot
	VARIABLES {
		alarmingObject,
		alarmType,
		alarmSeverity,
		alarmTime,
		alarmInformation,
		ntId
		}
	DESCRIPTION
		"Is sent when a the communication to a subrack 
		has been lost. An alarm ceasing report (alarm 
		severity = 'cleared') will be sent when the 
		communication has been re-established.
		The alarm type is by default 'communication alarm'."
	::= 1



boardReplaced TRAP-TYPE
	ENTERPRISE slot
	VARIABLES {
		ansIdentifier,
		eventTime,
		ntId
		}
	DESCRIPTION
		"Is sent when the insertion of a circuit board
		that matches the configured data for the slot
		(but not the serial number) has been detected."
	::= 2


--********************************************
--
-- Alarm traps on a slot
--

boardRemovedAlarm TRAP-TYPE
	ENTERPRISE slot
	VARIABLES {
		alarmingObject,
		alarmType,
		alarmSeverity,
		alarmTime,
		alarmInformation,
		ntId
		}
	DESCRIPTION
		"Is sent when the removal of a matching circuit board
		from a slot has been detected. An alarm ceasing report 
		(alarm severity = 'cleared') will be sent when a board 
		is inserted in the slot, or when the configuration data 
		is cleared for the slot. 
		The alarm type is by default 'equipment alarm'."
	::= 4

boardMismatch TRAP-TYPE
	ENTERPRISE slot
	VARIABLES {
		alarmingObject,
		alarmType,
		alarmSeverity,
		alarmTime,
		alarmInformation,
		ntId
		}
	DESCRIPTION
		"Is sent when the insertion of a circuit board
		that does not match the configured data for the
		slot has been detected. An alarm ceasing report 
		(alarm severity = 'cleared') will be sent when 
		matching configuration data is set for the slot, 
		when the configuration data is cleared for
		the slot, or when the board is removed.
		The alarm type is by default 'equipment alarm'."
	::= 6

boardUnrecognized TRAP-TYPE
	ENTERPRISE slot
	VARIABLES {
		alarmingObject,
		alarmType,
		alarmSeverity,
		alarmTime,
		alarmInformation,
		ntId
		}
	DESCRIPTION
		"Is sent when the insertion of a circuit board
		in a slot has been detected, but the board ID 
		is not recognizable (or not valid for this system).
		An alarm ceasing report (alarm severity = 'cleared')
		will be sent when the board is removed.
		The alarm type is by default 'equipment alarm'."
	::= 7

powerLoss TRAP-TYPE
	ENTERPRISE slot
	VARIABLES {
		alarmingObject,
		alarmType,
		alarmSeverity,
		alarmTime,
		alarmInformation,
		ntId
		}
	DESCRIPTION
		"Is sent when a power loss occurs at a board."
	::= 8

interfaceFailure TRAP-TYPE
	ENTERPRISE slot
	VARIABLES {
		alarmingObject,
		alarmType,
		alarmSeverity,
		alarmTime,
		alarmInformation,
		ntId
		}
	DESCRIPTION
		"Is sent when an interface on a board has failed."
	::= 9



