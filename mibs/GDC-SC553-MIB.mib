Sc553MaintenanceEntry ::=
SEQUENCE {
	sc553MaintenanceIndex      SCinstance,
	sc553LedStatus             OCTET STRING,
        sc553SoftReset             INTEGER,
	sc553DefaultInit           INTEGER,
	sc553FrontPanel            INTEGER,
        sc553ProductType           INTEGER,
        sc553ResetStatistics       INTEGER,
        sc553ValidUserIntervals    INTEGER,
        sc553ValidNetworkIntervals INTEGER,
        sc553ValidFarEndIntervals  INTEGER,
        sc553CascadePresent        INTEGER,
        sc553ReceiveLevel          INTEGER,
	sc553DteStat		   OCTET STRING,		
        sc553CasReceiveLevel       INTEGER,
