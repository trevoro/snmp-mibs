-- 
--
--                    Network/ Channel Save
-- 





sc553ConfigurationSave OBJECT-TYPE
    SYNTAX INTEGER {
          norm(1),		-- read-only
          saveall(2)        	-- write-only
    }
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
        "This variable is used to set the update status flag for saving 
	network and channel configuration settings.
        
	Management can only set a saveall(2).

	A norm(1) will always be read.
	
	Configuration settings are not saved to network element by default.
	This permits the configuration of several objects before saving.  
	After all objects are set-up, a (2) must be sent.  This will
	enable and save the new configuration(s).
	
 	"

    ::= { sc553Configuration 3 }
 











-- ****************************************************************************
--                        Diagnostics table
-- ****************************************************************************

sc553DiagTable OBJECT-TYPE
    SYNTAX SEQUENCE OF Sc553DiagEntry
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
        "The SC553 Diagnostics table."
    ::= { sc553Diagnostics 1 }
 
sc553DiagEntry OBJECT-TYPE
    SYNTAX Sc553DiagEntry
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
        "The SNMP553S Diagnostics table entry."
    INDEX { sc553DiagIndex }
    ::= { sc553DiagTable 1 }
 
Sc553DiagEntry ::=
    SEQUENCE {
        sc553DiagIndex 			SCinstance,
        sc553DiagTestDuration 		INTEGER,
        sc553DiagProgPattern 		INTEGER,
        sc553InsertBitError 		INTEGER,
        sc553DiagDS0Number 		INTEGER,
        sc553DiagT1SelfTestPattern 	INTEGER,
        sc553DiagT1Test 		INTEGER,
        sc553DiagDS0SelfTestPattern 	INTEGER,
        sc553DiagDS0Test 		INTEGER,
        sc553DiagChannelSelfTestPattern INTEGER,
        sc553DiagChannelTest 		INTEGER,
        sc553DiagTestResults  		OCTET STRING,
	sc553DiagTestStatus 		INTEGER,
	sc553DiagResetTestToNormal 	INTEGER,
	sc553DiagResetTestResults	INTEGER,
	sc553DiagT1TestDirection 	INTEGER,
	sc553DiagDS0TestDirection	INTEGER
	
 }

sc553DiagIndex OBJECT-TYPE
    SYNTAX SCinstance
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "The index value which uniquely identifies the interface to
        which this entry is applicable.  This has the form of a
        SCinstance which defines the slot, line, drop, and sub-identifier,
        which in this case is a network interface."
    ::= { sc553DiagEntry 1 }
 
sc553DiagTestDuration OBJECT-TYPE
    SYNTAX INTEGER     {
	testTime30Secs(1),
        testTime1Min(2),
        testTime2Mins(3),
        testTime3Mins(4),
        testTime4Mins(5),
        testTime5Mins(6),
        testTime6Mins(7),
        testTime7Mins(8),
        testTime8Mins(9),
        testTime9Mins(10),
        testTime10Mins(11),
        testTime15Mins(12),
        testTime20Mins(13),
        testTime25Mins(14),
        testTime30Mins(15),
	testTimeInfinite(16)
    }
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
        "Selects the duration to run a diagnostic test.  
	"

    ::= { sc553DiagEntry 2 }




sc553DiagProgPattern OBJECT-TYPE
     SYNTAX INTEGER (0..'ffff'h)
     ACCESS read-write
     STATUS mandatory
     DESCRIPTION
        "The 16 bit user programmable test pattern.  This object
        works with the  sc553DiagT1SelfTestPattern object in that when it
        is set to t1ProgPattern(4), the the value of this object is
        used as the 16 bit user programmable test pattern.
 
        This object is used for all tests defined in both the GDC
        dsx1 mib."
 
     ::= { sc553DiagEntry 3}




sc553InsertBitError OBJECT-TYPE
    SYNTAX INTEGER {

	insertBitErrorNorm(1),		-- Read Only
	insertOneBitError(2)		         	

    }
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION "A set of 2 will insert one bit error for test.
		Management may only write a 2.  A 1 will always be read.
	"
    ::= { sc553DiagEntry 4 }




sc553DiagDS0Number OBJECT-TYPE
    SYNTAX INTEGER (1..24)
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
        "Indicates the DS0 number for the test.  The valid range is between
	one (1) and twenty-four (24)."
    ::= { sc553DiagEntry 5 }
 

sc553DiagT1SelfTestPattern OBJECT-TYPE
    SYNTAX INTEGER {

	t1511Pattern(1),		 
	t12047Pattern(2),		 
	t1QRSPattern(3),		 
	t1ProgPattern(4),		 
	t13in24(5)     		         
   }
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION "The selected option is used when a sc553DiagT1Test is performed.
                 
	"
    ::= { sc553DiagEntry 6 }


sc553DiagT1Test OBJECT-TYPE
    SYNTAX INTEGER {

	t1SelfTest(1),			-- Network:  	Self Tests
	t1LocalSelfTest(2),		--
	t1RemoteSelfTest(3),		--
	t1LocalTest(4),		        -- 		Loopbacks
