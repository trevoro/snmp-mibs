    }
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "Identifies whether or not the cascade interface cards
        are installed."
    ::= { sc553MaintenanceEntry 11}

sc553ReceiveLevel    OBJECT-TYPE
    SYNTAX INTEGER {
	invalidNiDSX1(1),
	noSignal(2),
	pos2toNeg7andOneHalf(3),
	neg7andOneHalftoNeg15(4),
	neg15toNeg22andOneHalf(5),
	lessThanNeg22andOneHalf(6)				
     }
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        " invalidNiDSX1(1) indicates an invalid request because the Network Interface
	  is configured as DSX-1.  noSignal(2) is present when there is no T1 signal
	  detected (NS led on front panel will be on).

	  The last 4 options (3) - (6) indicate the ranges for receive level:

		(3):	+2db   to -7.5db
		(4):	-7.5db to -15db
		(5):	-15db  to -22.5db
		(6):	less than -22.5db

		"

      ::= { sc553MaintenanceEntry 12 }

sc553DteStat OBJECT-TYPE
        SYNTAX  OCTET STRING (SIZE(2))
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
        "Returns a bitwise snapshot of the DTE signals.

        Octet 1 bit 7 - not used
                bit 6 - CTS     (1:ON; 0:OFF)
                bit 5 - RTS     (1:ON; 0:OFF)
                bit 4 - DTR     (1:ON; 0:OFF)
                bit 3 - DSR     (1:ON; 0:OFF)
                bit 2 - future use 
                bit 1 - DCD     (1:ON; 0:OFF)
                bit 0 - future use
 
        Octet 2 bit 7 - not used
                bit 6 - RXC     (1:Transitions)
                bit 5 - DCD     (1:Transitions)
                bit 4 - RTS     (1:Transitions)
                bit 3 - CTS     (1:Transitions)
                bit 2 - RXD     (1:Transitions)
                bit 1 - TXC     (1:Transitions)
                bit 0 - TXD     (1:Transitions)"
    ::= { sc553MaintenanceEntry 13 }
 




sc553CasReceiveLevel    OBJECT-TYPE
    SYNTAX INTEGER {
	invalidNiDSX1(1),
	noSignal(2),
	pos2toNeg7andOneHalf(3),
	neg7andOneHalftoNeg15(4),
	neg15toNeg22andOneHalf(5),
	lessThanNeg22andOneHalf(6)				
     }
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        " invalidNiDSX1(1) indicates an invalid request because the Cascade Interface
	  is configured as DSX-1.  noSignal(2) is present when there is no T1 signal
	  detected (NS led on front panel will be on).

	  The last 4 options (3) - (6) indicate the ranges for receive level:

		(3):	+2db   to -7.5db
		(4):	-7.5db to -15db
		(5):	-15db  to -22.5db
		(6):	less than -22.5db

		"

      ::= { sc553MaintenanceEntry 14 }
 

sc553ShelfType OBJECT-TYPE
     SYNTAX INTEGER
     {
         spectracomm(1),
         twinPack(2)
     }
     ACCESS read-only
     STATUS mandatory
     DESCRIPTION
     "Type of shelf the sc553 is located in."
     ::= { sc553MaintenanceEntry 15 }

sc553TwinPackPowerSupply OBJECT-TYPE
