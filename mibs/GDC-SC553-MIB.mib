			 bit 6 - 2^6 	Loss of Frame Count
			 bit 5 - 2^5
			 bit 4 - 2^4
			 bit 3 - 2^3
			 bit 2 - 2^2
			 bit 1 - 2^1
			 bit 0 - 2^0

		Octet 13 bit 7 - parity   
			 bit 6 - 2^13	CRC's
			 bit 5 - 2^12
			 bit 4 - 2^11
			 bit 3 - 2^10
			 bit 2 - 2^9
			 bit 1 - 2^8
			 bit 0 - 2^7

		Octet 14 bit 7 - parity   
			 bit 6 - 2^6 	CRC's
			 bit 5 - 2^5
			 bit 4 - 2^4
			 bit 3 - 2^3
			 bit 2 - 2^2
			 bit 1 - 2^1
			 bit 0 - 2^0

		Octet 15 bit 7 - parity   
			 bit 6 - 2^6 	Percent Complete
			 bit 5 - 2^5
			 bit 4 - 2^4
			 bit 3 - 2^3
			 bit 2 - 2^2
			 bit 1 - 2^1
			 bit 0 - 2^0
		"

    ::= { sc553CurrentUserEntry 2 }
 


