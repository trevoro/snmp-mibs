mcmFrAllSVCCircuitSetupPriority OBJECT-TYPE
        SYNTAX INTEGER (1..5)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
           DESC = This object indicates the priority of a path at \
		  call-establishment time, and also determines \
		  the potential for bumping established paths \
		  to acquire their bandwidth.  A value of 1 is \
		  the highest priority, and 5 is the lowest.;
           HELP = ;
	   CAPABILITIES = ;"
	::= {mcmFrAllSVCCircuitEntry 30}


mcmFrAllSVCCircuitHoldingPriority OBJECT-TYPE
        SYNTAX INTEGER (1..5)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
           DESC = This object indicates the priority that a path \
