nvmFrPVCPhyEntry OBJECT-TYPE
	SYNTAX	NvmFrPVCPhyEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
           DESC = Entry into the micom PVC LMI \
                  configuration extensions;
           HELP = Entry in the PVC Phys Table. \
                  Additional information required \
                  (not in RFC 1315 DLCI table);
	   CAPABILITIES = ;"
	INDEX	{nvmFrPVCPhyIfIndex}
	::= {nvmFrPVCPhyTable 1}

NvmFrPVCPhyEntry ::= SEQUENCE {
		nvmFrPVCPhyIfIndex	INTEGER,
		nvmFrPVCLMIMode	INTEGER,
		nvmFrPVCBiDir		INTEGER
