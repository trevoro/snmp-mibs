nvmFrPVCPhyIfIndex OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = IfIndex of the physical port \
		  the extension information \
		  is associated with;
	   HELP = ;
	   CAPABILITIES = ;"
	::= {nvmFrPVCPhyEntry 1}

nvmFrPVCLMIMode OBJECT-TYPE
	SYNTAX	INTEGER {
		dte (1),
		dce (2)
	}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
           DESC = Indicates whether the local PVC \
                  LMI is performing in the DTE or \
                  DCE mode;
           HELP = DTE or DCE mode of operation for \
                  the Frame Relay PVC LMI. (In DTE \
                  mode the Status Enquiry messages \
                  will be sent.  In DCE mode, Status \
                  Reports will be sent in response \
                  to Status Enquiry mesages.;
	   CAPABILITIES = ;"
