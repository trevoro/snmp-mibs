A3ComSysInterfaceLocationEntry ::=
	SEQUENCE {
		a3ComSysInterfaceLocationIfIndex
			INTEGER,
		a3ComSysInterfaceLocationInterfaceType
			INTEGER,
		a3ComSysInterfaceLocationType
			INTEGER,
