a3ComSysInterfaceLocationType OBJECT-TYPE
	SYNTAX	INTEGER {
				other(1),
				modularSlot(2),
				chassis(3),
				modularCard(4)
			}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
