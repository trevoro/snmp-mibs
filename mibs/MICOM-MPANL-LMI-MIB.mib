--  the MPANL LMI Statistics Group (Operational Parameters - Counters)

--  This table provides counters to be used for monitoring during debugging and
--  performance characterization. 

mcmMLMIStatsActiveVCs OBJECT-TYPE
	SYNTAX	Counter
    	ACCESS  read-only
     	STATUS  mandatory
     	DESCRIPTION
          "NAME = ;
	   DESC = Counter representing the number of active VCs \
		  maintained by the MLMI.
	   HELP = ;
	   CAPABILITIES = NET_CFG, VPN_DISP;"
     	::= { mlmi-statistics 1 }

mcmMLMIStatsRequestedCalls OBJECT-TYPE
	SYNTAX	Counter
	ACCESS  read-only
     	STATUS  mandatory
     	DESCRIPTION
       	  "NAME = ; 
	   DESC = Counter representing the number of SL_CONNECT_REQ \
		  received by the MLMI from upper applications. ;
	   HELP = ;
	   CAPABILITIES = NET_CFG, VPN_DISP;"
	::= { mlmi-statistics 2 }

mcmMLMIStatsInitiatedCalls OBJECT-TYPE
	SYNTAX	Counter
    	ACCESS  read-only
     	STATUS  mandatory
     	DESCRIPTION
          "NAME = ;
	   DESC = Counter representing the number of setup messages \
