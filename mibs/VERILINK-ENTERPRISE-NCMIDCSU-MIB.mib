             ncmidcsuThresholdIntervalIndex
                 INTEGER, 
	     ncmidcsuBERThreshold
                 INTEGER,
	     ncmidcsubpvSecThreshold
                 INTEGER,
             ncmidcsubpvSecInterval
	 	INTEGER,    
             ncmidcsuESThreshold
                 INTEGER,
             ncmidcsuESInterval
                 INTEGER,
             ncmidcsuUASThreshold
                 INTEGER,
             ncmidcsuUASInterval
                 INTEGER


		}

