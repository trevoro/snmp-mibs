    -- ------------------------------------------------------------------------
    -- FR manage variable
    -- ------------------------------------------------------------------------
    hwMusaFrcMib    OBJECT IDENTIFIER ::= { hwMa5100Mib 9 }
    
    
    -- ******************************************************************
    -- FRC board attribute table
    hwMusaFrcBoardTable    OBJECT-TYPE
        SYNTAX      SEQUENCE OF HwMusaFrcBoardEntry
        ACCESS      not-accessible
        STATUS      mandatory
    DESCRIPTION
    "Musa Frc board configuration table."
    ::= { hwMusaFrcMib 1 }

   hwMusaFrcBoardEntry    OBJECT-TYPE
        SYNTAX         HwMusaFrcBoardEntry
        ACCESS         not-accessible
        STATUS         mandatory
    DESCRIPTION
    "This list contains Musa Frc Board variables."
    INDEX { hwMusaFrameIndex,
            hwMusaSlotIndex}
    ::= { hwMusaFrcBoardTable  1 }
 
   HwMusaFrcBoardEntry    ::= SEQUENCE  {
            hwMusaFrcPvcAlarmEnable       INTEGER,
            hwMusaFrcBoardClearStatistic    INTEGER,   
            hwMusaFrcClockMode   			INTEGER, 
            hwMusaFrcPhyPortId   			INTEGER,
            hwMusaFrcPhyPortType			INTEGER
            } 

    hwMusaFrcPvcAlarmEnable  OBJECT-TYPE
        SYNTAX  INTEGER{
                        disable(0),
                        enable(1)
                        }
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
        "The Frc PVC alarm enable or disable."
    ::= { hwMusaFrcBoardEntry 1 }   

    hwMusaFrcBoardClearStatistic  OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  write-only
        STATUS  mandatory
        DESCRIPTION
        "Clear the Frc board statisitcs."
    ::= { hwMusaFrcBoardEntry 2 }   

    hwMusaFrcClockMode  OBJECT-TYPE
        SYNTAX  INTEGER{
                        local(0),
                        backplate(1),
                        line(2)
                        }
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
        "The phy port Clock Mode."
    ::= { hwMusaFrcBoardEntry 3 }

    hwMusaFrcPhyPortId  OBJECT-TYPE
        SYNTAX  INTEGER(0..7)
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
        "the index of the port."
    ::= { hwMusaFrcBoardEntry 4 }     

    hwMusaFrcPhyPortType  OBJECT-TYPE
        SYNTAX  INTEGER{
                        e1(1),
                        t1(2),
                        v35(3)
                        }
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
        "physical port type."
    ::= { hwMusaFrcBoardEntry 5 } 
            
    -- **************************************************
    -- FRC PHY port line interface table
    
    hwMusaFrcPhyPortLineIfTable    OBJECT-TYPE
        SYNTAX      SEQUENCE OF HwMusaFrcPhyPortLineIfEntry
        ACCESS      not-accessible
        STATUS      mandatory
    DESCRIPTION
    "Musa Frc phy port line Interface table."
    ::= { hwMusaFrcMib 2 }

   hwMusaFrcPhyPortLineIfEntry    OBJECT-TYPE
        SYNTAX         HwMusaFrcPhyPortLineIfEntry
        ACCESS         not-accessible
        STATUS         mandatory
    DESCRIPTION
    "This list contains Musa Frc phy port line If variables."
    INDEX { hwMusaFrameIndex,
            hwMusaSlotIndex ,
            hwMusaFrcPhyPortId   }
    ::= { hwMusaFrcPhyPortLineIfTable  1 }
 
   HwMusaFrcPhyPortLineIfEntry    ::= SEQUENCE  {
            hwMusaFrcMainClock                  INTEGER,
            hwMusaFrcT1HaulMode          	INTEGER,
            hwMusaFrcT1E1Crc               	INTEGER,
            hwMusaFrcT1FrameFormat              INTEGER,
            hwMusaFrcT1LineCode              	INTEGER,
            hwMusaFrcV35ClockMode               INTEGER,
            hwMusaFrcT1E1TsBitMap		INTEGER,
            hwMusaFrcClockLoopMode     		INTEGER,
            hwMusaFrcLoopState     		INTEGER,
            hwMusaFrcV35InvertTxClock 		INTEGER,
            hwMusaFrcSingleTimeSlotId           INTEGER
            } 
                       
    hwMusaFrcMainClock  OBJECT-TYPE
        SYNTAX  INTEGER{
                        local(0),
                        backplate(1),
                        line(2)
                        }
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
        "The main clock.
         local,backplate,line."
    ::= { hwMusaFrcPhyPortLineIfEntry 1 }

    hwMusaFrcT1HaulMode  OBJECT-TYPE
        SYNTAX  INTEGER{
                        short(0),
                        long(1)
                        }
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
        "The T1 short/long haul mode."
    ::= { hwMusaFrcPhyPortLineIfEntry 2 }
                        
    hwMusaFrcT1E1Crc  OBJECT-TYPE
        SYNTAX  INTEGER{
                        no(0),
                        yes(1)
                        }
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
        "The T1 crc, when T1 is ESF frame format."
    ::= { hwMusaFrcPhyPortLineIfEntry 3 }
                        
    hwMusaFrcT1FrameFormat  OBJECT-TYPE
        SYNTAX  INTEGER{
                        t1esf(0),
                        t1d44mf(1),
                        t1d412mf(2)
                        }
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
        "The T1 frame format."
    ::= { hwMusaFrcPhyPortLineIfEntry 4 }

    hwMusaFrcT1LineCode  OBJECT-TYPE
        SYNTAX  INTEGER{
                        b8zs(0),
                        ami(1)
                        }
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
        "The T1 line code."
    ::= { hwMusaFrcPhyPortLineIfEntry 5 }
    
    hwMusaFrcV35ClockMode  OBJECT-TYPE
        SYNTAX  INTEGER{
                        dceinner(0),
                        dceslave(1),
                        dceouter(2),
                        dteinner(3),
                        dteslave(4),
                        dteouter(5)                        
                        }
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
        "The V35 clock mode."
    ::= { hwMusaFrcPhyPortLineIfEntry 6 }

    hwMusaFrcT1E1TsBitMap  OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
        "The TS bit map."
    ::= { hwMusaFrcPhyPortLineIfEntry 7 }
    
        
    hwMusaFrcClockLoopMode  OBJECT-TYPE
        SYNTAX  INTEGER{
                        payloop(0),
                        remoteloop(1),
                        single-timeslot(2)
                        }
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
        "The phy port Clock loop mode."
    ::= { hwMusaFrcPhyPortLineIfEntry 8 }       
    
        hwMusaFrcLoopState  OBJECT-TYPE
        SYNTAX  INTEGER{
                        noloop(0),
                        loop(1)
                        }
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
        "The phy port Clock loop state."
    ::= { hwMusaFrcPhyPortLineIfEntry 9 } 

        hwMusaFrcV35InvertTxClock  OBJECT-TYPE
        SYNTAX  INTEGER{
                        off(0),
                        on(1)
                        }
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
        "The V35 port Clock invert phase."
    ::= { hwMusaFrcPhyPortLineIfEntry 10 } 
    
        hwMusaFrcSingleTimeSlotId  OBJECT-TYPE
        SYNTAX  INTEGER(1..31)
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
        "The timeslot id of single timeslot loopback."
    ::= { hwMusaFrcPhyPortLineIfEntry 11 }
    
    -- ******************************************************
    -- FRC PHY port alarm table
    
    hwMusaFrcPhyPortAlarmTable    OBJECT-TYPE
        SYNTAX      SEQUENCE OF HwMusaFrcPhyPortAlarmEntry
        ACCESS      not-accessible
        STATUS      mandatory
    DESCRIPTION
    "Musa Frc phy port alarm table."
    ::= { hwMusaFrcMib 3 }

   hwMusaFrcPhyPortAlarmEntry    OBJECT-TYPE
        SYNTAX         HwMusaFrcPhyPortAlarmEntry
        ACCESS         not-accessible
        STATUS         mandatory
    DESCRIPTION
    "This list contains Musa Frc phy port Alarm variables."
    INDEX { hwMusaFrameIndex,
            hwMusaSlotIndex ,
            hwMusaFrcPhyPortId   }
    ::= { hwMusaFrcPhyPortAlarmTable  1 }
 
   HwMusaFrcPhyPortAlarmEntry    ::= SEQUENCE  {
            hwMusaFrcAlarmSw                 INTEGER,
            hwMusaFrcRACTS        INTEGER,
            hwMusaFrcLFADSR          INTEGER,
            hwMusaFrcLOSPLL               INTEGER,
            hwMusaFrcAISDCD              INTEGER,
            hwMusaFrcAlarmCount              INTEGER,
            hwMusaFrcDTR             INTEGER,
            hwMusaFrcRTS             INTEGER, 
            hwMusaFrcSCOL            INTEGER 
            } 
            
    hwMusaFrcAlarmSw  OBJECT-TYPE
        SYNTAX  INTEGER{
                        disable(0),
                        enable(1)
                        }
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
        "The phy port alarm switch."
    ::= { hwMusaFrcPhyPortAlarmEntry 1 }                    
            
    hwMusaFrcRACTS  OBJECT-TYPE
        SYNTAX  INTEGER{
                        noalarmup(0),
                        alarmdown(1)   
                        }
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
        "The E1/T1 phy port remote alarm or V35 phy port CTS alarm."
    ::= { hwMusaFrcPhyPortAlarmEntry 2 }  
        
    hwMusaFrcLFADSR  OBJECT-TYPE
        SYNTAX  INTEGER{
                        noalarmup(0),
                        alarmdown(1)   
                        }
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
        "The E1/T1 phy port LFA or V35 phy port DSR alarm."
    ::= { hwMusaFrcPhyPortAlarmEntry 3 }
    
    hwMusaFrcLOSPLL  OBJECT-TYPE
        SYNTAX  INTEGER{
                        noalarmup(0),
                        alarmdown(1)   
                        }
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
        "The E1/T1 phy port LOS alarm or V35 phy port PLL alarm."
    ::= { hwMusaFrcPhyPortAlarmEntry 4 }
    
    hwMusaFrcAISDCD  OBJECT-TYPE
        SYNTAX  INTEGER{
                        noalarmup(0),
                        alarmdown(1)                        
                        }
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
        "The E1/T1 phy port AIS alarm OR V35 phy port DCD alarm."
    ::= { hwMusaFrcPhyPortAlarmEntry 5 }    
    
    hwMusaFrcAlarmCount  OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
        "The phy port alarm count."
    ::= { hwMusaFrcPhyPortAlarmEntry 6 } 
    
    hwMusaFrcDTR       OBJECT-TYPE       
        SYNTAX  INTEGER{
                        up(0),
                        down(1)   
                        }
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
        "The V35 phy port DTR alarm."
    ::= { hwMusaFrcPhyPortAlarmEntry 7 } 
    
    hwMusaFrcRTS       OBJECT-TYPE       
        SYNTAX  INTEGER{
                        up(0),
                        down(1)   
                        }
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
        "The V35 phy port RTS alarm."
    ::= { hwMusaFrcPhyPortAlarmEntry 8 }     
   
    hwMusaFrcSCOL       OBJECT-TYPE       
        SYNTAX  INTEGER{
                        noalarm(0),
                        alarm(1)   
                        }
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
        "The phy port SCOL alarm."
    ::= { hwMusaFrcPhyPortAlarmEntry 9 }         
       

