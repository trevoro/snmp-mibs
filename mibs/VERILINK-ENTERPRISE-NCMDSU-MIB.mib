        DESCRIPTION
               "An entry in the ncm dsu stat table."
        INDEX   { ncmdsuNIDstatIndex, ncmdsustatIndex }
        ::= { ncmdsustatTable 1 }


        NcmdsustatEntry ::= SEQUENCE {

	     ncmdsuNIDstatIndex
		 INTEGER,
             ncmdsustatIndex
                 INTEGER,
             ncmdsuPortLowbattery
                 INTEGER,
             ncmdsuPortLOSstatus
                 INTEGER,
             ncmdsuPortblockerrorcounter
                 Gauge,
             ncmdsuPortTestSeconds
                 Gauge,
             ncmdsuPortrateper8000
                 INTEGER,
             ncmdsuPortLeadStatus1
                 INTEGER,
             ncmdsuPortLeadStatus2
                 INTEGER,
             ncmdbuPrimaryLineStatus
                INTEGER,
             ncmdbuLineStatus
                INTEGER,
             ncmdbuSwitchBy
                INTEGER,
             ncmdbuReceivedCode
                INTEGER,
             ncmdsuPortTestResults
                INTEGER,
             ncmdsuPortDiagStatus
                INTEGER
		}



	ncmdsuNIDstatIndex OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
	DESCRIPTION
		"This object specifies the node id of the node."
         ::= { ncmdsustatEntry 1 }


	ncmdsustatIndex OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
 
         ::= { ncmdsustatEntry 2 }
 
 
        ncmdsuPortLowbattery OBJECT-TYPE
        SYNTAX INTEGER {
                        not-low-battery(1),
                        low-battery(2)
                
               }
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
               "The ncmdsu Lowbattery status for Port."
        ::= { ncmdsustatEntry 3 }
 

	ncmdsuPortLOSstatus OBJECT-TYPE
        SYNTAX INTEGER {
 
                       not-Los-status(1),
                       los-status(2)
                
               }
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
               "The ncmdsu LOS status for Port."
        ::= { ncmdsustatEntry 4 }
 
         
 
        ncmdsuPortblockerrorcounter OBJECT-TYPE
        SYNTAX Gauge
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
 
        "The number of block errors produced on port."
 
        ::= {ncmdsustatEntry 5 }
 
 
        ncmdsuPortTestSeconds OBJECT-TYPE
        SYNTAX Gauge
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
 
        "The number of errors produced by a test on port."
 
        ::= {ncmdsustatEntry 6 }
        
        ncmdsuPortrateper8000 OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
 
        "The rate/8000 on port."
 
        ::= {ncmdsustatEntry 7 }


	ncmdsuPortLeadStatus1 OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
 
        "This variable indicates the Lead Status (part 1)
         of the port.
 
        The various bit positions are:
        Bit     Value   Name
        0       1       DTR (1 = on; 0 = off)
        1       2       RTS
        2       4       DSR
        3       8       CTS
        4       16      DCD
        5       32      TXD
        6       64      RXD
        7       128     Lead status available bit."
         
          ::= { ncmdsustatEntry 8 }
 
        ncmdsuPortLeadStatus2 OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
 
        "This variable indicates the Lead Status (part 2)
         of the port.The lead status2 is bit mapped.

	The various bit positions are:
        BIT     Value   Name
        0       0       no-loop
        0       1       local-loop
        1       2       remote-loop
        2       4       test-mode."
 
 
         ::= { ncmdsustatEntry 9 }
         
	ncmdbuPrimaryLineStatus OBJECT-TYPE
         SYNTAX  INTEGER
                 {
                oK(1),
                aSC(2),
                lOF(3),
                cD(4),
                uSER(5),
                yEL(6),
                bER(7),
                aLLONE(8),
                lOS(9)
 
                 
                        }
                
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
 
        "This variable indicates the Primary Line Status of the ncmdbu. "
        
        
         ::= { ncmdsustatEntry 10 }
 
 
         ncmdbuLineStatus OBJECT-TYPE
         SYNTAX  INTEGER
                 {
                sTANDBY(1),
                aCTIVE(2),
                eRROR(3)
                
                        }
                
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
 
        "This variable indicates the ncmdbu Line Status of the ncmdbu. "
        
        
         ::= { ncmdsustatEntry 11 }
 
 
 
 
        ncmdbuSwitchBy OBJECT-TYPE
         SYNTAX  INTEGER
                 {
                nONE(1),
                aSC(2),
                lOF(3),
                cD(4),
                uSER(5),
                yEL(6),
                bER(7),
                aLLONE(8),
                lOS(9)
 
                 
                        }
	ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
 
        "This variable indicates the cause of the ncmdbu switching. "
        
        
         ::= { ncmdsustatEntry 12 }
 
 
        ncmdbuReceivedCode OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
 
        "This variable indicates the ncmdbu  received code in hex. "
        
        
         ::= { ncmdsustatEntry 13 }
 
 
 
        ncmdsuPortTestResults OBJECT-TYPE
          SYNTAX INTEGER
          ACCESS read-only
          STATUS mandatory
          DESCRIPTION
               "The number of errors produced by a test on port ."
          ::= { ncmdsustatEntry 14  }
 
 
        ncmdsuPortDiagStatus OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
 
        "This variable indicates the Diagnostic Status
         of the port.
 
        The various bit positions are:
        Bit     Value   Name
        0       1       Reserved
        1       2       Reserved
        2       4       Reserved
        3       8       Sending Test Code
        4       16      Sending loopdown code
        5       32      Sending loopup code
        6       64      far end looped
        7       128     Near end looped."
         
          ::= { ncmdsustatEntry 15 }



