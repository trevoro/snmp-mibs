        SYNTAX INTEGER
                        {
                        mode-56k(1),
                        mode-64k(2),
                        mode-56kncmdds(3)
                        }
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
 
        "This variable specifies the mode of ncmdds."
 
        ::= {ncmddsConfigEntry 15 }

