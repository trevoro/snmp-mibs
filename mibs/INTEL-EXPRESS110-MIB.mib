rptrPortRunts   OBJECT-TYPE
    SYNTAX    Counter
    ACCESS    read-only
    STATUS    mandatory
    DESCRIPTION
          "The number of frames detected that are less than the minimum
           permitted frame size of 64 octets but have a good FCS. Runts
           usually indicate collision fragments, a normal network event. 

           (RFC2108 rptrMonitorPortRunts) "
    ::= { rptrPortStatEntry    10 }

rptrPortCollisions   OBJECT-TYPE
    SYNTAX    Counter
    ACCESS    read-only
    STATUS    mandatory
    DESCRIPTION
          "The number of collisions detected on a port.

           (RFC2108 rptrMonitorPortCollisions) "
    ::= { rptrPortStatEntry    11 }

rptrPortLateEvents   OBJECT-TYPE
    SYNTAX    Counter
    ACCESS    read-only
    STATUS    mandatory
    DESCRIPTION
          "The number of collisions detected after the allowable detection 
           period. 
           Usually indicates a network topology problem, such as daisy-chaining
           Class I 100Mbps hubs with UTP cable or a network diameter that is
           too wide.

           (RFC2108 rptrMonitorPortLateEvents) "
    ::= { rptrPortStatEntry    12 }

rptrPortVeryLongEvents   OBJECT-TYPE
    SYNTAX    Counter
    ACCESS    read-only
    STATUS    mandatory
    DESCRIPTION
          "The number of times MAU jabber lockup protection (MJLP) was detected
           due to transmission of data that exceeded 5 msec in duration 
           (octet count greater than maxFramSize). Can indicate a faulty device
           or port. 

           (RFC2108 rptrMonitorPortVeryLongEvents) "
    ::= { rptrPortStatEntry    13 }

rptrPortDataRateMismatches   OBJECT-TYPE
    SYNTAX    Counter
    ACCESS    read-only
    STATUS    mandatory
    DESCRIPTION
          "A count of the occurrences of out-of-specification bit rates. 
           This indicates the number of times the FIFO buffer overruns or 
           underruns due to transmission rate errors. This could indicate 
           an incorrect FIFO setting on a network adapter or a faulty adapter. 

           (RFC2108 rptrMonitorPortDataRateMismatches) "
    ::= { rptrPortStatEntry    14 }

rptrPortAutoPartitions   OBJECT-TYPE
    SYNTAX    Counter
    ACCESS    read-only
    STATUS    mandatory
    DESCRIPTION
          "The number of times this port was automatically partitioned. This
           condition occurs when 64 consecutive collisions are detected on the 
           port. These collisions are due to excessive traffic on the segment, 
           a malfunctioning port, or a malfunctioning adapter that is jabbering. 

           (RFC2108 rptrMonitorPortAutoPartitions) "
    ::= { rptrPortStatEntry    15 }

rptrPortTotalErrors   OBJECT-TYPE
    SYNTAX    Counter
    ACCESS    read-only
    STATUS    mandatory
    DESCRIPTION
          "The total number of errors detected. Total errors include FCS errors,
           alignment errors, frame too long, short events, late events, very 
           long event, rate mismatch, and symbol errors.

           (RFC2108 rptrMonitorPortTotalErrors) "
    ::= { rptrPortStatEntry    16 }

rptrPortLastMACAddress   OBJECT-TYPE
    SYNTAX    DisplayString
    ACCESS    read-only
    STATUS    mandatory
    DESCRIPTION
          "The source address of the last readable frame received
           by this port. If no frames have been received on this
           port, the length of the returned DisplayString is zero.

           (RFC2108 rptrAddrTrackNewLastSrcAddress) "
    ::= { rptrPortStatEntry    17 }

rptrPortNumberofMACAddressChanges   OBJECT-TYPE
    SYNTAX    Counter
    ACCESS    read-only
    STATUS    mandatory
    DESCRIPTION
          "The number of times that the value of rptrPortLastMACAddress has 
           changed.
           The typical value for this port is 1, meaning the value of
           rptrPortLastMACAddress changed from all zeros to a MAC address.
           A high number of changes indicates a port connected to a switch or 
           bridge.

           (RFC2108 rptrAddrTrackSourceAddrChanges) "
    ::= { rptrPortStatEntry    18 }

rptrPortSymbolErrors   OBJECT-TYPE
    SYNTAX    Counter
    ACCESS    read-only
    STATUS    mandatory
    DESCRIPTION
          "This counter is incremented by one each time a
           valid length packet was received at the port and
           there was at least one occurrence of an invalid
           data symbol. This can increment only once per valid
           carrier event. A collision presence at any port of
           the repeater containing port N will not cause this
           attribute to increment. "
    ::= { rptrPortStatEntry    19 }

rptrChassisSegmentStatTable   OBJECT-TYPE
    SYNTAX    SEQUENCE OF RptrChassisSegmentStatEntry
    ACCESS    not-accessible
    STATUS    mandatory
    DESCRIPTION
          "Repeater status information by segment at the chassis level. "
    ::= { express110    15 }

rptrChassisSegmentStatEntry   OBJECT-TYPE
    SYNTAX    RptrChassisSegmentStatEntry
    ACCESS    not-accessible
    STATUS    mandatory
    DESCRIPTION
          "A table entry, containing repeater status information
           about a single chassis and segment. "
    INDEX    {  rptrChassisIndex, rptrChassisSegmentIndex  }
        ::= { rptrChassisSegmentStatTable    1 }

RptrChassisSegmentStatEntry ::=
    SEQUENCE {
        rptrChassisIndex
            INTEGER,
        rptrChassisSegmentIndex
            INTEGER,
        rptrChassisSegmentTotalFrames
            Counter,
        rptrChassisSegmentTotalOctets
            Counter,
        rptrChassisSegmentTotalErrors
            Counter,
        rptrChassisSegmentFCSErrors
            Counter,
        rptrChassisSegmentAlignmentErrors
            Counter,
        rptrChassisSegmentFrameTooLongs
            Counter,
        rptrChassisSegmentShortEvents
            Counter,
        rptrChassisSegmentRunts
            Counter,
        rptrChassisSegmentCollisions
            Counter,
        rptrChassisSegmentLateEvents
            Counter,
        rptrChassisSegmentVeryLongEvents
            Counter,
        rptrChassisSegmentDataRateMismatches
            Counter,
        rptrChassisSegmentAutoPartitions
            Counter,
        rptrChassisSegmentSymbolErrors
            Counter
    }
    
rptrChassisIndex   OBJECT-TYPE
    SYNTAX    INTEGER
    ACCESS    read-only
    STATUS    mandatory
    DESCRIPTION
          "This index is the chassis number in the stack.
           The chassis at the top of the stack is number 1.  "
    ::= { rptrChassisSegmentStatEntry    1 }

rptrChassisSegmentIndex   OBJECT-TYPE
    SYNTAX    INTEGER
    ACCESS    read-only
    STATUS    mandatory
    DESCRIPTION
          "This index is the segment number in the stack."
    ::= { rptrChassisSegmentStatEntry    2 }

rptrChassisSegmentTotalFrames   OBJECT-TYPE
    SYNTAX    Counter
    ACCESS    read-only
    STATUS    mandatory
    DESCRIPTION
          "The number of frames detected without errors. Includes unicast, 
           broadcast, and multicast frames. Does not include frames received 
           with frames too long, runt, FCS, or alignment errors. This is the 
           sum of all valid received frames for all ports in this repeater 
           module.

           (RFC2108 rptrMonitorGroupTotalFrames) "
    ::= { rptrChassisSegmentStatEntry    3 }

rptrChassisSegmentTotalOctets   OBJECT-TYPE
    SYNTAX    Counter
    ACCESS    read-only
    STATUS    mandatory
    DESCRIPTION
          "The number of octets (bytes) contained in frames received that had no
           errors. This includes octets in unicast, broadcast, and multicast 
           frames. 
           It also includes octets after the start frame delimiter up to FCS 
           but not including FCS octets. This is the sum of all valid received
           octets for all ports in this repeater module.

           (RFC2108 rptrMonitorGroupTotalOctets) "
    ::= { rptrChassisSegmentStatEntry    4 }

rptrChassisSegmentTotalErrors   OBJECT-TYPE
    SYNTAX    Counter
    ACCESS    read-only
    STATUS    mandatory
    DESCRIPTION
          "The total number of errors detected. Total errors include FCS errors,
           alignment errors, frame too long, short events, late events, very 
           long event, and rate mismatch errors. This is the sum of all errors
           for all ports in this repeater module.

           (RFC2108 rptrMonitorGroupTotalErrors) "
    ::= { rptrChassisSegmentStatEntry    5 }

rptrChassisSegmentFCSErrors   OBJECT-TYPE
    SYNTAX    Counter
    ACCESS    read-only
    STATUS    mandatory
    DESCRIPTION
          "The number of frames detected that are free of partial octets 
           and do not pass the FCS check. Usually caused by adapter underrun 
           (when the adapter can't get enough bus bandwidth). FCS errors do 
           not necessarily indicate that data has been lost.  "
    ::= { rptrChassisSegmentStatEntry    6 }

rptrChassisSegmentAlignmentErrors   OBJECT-TYPE
    SYNTAX    Counter
    ACCESS    read-only
    STATUS    mandatory
    DESCRIPTION
          "The number of frames detected that contain partial octets and do not 
           pass the FCS check.  "
    ::= { rptrChassisSegmentStatEntry    7 }

rptrChassisSegmentFrameTooLongs   OBJECT-TYPE
    SYNTAX    Counter
    ACCESS    read-only
    STATUS    mandatory
    DESCRIPTION
          "The number of frames detected that exceed the maximum permitted 
           frame size. "
    ::= { rptrChassisSegmentStatEntry    8 }

rptrChassisSegmentShortEvents   OBJECT-TYPE
    SYNTAX    Counter
    ACCESS    read-only
    STATUS    mandatory
    DESCRIPTION
          "The number of fragments detected with ActivityDuration less than
           ShortEventMaxTime (greater than 74 bit times and less than 82 bit
           times). Usually indicates a network topology problem. "
    ::= { rptrChassisSegmentStatEntry    9 }

rptrChassisSegmentRunts   OBJECT-TYPE
    SYNTAX    Counter
    ACCESS    read-only
    STATUS    mandatory
    DESCRIPTION
          "The number of frames detected that are less than the minimum
           permitted frame size of 64 octets but have a good FCS. Runts usually
           indicate collision fragments, a normal network event. "
    ::= { rptrChassisSegmentStatEntry    10 }

rptrChassisSegmentCollisions   OBJECT-TYPE
    SYNTAX    Counter
    ACCESS    read-only
    STATUS    mandatory
    DESCRIPTION
          "Sum of all collisions for all ports in this repeater module.  "
    ::= { rptrChassisSegmentStatEntry    11 }

rptrChassisSegmentLateEvents   OBJECT-TYPE
    SYNTAX    Counter
    ACCESS    read-only
    STATUS    mandatory
    DESCRIPTION
          "The number of collisions detected after the allowable detection 
           period. 
           Usually indicates a network topology problem, such as daisy-chaining
           class I 100Mbps hubs with UTP cable or a network diameter that is 
           too wide. "
    ::= { rptrChassisSegmentStatEntry    12 }

rptrChassisSegmentVeryLongEvents   OBJECT-TYPE
    SYNTAX    Counter
    ACCESS    read-only
    STATUS    mandatory
    DESCRIPTION
          "The number of times MAU jabber lockup protection (MJLP) was detected
           due to transmission of data that exceeded 5 msec in duration 
           (octet count greater than maxFrameSize). Can indicate a faulty 
           device or port. "
    ::= { rptrChassisSegmentStatEntry    13 }

rptrChassisSegmentDataRateMismatches   OBJECT-TYPE
    SYNTAX    Counter
    ACCESS    read-only
    STATUS    mandatory
    DESCRIPTION
          "A count of the occurrences of out-of-specification bit rates. 
           This indicates the number of times the FIFO buffer overruns or 
           underruns due to transmission rate errors. This could indicate 
           an incorrect FIFO setting on a network adapter or a faulty adapter. "
    ::= { rptrChassisSegmentStatEntry    14 }

rptrChassisSegmentAutoPartitions   OBJECT-TYPE
    SYNTAX    Counter
    ACCESS    read-only
    STATUS    mandatory
    DESCRIPTION
          "The number of times this port was automatically partitioned. This 
           condition occurs when too many consecutive collisions are detected
           on the port. These collisions are due to excessive traffic on the 
           segment, a malfunctioning port, or a malfunctioning adapter that
           is jabbering. "
    ::= { rptrChassisSegmentStatEntry    15 }

rptrChassisSegmentSymbolErrors   OBJECT-TYPE
    SYNTAX    Counter
    ACCESS    read-only
    STATUS    mandatory
    DESCRIPTION
          "This counter is incremented by one when a valid
           length packet was received at the port and
           there was at least one occurrence of an invalid
           data symbol. This can increment only once per valid
           carrier event. A collision presence at any port of
           the repeater containing port N will not cause this
           attribute to increment. "
    ::= { rptrChassisSegmentStatEntry    16 }

tBrdgAdminTable   OBJECT-TYPE
    SYNTAX    SEQUENCE OF TBrdgAdminEntry
    ACCESS    not-accessible
    STATUS    mandatory
    DESCRIPTION
          "Embedded transparent bridge administration."
    ::= { express110    16 }

tBrdgAdminEntry   OBJECT-TYPE
    SYNTAX    TBrdgAdminEntry
    ACCESS    not-accessible
    STATUS    mandatory
    DESCRIPTION
          "A table entry, containing administration information 
           for a single embedded transparent bridge span."
    INDEX    { tBrdgAdminSpanIndex }
    ::= { tBrdgAdminTable    1 }

TBrdgAdminEntry ::=
    SEQUENCE {
        tBrdgAdminSpanIndex 
            INTEGER,
        tBrdgAdminState 
            INTEGER,
        tBrdgReset
            INTEGER,
        tBrdgThrshld
            INTEGER,
        tBrdgPeriod
            INTEGER,
        tBrdgLockout
            INTEGER,
        tBrdgHubId
            INTEGER,
        tBrdgSegments
            OCTET STRING
    }
    
tBrdgAdminSpanIndex   OBJECT-TYPE
    SYNTAX    INTEGER
    ACCESS    read-only
    STATUS    mandatory
    DESCRIPTION
          "Embedded transparent bridge span number.  
           A bridge span consists of one or more embedded transparent bridges
           forwarding packets between two segments."
    ::= { tBrdgAdminEntry    1 }

tBrdgAdminState OBJECT-TYPE
    SYNTAX    INTEGER  {
                enable(1),
                disable(2)
                }
    ACCESS    read-write
    STATUS    mandatory
    DESCRIPTION
          "The state of the embedded transparent bridging span.
           enable  - Allows packets to be forwarded by embedded bridging
                     to the peer segment.
           disable - Prevents packets from being forwarded by embedded 
                     bridging to the peer segment."
    ::= { tBrdgAdminEntry 2 }

tBrdgReset OBJECT-TYPE
    SYNTAX    INTEGER  {
                reset(1),
                resetCounters(2),
                notreset(3),
                notSupported(4)
                }
    ACCESS    read-write
    STATUS    mandatory
    DESCRIPTION
          "'reset' resets all enabled transparent bridges in the span.
           'resetCounters' clears all transparent bridge statistics."
    ::= { tBrdgAdminEntry 3 }

tBrdgThrshld OBJECT-TYPE
    SYNTAX    INTEGER 
    ACCESS    read-write
    STATUS    mandatory
    DESCRIPTION
          "If the number of times the transparent bridge indicates buffer full
           exceeds this value, a trap is generated.  Use 0 to disable.  
           This trap is based on the number of tBrdgBufferFullCount in the last 
           tBrdgPeriod.  "
    ::= { tBrdgAdminEntry 4 }

tBrdgPeriod OBJECT-TYPE
    SYNTAX    INTEGER 
    ACCESS    read-write
    STATUS    mandatory
    DESCRIPTION
          "Time period (in minutes) that the transparent bridge buffer full
           count must exceed the trap threshold value in order to generate 
           a trap."
    ::= { tBrdgAdminEntry 5 }

tBrdgLockout   OBJECT-TYPE
    SYNTAX    INTEGER  {
                        yButtonyMgmt(1),
                        nButtonyMgmt(2),
                        yButtonnMgmt(3),
                        nButtonnMgmt(4)
                       }
    ACCESS    read-write
    STATUS    mandatory
    DESCRIPTION
          "Prevents embedded transparent bridging from being enabled or
           disabled via a button on the front of a hub or by the local
           management console interface."
    ::= { tBrdgAdminEntry 6 }

tBrdgHubId   OBJECT-TYPE
    SYNTAX    INTEGER 
    ACCESS    read-only
    STATUS    mandatory
    DESCRIPTION
          "chassisConfigTableIndex of the chassis containing the primary
           internal bridge for the span.  
           All internal bridge statistics are reported as ports 101 and 102 
           within this hub chassis."
    ::= { tBrdgAdminEntry 7 }

tBrdgSegments   OBJECT-TYPE
    SYNTAX    OCTET STRING
    ACCESS    read-only
    STATUS    mandatory
    DESCRIPTION
          "Embedded transparent bridge span segments.
           This is a list of segments (1 per byte) in this bridge span."
    ::= { tBrdgAdminEntry 8 }

tBrdgStatsTable   OBJECT-TYPE
    SYNTAX    SEQUENCE OF TBrdgStatsEntry
    ACCESS    not-accessible
    STATUS    mandatory
    DESCRIPTION
          "Embedded transparent bridge statistics."
    ::= { express110    17 }

tBrdgStatsEntry   OBJECT-TYPE
    SYNTAX    TBrdgStatsEntry
    ACCESS    not-accessible
    STATUS    mandatory
    DESCRIPTION
          "A table entry, containing statistics for a single
           embedded transparent bridge span domain."
    INDEX    { tBrdgStatsSpan, tBrdgStatsSegment }
    ::= { tBrdgStatsTable    1 }

TBrdgStatsEntry ::=
    SEQUENCE {
        tBrdgStatsSpan
            INTEGER,
        tBrdgStatsSegment
            INTEGER,
        tBrdgUtilIn
            INTEGER,
        tBrdgUtilOut
            INTEGER,
        tBrdgBufferFullCount
            INTEGER
    }
    
tBrdgStatsSpan   OBJECT-TYPE
    SYNTAX    INTEGER
    ACCESS    read-only
    STATUS    mandatory
    DESCRIPTION
          "Embedded transparent bridge span number.  
           A bridge span consists of one or more embedded transparent bridges
           forwarding packets between two segments."
    ::= { tBrdgStatsEntry 1 }

tBrdgStatsSegment   OBJECT-TYPE
    SYNTAX    INTEGER
    ACCESS    read-only
    STATUS    mandatory
    DESCRIPTION
          "Embedded transparent bridge statistics segment.
           This index specifies which segment is associated with the
           transparent bridge statistics."
    ::= { tBrdgStatsEntry 2 }

tBrdgUtilIn OBJECT-TYPE
    SYNTAX    INTEGER 
    ACCESS    read-only
    STATUS    mandatory
    DESCRIPTION
          "The percentage of this segment's traffic that was bridged from 
           the peer segment during the previous second."
    ::= { tBrdgStatsEntry 3 }

tBrdgUtilOut OBJECT-TYPE
    SYNTAX    INTEGER 
    ACCESS    read-only
    STATUS    mandatory
    DESCRIPTION
          "The percentage of this segment's traffic which was bridged to 
           the peer segment during the previous second."
    ::= { tBrdgStatsEntry 4 }

tBrdgBufferFullCount OBJECT-TYPE
    SYNTAX    INTEGER 
    ACCESS    read-only
    STATUS    mandatory
    DESCRIPTION
          "The number of times internal buffer capacity was exceeded
           while bridging frames out of this segment."
    ::= { tBrdgStatsEntry 5 }

