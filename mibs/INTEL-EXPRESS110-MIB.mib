    DESCRIPTION
          "Sets the IP address for a trap receiving station.  "
    ::= { e300snmpTrapAddressEntry    2 }

e300trapCommunityString   OBJECT-TYPE
    SYNTAX    DisplayString (SIZE (0..32))
    ACCESS    read-write
    STATUS    mandatory
    DESCRIPTION
          "Sets the password for sending traps from the SNMP agent
           to the SNMP application on the trap receiving station. "
    ::= { e300snmpTrapAddressEntry    3 }

e300trapStatus   OBJECT-TYPE
    SYNTAX    INTEGER  {
                active(1),
                ignore(2)
                }
    ACCESS    read-write
    STATUS    mandatory
    DESCRIPTION
          "Displays whether traps should be sent to this station (active)
           or whether traps should not be sent to this station (ignore). "
    ::= { e300snmpTrapAddressEntry    4 }


e300agentExtConfiguration        OBJECT IDENTIFIER ::= { express-snmp-agents 4 }

e300agentSwUpdateMode OBJECT-TYPE
    SYNTAX  INTEGER {
            other(1),
            network-load(2),
            out-of-band-load(3)
    }
    ACCESS  read-write
    STATUS
    mandatory
    DESCRIPTION
    "The download media used by the system to download the runtime firmware."
    ::= { e300agentExtConfiguration 1 }

e300agentConfigUpdateCtrl OBJECT-TYPE
    SYNTAX  INTEGER {
            enable(1),
            disable(2)
            }
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
    "Current status of configuration file download control. The setting
    is effective the next time you reset or power on the hub."
    ::= { e300agentExtConfiguration 2 }

e300agentConfigFilename OBJECT-TYPE
    SYNTAX  DisplayString  (SIZE (0..64))
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
    "The name of the configuration file to be downloaded from the BootP or TFTP
    server when agentConfigUpdateCtrl is enabled."
    ::= { e300agentExtConfiguration 3 }

e300agentImageUpdateCtrl OBJECT-TYPE
    SYNTAX  INTEGER {
            enable(1),
            disable(2)
            }
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
    "Current status of firmware image download control. The setting
    is effective the next time you reset or power on the hub."
    ::= { e300agentExtConfiguration 4 }

e300agentImageFilename OBJECT-TYPE
    SYNTAX  DisplayString  (SIZE (0..64))
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
    "The name of the image file to be downloaded from the TFTP server when
    agentImageUpdateCtrl is enabled."
    ::= { e300agentExtConfiguration 5 }


e300agentRs232PortConfig OBJECT-TYPE
    SYNTAX  INTEGER {
            other(1),
            console(2),
            out-of-band(3)
    }
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
    "The current setting of RS-232C port.
    The RS-232C port can be configured as the console port or
        used for out-of-band connection."
    ::= { e300agentExtConfiguration 6 }

e300agentOutOfBandBaudRateConfig OBJECT-TYPE
    SYNTAX  INTEGER {
            other(1),
            baudRate-1200 (2),
            baudRate-2400 (3),
            baudRate-9600 (4),
            baudRate-19200(5)
    }
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
    "The baud rate setting for out of band connection.
    The baud rate for the console interface is always 9600."
    ::= { e300agentExtConfiguration 7 }


e300slipConfiguration    OBJECT IDENTIFIER ::= { express-snmp-agents 5 }

e300slipAddressInUse   OBJECT-TYPE
    SYNTAX    IpAddress
    ACCESS    read-only
    STATUS    mandatory
    DESCRIPTION
          "Displays the IP address currently in use.  "
    ::= { e300slipConfiguration    1 }

e300slipGatewayorRouterAddrInUse   OBJECT-TYPE
    SYNTAX    IpAddress
    ACCESS    read-only
    STATUS    mandatory
    DESCRIPTION
          "Displays the default gateway address currently in use. 
           This is the device that routes to a different network. 
           Usually a router or a routing server.  "
    ::= { e300slipConfiguration    2 }

e300slipNetworkMaskInUse   OBJECT-TYPE
    SYNTAX    IpAddress
    ACCESS    read-only
    STATUS    mandatory
    DESCRIPTION
          "Displays the network (subnet) mask currently in use. This
           must match the masks of other devices on the same network.  "
    ::= { e300slipConfiguration    3 }

e300slipBroadcastAddressInUse   OBJECT-TYPE
    SYNTAX    IpAddress
    ACCESS    read-only
    STATUS    mandatory
    DESCRIPTION
          "Displays the broadcast address currently in use. "
    ::= { e300slipConfiguration    4 }

e300slipAddressNextReset   OBJECT-TYPE
    SYNTAX    IpAddress
    ACCESS    read-write
    STATUS    mandatory
    DESCRIPTION
          "Displays the IP address the SNMP agent will use the next
           time it is reset.  "
    ::= { e300slipConfiguration    5 }

e300slipGatewayorRouterAddrNextReset   OBJECT-TYPE
    SYNTAX    IpAddress
    ACCESS    read-write
    STATUS    mandatory
    DESCRIPTION
          "Displays the default gateway the SNMP agent will use the next
           time it is reset. "
    ::= { e300slipConfiguration    6 }

e300slipNetworkMaskNextReset   OBJECT-TYPE
    SYNTAX    IpAddress
    ACCESS    read-write
    STATUS    mandatory
    DESCRIPTION
          "Displays the netmask (subnet mask) the SNMP agent will use the next 
           time it is reset. "
    ::= { e300slipConfiguration    7 }

e300mgmtBasicInfoTable   OBJECT-TYPE
    SYNTAX    SEQUENCE OF E300mgmtBasicInfoEntry
    ACCESS    not-accessible
    STATUS    mandatory
    DESCRIPTION
          "A list of SNMP agent basic information."
    ::= { express-snmp-agents 6 }

e300mgmtBasicInfoEntry   OBJECT-TYPE
    SYNTAX    E300mgmtBasicInfoEntry
    ACCESS    not-accessible
    STATUS    mandatory
    DESCRIPTION
          "A table entry, containing SNMP agent basic information."
    INDEX    {  e300mgmtChassisIndex  }
    ::= { e300mgmtBasicInfoTable    1 }

E300mgmtBasicInfoEntry ::=
    SEQUENCE {
        e300mgmtChassisIndex
            INTEGER,
        e300mgmtIPAddress
            IpAddress,
        e300mgmtPhysicalAddress
            PhysAddress,
        e300mgmtRole   
            INTEGER
    }
    
e300mgmtChassisIndex   OBJECT-TYPE
    SYNTAX    INTEGER
    ACCESS    read-only
    STATUS    mandatory
    DESCRIPTION
          "Chassis number in the stack which SNMP agent located."
    ::= { e300mgmtBasicInfoEntry    1 }

e300mgmtIPAddress   OBJECT-TYPE
    SYNTAX    IpAddress
    ACCESS    read-only
    STATUS    mandatory
    DESCRIPTION
          "IP address of the SNMP agent."
    ::= { e300mgmtBasicInfoEntry    2 }

e300mgmtPhysicalAddress   OBJECT-TYPE
    SYNTAX    PhysAddress
    ACCESS    read-only
    STATUS    mandatory
    DESCRIPTION
          "Physical address of the SNMP agent."
    ::= { e300mgmtBasicInfoEntry    3 }

e300mgmtRole   OBJECT-TYPE
    SYNTAX  INTEGER {
            other(1),
            primaryAgent(2),
            backupAgent(3)
                        }
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
    "The current role of the SNMP agent within this hub stack."
    ::= { e300mgmtBasicInfoEntry    4 }

-- Trap definitions begin

utilizationThresholdExceeded        TRAP-TYPE
ENTERPRISE        express110
VARIABLES        {  rptrSegmentStatIndex }
DESCRIPTION
          "Utilization rate in last rptrSegmentUtilizationTrapPeriod 
           exceeded rptrSegmentUtilizationTrapThreshold.  "
    ::= 0

collisionThresholdExceeded        TRAP-TYPE
ENTERPRISE        express110
VARIABLES        {  rptrSegmentStatIndex }
DESCRIPTION
          "Collisions in last rptrSegmentCollisionTrapPeriod exceeded 
           rptrSegmentCollisionTrapThreshold.  "
    ::= 1

configurationChange        TRAP-TYPE
ENTERPRISE        express110
VARIABLES        {  hubDescriptionString }
DESCRIPTION
          "A chassis was added to the stack or removed, a chassis
           lost power, a module was reset or powered on, or,
           a repeater module changed speeds.  "
    ::= 2

tBrdgBufferFull      TRAP-TYPE
ENTERPRISE        express110
DESCRIPTION
          "Buffer full rate in last tBrdgPeriod exceeded tBrdgThrshld."
    ::= 3

chassisRPSActive  TRAP-TYPE
ENTERPRISE        express110
VARIABLES        {  chassisConfigTableIndex }
DESCRIPTION
          "The power supply has failed and the optional Redundant Power
           Supply for this chassis has activated."
    ::= 4

portSecurityViolation TRAP-TYPE
ENTERPRISE        express110
VARIABLES        {  chassisConfigTableIndex, portIndex  }
DESCRIPTION
          "An ethernet frame with an unauthorized source MAC address was
           detected on this port and the port has been disabled."
    ::= 5

mgmtSwitchedToBackupMgmt TRAP-TYPE
ENTERPRISE                express110
VARIABLES        {  chassisConfigTableIndex }
DESCRIPTION
          "Primary management module failed and switched to backup module.
                   The value associated is the hub number of the active management module."
    ::= 6

bridgeConfigChangeEvent TRAP-TYPE
ENTERPRISE                express110
VARIABLES                 { tBrdgHubId }
DESCRIPTION
    "The trap is sent whenever a stack's bridge configuration is changed.
         The value associated is the hub number of the active bridge."
    ::= 7

bridgeConfigNoBridgeEvent TRAP-TYPE
ENTERPRISE                express110
DESCRIPTION
    "The trap is sent whenever a stack without a bridge warning"
        ::= 8

portlinkChangeEvent TRAP-TYPE
ENTERPRISE                express110
VARIABLES                 { portChassisIndex, portModuleIndex, portIndex, portType, portOperStatus, portLinkTestStatusTrapCtrl }
DESCRIPTION
    "The trap is sent whenever the link state of a port changes
    from link up to link down or from link down to link up"
        ::= 9

portSpeedChangeEvent TRAP-TYPE
ENTERPRISE                express110
VARIABLES                 { portChassisIndex, portModuleIndex, portIndex, portType, portLinkSpeed, portSpeedStatusTrapCtrl}
DESCRIPTION
    "The trap is sent whenever the speed status of a port changes
     from link up to link down or from link down to link up"
        ::= 10

END








