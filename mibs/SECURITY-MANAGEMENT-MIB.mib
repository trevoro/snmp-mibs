		secMngProtoId OBJECT-TYPE
			SYNTAX INTEGER
				{
				scpConfigFiles(1),
				scpImageFiles(2),
				ssh(3),
				telnet(4),
				snmpv3(5),
				http(6),
				https(7),
				telnetClient(8),
				icmpRedirection(9),
				icmp(10),
				recoveryPassword(11),
				sshClient(12),
				snmpv1(13),
