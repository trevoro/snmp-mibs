		active(1), -- Obviously Active on this interface.
		notInService(2) -- Configured on Interface but disabled.
		notReady(3), -- Interface entry NOT FULLY configured.
		createAndGo(4), -- NOT SUPPORTED.
		createAndWait(5)  
