    	SEQUENCE {
        	cjnOspfAreaId
            		AreaID,
		cjnOspfAreaRowStatus
			RowStatus,
        	cjnOspfAreaType
            		INTEGER,
        	cjnOspfAreaTranslate
            		INTEGER,
		cjnOspfAreaStubCost
			INTEGER,
        	cjnOspfAreaT3Filter
