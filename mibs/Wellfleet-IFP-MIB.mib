        FROM Wellfleet-COMMON-MIB;

    wfIfpDrvCfgTable OBJECT-TYPE
        SYNTAX   SEQUENCE OF WfIfpDrvCfgEntry
        ACCESS   not-accessible
        STATUS   mandatory
        DESCRIPTION
                "Internet Fowarding Processor(IFP) Configuration Record"
        ::= { wfIfpGroup 1 }

