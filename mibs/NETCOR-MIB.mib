                  	ipNetToMediaType
                      		INTEGER
              		}

          ipNetToMediaIfIndex OBJECT-TYPE
              	SYNTAX  INTEGER
           -- 	ACCESS  read-write
              	ACCESS  read-only
              	STATUS  mandatory
              	::= { ipNetToMediaEntry 1 }
	      	
          ipNetToMediaPhysAddress OBJECT-TYPE
              	SYNTAX  PhysAddress
              	ACCESS  read-write
              	STATUS  mandatory
              	::= { ipNetToMediaEntry 2 }
	      	
          ipNetToMediaNetAddress OBJECT-TYPE
              	SYNTAX  IpAddress
              	ACCESS  read-write
              	STATUS  mandatory
              	::= { ipNetToMediaEntry 3 }
	      	
          ipNetToMediaType OBJECT-TYPE
              	SYNTAX  INTEGER {
              	            	other(1),        -- none of the following
              	            	invalid(2),      -- an invalidated mapping
              	            	dynamic(3),
              	            	static(4)
              	        	}
              	ACCESS  read-write
              	STATUS  mandatory
              	::= { ipNetToMediaEntry 4 }

