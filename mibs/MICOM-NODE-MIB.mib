MICOM-NODE-MIB DEFINITIONS ::= BEGIN
	IMPORTS
		Counter, Gauge, IpAddress, enterprises
                   		 	FROM RFC1155-SMI
		OBJECT-TYPE
                    			FROM RFC-1212
		TRAP-TYPE
                			FROM RFC-1215
		IpAddress
				FROM RFC1065-SMI
		DisplayString 
				FROM SNMPv2-TC;

-- group OBJECT IDENTIFIERS

--	iso							OBJECT IDENTIFIER ::= {1}
--	org							OBJECT IDENTIFIER ::= {iso 3}
--	dod							OBJECT IDENTIFIER ::= {org 6}
--	internet					OBJECT IDENTIFIER ::= {dod 1}
--	mgmt						OBJECT IDENTIFIER ::= {internet 2}
--	private						OBJECT IDENTIFIER ::= {internet 4}
--	enterprises					OBJECT IDENTIFIER ::= {private 1}
	micom						OBJECT IDENTIFIER ::= {enterprises 335}
	products					OBJECT IDENTIFIER ::= {micom 1}

	marathon-netrunner			OBJECT IDENTIFIER ::= {products 1}
	micom-proxy					OBJECT IDENTIFIER ::= {products 2}

	marathon-netrunner-proxy	OBJECT IDENTIFIER ::= {micom-proxy 1}

	security 					OBJECT IDENTIFIER ::= {marathon-netrunner-proxy 1}
	diagnostics	 				OBJECT IDENTIFIER ::= {marathon-netrunner-proxy 2}
	configuration		 		OBJECT IDENTIFIER ::= {marathon-netrunner-proxy 3}
	control	 					OBJECT IDENTIFIER ::= {marathon-netrunner-proxy 4}
	status						OBJECT IDENTIFIER ::= {marathon-netrunner-proxy 5}
	statistics 					OBJECT IDENTIFIER ::= {marathon-netrunner-proxy 6}

	system 						OBJECT IDENTIFIER ::= {configuration 1}
	card 						OBJECT IDENTIFIER ::= {configuration 2}
	port 						OBJECT IDENTIFIER ::= {configuration 3}
	channel 					OBJECT IDENTIFIER ::= {configuration 4}
	link 						OBJECT IDENTIFIER ::= {configuration 5}
	interface					OBJECT IDENTIFIER ::= {configuration 6}
	trap 						OBJECT IDENTIFIER ::= {configuration 7}
	proxy 						OBJECT IDENTIFIER ::= {configuration 8}

	attributes 					OBJECT IDENTIFIER ::= {system 1}
	messages 					OBJECT IDENTIFIER ::= {system 2}

	sync-channel				OBJECT IDENTIFIER ::= {channel 1}
	async-channel				OBJECT IDENTIFIER ::= {channel 2}
	voice-fax-channel			OBJECT IDENTIFIER ::= {channel 3}
	digital-voice-channel		OBJECT IDENTIFIER ::= {channel 4}

	mux-link					OBJECT IDENTIFIER ::= {link 1}
	esi-link					OBJECT IDENTIFIER ::= {link 2}
	x-dot-21-link				OBJECT IDENTIFIER ::= {link 3}
	local-esi-link				OBJECT IDENTIFIER ::= {link 4}
	esi-secondary-link			OBJECT IDENTIFIER ::= {link 5}
	frame-relay-link			OBJECT IDENTIFIER ::= {link 6}

	t1-interface				OBJECT IDENTIFIER ::= {interface 1}
	e1-interface				OBJECT IDENTIFIER ::= {interface 2}

	statistics-configuration	OBJECT IDENTIFIER ::= {statistics 1}
	statistics-periodic			OBJECT IDENTIFIER ::= {statistics 2}
	statistics-accumulated		OBJECT IDENTIFIER ::= {statistics 3}

-- Security Group

-- N_CMOS.O_LCDPSW ======================================================

lcdSecurityPassword OBJECT-TYPE
	SYNTAX	DisplayString (SIZE (0..8))
	ACCESS	write-only
	STATUS	mandatory
	DESCRIPTION	"Access password to the LCD/Keypad located on front of 
			the integration unit."
	::= {security 1}

-- N_CMOS.O_CMDPSW =====================================================

cmdPortSecurityGroup    OBJECT IDENTIFIER ::= {security 2}

cmdPortSecurityGlobalPassword OBJECT-TYPE
	SYNTAX	DisplayString (SIZE (0..8))
	ACCESS	write-only
	STATUS	mandatory
	DESCRIPTION	"Access password to all functions. Password must be 
			alphanumeric characters (A-Z, 0-9) up to 8 characters." 
	 ::= { cmdPortSecurityGroup 1}

cmdPortSecurityStatusPassword OBJECT-TYPE
	SYNTAX	DisplayString (SIZE (0..8))
	ACCESS	write-only
	STATUS	mandatory
	DESCRIPTION	"Access password to view, status, and exit Command 
			Facility functions. Password must be alphanumeric
		 	characters (A-Z, 0-9) up to 8 characters." 
	::= { cmdPortSecurityGroup 2}

-- N_CMOS.O_NMSSYS ==================================================

nmsSecurityPPPPassword OBJECT-TYPE
	SYNTAX	DisplayString (SIZE (0..8))
	ACCESS	write-only
	STATUS	mandatory
	DESCRIPTION	"Access password to the $PPP Facility. Password must be 
			alphanumeric characters (A-Z, 0-9) upto 8 characters." 
	::= {security 3}


-- Diagnostics Group

-- N_DIAG.O_PING ==========================================================

pingResponse OBJECT-TYPE
	SYNTAX	INTEGER {
		responded (1),
		notResponding (2)
		}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Determines whether or not the node is reachable." 
	::= {diagnostics 1}

-- N_DIAG.O_VOICE =========================================================

voiceChannelDiagTable OBJECT-TYPE
	SYNTAX	SEQUENCE OF VoiceChannelDiagEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"This table provides the system diagnostics function to test the 
			voice/fax module(s) if installed." 
	::= {diagnostics 2}

voiceChannelDiagEntry OBJECT-TYPE
	SYNTAX	VoiceChannelDiagEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"An entry within the table specifying particular channel on voice module." 
	INDEX		{voiceChannelDiagCardNumber, voiceChannelDiagChannelNumber}
	::= {voiceChannelDiagTable 1}

VoiceChannelDiagEntry ::= SEQUENCE {
	voiceChannelDiagCardNumber		INTEGER,
	voiceChannelDiagChannelNumber		INTEGER,
	voiceChannelDiagCommand		INTEGER
	}

voiceChannelDiagCardNumber OBJECT-TYPE
	SYNTAX	INTEGER (1..5)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the module/card number for the specified channel 
			to be tested." 
	::= {voiceChannelDiagEntry 1}

voiceChannelDiagChannelNumber OBJECT-TYPE
	SYNTAX	INTEGER (1..12)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the channel number to which this entry pertains." 
	::= {voiceChannelDiagEntry 2}

voiceChannelDiagCommand OBJECT-TYPE
	SYNTAX	INTEGER {
		terminateTest (1),
		inputLevelDisplay (2),
		selfTest (3),
		putChannelInLoopback (4)
		}
	ACCESS	write-only
	STATUS	mandatory
	DESCRIPTION	"Options to initiate different type of tests or to terminate the tests.
			Option 2 allows monitoring of the level of the input voice signal. Option
			3 verifies that the voice channel is working properly. Option 4 causes
			the voice channel to compress the local speech signal and then to pass
			this signal through the reconstruction process back to the local receiver." 
	::= {voiceChannelDiagEntry 3}

-- N_DIAG.O_SYNC ==========================================================

syncChannelDiagTable OBJECT-TYPE
	SYNTAX	SEQUENCE OF SyncChannelDiagEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Table to control the testing and diagnosing of specific 
			sync channel." 
	::= {diagnostics 3}

syncChannelDiagEntry OBJECT-TYPE
	SYNTAX	SyncChannelDiagEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Entry into the diagnostic table for a specific channel on 
			specified module." 
	INDEX		{syncChannelDiagCardNumber, syncChannelDiagChannelNumber}
	::= {syncChannelDiagTable 1}

SyncChannelDiagEntry ::= SEQUENCE {
	syncChannelDiagCardNumber		INTEGER,
	syncChannelDiagChannelNumber		INTEGER,
	syncChannelDiagCommand		INTEGER
	}

syncChannelDiagCardNumber OBJECT-TYPE
	SYNTAX	INTEGER (1..5)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"The specified module/card number of the channel to be tested." 
	::= {syncChannelDiagEntry 1}

syncChannelDiagChannelNumber OBJECT-TYPE
	SYNTAX	INTEGER (1..6)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"The specified channel number to be tested." 
	::= {syncChannelDiagEntry 2}

syncChannelDiagCommand OBJECT-TYPE
	SYNTAX	INTEGER {
		terminateTest (1),
		putChannelInLoopback (2)
		}
	ACCESS	write-only
	STATUS	mandatory
	DESCRIPTION	"Command option to terminate and initiate the test. The test
			loops the control signals and data back." 
	::= {syncChannelDiagEntry 3}

-- System Attribute Group

-- N_CMOS.O_PROMID ========================================================

sysPromID OBJECT-TYPE
	SYNTAX	DisplayString (SIZE (1..18)) 
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the unique PROM identifier." 
	::= {attributes 1}

-- N_CMOS.O_HSTYPE ========================================================

sysTypeGroup    OBJECT IDENTIFIER ::= {attributes 2}

sysSoftwareType OBJECT-TYPE
	SYNTAX	INTEGER {
		marathon(1),
		netrunner(2)
		}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the software base type running on the Integration unit." 
	::= {sysTypeGroup 1}

sysHardwareType OBJECT-TYPE
	SYNTAX	INTEGER {
		unknown (1),
		fiftyECCM (2),
		oneKCCM (3),
		fiveKCCM (4),
		tenKCCM (5),
		fiveKTurboCCM (6),
		twentyKCCM (7),
		seventyfiveECCM (8),
		seventyfiveEwithFR (9),
		twoKCCM (10),
		threeK (11),
		twoKTPro (12),
		fiveKTPro (13),
		twentyKPro (14)
		}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the type of CCM module present in the Integration unit." 
	::= {sysTypeGroup 2}

sysOperationalType OBJECT-TYPE
	SYNTAX	INTEGER {
		node (1),
		feeder (2)
		}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the operational type of the Integration unit as node 
			or feeder." 
	::= {sysTypeGroup 3}

sysSoftwarePhase OBJECT-TYPE
	SYNTAX	INTEGER {
		phase3dot0 (1),
		phase3dot1 (2),
		phase3dot2 (3),
		phase3dot2C (4),
		phase4dot0 (5),
		phase4dot1 (6),
		phase4dot2 (7),
		phase4dot1G (8),
		phase4dot2c (9),
		phase4dot3 (10),
		phase5dot0 (11),
		phase4dot3BorC (12),
		phase5dot0B (13),
		aboveKnownPhases (999)
		}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the revision of the operating software running on 
			the node." 
	::= {sysTypeGroup 4}

sysFeatureType OBJECT-TYPE
	SYNTAX	INTEGER {
		featurePak (1),
		dualBank (2),
		singleBank (3),
		simSingleDual (4)
		}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the type of the feature pack installed on the unit." 
	::= {sysTypeGroup 5}

sysFeatureRTC OBJECT-TYPE
	SYNTAX	INTEGER {
		present (1),
		notPresent (2)
		}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies whether or not the real time clock is present on the unit.
			Note that NMS modules are available with RTC." 
	::= {sysTypeGroup 6}

-- N_CMOS.O_NODNAM ========================================================

sysLocNodeLinkTable OBJECT-TYPE
	SYNTAX	SEQUENCE OF SysLocNodeLinkEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Table containing MB-Feeder connection information." 
	::= {attributes 3}

sysLocNodeLinkEntry OBJECT-TYPE
	SYNTAX	SysLocNodeLinkEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Entry within the table identifying the name, type, and number
			of connected MB-Feeder (if any)." 
	INDEX		{sysLocNodeLinkIndex}
	::= {sysLocNodeLinkTable 1}

SysLocNodeLinkEntry ::= SEQUENCE {
	sysLocNodeLinkIndex			INTEGER,
	sysLocNodeLinkNodeNameOfFeeder	DisplayString,
	sysLocNodeLinkNodeType		INTEGER,
	sysLocNodeLinkNodeNumber		INTEGER
	}

sysLocNodeLinkIndex OBJECT-TYPE
	SYNTAX	INTEGER (1..6)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Instances into the table for each (1-6) link entry." 
	 ::= {sysLocNodeLinkEntry 1}

sysLocNodeLinkNodeNameOfFeeder OBJECT-TYPE
	SYNTAX	DisplayString (SIZE(0..8))
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Indicates the name of the connected (if any) feeder to a 
			particular port." 
	 ::= {sysLocNodeLinkEntry 2}

sysLocNodeLinkNodeType OBJECT-TYPE
	SYNTAX	INTEGER {
		notConfigured (1),
		marathon (2),
		mbFeeder (3)
		}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies whether or not the MB-Feeder is connected to
			a particular port." 
	 ::= {sysLocNodeLinkEntry 3}

sysLocNodeLinkNodeNumber OBJECT-TYPE
	SYNTAX	INTEGER (0..255)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Indicates the number of the connected (if any) feeder to a 
			particular port. A value of zero represent that the feeder is 
			not present." 
	 ::= {sysLocNodeLinkEntry 4}


-- N_CMOS.O_DATTIM ======================================================

sysDateTime OBJECT-TYPE
	SYNTAX	OCTET STRING (SIZE (7))
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Sets the current date and the time on the unit.  Format (byte 
			sequence) is Month, Day, Hour, Minutes, Seconds, Day of Week, Year." 
	::= { attributes 4}


-- N_CMOS. O_RCLASS ========================================================

sysClassCfgTable OBJECT-TYPE
	SYNTAX	SEQUENCE OF SysClassCfgEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Table to configure a class. Class is defined as a port or grouping
			of ports dedicated to a specific application or location." 
	::= {attributes 5}

sysClassCfgEntry OBJECT-TYPE
	SYNTAX	SysClassCfgEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"An entry into the table for a specific class number." 
	INDEX		{sysClassCfgClassNumber}
	 ::= {sysClassCfgTable 1}

SysClassCfgEntry ::= SEQUENCE {
	sysClassCfgClassNumber		INTEGER,
	sysClassCfgClassName		DisplayString,
	sysClassCfgClassSecondary	INTEGER,
	sysClassCfgClassMessage		DisplayString,
	sysClassCfgClassPassword	DisplayString,
	sysClassCfgNoActivityTimeOut	INTEGER,
	sysClassCfgEntryStatus		INTEGER
	}

sysClassCfgClassNumber OBJECT-TYPE
	SYNTAX	INTEGER (1..64)
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"The available class to be configured numbered from 1 to 64." 
	 ::= {sysClassCfgEntry 1}

sysClassCfgClassName OBJECT-TYPE
	SYNTAX	DisplayString  (SIZE (2..8))
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Assigns a class name to a selected class number. Class name
			can be up to eight characters long and may use characters A-Z, 
			0-9, and _." 
	 ::= {sysClassCfgEntry 2}

sysClassCfgClassSecondary OBJECT-TYPE
	SYNTAX	INTEGER (0..64)
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"The designated secondary class to the specified class used to 
			automatically route connections to this class if the primary
			class channels are busy." 
	 ::= {sysClassCfgEntry 3}

sysClassCfgClassMessage OBJECT-TYPE
	SYNTAX	DisplayString  (SIZE (0..61))
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Displays (on command facility) when the user connects to 
			a port within the class." 
	::= {sysClassCfgEntry 4}

sysClassCfgClassPassword OBJECT-TYPE
	SYNTAX	DisplayString  (SIZE (0..8))
	ACCESS	write-only
	STATUS	mandatory
	DESCRIPTION	"Protects the access to a particular class. The default is no
			password (NULL Password)." 
	::= {sysClassCfgEntry 5}

sysClassCfgNoActivityTimeOut OBJECT-TYPE
	SYNTAX	INTEGER (0..255)
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"The no activity feature monitors the class channel.  After a
			specified period of time, if there is no activity on the channel, it
			will be disconnected. Value 0 = disabled; values 1-255 = the timeout
			interval in minutes." 
	::= {sysClassCfgEntry 6}

sysClassCfgEntryStatus OBJECT-TYPE
	SYNTAX	INTEGER {
		active (1),
		notInService (2),
		notReady (3),
		createAndWait (5),
		destroy (6)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Object to create conceptual row entry in the table.  Internally used
			to cache all the column objects till the commit is done through active(1).
			Option 6 (destroy) deletes the entry (class) from the configured classes." 
	::= { sysClassCfgEntry 7}

-- N_CMOS.O_ACCNOD ========================================================

sysAccessNodeName OBJECT-TYPE
	SYNTAX	DisplayString (SIZE (0..8))
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"This is the name of the node which has been selected as the
				access point for the MICOMVision Manager in the Network.
				Configuring this object for a node results in the MICOM Alarms
				being forwarded to this specified Access node and the NMS system
				being informed of pending alarms awaiting retrieval."
	 ::= {attributes 6}

-- N_CMOS.O_FLASH ========================================================

sysFlashDownloadGroup  OBJECT IDENTIFIER ::= { attributes 7}

sysFlashDownloadPassword OBJECT-TYPE
	SYNTAX	DisplayString  (SIZE(0..8))
	ACCESS	write-only
	STATUS	mandatory
	DESCRIPTION	"Configures the security password for the download facility." 
	::= {sysFlashDownloadGroup 1}

sysFlashDownloadUseFile OBJECT-TYPE
	SYNTAX	INTEGER {
		mostRecent(1),
		useFileOne (2),
		useFileTwo (3)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Configures which Flash Bank that will be loaded from when the
				marathon is reset to re-load from flash." 
	::= {sysFlashDownloadGroup 2}

sysFlashDownloadDataActivityTimeout OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-write
	STATUS	mandatory
--	DEFVAL	2
	DESCRIPTION	"Configures the Data Activity Timeout to be used while downloading
				an image to Flash Memory.  The range is 30 - 240 seconds." 
	::= {sysFlashDownloadGroup 3}

-- N_CMOS.O_MESH ======================================================

sysMeshRouting OBJECT-TYPE
	SYNTAX	INTEGER {
		enable (1),
		disable (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Configures whether mesh routing is enabled for this Integration unit." 
	::= {attributes 8}

-- N_CMOS.O_BOOTID ======================================================

sysBootID OBJECT-TYPE
	SYNTAX	DisplayString (SIZE (1..18)) 
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Indicates the boot identifier if boot prom is present." 
	::= {attributes 9}

-- N_CMOS.O_NETTIM ======================================================

sysNetTime OBJECT-TYPE
	SYNTAX	OCTET STRING (SIZE (7))
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"The date and time to be configured on all the Integration units 
			on the network.  Format (byte sequence) is Month, Day, Hour, Minutes,
			Seconds, Day of Week, Year." 
	::= { attributes 10}

--CODE DOWNLOD ======================================================

sysCodeDownloadGroup  OBJECT IDENTIFIER ::= { attributes 11}

sysCodeDownloadType OBJECT-TYPE
	SYNTAX	INTEGER {
		ccmBase (1),
		voiceModule (2),
		tamModule (3),
		eamModule (4),
		eli2Module (5)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Identifies the type of Code Download that is to be performed."
	::= { sysCodeDownloadGroup 1}

sysCodeDownloadNodeName OBJECT-TYPE
	SYNTAX	DisplayString (SIZE(1..8))
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"The name of the node to which software is to be downloaded." 
	::= { sysCodeDownloadGroup 2}

sysCodeDownloadSlotNumber OBJECT-TYPE
	SYNTAX	INTEGER (1..5)
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"The slot number of the module to which software is to be downloaded." 
	::= { sysCodeDownloadGroup 3}

sysCodeDownloadChannelNumber OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"The Voice Channel number to which software is to be downloaded. 
			Only valid for Voice." 
	::= { sysCodeDownloadGroup 4}

sysCodeDownloadImageFile OBJECT-TYPE
	SYNTAX	DisplayString (SIZE(0..15))
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"The name of the code image file to be downloaded." 
	::= { sysCodeDownloadGroup 5}

sysCodeDownloadFlashBank OBJECT-TYPE
	SYNTAX	INTEGER (1..2)
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"The destination flash bank to be downloaded.
			Only for CCM download." 
	::= { sysCodeDownloadGroup 6}

sysCodeDownloadRestartFromBank OBJECT-TYPE
	SYNTAX	INTEGER {
		bankOne (1),
		bankTwo (2),
		mostRecent (3)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Identifies the subsequent flash bank to be started from. 
			Only for CCM download." 
	::= { sysCodeDownloadGroup 7}

sysCodeDownloadReset OBJECT-TYPE
	SYNTAX	INTEGER {
		resetAfterDownload (1),
		dontResetAfterDownload (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Value of 1 identifies that the reset of the module/node 
			is required after successful download." 
	::= { sysCodeDownloadGroup 8}

sysCodeDownloadActionStatus OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Option 0 initiates a request for the code download. The status 
			of the process is conveyed via options 1 through n." 
	::= { sysCodeDownloadGroup 9}

sysCodeDownloadFileSize OBJECT-TYPE
	SYNTAX	INTEGER 
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"This is the Total Number of Bytes in the image file that is being 
			downloaded.  Valid only after a download has started." 
	::= { sysCodeDownloadGroup 10}

sysCodeDownloadBytesDownloaded OBJECT-TYPE
	SYNTAX	INTEGER 
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"This is the Total Number of Bytes that have been sent to the Integration
			unit.  Upon completion, this should equal the sysCodeDownloadFileSize
			paramater.  Valid only after a download has been started." 
	::= { sysCodeDownloadGroup 11}

sysCodeDownloadNodeNumber OBJECT-TYPE
	SYNTAX	INTEGER 
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"The numeric value (Node ID #) that represents this node in the network." 
	::= { sysCodeDownloadGroup 12}

-- System Messages Group

-- This groups configures dialog messages which are asynchronous, configurable messages
-- and prompts displayed for information, configuration or requesting inputs on the command
-- line facility

-- N_CMOS.O_WELCM ======================================================

sysMessageWelcome OBJECT-TYPE
	SYNTAX	DisplayString  (SIZE(1..61))
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Displays when the user first successfully connects to the Integration unit." 
	 ::= {messages 1}

-- N_CMOS.O_CHPMS ======================================================

sysMessageChannelPasswd OBJECT-TYPE
	SYNTAX	DisplayString  (SIZE(1..31))
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Prompts the user for the password associated for the sign-on
			channel. This is the first dialog message that appears when the
			user attempts to connect to the network from the command 
			line facility." 
	::= { messages 2}

-- N_CMOS.O_CLREQ ======================================================

sysMessageClassRequest OBJECT-TYPE
	SYNTAX	DisplayString  (SIZE(1..31))
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Prompts (at the command facility line) the async channel 
			user for the destination class or channel." 
	 ::= { messages 3}

-- N_CMOS.O_CONMS ======================================================

sysMessageConnected OBJECT-TYPE
	SYNTAX	DisplayString  (SIZE(1..31))
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Displays (on the command facility line) when a connection 
			to any resource (class or matrix switch) has been established
			across an interconnect link ." 
	 ::= { messages 4}

-- N_CMOS.O_QUEMS ======================================================

sysMessageQueued OBJECT-TYPE
	SYNTAX	DisplayString  (SIZE(1..17))
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Displays (on the command facility line) when a user is waiting 
			far a channel in wait queue." 
	 ::= { messages 5}

-- N_CMOS.O_DISMS ======================================================

sysMessageDisconnected OBJECT-TYPE
	SYNTAX	DisplayString  (SIZE(1..17))
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Displays (on the command facility line) when the connection
			has been broken. It is only sent to the call originator using
			switching (This is not for force connections)." 
	 ::= { messages 6}

-- N_CMOS.O_CALMS ======================================================

sysMessageCallInProgress OBJECT-TYPE
	SYNTAX	DisplayString  (SIZE(1..17))
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Displays (on the command facility line) while the call is in
			process of being connected." 
	 ::= { messages 7}

-- N_CMOS.O_NAWMS ======================================================

sysMessageNoAnswer OBJECT-TYPE
	SYNTAX	DisplayString  (SIZE(1..17))
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Displays (on the command facility line) when there is no
			answer from the destination port." 
	 ::= { messages 8}

-- N_CMOS.O_UNAMS ======================================================

sysMessageUnassigned OBJECT-TYPE
	SYNTAX	DisplayString  (SIZE(1..17))
