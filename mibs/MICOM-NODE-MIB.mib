	esiOrSecLinkTODActivationChannelNumber	INTEGER,
	esiOrSecLinkTODActivationSetSchedule		INTEGER,
	esiOrSecLinkTODActivationTenMinInterval	INTEGER,
	esiOrSecLinkTODActivationLinkState		INTEGER
	}

esiOrSecLinkTODActivationCardNumber OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the module/card number specific to an ESI or secondary
			link for the TOD activation/deactivation schedule." 
	 ::= {esiOrSecLinkTODActivationEntry 1}

esiOrSecLinkTODActivationChannelNumber OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the port location specific to an ESI or secondary
			link for the TOD activation/deactivation schedule." 
	 ::= {esiOrSecLinkTODActivationEntry 2}

esiOrSecLinkTODActivationSetSchedule OBJECT-TYPE
	SYNTAX	INTEGER {
		sunday (1),
		weekdays (2),
		saturday (3)
		}
	ACCESS	read-only
	STATUS	mandatory
