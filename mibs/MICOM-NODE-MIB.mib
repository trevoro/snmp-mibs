	SYNTAX	INTEGER {
		superFrame (1),
		extendedSuperFrame (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Configures the local T1 interface frame formats to either (SF)
			super frame or (ESF) extended super frame." 
--	DEFVAL	1
	::= {t1CfgGroup 1}

t1CfgDSX1LineCode OBJECT-TYPE
	SYNTAX	INTEGER {
		ami (1),
		b8zs (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Configures the line coding method (at the Local interface) either
			to Alternate Mark Inversion (AMI) coding or Bipolar Eight Zero 
			Suppression (B8ZS). In AMI 0's are indicated by the absence of a 
			pulse, and 1's are indicated by either a positive or negative polarity 
			pulse. In B8ZS an all 0 byte is replaced by a special code of 00011011 
			which indicate two fixed bipolar code violations. On remote end these 
			violations are ignored and the data is retrieved." 
--	DEFVAL	1
	::= {t1CfgGroup 2}

t1CfgDSX1IdleCode OBJECT-TYPE
	SYNTAX	OCTET STRING (SIZE(1))
