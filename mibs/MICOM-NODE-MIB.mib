esiOrSecLinkTODActivationLinkState OBJECT-TYPE
	SYNTAX	INTEGER {
		off (1),
		on (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Sets the TOD link state for an ESI or secondary link.  This 
			parameter is used to configure a link to active (on) or not active 
			(off) for a particular 10 minute interval as specified by the 
			previous 4 indexing parameters contained in this table." 
	 ::= {esiOrSecLinkTODActivationEntry 5}


x21LinkCfgTable OBJECT-TYPE
	SYNTAX	SEQUENCE OF X21LinkCfgEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Table to configure the X.21 links. It defines the X.21 parameters
			for the Integration unit and for the MICOM mux side." 
	::= {x-dot-21-link 1}

x21LinkCfgEntry OBJECT-TYPE
	SYNTAX	X21LinkCfgEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Entry in the table for a particular X.21 link.  Note that a link 
			reset is required if any value in this table is changed." 
	INDEX		{x21LinkCfgCardNumber, x21LinkCfgChannelNumber}
	::= {x21LinkCfgTable 1} 

X21LinkCfgEntry ::= SEQUENCE {
	x21LinkCfgCardNumber			INTEGER,
	x21LinkCfgChannelNumber		INTEGER,
	x21LinkCfgLocalConnectMode		INTEGER,
	x21LinkCfgRemoteConnectMode		INTEGER,
	x21LinkCfgLocalConnectTimeout		INTEGER,
	x21LinkCfgRemoteConnectTimeout	INTEGER,
	x21LinkCfgLocalConnectRetries		INTEGER,
	x21LinkCfgRemoteConnectRetries		INTEGER
	}

x21LinkCfgCardNumber OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"The card/module number of the X.21 link channel the entry pertains to." 
	::= {x21LinkCfgEntry 1}

x21LinkCfgChannelNumber OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"The particular X.21 link channel on specific module to be configured." 
	::= {x21LinkCfgEntry 2}

x21LinkCfgLocalConnectMode OBJECT-TYPE
	SYNTAX	INTEGER {
		dataActivityConnect (1),
		dtrConnect (2)
		}
	ACCESS	read-write
	STATUS	mandatory
--	DEFVAL	1
	DESCRIPTION	"Configures the Integration unit side of the X.21 link.  
			In Integration units, the only connect mode for X.21 
			links is data activity connect. In this mode, the Integration
			unit will place a call over the X.21 network whenever data 
			activity is detected on any force connected channel, matrix
			connection, or port connected to a class defined with ports on 
			a MICOM mux attached to an X.21 link." 
	::= {x21LinkCfgEntry 3}

x21LinkCfgRemoteConnectMode OBJECT-TYPE
	SYNTAX	INTEGER {
		dataActivityConnect(1),
		dtrConnect (2)
		}
	ACCESS	read-write
	STATUS	mandatory
--	DEFVAL	1
	DESCRIPTION	"Configures the MICOM mux side of the X.21 link. There 
			are two options: DTR and Data Activity Connect.  DTR 
			(DTR connect) establishes the X.21 connection whenever 
			DTR is raised on any terminal connected to the MICOM mux.
			Data Activity Connect establishes the X.21 connection 
			whenever data activity is detected on any terminal connected 
			to the MICOM mux." 
	::= {x21LinkCfgEntry 4}

x21LinkCfgLocalConnectTimeout OBJECT-TYPE
	SYNTAX	INTEGER (0..20)
	ACCESS	read-write
	STATUS	mandatory
--	DEFVAL	5
	DESCRIPTION	"The value in this field specifies the amount of time that if no 
			data activity is detected, the X.21 connection will be dropped. 
			The default is 5 seconds." 
	::= {x21LinkCfgEntry 5}

x21LinkCfgRemoteConnectTimeout OBJECT-TYPE
	SYNTAX	INTEGER (0..20)
	ACCESS	read-write
	STATUS	mandatory
--	DEFVAL	5
	DESCRIPTION	"If data activity connect mode is selected for the remote MICOM 
			mux, this field specifies the amount of time that if no data 
			activity is detected on any of the MICOM mux's channels, the X.21 
			connection will be dropped. The default is 5 seconds." 
	::= {x21LinkCfgEntry 6}

x21LinkCfgLocalConnectRetries OBJECT-TYPE
	SYNTAX	INTEGER (1..64)
	ACCESS	read-write
	STATUS	mandatory
--	DEFVAL	20
	DESCRIPTION	"Configures the number of attempts the Integration unit will make to
			place a call over the X.21 network." 
	::= {x21LinkCfgEntry 7}

x21LinkCfgRemoteConnectRetries OBJECT-TYPE
	SYNTAX	INTEGER (1..64)
