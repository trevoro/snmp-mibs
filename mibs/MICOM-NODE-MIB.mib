asyncChPhyParaCRDelay OBJECT-TYPE
	SYNTAX	INTEGER (0..99)
	ACCESS	read-write
	STATUS	mandatory
--	DEFVAL	0
	DESCRIPTION	"Sets the delay in character times to allow for mechanical
			action to take place when a carriage return control character
			is transmitted to a channel." 
	::= {asyncChPhyParaEntry 10}

asyncChPhyParaLFDelay OBJECT-TYPE
	SYNTAX	INTEGER (0..99)
	ACCESS	read-write
	STATUS	mandatory
--	DEFVAL	0
	DESCRIPTION	"Sets the delay in character times to allow for mechanical
			action to take place when a line feed control character is
			transmitted to a channel." 
	::= {asyncChPhyParaEntry 11}

asyncChPhyParaFFDelay OBJECT-TYPE
	SYNTAX	INTEGER (0..99)
	ACCESS	read-write
	STATUS	mandatory
--	DEFVAL	0
	DESCRIPTION	"Sets the delay in character times to allow for mechanical
			action to take place when a form feed control character is
			transmitted to a channel." 
	::= {asyncChPhyParaEntry 12}

asyncChPhyParaXONChar OBJECT-TYPE
	SYNTAX	INTEGER {
		dc1 (1),
		dc2 (2),
		dc3 (3),
		dc4 (4)
		}
	ACCESS	read-write
	STATUS	mandatory
--	DEFVAL	1
	DESCRIPTION	"Defines the different XON characters." 
	::= {asyncChPhyParaEntry 13}

asyncChPhyParaXOFFChar OBJECT-TYPE
	SYNTAX	INTEGER {
		dc1 (1),
		dc2 (2),
		dc3 (3),
		dc4 (4)
		}
	ACCESS	read-write
--	DEFVAL	3
	STATUS	mandatory
	DESCRIPTION	"Defines the different XOFF characters." 
	::= {asyncChPhyParaEntry 14}

asyncChPhyParaCmdFacilityAccess OBJECT-TYPE
	SYNTAX	INTEGER {
		enable (1),
		disable (2)
		}
	ACCESS	read-write
	STATUS	mandatory
--	DEFVAL	1
	DESCRIPTION	"Permits or denies (the port being configured) access to the
			Command Facility Main Menu to view or change the system
			operating parameters." 
	::= {asyncChPhyParaEntry 15}

asyncChPhyParaCmdModeEntry OBJECT-TYPE
	SYNTAX	INTEGER {
		ctlXctlY (1),
		ctlXbreak (2)
		}
	ACCESS	read-write
	STATUS	mandatory
--	DEFVAL	2
	DESCRIPTION	"Selects the control character sequence to access to the 
			Command Mode." 
	::= {asyncChPhyParaEntry 16}

asyncChPhyParaLocChannelConfig OBJECT-TYPE
	SYNTAX	INTEGER {
		enable (1),
		disable (2)
		}
	ACCESS	read-write
	STATUS	mandatory
--	DEFVAL	1
	DESCRIPTION	"Controls the access to the Local Channel Configuration Menu." 
	::= {asyncChPhyParaEntry 17}

asyncChPhyParaCmdModeAccess OBJECT-TYPE
	SYNTAX	 INTEGER {
		enable (1),
		disable (2)
		}
	ACCESS	read-write
	STATUS	mandatory
--	DEFVAL	1
	DESCRIPTION	"Permits or deny (the port being configured) access to
			the Command Mode." 
	::= {asyncChPhyParaEntry 18}

asyncChPhyParaTandemSupport OBJECT-TYPE
	SYNTAX	INTEGER {
		on (1),
		off (2)
		}
	ACCESS	read-write
	STATUS	mandatory
--	DEFVAL	2
	DESCRIPTION	"Should set to ON only when Tandem Computers are connected
			to the channel being configured." 
	::= {asyncChPhyParaEntry 19}

asyncChPhyParaSmoothScroll OBJECT-TYPE
	SYNTAX	INTEGER {
		on (1),
		off (2)
		}
	ACCESS	read-write
	STATUS	mandatory
--	DEFVAL	2
	DESCRIPTION	"Should be set to ON only when DEC VT100 smooth scroll
			terminals are connected to the system." 
	::= {asyncChPhyParaEntry 20}

asyncChPhyParaFlowControl OBJECT-TYPE
	SYNTAX	INTEGER {
		none (1),
		xonXof (2),
		dtr (3),
		xonXofDtr (4),
		wangx (5),
		rts (6),
		dataGeneral (7)
		}
	ACCESS	read-write
	STATUS	mandatory
--	DEFVAL	2
	DESCRIPTION	"An attached device uses flow control to start or stop data
			flow from the Integration unit. The attached device may control data
			flow by use of XON/XOFF, DTR, XON/XOFF + DTR or a
			special WANGX character." 
	::= {asyncChPhyParaEntry 21}

asyncChPhyParaBufferControl OBJECT-TYPE
	SYNTAX	INTEGER {
		none (1),
		xonXof (2),
		cts (3),
		wangx (4)
		}
	ACCESS	read-write
	STATUS	mandatory
--	DEFVAL	2
	DESCRIPTION	"Buffer control to start or stop the data flow from the 
			attached device. The attached device may control data
			flow by use of XON/XOFF, CTS, or a special WANGX
			character." 
	::= {asyncChPhyParaEntry 22}

asyncChPhyParaFlowControlStrip OBJECT-TYPE
	SYNTAX	INTEGER {
		strip (1),
		pass (2)
		}
	ACCESS	read-write
	STATUS	mandatory
--	DEFVAL	2
	DESCRIPTION	"Allows the flow control characters to be passed through or
			stripped from the Integration unit." 
	::= {asyncChPhyParaEntry 23}

asyncChPhyParaSyncLossDisconnect OBJECT-TYPE
	SYNTAX	INTEGER {
		enable (1),
		disable (2)
		}
	ACCESS	read-write
	STATUS	mandatory
--	DEFVAL	2
	DESCRIPTION	"When enabled, the local node toggles the controls and
			purges the data. This occurs when the link has experienced
			a sync loss condition for more than 20 seconds." 
	::= {asyncChPhyParaEntry 24}

asyncChPhyParaEIAControls OBJECT-TYPE
	SYNTAX	INTEGER {
		enable (1),
		disable (2)
		}
	ACCESS	read-write
	STATUS	mandatory
--	DEFVAL	1
	DESCRIPTION	"Ability to recognize or ignore the EIA control signals from the
			attached device." 
	::= {asyncChPhyParaEntry 25}

asyncChPhyParaPriority OBJECT-TYPE
	SYNTAX	INTEGER {
		high (1),
		low (2)
		}
	ACCESS	read-write
	STATUS	mandatory
--	DEFVAL	1
	DESCRIPTION	"Decides whether or not this channel is to receive a high
			or low priority for use. A high priority channel will take
			precedence over a channel with low priority." 
	::= {asyncChPhyParaEntry 26}

asyncChPhyParaParity OBJECT-TYPE
	SYNTAX	INTEGER {
		space (1),
		odd (2),
		even (3),
		mark (4),
		none (5)
		}
	ACCESS	read-write
	STATUS	mandatory
--	DEFVAL	5
	DESCRIPTION	"Sets the message and data parity for the Integration unit. Depending on the
			asyncChPhyParaCodeLevel selection of 5-8 bits, the available parity
			is space, odd, even, mark, and none.  For code level 9 bits, the 
			available choice is odd or even." 
	::= {asyncChPhyParaEntry 27}

asyncChPhyParaCompression OBJECT-TYPE
	SYNTAX	INTEGER {
		enable (1),
		disable (2)
		}
	ACCESS	read-write
	STATUS	mandatory
--	DEFVAL	1
	DESCRIPTION	"When enabled, the unit compresses a block of data when transmitting.
			Fewer bytes are required for the transfer thus increasing the data
			transfer rate (making it faster)." 
	::= {asyncChPhyParaEntry 28}


-- N_CMOS.O_ASYSWI =======================================================

asyncChSwParaTable OBJECT-TYPE
	SYNTAX	SEQUENCE OF AsyncChSwParaEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Table for configuration of async channel switching parameters." 
	::= {async-channel 2}

asyncChSwParaEntry OBJECT-TYPE
	SYNTAX	AsyncChSwParaEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Entry within the table to specify a particular async channel number
			on a specific module/card." 
	INDEX		{asyncChSwParaCardNumber, asyncChSwParaChannelNumber}
	::= {asyncChSwParaTable 1}

AsyncChSwParaEntry ::= SEQUENCE {
	asyncChSwParaCardNumber		INTEGER,
	asyncChSwParaChannelNumber		INTEGER,
	asyncChSwParaDestinationClass		DisplayString,
	asyncChSwParaConnectProtocol		INTEGER,
	asyncChSwParaRcvInhibit			INTEGER,
	asyncChSwParaResourceClassNumber	INTEGER,
	asyncChSwParaCallInhibit		INTEGER,
	asyncChSwParaUnbalanceRates		INTEGER,
	asyncChSwParaCharacterSet		INTEGER,
	asyncChSwParaMatrixSwitching		INTEGER,
	asyncChSwParaChannelPassword		DisplayString
	}


asyncChSwParaCardNumber OBJECT-TYPE
	SYNTAX	INTEGER (1..5)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the module/card number for the async channel specific
			to the channel switching parameters." 
	::= {asyncChSwParaEntry 1}

asyncChSwParaChannelNumber OBJECT-TYPE
	SYNTAX	INTEGER (1..32)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the async channel specific to the channel switching parameters." 
	::= {asyncChSwParaEntry 2}

asyncChSwParaDestinationClass OBJECT-TYPE
	SYNTAX	DisplayString  (SIZE (0..17))
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Assign the destination class name to the async channel for class switching." 
	::= {asyncChSwParaEntry 3}

asyncChSwParaConnectProtocol OBJECT-TYPE
	SYNTAX	INTEGER {
		dedicated (1),
		dtr (2),
		autoAnswer (3)
		}
	ACCESS	read-write
	STATUS	mandatory
--	DEFVAL	1
	DESCRIPTION	"Selects the connect protocol for the port being configured. The
			protocol must be compatible with the attached device." 
	::= {asyncChSwParaEntry 4}

asyncChSwParaRcvInhibit OBJECT-TYPE
	SYNTAX	INTEGER {
		enable (1),
		disable (2)
		}
	ACCESS	read-write
	STATUS	mandatory
--	DEFVAL	2
	DESCRIPTION	"Controls the port being configured from receiving a connection
			request." 
	::= {asyncChSwParaEntry 5}

asyncChSwParaResourceClassNumber OBJECT-TYPE
	SYNTAX	INTEGER (0..64)
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Assigns a resource class number (1 - 64) to the async channel.
			Zero reflects no resource class assignment." 
	::= {asyncChSwParaEntry 6}

asyncChSwParaCallInhibit OBJECT-TYPE
	SYNTAX	INTEGER {
		enable (1),
		disable (2)
		}
	ACCESS	read-write
	STATUS	mandatory
--	DEFVAL	2
	DESCRIPTION	"Controls the port being configured from making a connection
			request." 
	::= {asyncChSwParaEntry 7}

asyncChSwParaUnbalanceRates OBJECT-TYPE
	SYNTAX	INTEGER {
		enable (1),
		disable (2)
		}
	ACCESS	read-write
	STATUS	mandatory
--	DEFVAL	1
	DESCRIPTION	"Controls the connections of ports which have dissimilar
			baud rates (different speed)." 
	::= {asyncChSwParaEntry 8}

asyncChSwParaCharacterSet OBJECT-TYPE
	SYNTAX	INTEGER {
		ascii (1),
		nonAscii (2)
		}
	ACCESS	read-write
	STATUS	mandatory
--	DEFVAL	1
	DESCRIPTION	"Option to select either an ASCII or non-ASCII character set." 
	::= {asyncChSwParaEntry 9}

asyncChSwParaMatrixSwitching OBJECT-TYPE
	SYNTAX	INTEGER {
		enable (1),
		disable (2)
		}
	ACCESS	read-write
	STATUS	mandatory
--	DEFVAL	1
	DESCRIPTION	"The ability of the port to make a connection request by 
			specifying a node/channel number." 
	::= {asyncChSwParaEntry 10}

asyncChSwParaChannelPassword OBJECT-TYPE
	SYNTAX	DisplayString  (SIZE (0..8))
	ACCESS	write-only
	STATUS	mandatory
	DESCRIPTION	"A unique password attached to the port being configured
			to control access for security." 
	::= {asyncChSwParaEntry 11}


-- VOICE CHANNEL

-- N_CMOS.O_VOICE =========================================================

voiceFaxChPhyParaTable OBJECT-TYPE
	SYNTAX	SEQUENCE OF VoiceFaxChPhyParaEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Table for configuring the voice/Fax channel physical parameters." 
	::= {voice-fax-channel 1}

voiceFaxChPhyParaEntry OBJECT-TYPE
	SYNTAX	VoiceFaxChPhyParaEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Entry within the table to specify a particular voice/Fax channel number
			on a specific module/card." 
	INDEX		{voiceFaxChPhyParaCardNumber, voiceFaxChPhyParaChannelNumber}
	::= {voiceFaxChPhyParaTable 1}

VoiceFaxChPhyParaEntry ::= SEQUENCE {
	voiceFaxChPhyParaCardNumber				INTEGER,
	voiceFaxChPhyParaChannelNumber			INTEGER,
	voiceFaxChPhyParaWireOperation			INTEGER,
	voiceFaxChPhyParaViewSignallingType		INTEGER,
	voiceFaxChPhyParaBergStrapType			INTEGER,
	voiceFaxChPhyParaSuppression			INTEGER,
	voiceFaxChPhyParaCfgSignallingType		INTEGER,
	voiceFaxChPhyParaOPXNumberOfRings		INTEGER,
	voiceFaxChPhyParaFXOSupervisorDisc		INTEGER,
	voiceFaxChPhyParaRingFrequency			INTEGER,
	voiceFaxChPhyParaJitter					INTEGER,
	voiceFaxChPhyParaDTMFRegeneration		INTEGER,
	voiceFaxChPhyParaAutoLevelEnhancement	INTEGER,
	voiceFaxChPhyParaBackground				INTEGER,
	voiceFaxChPhyParaDigitizingRate			INTEGER,
	voiceFaxChPhyParaBusyMode				INTEGER,
	voiceFaxChPhyParaVoiceFaxMode			INTEGER,
	voiceFaxChPhyParaBandwidthAssignment	INTEGER,
	voiceFaxChPhyParaInputGain				INTEGER,
	voiceFaxChPhyParaChannelType			INTEGER,
	voiceFaxChPhyParaEMTypeStrapping		INTEGER,
	voiceFaxChPhyParaOutputAttenuation		INTEGER,
	voiceFaxChPhyParaPriority				INTEGER,
	voiceFaxChPhyParaFaxDigitizingRate		INTEGER,
	voiceFaxChPhyParaLineImpedance			INTEGER,
	voiceFaxChPhyParaMaxOutputLevel			INTEGER,
	voiceFaxChPhyParaRegenerationDelay		INTEGER,
	voiceFaxChPhyParaMaxDialDigitTimeLimit	INTEGER,
	voiceFaxChPhyParaMaxNumFwdDigits		INTEGER,
	voiceFaxChPhyParaCallProgressionTones	INTEGER,
	voiceFaxChPhyParaRegenFormat			INTEGER,
	voiceFaxChPhyParaChannelVersion			INTEGER
	}

voiceFaxChPhyParaCardNumber OBJECT-TYPE
	SYNTAX	INTEGER (2..5)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the module/card number for the voice/Fax channel specific
			to the channel's physical parameters." 
	::= {voiceFaxChPhyParaEntry 1}

voiceFaxChPhyParaChannelNumber OBJECT-TYPE
	SYNTAX	INTEGER (1..2)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the voice/Fax channel specific to the channel's 
			physical parameters." 
	::= {voiceFaxChPhyParaEntry 2}

voiceFaxChPhyParaWireOperation OBJECT-TYPE
	SYNTAX	INTEGER {
		fourWire (1),
		twoWire (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Sets the voice channel for either 2-wire or 4-wire analog 
			operation. Valid only when  voiceFaxChPhyParaBergStrapType
			indicates em(2) or enhancedEm(5)."  
	::= {voiceFaxChPhyParaEntry 3}
		
voiceFaxChPhyParaViewSignallingType OBJECT-TYPE
	SYNTAX	INTEGER {
		emTypeIdc (1),
		emTypeIIdc (2),
		emTypeIIIdc (3),
		emTypeIVdc (4),
		emTypeVdc (5),
		emTypeI2280 (6),
		emTypeII2280 (7),
		emTypeIII2280 (8),
		emTypeIV2280 (9),
		emTypeV2280 (10),
		emPulsedDc (11),
		emPulsedIDc (12),
		emPulsedIIDc (13),
		emPulsedIIIDc (14),
		emPulsedIVDc (15),
		emPulseVDc (16),
		ktsRepeated (17),
		ktsInterrupt2by4 (18),
		ktsInterrupt1by2(19),
		opx (20),
		fxsRepeated (21),
		fxsInterrupt2by4 (22),
		fxsInterrupt1by2 (23),
		fxsFaxRepeated (24),
		fxsFaxInterrupt2by4 (25),
		fxsFaxInterrupt1by2 (26),
		fxo (27),
		fxsIterrupted (28),
		faxIterrupted (29)
		}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Indicates the signalling type.  This parameter is computed
			from the strapping and signalling interface configuration." 
	::= {voiceFaxChPhyParaEntry 4}

voiceFaxChPhyParaBergStrapType OBJECT-TYPE
	SYNTAX	INTEGER {
		kts (1),
		em (2),
		opx (3),
		enhancedFxs (4),
		enhancedEm (5),
		enhancedFxo (6),
		standardFxs (7),
		standardEm (8),
		standardFxo (9),
		groundstartFxs (10),
		groundstartFxo (11),
		threePort (12)
		}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Indicates the Berg Hardware Strap configuration on the 
			voice/Fax channel." 
	::= {voiceFaxChPhyParaEntry 5}

voiceFaxChPhyParaSuppression OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"During idle periods in which suppression is enabled (=0),
			the sending voice channel DSP generates suppression service
			frames containing a noise level estimate and no data.  This 
			is sent to the receiving voice channel DSP which will either
			regenerate the noise level or silence the output depending on
			the configuration of voiceFaxChPhyParaBackground." 
	::= {voiceFaxChPhyParaEntry 6}

voiceFaxChPhyParaCfgSignallingType OBJECT-TYPE
	SYNTAX	INTEGER {
		emDc (1),
		em2280Tone (2),
		emWink (3),
		emPulsedDc (4),
		repeated (5),
		interruptOR2by4 (6),
		interrupt1by2 (7)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Configures where the ring signal is generated (locally or by
			remote PBX equipment) and the ring cadence (timing of rings and
			silence periods).  This parameter is ONLY configurable for E&M,
			Enhanced and Standard E&M, 3-Port Fax, and Enhanced and Standard
			FXS voice modules.  The following is a matrix of valid signaling
			type options for each configurable voice module, given the Channel
			Version value (voiceFaxChPhyParaChannelVersion) and Integration
			Unit software release (sysSoftwarePhase).

			Voice Module    Configurable Options     Chan Version   Release
			------------    --------------------     ------------   -------
			E&M             emDC,em2280,emPulseDc         2         ALL

            KTS             repeated,interruptOR2by4,     2         ALL
			                interrupt1by2

			Std/Enh E&M     emDC,em2280,emPulseDC        2,3,5      ALL

			                emDC,em2280,emPulseDC,       6,7        Phase 5.1
			                emWink                                  and Above

			3-Port Fax      repeated,interruptOR2by4     3,5-7      ALL

			Std/Enh FXS     repeated,interruptOR2by4,    2,3        ALL
			                interrupt1by2

			                repeated,interruptOR2by4     5-7        Phase 5.0
			                                                        and Above " 
	::= {voiceFaxChPhyParaEntry 7}

voiceFaxChPhyParaOPXNumberOfRings OBJECT-TYPE
	SYNTAX	INTEGER (1..9)
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"From customer equipment, the number of rings generated
			before hearing the voice/fax module dial tone of the local
			Integration unit.  This parameter is ONLY configurable for
			the OPX, and Standard and Enhanced FXO voice modules." 
	::= {voiceFaxChPhyParaEntry 8}

voiceFaxChPhyParaFXOSupervisorDisc OBJECT-TYPE
	SYNTAX	INTEGER {
		powerInterrupt (1),
		tone (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Configures the means to disconnect a telephone connection.  
			Either an absence of loop current (powerInterrupt) or presence
			of a call progress tone (tone).  Select configuration to match
			customer equipment provisions. This parameter is ONLY configurable
			for the Standard and Enhanced FXO voice modules." 
	::= {voiceFaxChPhyParaEntry 9}

voiceFaxChPhyParaRingFrequency OBJECT-TYPE
	SYNTAX	INTEGER {
		freq25Hz (1),
		freq50Hz (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Selects the ringing frequency to be used to indicate an
			incoming call.  This parameter is ONLY configurable for the KTS,
			3-Port Fax, and Standard and Enhanced FXS voice modules." 
	::= {voiceFaxChPhyParaEntry 10}

voiceFaxChPhyParaJitter OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Maintains continuous voice delivery by compensating (in
			milliseconds) for the transmission delay of packets.  Jitter
			provides compensation for the difference in arrival time and
			expected arrival time of voice packets. (Range is 1 - 200 ms.)
			This parameter is configurable by all voice modules with a
			Channel Version (voiceFaxChPhyParaChannelVersion) equal to '5'
			or higher and residing in an Integration Unit with a software
			release (sysSoftwarePhase) of Phase 5.1 and Above (EXCLUDES 
			the E&M, KTS and OPX voice modules)."
	::= {voiceFaxChPhyParaEntry 11}

voiceFaxChPhyParaDTMFRegeneration OBJECT-TYPE
	SYNTAX	INTEGER {
		disable (1),
		enable (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"DTMF Regeneration, when enabled, ensures that the DTMF
			signals at the remote side are regenerated with a uniform
			on/off time of 100 milliseconds.  This option is only compatible
			with other Voice/Fax modules that configure this parameter.
			This parameter is configurable by all voice modules with a
			Channel Version (voiceFaxChPhyParaChannelVersion) equal to '7'
			or higher and residing in an Integration Unit with a software
			release (sysSoftwarePhase) of Phase 5.1C and Above (EXCLUDES
			the E&M, KTS and OPX voice modules)."
	::= {voiceFaxChPhyParaEntry 12}

voiceFaxChPhyParaAutoLevelEnhancement OBJECT-TYPE
	SYNTAX	INTEGER {
		disable (1),
		enable (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Automatic Level Enhancement, when enabled, boosts weak 
			input signals above the noise threshold to a minimum usable
			level for voice/Fax channels.  This parameter is configurable
			by all voice modules with a Channel Version
			(voiceFaxChPhyParaChannelVersion) equal to '5' or higher and
			residing in an Integration Unit with a software release
			(sysSoftwarePhase) of Phase 5.1 and Above (EXCLUDES the E&M,
			KTS and OPX voice modules)."
	::= {voiceFaxChPhyParaEntry 13}

voiceFaxChPhyParaBackground OBJECT-TYPE
	SYNTAX	INTEGER {
		regenerated (1),
		silence (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"The ability of passing background noise or silencing the
			channel by the receiving voice channel DSP during idle periods.
			First option allows the background noise to be regenerated
			during the periods, whereas the second option suppresses
			background noise during idle periods."
	::= {voiceFaxChPhyParaEntry 14}


voiceFaxChPhyParaDigitizingRate OBJECT-TYPE
	SYNTAX	INTEGER {
		rate4000 (1),
		rate4800 (2),
		rate6400 (3),
		rate7200 (4),
		rate8000 (5),
		rate9600 (6),
		rate12000 (7),
		rate14400 (8),
		rate16000 (9),
		rateG729 (10)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Configures the compression bandwidth for the voice channel. 
			Larger bandwidths produce crisper voice communications.  However,
			larger voice channel bandwidths may result in overbooking of
			composite bandwidth.  The G.729 Digitizing Rate option (rateG729)
			is ONLY configurable for voice modules with a Channel Version
			(voiceFaxChPhyParaChannelVersion) equal to '5' or higher (EXCLUDES 
			the E&M, KTS and OPX voice modules)."
	::= {voiceFaxChPhyParaEntry 15}

voiceFaxChPhyParaBusyMode OBJECT-TYPE
	SYNTAX	INTEGER {
		forcedOff (1),
		forcedOn (2),
		systemControlled (3)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Controls how the busyout state of the channel is determined." 
	::= {voiceFaxChPhyParaEntry 16}

voiceFaxChPhyParaVoiceFaxMode OBJECT-TYPE
	SYNTAX	INTEGER {
		voiceFax (1),
		voiceOnly (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Selects the channel signal processing type, voice/fax or voice only." 
	::= {voiceFaxChPhyParaEntry 17}

voiceFaxChPhyParaBandwidthAssignment OBJECT-TYPE
	SYNTAX	INTEGER {
		voiceActivated (1),
		continuous (2),
		dynamic (3)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Selects how the channel uses link bandwidth: all the
			time (continuous); only when voice/fax signals are present
			(voiceActivated); or dynamically.  Option 3 (dynamic) is not
			valid when the voiceFaxChPhyParaDigitizingRate value is
			configured for G.729." 
	::= {voiceFaxChPhyParaEntry 18}

voiceFaxChPhyParaInputGain OBJECT-TYPE
	SYNTAX	INTEGER (-6..22)
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Selects one of 29 input level gain values in 1 dB increments.
			This range of values applies only to the KTS, OPX, and E&M voice
			modules.  The remaining voice modules utilize the following 'value'
			range: -6 thru 19, inclusive." 
	::= {voiceFaxChPhyParaEntry 19}

voiceFaxChPhyParaChannelType OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Unconfigurable by the User" 
	::= {voiceFaxChPhyParaEntry 20}

voiceFaxChPhyParaEMTypeStrapping OBJECT-TYPE
	SYNTAX	INTEGER {
		emTypeI (1),
		emTypeII (2),
		emTypeIII (3),
		emTypeIV (4),
		emTypeV (5)
		}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Reflects the Ear and Mouth circuit configuration type (I - V)
			on voice/Fax channels that are strapped for E&M." 
	::= {voiceFaxChPhyParaEntry 21}

voiceFaxChPhyParaOutputAttenuation OBJECT-TYPE
	SYNTAX	INTEGER (0..28)
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Selects one out of 29 output level attenuation values in 1 dB 
			increments. 0 = no attenuation, 28 = maximum attenuation.  This
			range of values applies only to the KTS, OPX, and E&M voice modules.
			The remaining voice modules utilize the following 'value' range:
			0 thru 25, inclusive." 
	::= {voiceFaxChPhyParaEntry 22}

voiceFaxChPhyParaPriority OBJECT-TYPE
	SYNTAX	INTEGER {
		high (1),
		low (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Selects the priority of the channel, either high or low." 
	::= {voiceFaxChPhyParaEntry 23}

voiceFaxChPhyParaFaxDigitizingRate OBJECT-TYPE
	SYNTAX	INTEGER {
		useVoiceRate (1),
		rate2400 (2),
		rate4800 (3),
		rate7200 (4),
		rate9600 (5)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Sets the compression bandwidth for voice channel Fax operations." 
	::= {voiceFaxChPhyParaEntry 24}

voiceFaxChPhyParaLineImpedance OBJECT-TYPE
	SYNTAX	INTEGER {
		impedance600Ohms (1),
		complex (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Provides impedance matching choices for the Enhanced FXS,
			Enhanced FXO, and Enhanced E&M 2-wire interface modules. This
			parameter is ONLY configurable for these voice modules.  The
			port impedance of the E&M interface module configured for 4-wire
			operation is fixed at 600 ohms resistive and is not configurable." 
	::= {voiceFaxChPhyParaEntry 25}

voiceFaxChPhyParaMaxOutputLevel OBJECT-TYPE
	SYNTAX	INTEGER {
		level0dBmNominal (1),
		levelPlus7dBm (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Intended to be used for equipment operated outside of U.S.A
			and Canada. This parameter is ONLY configurable for the Enhanced
			E&M voice module.  Option (1) must be selected within the U.S.A 
			(or Canada) if this module was originally configured for 4-wire
			operation and you have just reconfigured it to 2-wire operation.
			Otherwise, the voice/fax module will not operate properly." 
	::= {voiceFaxChPhyParaEntry 26}

voiceFaxChPhyParaRegenerationDelay OBJECT-TYPE
	SYNTAX	INTEGER (1..15)
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"The selected delay, ranging from 1 to 15 seconds,
			compensates for the time it takes for the remote PBX to
			go off-hook and provide a dial tone to the voice channel
			for completing the call setup.  The default is 1 second.
			This parameter is configurable for all voice modules with
			a Channel Version (voiceFaxChPhyParaChannelVersion) greater
			than or equal to '3' (EXCLUDES the E&M, KTS and OPX voice
			modules)."
	::= {voiceFaxChPhyParaEntry 27}

voiceFaxChPhyParaMaxDialDigitTimeLimit OBJECT-TYPE
	SYNTAX	INTEGER (1..15)
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"The dial digit time limit is the maximum time allowed
			for accepting dialed digits.  When the dial digit time
			limit is reached, further digits are not accepted, and the
			accumulated digits are compared to the configured number of
			digits.  If the accumulated number is less than the configured
			number, a fast busy response is generated.  If the accumulated
			number is equal to or more than the configured number, the
			digits are forwarded.  The default is 10 seconds.  This
			parameter is configurable for all voice modules with a Channel
			Version (voiceFaxChPhyParaChannelVersion) greater than or equal
			to '3' (EXCLUDES the E&M, KTS and OPX voice modules)."
	::= {voiceFaxChPhyParaEntry 28}

voiceFaxChPhyParaMaxNumFwdDigits OBJECT-TYPE
	SYNTAX	INTEGER (0..16)
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"The selected value specifies the maximum number of
			dialing digits to be forwarded to the destination voice
			channel without having to wait until the dial digit time
			limit is reached.  The range is 0-16, with zero disabling
			the store and forward feature. The default value is 0.
			This parameter is configurable for all voice modules with
			a Channel Version (voiceFaxChPhyParaChannelVersion) greater
			than or equal to '3' (EXCLUDES the E&M, KTS and OPX voice
			modules)."
	::= {voiceFaxChPhyParaEntry 29}

voiceFaxChPhyParaCallProgressionTones OBJECT-TYPE
	SYNTAX	INTEGER {
		northAmerica (1),
		japan (2),
		unitedKingdom (3),
		germany (4),
		france (5),
		centralAmerica (6),
		chile (7),
		australia (8)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"This option allows you to match the call progress to 
			those of one of eight countries or areas.  This parameter
			is configurable for all voice modules with a Channel Version
			(voiceFaxChPhyParaChannelVersion) greater than or equal to
			'3' (EXCLUDES the E&M, KTS and OPX voice modules)."
	::= {voiceFaxChPhyParaEntry 30}

voiceFaxChPhyParaRegenFormat OBJECT-TYPE
	SYNTAX	INTEGER {
		pulsedDialing (1),
		dtmfDialing (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Regeneration format supports the store and forward function.
			Dial format allows the voice/fax channel to match the outgoing 
			dialing digits format with the format of the interfacing PBX.
			This parameter is configurable for all voice modules with
			a Channel Version (voiceFaxChPhyParaChannelVersion) greater
			than or equal to '3' (EXCLUDES the E&M, KTS and OPX voice
			modules)."
	::= {voiceFaxChPhyParaEntry 31}

voiceFaxChPhyParaChannelVersion OBJECT-TYPE
	SYNTAX	INTEGER {
		phase1and2and2dot5Fconn (1),
		phase3VSW(2),
		phase3dot1UV (3),
		voiceDialingImprovement (4),
		g729DigitizingRateSupported (5),
		phase5dot1 (6),
		dtmfRegne (7),
		casVer2 (8)
		}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the voice/Fax compression algorithm used (installed 
			software version) by the voice/Fax channel." 
	::= {voiceFaxChPhyParaEntry 32}


-- N_CMOS.O_VOCSWI =======================================================

voiceFaxChSwParaTable OBJECT-TYPE
	SYNTAX	SEQUENCE OF VoiceFaxChSwParaEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Table for configuring the voice/Fax channel switching parameters." 
	::= {voice-fax-channel 2}

voiceFaxChSwParaEntry OBJECT-TYPE
	SYNTAX	VoiceFaxChSwParaEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Entry within the table to specify a particular voice/Fax channel number
			on a specific module/card for channel switching configuration." 
	INDEX		{voiceFaxChSwParaCardNumber, voiceFaxChSwParaChannelNumber}
	::= {voiceFaxChSwParaTable 1}

VoiceFaxChSwParaEntry ::= SEQUENCE {
	voiceFaxChSwParaCardNumber		INTEGER,
	voiceFaxChSwParaChannelNumber	INTEGER,
	voiceFaxChSwParaVoiceExtension		DisplayString,
	voiceFaxChSwParaFaxExtension		DisplayString,
	voiceFaxChSwParaAutoCallNumber	DisplayString,
	voiceFaxChSwParaReceiveInhibit		INTEGER,
	voiceFaxChSwParaCallInhibit		INTEGER
	}

voiceFaxChSwParaCardNumber OBJECT-TYPE
	SYNTAX	INTEGER (1..5)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the module/card number for the voice/Fax channel specific
			to the channel's switching parameters." 
	::= {voiceFaxChSwParaEntry 1}

voiceFaxChSwParaChannelNumber OBJECT-TYPE
	SYNTAX	INTEGER (1..6)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the voice/Fax channel specific to the channel's 
			switching parameters." 
	::= {voiceFaxChSwParaEntry 2}

voiceFaxChSwParaVoiceExtension OBJECT-TYPE
	SYNTAX	DisplayString  (SIZE (1..4))
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Defines the voice extension for the voice/fax channel.
			The voice extension must consist of NUMERIC characters
			only; and the number of digits in the extension MUST match
			the value set for the voiceFaxChSwNumberOfDigitsInPhoneNumber
			parameter." 
	::= {voiceFaxChSwParaEntry 3}

voiceFaxChSwParaFaxExtension OBJECT-TYPE
	SYNTAX	DisplayString  (SIZE (1..4))
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Defines the fax extension for the voice/fax channel.
			The fax extension must consist of NUMERIC characters only;
			and the number of digits in the extension MUST match the
			value set for the voiceFaxChSwNumberOfDigitsInPhoneNumber
			parameter.  This parameter is ONLY valid for 3-Port Fax
			modules." 
	::= {voiceFaxChSwParaEntry 4}

voiceFaxChSwParaAutoCallNumber OBJECT-TYPE
	SYNTAX	DisplayString  (SIZE (1..4))
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"This is the extension number of a voice/fax port that is 
			called immediately if this voice/fax port goes off hook. 
			If no entry is made in this field, this function is disabled.
			The Auto Call Number must consist of NUMERIC characters only." 
	::= {voiceFaxChSwParaEntry 5}

voiceFaxChSwParaReceiveInhibit  OBJECT-TYPE
	SYNTAX	INTEGER {
		on (1),
		off (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Option (1) prevents this voice/fax channel from receiving calls.
			The default is Option (2)." 
	::= {voiceFaxChSwParaEntry 6}

voiceFaxChSwParaCallInhibit  OBJECT-TYPE
	SYNTAX	INTEGER{
		on (1),
		off (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Option (1) prevents this voice/fax channel from initiating calls.
			The default is Option (2)." 
	::= {voiceFaxChSwParaEntry 7}


-- N_CMOS.O_VOCSYS ========================================================

voiceFaxChSwNumberOfDigitsInPhoneNumber OBJECT-TYPE
	SYNTAX	INTEGER (1..4)
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Number of digits in a phone number (1 - 4).  Note that all
			Integration units in a network must have the same value for
			this parameter." 
	 ::= {voice-fax-channel 3}

-- DIGITAL VOICE CHANNEL

-- N_CMOS.O_VOICE =========================================================

digitalVoiceChPhyParaTable OBJECT-TYPE
	SYNTAX	SEQUENCE OF DigitalVoiceChPhyParaEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Table for configuring digital voice module (DVM) physical 
			parameters." 
	::= {digital-voice-channel 1}

digitalVoiceChPhyParaEntry OBJECT-TYPE
	SYNTAX	DigitalVoiceChPhyParaEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Entry within the table to specify a particular DVM channel number
			on a specific module/card." 
	INDEX		{digitalVoiceChPhyParaCardNumber, digitalVoiceChPhyParaChannelNumber}
	::= {digitalVoiceChPhyParaTable 1}

DigitalVoiceChPhyParaEntry ::= SEQUENCE {
	digitalVoiceChPhyParaCardNumber			INTEGER,
	digitalVoiceChPhyParaChannelNumber		INTEGER,
	digitalVoiceChPhyParaViewSignallingType		INTEGER,
	digitalVoiceChPhyParaBergStrapType		INTEGER,
	digitalVoiceChPhyParaPortEmulation 		INTEGER,
	digitalVoiceChPhyParaSuppression		INTEGER,
	digitalVoiceChPhyParaJitter			INTEGER,
	digitalVoiceChPhyParaDTMFRegeneration	INTEGER,
	digitalVoiceChPhyParaAutoGainControl		INTEGER,
	digitalVoiceChPhyParaBackground			INTEGER,
	digitalVoiceChPhyParaDigitizingRate		INTEGER,
	digitalVoiceChPhyParaBusyMode			INTEGER,
	digitalVoiceChPhyParaVoiceFaxMode		INTEGER,
	digitalVoiceChPhyParaBandwidthAssignment	INTEGER,
	digitalVoiceChPhyParaInputGain			INTEGER,
	digitalVoiceChPhyParaChannelType		INTEGER,
	digitalVoiceChPhyParaOutputAttenuation		INTEGER,
	digitalVoiceChPhyParaPriority			INTEGER,
	digitalVoiceChPhyParaFaxDigitizingRate		INTEGER,
	digitalVoiceChPhyParaRegenerationDelay		INTEGER,
	digitalVoiceChPhyParaMaxDialDigitTimeLimit	INTEGER,
	digitalVoiceChPhyParaMaxNumFwdDigits		INTEGER,
	digitalVoiceChPhyParaCallProgressionTones	INTEGER,
	digitalVoiceChPhyParaRegenerationFormat		INTEGER,
	digitalVoiceChPhyParaCompander			INTEGER,
	digitalVoiceChPhyParaPremiumVoice 		INTEGER,
	digitalVoiceChPhyParaModuleIdentification		INTEGER,
	digitalVoiceChPhyParaChannelVersion		INTEGER
	}

digitalVoiceChPhyParaCardNumber OBJECT-TYPE
	SYNTAX	INTEGER (2..5)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the module/card number for the DVM channel specific
			to the channel's physical parameters." 
	::= {digitalVoiceChPhyParaEntry 1}

digitalVoiceChPhyParaChannelNumber OBJECT-TYPE
	SYNTAX	INTEGER (1..12)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the DVM channel specific to the channel's 
			physical parameters." 
	::= {digitalVoiceChPhyParaEntry 2}

digitalVoiceChPhyParaViewSignallingType OBJECT-TYPE
	SYNTAX	INTEGER {
		tieTrunk (1),
		transparent (2),
		dc5bInvert (3),
		tieInvert (4),
		dc5b (5),
		casVers01 (6),
		r2byQdot421 (7)
		}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Indicates the DVM signalling type.  Note that Options 3, 5, 6,
			and 7 are only valid for E1 interfaces." 
	::= {digitalVoiceChPhyParaEntry 3}

digitalVoiceChPhyParaBergStrapType OBJECT-TYPE
	SYNTAX	INTEGER {
		digitalVoiceModule (1)
		}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Indicates the Berg Hardware Strap configuration which is
			a constant value for DVM channels." 
	::= {digitalVoiceChPhyParaEntry 4}

digitalVoiceChPhyParaPortEmulation OBJECT-TYPE
	SYNTAX	INTEGER {
		tieTrunk (1),
		tieInvert (2),
		dc5bInvert (3),
		transparent (4),
		dc5b (5),
		casVers01 (6),
		r2byQdot421 (7)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Indicates the DVM port emulation.  Note that Options 3, 5,
			6, and 7 are only valid for E1 interfaces.  Options 3, 6, and 7
			are only configurable for DVMs with a Channel Version of '6' or
			greater, and which are running in a CCM software release environment
			of Phase 5.1 or greater." 
	::= {digitalVoiceChPhyParaEntry 5}

digitalVoiceChPhyParaSuppression OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"During idle periods in which suppression is enabled (=0),
			the sending DVM channel DSP generates suppression service
			frames containing a noise level estimate and no data.  This 
			is sent to the receiving DVM channel DSP which will either
			regenerate the noise level or silence the output depending on
			the configuration of digitalVoiceChPhyParaBackground." 
	::= {digitalVoiceChPhyParaEntry 6}

digitalVoiceChPhyParaJitter OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Maintains continuous voice delivery by compensating (in
			milliseconds) for the transmission delay of packets.  Jitter
			provides compensation for the difference in arrival time and
			expected arrival time of DVM packets. (Range is 1 - 200 ms.).
			This parameter is configurable for DVMs with a Channel Version
			(voiceFaxChPhyParaChannelVersion) equal to '6' or higher, and
			residing in an Integration Unit with a software release
			(sysSoftwarePhase) of Phase 5.1 or Above."
	::= {digitalVoiceChPhyParaEntry 7}

digitalVoiceChPhyParaDTMFRegeneration OBJECT-TYPE
	SYNTAX	INTEGER {
		disable (1),
		enable (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"DTMF Regeneration, when enabled, ensures that the DTMF
			signals at the remote side are regenerated with a uniform
			on/off time of 100 milliseconds.  This option is only compatible
			with other Voice/Fax modules that configure this parameter.
			This parameter is configurable DVMs with a Channel Version
			(voiceFaxChPhyParaChannelVersion) of '7' or greater and residing
			in an Integration Unit with a software release (sysSoftwarePhase)
			of Phase 5.1C and Above."
	::= {digitalVoiceChPhyParaEntry 8}

digitalVoiceChPhyParaAutoGainControl OBJECT-TYPE
	SYNTAX	INTEGER {
		disable (1),
		enable (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Automatic Gain Control, when enabled, boosts weak 
			input signals above the noise threshold to a minimum 
			usable level for DVM channels." 
	::= {digitalVoiceChPhyParaEntry 9}

digitalVoiceChPhyParaBackground OBJECT-TYPE
	SYNTAX	INTEGER {
		regenerated (1),
		silence (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"The ability of passing background noise or silencing the channel 
			by the receiving DVM channel DSP during idle periods. First option
			allows the background noise to be regenerated during the periods, 
			whereas the second option suppresses background noise during 
			idle periods."
	::= {digitalVoiceChPhyParaEntry 10}


digitalVoiceChPhyParaDigitizingRate OBJECT-TYPE
	SYNTAX	INTEGER {
		rate4000 (1),
		rate4800 (2),
		rate6400 (3),
		rate7200 (4),
		rate8000 (5),
		rate9600 (6),
		rate12000 (7),
		rate14400 (8),
		rate16000 (9),
		rateG729 (10)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Configures the compression bandwidth for the DVM channel. 
			Larger bandwidths produce crisper voice communications. 
			However, larger DVM channel bandwidths may result in overbooking 
			of composite bandwidth.  The G.729 Digitizing Rate option
			(rateG729) is ONLY configurable for DVMs with a Channel Version
			(voiceFaxChPhyParaChannelVersion) of '5' or greater."
	::= {digitalVoiceChPhyParaEntry 11}

digitalVoiceChPhyParaBusyMode OBJECT-TYPE
	SYNTAX	INTEGER {
		forcedOff (1),
		forcedOn (2),
		systemControlled (3)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Controls how the busyout state of the DVM channel is determined." 
	::= {digitalVoiceChPhyParaEntry 12}

digitalVoiceChPhyParaVoiceFaxMode OBJECT-TYPE
	SYNTAX	INTEGER {
		voiceFax (1),
		voiceOnly (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Selects the DVM channel signal processing type, voice/fax or 
			voice only." 
	::= {digitalVoiceChPhyParaEntry 13}

digitalVoiceChPhyParaBandwidthAssignment OBJECT-TYPE
	SYNTAX	INTEGER {
		voiceActivated (1),
		continuous (2),
		dynamic (3)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Selects how the DVM channel uses link bandwidth: all the time 
			(continuous);only when voice/fax signals are present 
			(voiceActivated); or dynamically.  Option 3 not valid for G.729 
			value of digitalVoiceChPhyParaDigitizingRate." 
	::= {digitalVoiceChPhyParaEntry 14}

digitalVoiceChPhyParaInputGain OBJECT-TYPE
	SYNTAX	INTEGER (-6..8)
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Selects one of 15 input level gain values in 1 dB increments.
			The range is from -6 to 8, inclusive." 
	::= {digitalVoiceChPhyParaEntry 15}

digitalVoiceChPhyParaChannelType OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Unconfigurable by the User" 
	::= {digitalVoiceChPhyParaEntry 16}

digitalVoiceChPhyParaOutputAttenuation OBJECT-TYPE
	SYNTAX	INTEGER (0..8)
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Selects one out of 9 DVM output level attenuation values in 1 dB 
			increments. The range is 0 - 8, where =0 is no attenuation and =8 is
			 maximum attenuation." 
	::= {digitalVoiceChPhyParaEntry 17}

digitalVoiceChPhyParaPriority OBJECT-TYPE
	SYNTAX	INTEGER {
		high (1),
		low (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Selects the priority of the DVM channel, either high or low." 
	::= {digitalVoiceChPhyParaEntry 18}

digitalVoiceChPhyParaFaxDigitizingRate OBJECT-TYPE
	SYNTAX	INTEGER {
		useVoiceRate (1),
		rate2400 (2),
		rate4800 (3),
		rate7200 (4),
		rate9600 (5)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Sets the compression bandwidth for DVM channel Fax operations." 
	::= {digitalVoiceChPhyParaEntry 19}

digitalVoiceChPhyParaRegenerationDelay OBJECT-TYPE
	SYNTAX	INTEGER (1..15)
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"The selected delay, ranging from 1 to 15 seconds, compensates 
			for the time it takes for the remote PBX to go off-hook and 
			provide a dial tone to the DVM channel for completing the 
			call setup.  The default is 1 second." 
	::= {digitalVoiceChPhyParaEntry 20}

digitalVoiceChPhyParaMaxDialDigitTimeLimit OBJECT-TYPE
	SYNTAX	INTEGER (1..15)
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"The DVM dial digit time limit is the maximum time allowed for 
			accepting dialed digits.  When the dial digit time limit is 
			reached, further digits are not accepted, and the accumulated 
			digits are compared to the configured number of digits.  If the 
			accumulated number is less than the configured number, a fast 
			busy response is generated.  If the accumulated number is equal 
			to or more than the configred number, the digits are forwarded.
			The default is 10 seconds." 
	::= {digitalVoiceChPhyParaEntry 21}

digitalVoiceChPhyParaMaxNumFwdDigits OBJECT-TYPE
	SYNTAX	INTEGER (0..16)
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"The selected value specifies the maximum number of dialing 
			digits to be forwarded by the DVM to the destination voice 
			channel without having to wait until the DVM dial digit time limit 
			is reached.  The range is 0-16, with zero disabling the store 
			and forward feature. The default value is 0." 
	::= {digitalVoiceChPhyParaEntry 22}

digitalVoiceChPhyParaCallProgressionTones OBJECT-TYPE
	SYNTAX	INTEGER {
		northAmerica (1),
		japan (2),
		unitedKingdom (3),
		germany (4),
		france (5),
		centralAmerica (6),
		chile (7),
		australia (8)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"This option allows you to match the DVM channel call progress 
			to those of one of eight countries or areas." 
	::= {digitalVoiceChPhyParaEntry 23}

digitalVoiceChPhyParaRegenerationFormat OBJECT-TYPE
	SYNTAX	INTEGER {
		pulsedDialing (1),
		dtmfDialing (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"DVM regeneration format supports the store 
			and forward function.  Designed for E&M and FXO interfaces,
			regeneration format allows the DVM channel to match the outgoing 
			dialing digits format with the format of the interfacing PBX." 
	::= {digitalVoiceChPhyParaEntry 24}

digitalVoiceChPhyParaCompander OBJECT-TYPE
	SYNTAX	INTEGER {
		uLaw (1),
		aLaw (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Option (1), uLaw, should be selected as the standard t1 
			compander format.  Option (2), aLaw, should be selected as 
			the standard e1 compander format.  Note that there is a rare
			mismatch to t1 equipment that uses an aLaw compander format." 
	::= {digitalVoiceChPhyParaEntry 25}

digitalVoiceChPhyParaPremiumVoice OBJECT-TYPE
	SYNTAX	INTEGER {
		disable (1),
		enable (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Option (2) allows transparent pass-through of uncompressed
			voice frames between DVM ports.  This option is used when two
			Integration unit voice ports may connect through the same PBX.
			Option (1) is used for normal operation in which calls always
			terminate in a telephone set.  This parameter is configurable
			for DVMs with a Channel Version (voiceFaxChPhyParaChannelVersion)
			equal to '5' or higher."
	::= {digitalVoiceChPhyParaEntry 26}

digitalVoiceChPhyParaModuleIdentification OBJECT-TYPE
	SYNTAX	INTEGER {
		t1 (1),
		e1 (2),
		invalid (3)
		}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies whether the DVM interfaces with a t1 vs. an e1 card." 
	::= {digitalVoiceChPhyParaEntry 27}

digitalVoiceChPhyParaChannelVersion OBJECT-TYPE
	SYNTAX	INTEGER {
		digitalVoiceModule (1),
		g729DigitizingRateSupported (2),
		phase5dot1 (3),
		dtmfRegne (4),
		casVer2 (5)
		}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the compression algorithm used (installed software 
			version) by the DVM channel." 
	::= {digitalVoiceChPhyParaEntry 28}


