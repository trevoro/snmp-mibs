-- Proxy Object Group

pxyVersionNumber OBJECT-TYPE
	SYNTAX	DisplayString
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the version number of the proxy agent installed." 
	 ::= { proxy 1}

pxyCfgProxySwitch OBJECT-TYPE
	SYNTAX	INTEGER {
		enable (1),
