-- PORT GROUP

portCfgTable OBJECT-TYPE
	SYNTAX	SEQUENCE OF PortCfgEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Table to establish the type of port connection to be assigned to
			the selected port." 
	::= {port 1}

portCfgEntry OBJECT-TYPE
	SYNTAX	PortCfgEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Entry into the table for the selected port." 
	INDEX		{portCfgCardNumber, portCfgChannelNumber}
	::= {portCfgTable 1}

PortCfgEntry ::= SEQUENCE {
	portCfgCardNumber		INTEGER,
	portCfgChannelNumber		INTEGER,
	portCfgChannelType		INTEGER
	}

portCfgCardNumber OBJECT-TYPE
	SYNTAX	INTEGER (1..5)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Indicates the card/module of the channel to be configured." 
	::= {portCfgEntry 1}

portCfgChannelNumber OBJECT-TYPE
	SYNTAX	INTEGER (1..6)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Indicates the channel/port number to be configured." 
	::= {portCfgEntry 2}

portCfgChannelType OBJECT-TYPE
	SYNTAX	INTEGER {
		mux (1),
		async (2),
		sync (3),
		esi (4),
		x21 (5),
		localEsi (6),
		esiSecondary (7),
		frameRelay (8)
		}
	ACCESS	write-only
	STATUS	mandatory
	DESCRIPTION	"Assigns the type of connection to the selected port.
			Note:  A node reset is required after setting the port
			configuration.  Also note that if a channel is currently
			connected, forced or switch connected, the channel must first
			be disconnected.  The procedure to do this would be to place
			the channel out of service (chControlCommand disable(2)), then
			disconnect (chControlCommand forceDIsconnect(4)) the channel." 
	::= {portCfgEntry 3}


syncPortCfgTable OBJECT-TYPE
	SYNTAX	SEQUENCE OF SyncPortCfgEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Table to configure the protocol for the specified sync channel." 
	::= {port 2}

syncPortCfgEntry OBJECT-TYPE
	SYNTAX	SyncPortCfgEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Entry into the table specifying a particular channel on a particular
			module/card." 
	INDEX		{syncPortCfgCardNumber, syncPortCfgChannelNumber}
	::= {syncPortCfgTable 1} 

SyncPortCfgEntry ::= SEQUENCE {
	syncPortCfgCardNumber			INTEGER,
	syncPortCfgChannelNumber		INTEGER,
	syncPortCfgProtocol			INTEGER
	}

syncPortCfgCardNumber OBJECT-TYPE
	SYNTAX	INTEGER (1..5)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the card/module of the sync channel to be configured." 
	::= {syncPortCfgEntry 1}

syncPortCfgChannelNumber OBJECT-TYPE
	SYNTAX	INTEGER (1..6)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Indicates the sync channel number to be configured." 
	::= {syncPortCfgEntry 2}

syncPortCfgProtocol OBJECT-TYPE
	SYNTAX	INTEGER {
		dlcChannel (1),
		asciiBisync (2),
		ebcdicBisync (3),
		hpSync (4),
		rts (5),
		syncPad (6),
		micomDlc (7),
		micomVoice (8),
		fastPacket (9),
		tdm (10)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Assigns the sync protocol to the selected port. Note: A node reset is
			required after the protocol is configured." 
	::= {syncPortCfgEntry 3}


