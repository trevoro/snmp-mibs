		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"To enable or disable the proxy agent operation on the
			router module." 
	 ::= {proxy 2}

pxyAddressMapTableNumber OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"The number of entries in the address map table. This will give 
			 the highest numbered entry allocated."
	::= {proxy 3}

pxyAddressMapTable OBJECT-TYPE
	SYNTAX	SEQUENCE OF PxyAddressMapEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"A table of all the MICOM Nodes this proxy is responsible for."
	::= {proxy 4}

pxyAddressMapEntry OBJECT-TYPE
	SYNTAX	PxyAddressMapEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"An entry that describes the network node which this proxy
			agent manages."
	INDEX		{ pxyAddressMapNodeNumber }
	::= {pxyAddressMapTable 1}

PxyAddressMapEntry ::= SEQUENCE {
	pxyAddressMapNodeNumber		INTEGER,
	pxyAddressMapEntryStatus		INTEGER,
	pxyAddressMapNodeNameAndCommunity	DisplayString,
	pxyAddressMapChangeNodeNumberTo	INTEGER
	}

pxyAddressMapNodeNumber OBJECT-TYPE
	SYNTAX	INTEGER 
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"The node number used to communicate with the device associated 
			 with this entry."
	::= {pxyAddressMapEntry 1}

pxyAddressMapEntryStatus OBJECT-TYPE
	SYNTAX	INTEGER {
		active (1),
		notInService (2),
		notReady (3),
		createAndGo (4),
		createAndWait (5),
		destroy (6)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"A code representing the current state of  the table entry. This is 
			provided to define relationship within the table entry and to delete 
			and/or add devics to the Address Table. Default is notReady "
	::= {pxyAddressMapEntry 2}


pxyAddressMapNodeNameAndCommunity OBJECT-TYPE
	SYNTAX	DisplayString  (SIZE (1..8))
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"The node name used to communicate with the device associated with
			 this entry. This unique name is also used to de-multiplex the proxy 
			managed nodes."
	::= {pxyAddressMapEntry 3}

pxyAddressMapChangeNodeNumberTo OBJECT-TYPE
	SYNTAX	INTEGER 
	ACCESS	write-only
	STATUS	mandatory
	DESCRIPTION	"In order to change a node number of an existing node you must
			use this entry with the NodeNumber entry being the existing node
			number."
	::= {pxyAddressMapEntry 4}




pxySupportedVersionTable OBJECT-TYPE
	SYNTAX	SEQUENCE OF PxySupportedVersionEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"A table of sotware revision numbers this proxy agent support."
