            -- control of these parameters, and omitted in systems
            -- that do not.

            bootControl OBJECT IDENTIFIER ::= { xSystem 34 }


            bootControlLoadInternetFile OBJECT-TYPE
                SYNTAX DisplayString (SIZE (0..64))
                ACCESS read-write
                STATUS mandatory
                DESCRIPTION
