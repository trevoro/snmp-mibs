sxTrunkLinkCountTrap TRAP-TYPE
    ENTERPRISE sigma
    VARIABLES {
    sxtrapSeverity,
    sxtrunkLinkCount
    }
    DESCRIPTION
        "The number of ports in the trunking group has changed."
    ::= 15

sxDiagUnitBootedTrap TRAP-TYPE
    ENTERPRISE  sigma
