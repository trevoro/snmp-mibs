    ::= 32

sxhwDiagTrap TRAP-TYPE
    ENTERPRISE  sigma
    VARIABLES   {
        sxtrapSeverity,
        sxhwDiagCode
    }
    DESCRIPTION
        "Sent whenever the unit's diagnostics have indicated an error."
    ::= 100

