drSmonEntityPlacementTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF DrSmonEntityPlacementEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "A table containging a set of tables. 	Each table reports the 
	placment of entities in the counter matrix/table of a
	particular module. Entities could be IP hosts, IP subnets, or other 
	3rd layer groups specified in IP-address/SubnetMask format, for which 
	the device is capable of assigning an entry in its counter 
	matrix/table."
    ::= { drSmonConfiguration 2 }
drSmonEntityPlacementEntry OBJECT-TYPE
    SYNTAX      DrSmonEntityPlacementEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "A conceptual row in the drSmonEntityPlacementTable. Reports the index
	of a entity in the device's counter marix/table. 
	Reports the creator of this entity - filter table or device's internal 
	auto-learn process."     
    INDEX { drSmonEntityPlacementModuleID, drSmonEntityPlacementIndex }
    ::= { drSmonEntityPlacementTable 1 }

DrSmonEntityPlacementEntry ::= SEQUENCE {
    drSmonEntityPlacementModuleID        	Integer32,
    drSmonEntityPlacementIndex    		Integer32,
    drSmonEntityPlacementAddress    		IpAddress,
    drSmonEntityPlacementMask    		IpAddress,
    drSmonEntityPlacementType			INTEGER
}

drSmonEntityPlacementModuleID OBJECT-TYPE
SYNTAX     Integer32 (0..255)
MAX-ACCESS not-accessible
STATUS current
DESCRIPTION
	"The module group number (=slot number). Used as index."
	::= { drSmonEntityPlacementEntry 1}

drSmonEntityPlacementIndex OBJECT-TYPE
SYNTAX     Integer32 (1..2147483647)
MAX-ACCESS not-accessible
STATUS current
DESCRIPTION
	"The placment index of the particular entity. 
	The index determines the position of the entity in the counter 
	matrix/table.
	Example:
	For a matrix of A rows x B columns and index i:
	If i<A then 
		host is both on row i and column i
	else 
		host is on column i.
        The agent must provide entries in this table for all the 
        index values in the range of 1..(drSmonControlMatrixRows-1)
        Row 0 of a matrix/table is reserved for 'Others' entity, and is not
        reported in this MIB table."
	::= { drSmonEntityPlacementEntry 2}

drSmonEntityPlacementAddress OBJECT-TYPE
SYNTAX     IpAddress
MAX-ACCESS read-only
STATUS current
DESCRIPTION
	"The IP address of an entity  to explicitly monitor"
	::= { drSmonEntityPlacementEntry 3}
drSmonEntityPlacementMask OBJECT-TYPE
SYNTAX     IpAddress
MAX-ACCESS read-only
STATUS current
DESCRIPTION
	"The subnet mask of an entity. The value 
	255.255.255.255 signifies that this entity is a host."
	::= { drSmonEntityPlacementEntry 4}

drSmonEntityPlacementType OBJECT-TYPE
SYNTAX      INTEGER {
                    empty(1),
                    autoLearn(2),
                    filter(3)
                }
MAX-ACCESS read-only
STATUS current
DESCRIPTION
	"The Type of this entity:
	empty(1) - Contains no relevant information. Not yet allocated to
	a specific entity.
	autoLearn(2) -  The entity was learned by the auto-learn process of  
			the device. 
	filter(3) - The entity was created by a relevant filter entry in 
	the filter table. "
	::= { drSmonEntityPlacementEntry 5}
-- *****************************************************
drSmonProtocolDir OBJECT IDENTIFIER ::= { drSmon 2}
-- A group mirroring RMON-II protocol-dir group, in a 
-- per-module fashion. This group is supported only in EZ2route mode.
--
-- Protocol Directory Group
--
-- Lists the inventory of protocols the probe has the capability of
-- monitoring and allows the addition, deletion, and configuration of
-- entries in this list.

drSmonProtocolDirLCTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF  DrSmonProtocolDirLCEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "A table with protocolDirLastChange per module."
    ::= { drSmonProtocolDir 1 }

drSmonProtocolDirLCEntry OBJECT-TYPE
    SYNTAX       DrSmonProtocolDirLCEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "A conceptual row in the drSmonProtocolDirLastChangeTable. Contains
	information for a particular module."
    INDEX { 	 drSmonProtocolDirLCModuleID }
    ::= { drSmonProtocolDirLCTable  1 }

DrSmonProtocolDirLCEntry ::= SEQUENCE {
    drSmonProtocolDirLCModuleID        	Integer32,
    drSmonProtocolDirLCLastChange           TimeStamp
    }
	 
drSmonProtocolDirLCModuleID OBJECT-TYPE
SYNTAX     Integer32 (0..255)
MAX-ACCESS not-accessible
STATUS current
DESCRIPTION
	"The module group number (=slot number). Used as index."
	::= { drSmonProtocolDirLCEntry 1}

drSmonProtocolDirLCLastChange OBJECT-TYPE
    SYNTAX      TimeStamp
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Same function as RFC 2021 protocolDirLastChange, 
	but for a particular module."
    ::= { drSmonProtocolDirLCEntry 2 }


drSmonProtocolDirTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF DrSmonProtocolDirEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This table contain a set of protocolDirTables. Each protocolDirTable 		lists the protocols that a particular  module  has the
        capability to decode and count.  There is one entry in the per-module
        table for each such protocol.  These protocols represent
        different network layer, transport layer, and higher-layer
        protocols."
    ::= { drSmonProtocolDir 2 }

drSmonProtocolDirEntry OBJECT-TYPE
    SYNTAX      DrSmonProtocolDirEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "A conceptual row in the drSmonProtocolDirTable. Represent a protocol to
	monitor on a particulat module. All fields except for 
	drSmonProtocolDirModuleID have the same function as in RFC 2021
	protocolDirTable.
         An example of the indexing of this entry is
         drSmonProtocolDirLocalIndex.3.8.0.0.0.1.0.0.8.0.2.0.0, which is a 
	 protocol for module 3 with the
         encoding of a length of 8, followed by 8 subids encoding the
         drSmonProtocolDirID of 1.2048, followed by a length of 2 and the
         2 subids encoding zero-valued parameters."
    INDEX { drSmonProtocolDirModuleID,drSmonProtocolDirID, drSmonProtocolDirParameters }
    ::= { drSmonProtocolDirTable  1 }

DrSmonProtocolDirEntry ::= SEQUENCE {
    drSmonProtocolDirModuleID        	Integer32,
    drSmonProtocolDirID                   OCTET STRING,
    drSmonProtocolDirParameters           OCTET STRING,
    drSmonProtocolDirLocalIndex           Integer32,
    drSmonProtocolDirDescr                DisplayString,
    drSmonProtocolDirType                 BITS,
    drSmonProtocolDirAddressMapConfig     INTEGER,
    drSmonProtocolDirHostConfig           INTEGER,
    drSmonProtocolDirMatrixConfig         INTEGER,
    drSmonProtocolDirOwner                OwnerString,
    drSmonProtocolDirStatus               RowStatus
}
drSmonProtocolDirModuleID OBJECT-TYPE
SYNTAX     Integer32 (0..255)
MAX-ACCESS not-accessible
STATUS current
DESCRIPTION
	"The module group number (=slot number). Used as index."
	::= { drSmonProtocolDirEntry 1}

drSmonProtocolDirID OBJECT-TYPE
    SYNTAX      OCTET STRING
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "A unique identifier for a particular protocol.  
	same as RFC 2021, but for a particular module."
    ::= { drSmonProtocolDirEntry 2 }

drSmonProtocolDirParameters OBJECT-TYPE
    SYNTAX      OCTET STRING
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "A set of parameters for the associated drSmonProtocolDirID.
	Same as RFC 2021, but for a particular module."
    ::= { drSmonProtocolDirEntry 3 }

drSmonProtocolDirLocalIndex OBJECT-TYPE


    SYNTAX      Integer32 (1..2147483647)
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The locally arbitrary, but unique identifier associated
        with this drSmonProtocolDir entry.
	Same as RFC 2021, but for a particular module."
    ::= { drSmonProtocolDirEntry 4 }

drSmonProtocolDirDescr OBJECT-TYPE
    SYNTAX      DisplayString (SIZE (1..64))
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "A textual description of the protocol encapsulation.
	Same as RFC 2021, but for a particular module."
    ::= { drSmonProtocolDirEntry 5 }

drSmonProtocolDirType OBJECT-TYPE
    SYNTAX      BITS 
		{
                    extensible(0),
                    addressRecognitionCapable(1)
                }
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "This object describes 2 attributes of this protocol
         directory entry.
	Same as RFC 2021, but for a particular module."
    ::= { drSmonProtocolDirEntry 6 }

drSmonProtocolDirAddressMapConfig OBJECT-TYPE
    SYNTAX      INTEGER {
                    notSupported(1),
                    supportedOff(2),
                    supportedOn(3)
                }
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "This object describes and configures the probe's support for
        address mapping for this protocol.  
	Same as RFC 2021, but for a particular module."

    ::= { drSmonProtocolDirEntry 7 }

drSmonProtocolDirHostConfig OBJECT-TYPE
    SYNTAX      INTEGER {
                    notSupported(1),
                    supportedOff(2),
                    supportedOn(3)
                }
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "This object describes and configures the probe's support for
        the network layer and application layer host tables for this
        protocol.  
	Same as RFC 2021, but for a particular module."
    ::= { drSmonProtocolDirEntry 8 }

drSmonProtocolDirMatrixConfig OBJECT-TYPE
    SYNTAX      INTEGER {
                    notSupported(1),
                    supportedOff(2),
                    supportedOn(3)
                }
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "This object describes and configures the probe's support for
        the network layer and application layer matrix tables for this
        protocol.  
	Same as RFC 2021, but for a particular module."
    ::= { drSmonProtocolDirEntry 9 }

drSmonProtocolDirOwner OBJECT-TYPE
    SYNTAX      OwnerString
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The entity that configured this entry and is
        therefore using the resources assigned to it."
    ::= { drSmonProtocolDirEntry 10 }

drSmonProtocolDirStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The status of this protocol directory entry.
	Same as RFC 2021, but for a particular module."
    ::= { drSmonProtocolDirEntry 11 }
-- *****************************************************
drSmonFilter	OBJECT IDENTIFIER ::= { drSmon 3}
-- A group controling the SMON2 filtering table(s)

drSmonFilterTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF DrSmonFilterEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "A table containging a set of tables. Each table configure hosts and 
	subnets to explicitly monitor in a particular module. "
    ::= { drSmonFilter 1 }

drSmonFilterEntry OBJECT-TYPE
    SYNTAX      DrSmonFilterEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "A conceptual row in the drSmonFilterTable. Configures a host or 
	subnet to monitor in a particular module, at a particular row or 
	column. Typically, the NMS application conifgures these entries.
	"
    INDEX { drSmonFilterModuleID, drSmonFilterIndex }
    ::= { drSmonFilterTable 1 }

DrSmonFilterEntry ::= SEQUENCE {
    drSmonFilterModuleID        	Integer32,
    drSmonFilterIndex    		Integer32,
    drSmonFilterAddress    		IpAddress,
    drSmonFilterMask    		IpAddress,
    drSmonFilterStatus			RowStatus
}

drSmonFilterModuleID OBJECT-TYPE
SYNTAX     Integer32 (0..255)
MAX-ACCESS not-accessible
STATUS current
DESCRIPTION
	"The module group number (=slot number). Used as index."
	::= { drSmonFilterEntry 1}

drSmonFilterIndex OBJECT-TYPE
SYNTAX     Integer32 (1..2147483647)
MAX-ACCESS not-accessible
STATUS current
DESCRIPTION
	"The index of the particular host/subnet. The index determines the 
	position of the host/subnet in the counter matrix.
	Example:
	For a matrix of A rows x B columns and index i:
	If i<A then 
		host is both on row i and column i
	else 
		host is on column i."
	::= { drSmonFilterEntry 2}

drSmonFilterAddress OBJECT-TYPE
SYNTAX     IpAddress
MAX-ACCESS read-create
STATUS current
DESCRIPTION
	"The IP address of a host or subnet to explicitly monitor"
	::= { drSmonFilterEntry 3}
drSmonFilterMask OBJECT-TYPE
SYNTAX     IpAddress
MAX-ACCESS read-create
STATUS current
DESCRIPTION
	"The Mask of a host or subnet to explicitly monitor. The value 
	255.255.255.255 signifies that this entry is a host entry."
	::= { drSmonFilterEntry 4}

drSmonFilterStatus OBJECT-TYPE
SYNTAX     RowStatus
MAX-ACCESS read-create
STATUS current
DESCRIPTION
	"The status of the row, as per SMIv2 (RFC1903). The row is in 
	read-only mode when the status is set to active(1). Only active rows 
	are used as filters."
	::= { drSmonFilterEntry 5}
-- *****************************************************
drSmonActiveApplications OBJECT IDENTIFIER ::= { drSmon 4 }
-- This group contains information on the protocols detected by the modules
-- In the protocol distribution mode. It is similar in concept to
-- the xsActiveApplications group for the MMLS, with the following
-- differences:
-- a. Distribution accross modules
-- b. Support for both a TCP and a UDP port range, as well as other non TCP/UDP
-- protocols
drSmonActiveApplicationsTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF DrSmonActiveApplicationsEntry
    MAX-ACCESS  not-accessible
    STATUS  current
    DESCRIPTION
       "This table contain the 'active applications' or protocols from all
       the protocols the modules can detect. 'Activity' is determined and
       reported by each module, in a sub-table of this table.
       The table contain entries only for 'Active' protocols.
       For each active application, there is a global counter indicates
       the number of packets in the specific application monitored
       by a module."
::= { drSmonActiveApplications 1 }

drSmonActiveApplicationsEntry OBJECT-TYPE
    SYNTAX  DrSmonActiveApplicationsEntry
    MAX-ACCESS  not-accessible
    STATUS  current
    DESCRIPTION
       "The entry for the drSmonActiveApplicationsTable.
       A set of entries exist for each module.
       For each active application,  a global counter indicates
       the number of packets in the specific application monitored
       by the module. Entries exist only for 'active' applications."
       INDEX   { drSmonActiveApplicationsModuleID,drSmonActiveApplicationsType,
       		drSmonActiveApplicationsSubType }
    ::= { drSmonActiveApplicationsTable  1 }

DrSmonActiveApplicationsEntry ::= SEQUENCE {
        drSmonActiveApplicationsModuleID	Integer32,
        drSmonActiveApplicationsType	INTEGER,
        drSmonActiveApplicationsSubType	Integer32,
        drSmonActiveApplicationsPkts	Counter32
}

drSmonActiveApplicationsModuleID OBJECT-TYPE
SYNTAX     Integer32 (0..255)
MAX-ACCESS not-accessible
STATUS current
DESCRIPTION
	"The module group number (=slot number). Used as index."
	::= { drSmonActiveApplicationsEntry  1 }

drSmonActiveApplicationsType OBJECT-TYPE
SYNTAX    INTEGER {
	ethertype(1),
	ipProtocol(2),
	udpProtocol(3),
	tcpProtocol(4)
               }
MAX-ACCESS not-accessible
STATUS current
DESCRIPTION
	"The major type of the application. Note that TCP and UDP are 
	assumed to be 
	over IP. "
	::= { drSmonActiveApplicationsEntry  2 }

drSmonActiveApplicationsSubType OBJECT-TYPE
    SYNTAX  Integer32
    MAX-ACCESS  not-accessible
    STATUS  current
    DESCRIPTION
       "The application's sub-type value depends on the value of the type:
       Type             sub-type value 
       ===========      ==============
       ethertype(1)     1 - Sum of all IP traffic
                        2 - ARP traffic
       ipProtocol(2)    1 - ICMP traffic
                        2 - OSPF traffic
                        3 - Sum of all UDP traffic
                        4 - Sum of all TCP traffic
			5 - VRRP traffic
       udpProtocol(3)     - The SubType is the UDP port number
       tcpProtocol(4)     - The SubType is the TCP port number

       "
    ::= { drSmonActiveApplicationsEntry  3 }

drSmonActiveApplicationsPkts OBJECT-TYPE
     SYNTAX  Counter32
     MAX-ACCESS  read-only
     STATUS  current
     DESCRIPTION
        "The number of packets of the specified application 
        monitored by the probe."
     ::= { drSmonActiveApplicationsEntry  4 }


END

