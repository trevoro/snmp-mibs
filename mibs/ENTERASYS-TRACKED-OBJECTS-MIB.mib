IMPORTS
    MODULE-IDENTITY, OBJECT-TYPE, Counter32, Unsigned32, Gauge32, 
    TimeTicks
        FROM SNMPv2-SMI
    RowStatus, TruthValue
        FROM SNMPv2-TC
    MODULE-COMPLIANCE, OBJECT-GROUP
        FROM SNMPv2-CONF
