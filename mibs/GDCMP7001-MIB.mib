--
-- ****************************************************************************
--                          mib definitions begin
-- ****************************************************************************

GDCMP7001-MIB DEFINITIONS ::= BEGIN

IMPORTS
	 enterprises	FROM RFC1155-SMI
	 OBJECT-TYPE    FROM RFC-1212
	 DisplayString  FROM RFC1213-MIB
	 SCinstance     FROM GDCMACRO-MIB;

gdc           	 	OBJECT IDENTIFIER ::= { enterprises 498 }
dsx1           		OBJECT IDENTIFIER ::= { gdc 6 }
mp7001System   		OBJECT IDENTIFIER ::= { dsx1 10 }

mp7001Version          	OBJECT IDENTIFIER ::= { mp7001System 1 }
mp7001Maintenance     	OBJECT IDENTIFIER ::= { mp7001System 2 }
mp7001Configuration	OBJECT IDENTIFIER ::= { mp7001System 3 }
mp7001Diagnostics	OBJECT IDENTIFIER ::= { mp7001System 4 }
mp7001Performance	OBJECT IDENTIFIER ::= { mp7001System 5 }
mp7001AlarmConfig      	OBJECT IDENTIFIER ::= { mp7001System 6 }
mp7001SysConfig		OBJECT IDENTIFIER ::= { mp7001System 7 } 
 
-- This MIB module uses the extended OBJECT-TYPE macro as
-- defined in RFC 1212.

-- The implementation of all groups is mandatory for all products that
-- support a T1 interface.

-- Note:  All rows in all tables defined within this MIB module are
-- virtual.  They can neither be created or deleted by management
-- operations.

-- ****************************************************************************


mp7001MIBversion OBJECT-TYPE
	SYNTAX DisplayString (SIZE (5))
     	ACCESS read-only
     	STATUS mandatory
     	DESCRIPTION
	"Identifies the version of the MIB. The format of the version
	is x.yzT, where 'x' identifies the major revision number, 'y'
	identifies the minor revision number, 'z' identifies the
	typographical revision, and T identifies the test revision.
	Acceptable values for the individual revision components are
	as follows:

		 x: 1 - 9
		 y: 0 - 9
		 z: 0 - 9
		 T: A - Z

	Upon formal release, no designation for the test revision will
	be present."
     	::= { mp7001Version 1 }


--
-- ****************************************************************************
--                                Version Table
-- ****************************************************************************

mp7001VersionTable OBJECT-TYPE
	SYNTAX SEQUENCE OF Mp7001VersionEntry
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION " "
	::= { mp7001Version 2  }

mp7001VersionEntry OBJECT-TYPE
	SYNTAX Mp7001VersionEntry
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION
	"An entry in the GDC Mp7001System Version Table."
	INDEX  { mp7001VersionIndex }
	::= { mp7001VersionTable 1 }

Mp7001VersionEntry ::=
	SEQUENCE {
		  mp7001VersionIndex
			 SCinstance,
		  mp7001FirmwareRev
			 DisplayString,
		  mp7001CardType
			 DisplayString,
		  mp7001BootRev
			 DisplayString
	}

mp7001VersionIndex OBJECT-TYPE
	SYNTAX SCinstance
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
    	"Integer value which uniquely identifies the MP7001 to which this
    	entry is applicable. SCinstance is defined to be ssllddcc where:
    	ss (byte value) - physical shelf slot location (01-32)
    	ll (byte value) - line number (01-128)
    	dd (byte value) - drop number (00-31)
    	cc (byte value) - channel or interface number (always 00)"
	::= { mp7001VersionEntry 1 }

mp7001FirmwareRev 	OBJECT-TYPE
	SYNTAX DisplayString (SIZE(2))
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
	"The version number of the firmware, to allow products
  	to know which revision is installed.  The released
	version number is sequenced from --, A-, ... AA, ... ZZ."
	::= { mp7001VersionEntry 2 }

mp7001CardType	OBJECT-TYPE
	SYNTAX DisplayString (SIZE(0..16))
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
	"This variable is used to determine the type
	of card family installed ex. MP7001."
	::= { mp7001VersionEntry 3 }

mp7001BootRev 	OBJECT-TYPE
	SYNTAX DisplayString (SIZE(2))
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
	"The version number of the boot firmware, to allow products
	to know which revision is installed.  The released version
	number is sequenced from --, A-, ... AA, ... ZZ.  "
	::= { mp7001VersionEntry 4 }


-- 
-- ********************************************************************
-- GDC 7001 Maintenance Table
-- ********************************************************************

mp7001MaintenanceTable OBJECT-TYPE
	SYNTAX SEQUENCE OF Mp7001MaintenanceEntry
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION
	"The table describes the maintenance objects for MP 7001." 
	::= { mp7001Maintenance 1  }

mp7001MaintenanceEntry OBJECT-TYPE
	SYNTAX Mp7001MaintenanceEntry
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION
	"An entry in the GDC 701 Maintenance Table."
	INDEX  { mp7001MaintenanceLineIndex }
	::= { mp7001MaintenanceTable 1 }

Mp7001MaintenanceEntry ::=
	SEQUENCE {
		  mp7001MaintenanceLineIndex
			 SCinstance,
		  mp7001SoftReset
			 INTEGER,
		  mp7001SysUpTime
			 INTEGER,
		  mp7001DefaultInit
			 INTEGER,
		  mp7001ResetStats
			 INTEGER,
		  mp7001LedStatus
			 OCTET STRING,
		  mp7001T1CircuitName
			 DisplayString,
		  mp7001SetRealTime
			 INTEGER,
		  mp7001AlarmStatus
			 OCTET STRING,
		  mp7001SystemTimingGenStatus
			 INTEGER,
		  mp7001StatLastInitialized
			 INTEGER,
		  mp7001ValidIntervals
			 INTEGER
	}

mp7001MaintenanceLineIndex OBJECT-TYPE
	SYNTAX SCinstance
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
    	"Integer value which uniquely identifies the MP7001 to which this
    	entry is applicable. SCinstance is defined to be ssllddcc where:
    	ss (byte value) - physical shelf slot location (01-32)
    	ll (byte value) - line number (01-128)
    	dd (byte value) - drop number (00-31)
    	cc (byte value) - channel or interface number (always 00)"
	::= { mp7001MaintenanceEntry 1 }

mp7001SoftReset		OBJECT-TYPE
	SYNTAX INTEGER {
			 norm(1),
			 reset(2)
	   	       }
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
	"Supports the action of soft resetting the unit.  When this
	object is set to reset(2), then the unit performs a soft
	reset, whose meaning is specific to the type of unit being
	managed. The value of norm(1) will be returned when 
	the reset is complete.
	The value of norm(1) can not be set by management."
	::= { mp7001MaintenanceEntry 2 }

mp7001SysUpTime       	OBJECT-TYPE
	SYNTAX INTEGER (1..2147483647)
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
	"This variable is used to report the elapsed system tick
	time for conversion to real time at the controller and is
	not related to the sysUpTime referenced in MIB-II.
	Upon power-up of the unit, the elapsed time is cleared.
	The elapsed time counter rolls over upon reaching the
	maximum count."
	::= { mp7001MaintenanceEntry 3 }

mp7001DefaultInit     	OBJECT-TYPE
	SYNTAX INTEGER  {
			 normal(1),
			 factoryDefault(2)
			 }
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
	"This is used to allow the NonVolatile Configuration to 
	be set to a factory default state.  When this value is set to
	factoryDefault(2) the unit will perform  a reset to make 
	the default configuration take affect. 
	The value of normal(1) will be returned when the initialization
	is complete.
	The value of normal(1) can not be set by management."
	::= { mp7001MaintenanceEntry 4 }

mp7001ResetStats      	OBJECT-TYPE
	SYNTAX  INTEGER        {
			 norm(1),
			 reset(2)
		 }
	ACCESS  read-write
	STATUS  mandatory
	DESCRIPTION
	"Supports the action of soft resetting the mp7001ValidIntervals
	object.  When this object is set to reset(2), then the unit
	will reset the mp7001ValidIntervals object to zero.
	The value of norm(1) can not be set by management."
	::= { mp7001MaintenanceEntry 5 }

mp7001LedStatus 	OBJECT-TYPE
	SYNTAX  OCTET STRING (SIZE(2))
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	"Returns a bitwise snapshot of the front panel LED state.
		 Octet 1 bit 7 - not used
			 bit 6 - ON
			 bit 5 - INSV
			 bit 4 - RSP
			 bit 3 - TMG
			 bit 2 - future use
			 bit 1 - future use
			 bit 0 - future use

		 Octet 2 bit 7 - not used
			 bit 6 - future use
			 bit 5 - NTWK AIS
			 bit 4 - NTWK LCV
			 bit 3 - NTWK LOS
			 bit 2 - NTWK OOF
			 bit 1 - ALM
			 bit 0 - TM "
	::= { mp7001MaintenanceEntry 6 }

mp7001T1CircuitName	OBJECT-TYPE
	SYNTAX  DisplayString (SIZE(16))
	ACCESS  read-write
	STATUS  mandatory
	DESCRIPTION
	"Circuit name T1 is connected to."
	::= { mp7001MaintenanceEntry 7 }

mp7001SetRealTime      	OBJECT-TYPE
	SYNTAX  INTEGER (1..2147483647)
	ACCESS  read-write
	STATUS  mandatory
	DESCRIPTION
	"Number of seconds from midnight Dec 31, 1969."
	::= {mp7001MaintenanceEntry 8}

mp7001AlarmStatus	OBJECT-TYPE
	SYNTAX OCTET STRING (SIZE(4))
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
	"Returns a bit-wise Status of alarms
		 Octet 1 bit 7 - not used
			 bit 6 - NTWK LOS
			 bit 5 - NTWK LOF
			 bit 4 - NWTK_AIS
			 bit 3 - future use
			 bit 2 - NTWK Received YELLOW
			 bit 1 - NTWK Excessive Zeros - not supported
			 bit 0 - NTWK Low Average Density - not supported

		 Octet 2 bit 7 - not used
			 bit 6 - NTWK BiPolar Violations
			 bit 5 - NTWK CRC Errors
			 bit 4 - NTWK Controlled Slips
			 bit 3 - Unavailable Signal State
			 bit 2 - Status Change
			 bit 1 - Timing Loss
			 bit 0 - Unit Failure

		 Octet 3 bit 7 - not used
			 bit 6 - future use
			 bit 5 - future use
			 bit 4 - future use
			 bit 3 - future use
			 bit 2 - future use
			 bit 1 - future use
			 bit 0 - future use
		
		 Octet 4 bit 7 - not used
			 bit 6 - ES
			 bit 5 - BES
			 bit 4 - SES
			 bit 3 - UAS
			 bit 2 - Unsolicited Test
			 bit 1 - NVRAM checksum corrupted
			 bit 0 - Power-up Alarm "
	::= {mp7001MaintenanceEntry 9}

mp7001SystemTimingGenStatus	OBJECT-TYPE
	SYNTAX INTEGER {
			none(1),
			clk8khz(2),
			clk4mhz(3),
			clk8khzand4mhz(4)
	}
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
	"Indicates if the units configured for primary or secondary system clock are
	actually supplying clock and if it is the 8hkz, 4mhz, or both."
	::= {mp7001MaintenanceEntry 10}

mp7001StatLastInitialized      	OBJECT-TYPE
	SYNTAX  INTEGER (1..2147483647)
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	"Number of seconds from midnight Dec 31, 1969 up until
        mp7001ValidIntervals was last initialized to zero"
	::= {mp7001MaintenanceEntry 11}

mp7001ValidIntervals OBJECT-TYPE
	SYNTAX  INTEGER (1..96)
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	"The number of  previous  intervals  for  which
	valid data was collected.  The value will be 96
	unless the interface was brought on-line within
	the last 24 hours, in which case the value will
	be the number of complete 15  minute  intervals
	the since interface has been online."
	::= {mp7001MaintenanceEntry 12}

--
-- ********************************************************************
-- The GDC 7001 Configuration Table
-- ********************************************************************

mp7001ConfigTable OBJECT-TYPE
     SYNTAX SEQUENCE OF Mp7001ConfigEntry
     ACCESS not-accessible
     STATUS mandatory
     DESCRIPTION
	 "The GDC Configuration table."
     ::= { mp7001Configuration 1 }

mp7001ConfigEntry OBJECT-TYPE
     SYNTAX Mp7001ConfigEntry
     ACCESS not-accessible
     STATUS mandatory
     DESCRIPTION
	 "The GDC Configuration table entry."
     INDEX { mp7001ConfigIndex }
     ::= { mp7001ConfigTable 1 }

Mp7001ConfigEntry ::= 
     SEQUENCE {
	 mp7001ConfigIndex SCinstance,
	 mp7001NetworkInterfaceType INTEGER,
	 mp7001TransmitClockSource INTEGER,
	 mp7001FallbackClockSource INTEGER,
	 mp7001PreEqualization INTEGER,
	 mp7001Framing INTEGER,
	 mp7001LineCoding INTEGER,
         mp7001LineBuildout INTEGER,
         mp7001FDLMode INTEGER,
         mp7001FramingMode INTEGER,
         mp7001Loopback INTEGER,
         mp7001AISLoopdown INTEGER,
         mp7001SignalMode INTEGER,
	 mp7001LineBuildoutCtrl INTEGER
     	 }

mp7001ConfigIndex 	OBJECT-TYPE
     	 SYNTAX SCinstance
     	 ACCESS read-only
     	 STATUS mandatory
     	 DESCRIPTION
    	"Integer value which uniquely identifies the MP7001 to which this
    	entry is applicable. SCinstance is defined to be ssllddcc where:
    	ss (byte value) - physical shelf slot location (01-32)
    	ll (byte value) - line number (01-128)
    	dd (byte value) - drop number (00-31)
    	cc (byte value) - channel or interface number (always 00)"
         ::= { mp7001ConfigEntry 1 }

mp7001NetworkInterfaceType   	OBJECT-TYPE
	 SYNTAX INTEGER {
			ds1(1),
			dsx1(2)
		        }
	 ACCESS read-write
	 STATUS mandatory
	 DESCRIPTION
		"Sets the transmit range to DSX-1 short or DS-1 long "
	 ::= { mp7001ConfigEntry 2 }

mp7001TransmitClockSource OBJECT-TYPE
     	 SYNTAX INTEGER {
			system(1),
			recovered(2),
			internal(3)
     	 		}
     	 ACCESS read-write
     	 STATUS mandatory
     	 DESCRIPTION
		"Sets the transmit clock source."
     	 ::= { mp7001ConfigEntry 3 }

mp7001FallbackClockSource OBJECT-TYPE
     	 SYNTAX INTEGER {
			system(1),
			recovered(2),
			internal(3)
			}
     	 ACCESS read-write
     	 STATUS mandatory
    	 DESCRIPTION
	 "Identifies the fallback transmit clock source."

    	 ::= { mp7001ConfigEntry 4 }

mp7001PreEqualization OBJECT-TYPE
     	 SYNTAX INTEGER {
			none(1),
	 		feet130(2),                    
	 		feet260(3),
	 		feet390(4),
         		feet530(5),
         		feet655(6)
     			}
     	 ACCESS read-write
    	 STATUS mandatory
     	 DESCRIPTION
		"Pre-Equalization distance"
     	 ::= { mp7001ConfigEntry 5 }

mp7001Framing OBJECT-TYPE
	 SYNTAX INTEGER {
			eSF(1),
			d4(2)
			}
	 ACCESS read-write
	 STATUS mandatory
	 DESCRIPTION
		"Set the framing for the interface.
		 Abbreviations are as follows:
		 eSF - Extended Super Frame
		 d4  -  Super Frame"
	 ::= {mp7001ConfigEntry 6 }

mp7001LineCoding OBJECT-TYPE
	 SYNTAX INTEGER {
			b8zs(1),
			ami(2)
			}
	 ACCESS read-write
	 STATUS mandatory
	 DESCRIPTION
		"Sets the line coding for the interface."
	 ::= {mp7001ConfigEntry 7 }
 
mp7001LineBuildout OBJECT-TYPE
	SYNTAX INTEGER  {
			dB0(1),
			dB75(2),
                        dB15(3),
			dB22(4)
			}
	 ACCESS read-write
	 STATUS mandatory
	 DESCRIPTION
		"Sets the line build out for the interface
                         0, -7.5, -15 or -22.5dB"
	 ::= {mp7001ConfigEntry 8 }
 
mp7001FDLMode OBJECT-TYPE
	 SYNTAX INTEGER {
			none(1),
			aNSIT1403(2),
                        tR54016(3)
			}
	 ACCESS read-write
	 STATUS mandatory
	 DESCRIPTION
		"Sets the Facility Data Link Mode."
	 ::= {mp7001ConfigEntry 9 }
 
mp7001FramingMode OBJECT-TYPE
	 SYNTAX INTEGER {
			auto(1),
			manual(2)
			}
	 ACCESS read-write
	 STATUS mandatory
	 DESCRIPTION
		"Sets the framing mode"
	 ::= {mp7001ConfigEntry 10 }

mp7001Loopback OBJECT-TYPE
	 SYNTAX INTEGER {
			inhibitloop(1),
			payloadloop(2),
			lineloop(3)
			}
	 ACCESS read-write
	 STATUS mandatory
	 DESCRIPTION
		"Sets the loopback type"
	 ::= {mp7001ConfigEntry 11 }

mp7001AISLoopdown OBJECT-TYPE
	 SYNTAX INTEGER (4..60)
	 ACCESS read-write
	 STATUS mandatory
	 DESCRIPTION
		"Sets AIS unloop time where 4 = inhibit,
                         5 thru 60 = number of seconds"
 	 ::= {mp7001ConfigEntry 12 }

mp7001SignalMode OBJECT-TYPE
	 SYNTAX INTEGER {
			none(1),
			robbedbit(2)
			}
	 ACCESS read-write
	 STATUS mandatory
	 DESCRIPTION
		"Sets signalling mode"
	 ::= {mp7001ConfigEntry 13 }

mp7001LineBuildoutCtrl OBJECT-TYPE
	 SYNTAX INTEGER {
			auto(1),
			manual(2)
			}
	 ACCESS read-write
	 STATUS mandatory
	 DESCRIPTION
		"Sets the line buildout control"
	 ::= {mp7001ConfigEntry 14 }
 
--
-- ********************************************************************
-- The GDC 7001 System Configuration Table
-- ********************************************************************

mp7001SysTimingTable OBJECT-TYPE
     SYNTAX SEQUENCE OF Mp7001SysTimingEntry
     ACCESS not-accessible
     STATUS mandatory
     DESCRIPTION
	 "The GDC System Timing table."
     ::= { mp7001SysConfig 1 }

mp7001SysTimingEntry OBJECT-TYPE
     SYNTAX Mp7001SysTimingEntry
     ACCESS not-accessible
     STATUS mandatory
     DESCRIPTION
	 "The GDC Configuration table entry."
     INDEX { mp7001SysTimingIndex }
     ::= { mp7001SysTimingTable 1 }

Mp7001SysTimingEntry ::= 
     SEQUENCE 	{
		mp7001SysTimingIndex SCinstance,
		mp7001SysTimingGen INTEGER
		}

mp7001SysTimingIndex OBJECT-TYPE
     SYNTAX SCinstance
     ACCESS read-only
     STATUS mandatory
     DESCRIPTION
    	"Integer value which uniquely identifies the MP7001 to which this
    	entry is applicable. SCinstance is defined to be ssllddcc where:
    	ss (byte value) - physical shelf slot location (01-32)
    	ll (byte value) - line number (01-128)
    	dd (byte value) - drop number (00-31)
    	cc (byte value) - channel or interface number (always 00)"
     ::= { mp7001SysTimingEntry 1 }

mp7001SysTimingGen     OBJECT-TYPE 
	SYNTAX INTEGER {
			none(1),
			primary(2),
			secondary(3)
			}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"Selects if the unit provides system timing.
		none(1) - does not provide Timing Generator
		primary(2) - unit is primary Timing Generator for the shelf.
		secondary(3) - unit is secondary Timing Generator for the 
			shelf.

		Only 1 unit in the shelf can the primary or secondary 
		timing generator. All other must be none."

	::= {mp7001SysTimingEntry 2}

--
-- *********************************************************************
-- The GDC 7001 DCC Configuration Table
-- *********************************************************************

mp7001DCCConfigurationTable OBJECT-TYPE
     SYNTAX SEQUENCE OF Mp7001DCCConfigurationEntry
     ACCESS not-accessible
     STATUS mandatory
     DESCRIPTION
	 "The GDC DCC Configuration table. These are Diagnostic Channel 
	  configuration options."
     ::= { mp7001Configuration 2}

mp7001DCCConfigurationEntry OBJECT-TYPE
     SYNTAX Mp7001DCCConfigurationEntry
     ACCESS not-accessible
     STATUS mandatory
     DESCRIPTION
	 "The GDC Diagnostic Configuration table entry."
     INDEX { mp7001DCCConfigurationIndex }
     ::= { mp7001DCCConfigurationTable 1 }

Mp7001DCCConfigurationEntry ::= 
     SEQUENCE {
	mp7001DCCConfigurationIndex SCinstance,
	mp7001TimeSlot INTEGER,
	mp7001Bandwidth INTEGER
	}

mp7001DCCConfigurationIndex OBJECT-TYPE
     SYNTAX SCinstance
     ACCESS read-only
     STATUS mandatory
     DESCRIPTION
    	"Integer value which uniquely identifies the MP7001 to which this
    	entry is applicable. SCinstance is defined to be ssllddcc where:
    	ss (byte value) - physical shelf slot location (01-32)
    	ll (byte value) - line number (01-128)
    	dd (byte value) - drop number (00-31)
    	cc (byte value) - channel or interface number (always 00)"
     ::= { mp7001DCCConfigurationEntry 1 }

mp7001TimeSlot OBJECT-TYPE
	SYNTAX INTEGER(1..24)
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"Time Slot DCC goes in"
	::={ mp7001DCCConfigurationEntry 2}

mp7001Bandwidth OBJECT-TYPE
	SYNTAX INTEGER {
		kbps8(1),
		kbps56(2),
		kbps64(3),
		notAssigned(4)
		}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"Bandwidth to use:
			8 kbps - bit 7 only
			56 kbps - bits 0 - 6
			64 kbps - all 8 bits"
	::={ mp7001DCCConfigurationEntry 3}

--
-- ********************************************************************
-- The GDC 7001 T1 Highway Assignment Table
-- ********************************************************************

mp7001HighwayAssignTable OBJECT-TYPE
     SYNTAX SEQUENCE OF Mp7001HighwayAssignEntry
     ACCESS not-accessible
     STATUS mandatory
     DESCRIPTION
	 "The GDC Highway/Timeslot Assignment Table."
     ::= { mp7001SysConfig 2}

mp7001HighwayAssignEntry OBJECT-TYPE
     SYNTAX Mp7001HighwayAssignEntry
     ACCESS not-accessible
     STATUS mandatory
     DESCRIPTION
	 "The GDC T1 Highway Assign table entry."
     INDEX { mp7001HighwayAssignIndex }
     ::= { mp7001HighwayAssignTable 1 }

Mp7001HighwayAssignEntry ::= 
     SEQUENCE {
	 mp7001HighwayAssignIndex SCinstance,
	 mp7001StartTimeSlot INTEGER,
	 mp7001NumberOfTimeSlots INTEGER,
	 mp7001BundleDestination INTEGER,
	 mp7001ExecuteAssign INTEGER
	}
 
mp7001HighwayAssignIndex OBJECT-TYPE
     SYNTAX SCinstance
     ACCESS read-only
     STATUS mandatory
     DESCRIPTION
    	"Integer value which uniquely identifies the MP7001 to which this
    	entry is applicable. SCinstance is defined to be ssllddcc where:
    	ss (byte value) - physical shelf slot location (01-32)
    	ll (byte value) - line number (01-128)
    	dd (byte value) - drop number (00-31)
    	cc (byte value) - channel or interface number (always 00)"
     ::= { mp7001HighwayAssignEntry 1 }

mp7001StartTimeSlot    OBJECT-TYPE
	SYNTAX  INTEGER (1..24)
	ACCESS  read-write
	STATUS  mandatory
	DESCRIPTION
		"Starting time slot of the bundle. If only 1 bundle is
		being assigned, or the first of 2 is being assigned, 
		it must start at 1. The second of 2 must start at the next
		available time slot after the first bundle."
	::= {mp7001HighwayAssignEntry 2 }

mp7001NumberOfTimeSlots        OBJECT-TYPE
	SYNTAX  INTEGER (1..24)
	ACCESS  read-write
	STATUS  mandatory
	DESCRIPTION
		"Number of time slots assigned to the bundle."
	::= {mp7001HighwayAssignEntry 3 }

mp7001BundleDestination      OBJECT-TYPE
	SYNTAX  INTEGER {
			notAssigned(1),
			highway1(2),
			highway2(3),
			highway3(4),
			highway4(5)
			}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"Defines the destination of the bundle."
	::= {mp7001HighwayAssignEntry 4 }

mp7001ExecuteAssign OBJECT-TYPE
	SYNTAX  INTEGER {
			normal(1),
			execute(2)
			}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"Causes the MP7001 to execute configuration of the assigned 
		 highway configuration. This object should be sent in a separate 
		 PDU after acknowlegement of the assign PDU(s) has been received."
	::= {mp7001HighwayAssignEntry 5 }


