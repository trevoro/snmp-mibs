			INTEGER,	
		apFwVer
			DisplayString,
		apDSchannel
			INTEGER
       	}

       	apAPMacAddr OBJECT-TYPE
               	SYNTAX PhysAddress
        	ACCESS read-only
                STATUS mandatory
                DESCRIPTION
                     	"Mac Address of this Known AP."
              	::= { apKnownAPsEntry 1 }

       	apAPIpAddr  OBJECT-TYPE
             	SYNTAX IpAddress 
               	ACCESS read-only
                STATUS mandatory
                DESCRIPTION
                     	"IP Address of this Known AP."
