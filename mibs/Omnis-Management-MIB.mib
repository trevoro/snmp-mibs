        SYNTAX  DisplayString
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION "The name of  the current OMNIS"
        ::= {sniOmnisGlobalData 6}
omnisGlobalDataTable       OBJECT-TYPE
        SYNTAX  SEQUENCE OF OmnisGlobalDataEntry
        ACCESS  not-accessible
        STATUS  mandatory
        DESCRIPTION "The OMNIS GlobalData-Table"
        ::= {sniOmnisGlobalData 7}
omnisGlobalDataEntry        OBJECT-TYPE
        SYNTAX  OmnisGlobalDataEntry
        ACCESS  not-accessible
        STATUS  mandatory
        DESCRIPTION "An entry in the table"
        INDEX {omnisGlobalDataOmnID}
        ::= {omnisGlobalDataTable 1}
OmnisGlobalDataEntry ::=
        SEQUENCE
        {
            omnisGlobalDataOmnID              INTEGER,
            omnisGlobalDataVersion            DisplayString,
            omnisGlobalDataOmnName            DisplayString,
            omnisGlobalDataState              INTEGER
        }
omnisGlobalDataOmnID               OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "OMNIS ID"
        ::= {omnisGlobalDataEntry 1}
omnisGlobalDataVersion             OBJECT-TYPE
        SYNTAX  DisplayString
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "OMNIS version"
        ::= {omnisGlobalDataEntry 2}
omnisGlobalDataOmnName             OBJECT-TYPE
        SYNTAX  DisplayString
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "OMNIS name"
        ::= {omnisGlobalDataEntry 3}
omnisGlobalDataState               OBJECT-TYPE
        SYNTAX  INTEGER {
                          open        (1),
                          close       (2)
                        }
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION "OMNIS state"
        ::= {omnisGlobalDataEntry 4}
