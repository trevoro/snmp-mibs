	-- the NCM Advanced T1 Port Configuration Table

	ncmadvancedT1ConfigTable OBJECT-TYPE
	SYNTAX  SEQUENCE OF NcmAdvancedT1ConfigEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
	"The ncmadvanced T1 Configuration table."
        --DEFAULT test-function    quad_test
        --DEFAULT next-function    quad_next
	 ::= { ncm-quad 11004 }


	ncmadvancedT1ConfigEntry OBJECT-TYPE
	SYNTAX  NcmAdvancedT1ConfigEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
	"An entry in the ncmadvanced T1 Configuration table."
	 INDEX   { ncmadvancedT1LineIndex }
	 ::= { ncmadvancedT1ConfigTable 1 }


	NcmAdvancedT1ConfigEntry ::=
	 SEQUENCE {
	     ncmadvancedT1LineIndex
		 INTEGER,
	     ncmportIdentifier
		 INTEGER,
	     ncmfdlMode
		 INTEGER,
	     ncmfdlStandard
		INTEGER,
	     ncmfdlPerformanceReport
		INTEGER,
             ncmfdlLBEnableDisable
                INTEGER,
             ncmfdlLLBT1BOPMsg
                INTEGER,
             ncmfdlPLBT1BOPMsg
                INTEGER,
	     ncmfdlIdlePattern
		INTEGER,
	     ncmfdlMonitoringCsuType
		INTEGER
		}



	ncmadvancedT1LineIndex OBJECT-TYPE
	SYNTAX  INTEGER (1..'7fffffff'h)
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	"The index value which uniquely identifies  the
	 interface  to which this entry is applicable."

	::= { ncmadvancedT1ConfigEntry 1 }


	ncmportIdentifier OBJECT-TYPE
	SYNTAX INTEGER {

		       port-1(0),
		       port-2(1),
		       port-3(2),
		       port-4(3)

	       }
	  ACCESS read-write
	  STATUS mandatory
	  DESCRIPTION
	  "The variable specifies the port identifier and port_3 and
	  port_4 are for Quad T1 only."

	::= { ncmadvancedT1ConfigEntry 2 }


	ncmfdlMode OBJECT-TYPE
	SYNTAX INTEGER {

		       terminated(0),
		       pass(1)

	       }
	  ACCESS read-write
	  STATUS mandatory
	  DESCRIPTION
	  "The variable specifies the ncmfdl mode which is bit 0 of  ncmfdl
	  Parameter1."

	::= { ncmadvancedT1ConfigEntry 3 }

	ncmfdlStandard OBJECT-TYPE
	SYNTAX INTEGER {

		       reserved(0),
		       type-54016(4),
		       type-T1-403(8),
		       type-both(12)

	       }
	  ACCESS read-write
	  STATUS mandatory
	  DESCRIPTION
	  "The variable specifies the ncmfdl standard which is bits 1
	  and bit 2 of ncmfdl Parameter1."

	::= { ncmadvancedT1ConfigEntry 4 }

	ncmfdlPerformanceReport OBJECT-TYPE
	SYNTAX INTEGER {

		       disable(0),
		       user(32),
		       telco(64)


	       }
	  ACCESS read-write
	  STATUS mandatory
	  DESCRIPTION
	  "The variable specifies the ncmfdl standard which is bits 3
	  and bit 4 of ncmfdl Parameter1."

	::= { ncmadvancedT1ConfigEntry 5 }


          ncmfdlLBEnableDisable OBJECT-TYPE
          SYNTAX INTEGER {

                       disabled(0),
                       enabled(1)

	       }
	  ACCESS read-write
	  STATUS mandatory
	  DESCRIPTION
          "The variable specifies the Disable/Enable of the ncmfdl Loopback
           which is bit 0 of the ncmfdl_loop"

           ::= { ncmadvancedT1ConfigEntry 6 }


          ncmfdlLLBT1BOPMsg OBJECT-TYPE
          SYNTAX INTEGER {

                       disabled(0),
                       enabled(2)

	       }
	  ACCESS read-write
	  STATUS mandatory
	  DESCRIPTION
          "The variable specifies the Disable/Enable of the send/receive of
           the LLB T1.403 BOP Messages. It is the bit 1 of the ncmfdl_loop"

           ::= { ncmadvancedT1ConfigEntry 7 }


          ncmfdlPLBT1BOPMsg OBJECT-TYPE
          SYNTAX INTEGER {

                       disabled(0),
                       enabled(4)

	       }
	  ACCESS read-write
	  STATUS mandatory
	  DESCRIPTION
          "The variable specifies the Disable/Enable of the send/receive of
           the PLB T1.403 BOP Messages. It is the bit 2 of the ncmfdl_loop"

           ::= { ncmadvancedT1ConfigEntry 8 }


	ncmfdlIdlePattern OBJECT-TYPE
	SYNTAX INTEGER {

		       all-ones(0),
		       flags(1)

	       }
	  ACCESS read-write
	  STATUS mandatory
	  DESCRIPTION
	  "The variable specifies the ncmfdl idle pattern which is bit 0 of ncmfdl
	  Parameter2."

	::= { ncmadvancedT1ConfigEntry 9 }


	ncmfdlMonitoringCsuType OBJECT-TYPE
	SYNTAX INTEGER {

		       polling(0),
		       no-polling(4),
		       unsolicited-messages(8)


	       }
	  ACCESS read-write
	  STATUS mandatory
	  DESCRIPTION
	  "The variable specifies the ncmfdl standard which is bits 1
	  and bit 2 of ncmfdl Parameter2."

	::= { ncmadvancedT1ConfigEntry 10 }


