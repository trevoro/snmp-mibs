    SYNTAX  SEQUENCE OF LserTapeEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION  "A table of cartridges in the library"
::= { lserInventory 2 }

lserTapeEntry OBJECT-TYPE
    SYNTAX  LserTapeEntry
    ACCESS  not-accessible
    STATUS  mandatory  
    DESCRIPTION  "A cartridge"
    INDEX { lserTapeIndex }
::= { lserTapeTable 1 }

LserTapeEntry ::= SEQUENCE
{
