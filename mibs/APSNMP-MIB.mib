    APSNMP-MIB DEFINITIONS ::= BEGIN
    IMPORTS
	TRAP-TYPE FROM RFC-1215
        enterprises
            FROM RFC1155-SMI
        OBJECT-TYPE
            FROM RFC-1212
        DisplayString
            FROM RFC1213-MIB;

    brand                      OBJECT IDENTIFIER ::= { enterprises 3716       }
    windowsNT                  OBJECT IDENTIFIER ::= { brand 1                }

    apolloStatsForNT           OBJECT IDENTIFIER ::= { windowsNT 1            }
    apolloThresholdForNT       OBJECT IDENTIFIER ::= { windowsNT 2            }
	apolloTrapsForNT           OBJECT IDENTIFIER ::= { windowsNT 3            }

	statsForPort               OBJECT IDENTIFIER ::= { apolloStatsForNT 1     }
	statsForCalls              OBJECT IDENTIFIER ::= { apolloStatsForNT 2     } 
	statsForSession            OBJECT IDENTIFIER ::= { apolloStatsForNT 3     }
	statsForData               OBJECT IDENTIFIER ::= { apolloStatsForNT 4     }
	statsForError              OBJECT IDENTIFIER ::= { apolloStatsForNT 5     }
	statsForDriver             OBJECT IDENTIFIER ::= { apolloStatsForNT 6     }
	statsForCompression        OBJECT IDENTIFIER ::= { apolloStatsForNT 7     }

	statsSessionForGeneral     OBJECT IDENTIFIER ::= { statsForSession  1     }
	statsSessionForApollo      OBJECT IDENTIFIER ::= { statsForSession  2     }
	statsSessionForPPP	       OBJECT IDENTIFIER ::= { statsForSession  3     }

	thresholdForPort           OBJECT IDENTIFIER ::= { apolloThresholdForNT 1 }
	thresholdForCalls          OBJECT IDENTIFIER ::= { apolloThresholdForNT 2 }
	thresholdForSession        OBJECT IDENTIFIER ::= { apolloThresholdForNT 3 }
