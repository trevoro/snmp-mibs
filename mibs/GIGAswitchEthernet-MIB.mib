            ResourceId,
         gsEBufferFabricPort
            ResourceId,
         gsEBufferFabricPortDirection
            INTEGER,
         gsEBufferSwitchPort
            ResourceId,
         gsEBufferMemory
            INTEGER,
         gsEBufferAgeTimer
            INTEGER,
         gsEBufferPriorityServicing
            INTEGER,
         gsEBufferPriorityAllocation
            INTEGER,
         gsEBufferPriorityThreshold
            INTEGER,
         gsEBufferCongestion
            INTEGER,
         gsEBufferHighOverflowDrops
            Counter,
         gsEBufferLowOverflowDrops
            Counter,
