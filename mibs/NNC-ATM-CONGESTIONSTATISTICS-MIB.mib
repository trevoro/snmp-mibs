-- nncAtmCongStatsLongIntvlTable --
------------------------------------
  
  nncAtmCongStatsLongIntvlTable OBJECT-TYPE 
        SYNTAX      SEQUENCE OF NncAtmCongStatsLongIntvlEntry 
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION 
          "The ATM Long Interval Congestion Statistics table.  
           This table maintains long interval per quality of service statistics 
           since the interface was started. There are 7 intervals, where the first
           interval (i.e., interval with index 1) is the most recently completed 
           interval and the length of each interval is 1 day periode."
  ::= { nncAtmCongStatsObjects 6 }
  
  nncAtmCongStatsLongIntvlEntry OBJECT-TYPE 
        SYNTAX      NncAtmCongStatsLongIntvlEntry 
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION 
          "An entry in the ATM Long Interval Congestion Statistics table."
        INDEX       { 
          entPhysicalEntityLongIntvlIndex, 
          nncAtmCongPointLongIntvlIndex, 
          nncAtmCongStatsQOSLongIntvlIndex,
          nncAtmCongStatsLongIntvlIntervalIndex
        }
  ::= { nncAtmCongStatsLongIntvlTable 1 }
  
  NncAtmCongStatsLongIntvlEntry ::=
    SEQUENCE {
      entPhysicalEntityLongIntvlIndex                PhysicalIndex,
      nncAtmCongPointLongIntvlIndex                  INTEGER,
      nncAtmCongStatsQOSLongIntvlIndex               INTEGER,
      nncAtmCongStatsLongIntvlIntervalIndex          INTEGER,
      nncAtmCongStatsLongIntvlState                  NncExtIntvlStateType,
      nncAtmCongStatsLongIntvlAbsoluteIntervalNumber INTEGER,
      nncAtmCongStatsLongIntvlTotalCellsArrived      NncExtCounter64,
      nncAtmCongStatsLongIntvlTotalCellsDiscarded    NncExtCounter64,
      nncAtmCongStatsLongIntvlClp0CellsDiscarded     NncExtCounter64,
      nncAtmCongStatsLongIntvlLowCongestedSeconds    Counter32,
      nncAtmCongStatsLongIntvlMediumCongestedSeconds Counter32,
      nncAtmCongStatsLongIntvlSevereCongestedSeconds Counter32,
      nncAtmCongStatsLongIntvlTotalSeconds           Counter32,
      nncAtmCongStatsLongIntvlCongestionState        INTEGER,
      nncAtmCongStatsLongIntvlAlarmStatus            INTEGER
    }
    
  entPhysicalEntityLongIntvlIndex OBJECT-TYPE
    SYNTAX      PhysicalIndex
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "A unique value that identifies the physical entity, whether it supports
         Congestion Statistics or not.  This value is equivalent to the value
         of PhysicalIndex."
  ::= { nncAtmCongStatsLongIntvlEntry 1 }
    
  nncAtmCongPointLongIntvlIndex OBJECT-TYPE
    SYNTAX      INTEGER (1..8)
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "A unique value between 1 and 8 that identifies the congestion point on
         an entity which supports Congestion Statistics."
  ::= { nncAtmCongStatsLongIntvlEntry 2 }
    
  nncAtmCongStatsQOSLongIntvlIndex OBJECT-TYPE
        SYNTAX      INTEGER (1..11)
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION 
          "A number between 1 and 11 that identifies the quality of service on 
           which the set of statistics was collected for an entity.  The 
           following table describes the quality of service categories available
           and their corresponding index.
           
           Quality of Service   Quality of Service Index
           ------------------   ------------------------
                  CBR                      1
                  UBR                      2
                rt-VBR1                    3
                rt-VBR2                    4
               nrt-VBR1-2                  5
                nrt-VBR3                   6
                  ABR                      7
               CBR-rtVBR                   8
              nrt-VBR/UBR                  9
                ABR/UBR                   10
                nrt-VBR                   11
          "  
  ::= { nncAtmCongStatsLongIntvlEntry 3 }
  
  nncAtmCongStatsLongIntvlIntervalIndex OBJECT-TYPE
    SYNTAX      INTEGER(1..6)
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "A unique value between 1 and 6 that identifies the interval index
         of the long interval statistics. Interval index 1 represents the most 
         recently completed interval of 1 day period and interval index 2 
         represents the second most recently completed interval, etc.  "
  ::= { nncAtmCongStatsLongIntvlEntry 4 }
  
  nncAtmCongStatsLongIntvlState OBJECT-TYPE
        SYNTAX      NncExtIntvlStateType
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION 
          "The state of the interval in the interval statistics interface.
           Possible states are:
           normal      (1) - normal interval; no change in time and no resets
           nonexistent (2) - statistics do not exist for this interval
           userReset   (3) - the user has reset statistics collection
                             during this interval
           start       (4) - this is the first interval
           timeChange  (5) - the system time has been changed during this
                             interval"
  ::= { nncAtmCongStatsLongIntvlEntry 5 }
 
  nncAtmCongStatsLongIntvlAbsoluteIntervalNumber OBJECT-TYPE
        SYNTAX                  INTEGER(1..7)
        MAX-ACCESS              read-only
        STATUS                  current
        DESCRIPTION 
          "The absolute interval number of the stats interval in 
           the interval statistics interface. If the interval is not defined yet, the absolute
           interval number will be -1.
           
           For the long, 1 day intervals the possible values are:
 
           absolute interval           interval  
                index                start    end
                                      day     day
           -----------------         ------------
                   1                   0       1 
                   2                   1       2 
                   3                   2       3
                   4                   3       4  
                   5                   4       5
                   6                   5       6
                   7                   6       7
          "
  ::= { nncAtmCongStatsLongIntvlEntry 6 }
 
  nncAtmCongStatsLongIntvlTotalCellsArrived OBJECT-TYPE
        SYNTAX      NncExtCounter64
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION 
          "Total number of cells received."
  ::= { nncAtmCongStatsLongIntvlEntry 7 }
  
  nncAtmCongStatsLongIntvlTotalCellsDiscarded OBJECT-TYPE
        SYNTAX      NncExtCounter64
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION 
          "Total number of cells discarded due to congestion."
  ::= { nncAtmCongStatsLongIntvlEntry 8 }
  
  nncAtmCongStatsLongIntvlClp0CellsDiscarded OBJECT-TYPE
        SYNTAX      NncExtCounter64
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION 
          "Number of CLP 0 cells discarded due to congestion."
  ::= { nncAtmCongStatsLongIntvlEntry 9 }
  
  nncAtmCongStatsLongIntvlLowCongestedSeconds OBJECT-TYPE
        SYNTAX      Counter32
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION 
          "Number of seconds the congestion point was in the Low Congestion
           State."
  ::= { nncAtmCongStatsLongIntvlEntry 10 }
  
  nncAtmCongStatsLongIntvlMediumCongestedSeconds OBJECT-TYPE
        SYNTAX      Counter32
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION 
          "Number of seconds the congestion point was in the Medium Congestion 
           State."
  ::= { nncAtmCongStatsLongIntvlEntry 11 }
  
  nncAtmCongStatsLongIntvlSevereCongestedSeconds OBJECT-TYPE
        SYNTAX      Counter32
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION 
          "Number of seconds the congestion point was in the Severe Congestion 
           State."
  ::= { nncAtmCongStatsLongIntvlEntry 12 }
  
  nncAtmCongStatsLongIntvlTotalSeconds OBJECT-TYPE
        SYNTAX      Counter32
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION 
          "Total number of seconds the congestion point was in the Normal, Low, 
           Medium, or Severe congestion states."
  ::= { nncAtmCongStatsLongIntvlEntry 13 }
  
  nncAtmCongStatsLongIntvlCongestionState OBJECT-TYPE
        SYNTAX INTEGER {
                 congStatsNormalState(1),
                 congStatsLowCongState(2),
                 congStatsMediumCongState(3),
                 congStatsSevereCongState(4)
               }
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION 
          "Congestion state of the congestion point - one of Normal, 
           Low, Medium, or Severe.  When the current congestion state is 
           determined, the corresponding congestion time counter will 
           increment."
  ::= { nncAtmCongStatsLongIntvlEntry 14 }
  
  nncAtmCongStatsLongIntvlAlarmStatus OBJECT-TYPE
        SYNTAX INTEGER {
                 congStatsAlarmOn(1),
                 congStatsAlarmOff(2)
               }
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION 
          "Current alarm status - either ON or OFF.  When a congestion point has 
           been severely congested for longer than the allowable threshold time 
           the value of this object is set to congStatsAlarmOn.  When severe 
           congestion clears, the value of this object is set to 
           congStatsAlarmOff."
  ::= { nncAtmCongStatsLongIntvlEntry 15 }
  
 
