--------------------------------------------------------------------------------
-- nncAtmCongPointInfoTable
  nncAtmCongPointInfoTable OBJECT-TYPE 
        SYNTAX      SEQUENCE OF NncAtmCongPointInfoEntry 
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION 
          "The ATM Congestion Point Information table.  This table maintains
           various information for the congestion point specified."
  ::= { nncAtmCongStatsObjects 1 }
  
  nncAtmCongPointInfoEntry OBJECT-TYPE 
        SYNTAX      NncAtmCongPointInfoEntry 
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION 
          "An entry in the ATM Congestion Point Information table."
        INDEX       { 
          entPhysicalEntityInfoIndex,
          nncAtmCongPointInfoIndex 
        }
  ::= { nncAtmCongPointInfoTable 1 }
  
  NncAtmCongPointInfoEntry ::=
    SEQUENCE {
      entPhysicalEntityInfoIndex  PhysicalIndex,
      nncAtmCongPointInfoIndex    INTEGER,
      nncAtmCongPointHardwareType INTEGER
    }

  entPhysicalEntityInfoIndex OBJECT-TYPE
    SYNTAX      PhysicalIndex
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "A unique value that identifies the physical entity, whether it supports
         Congestion Statistics or not.  This value is equivalent to the value
         of PhysicalIndex."
  ::= { nncAtmCongPointInfoEntry 1 }
    
  nncAtmCongPointInfoIndex OBJECT-TYPE
    SYNTAX      INTEGER (1..8)
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "A unique value between 1 and 8 that identifies the congestion point on
         an entity which supports Congestion Statistics."
  ::= { nncAtmCongPointInfoEntry 2 }
    
  nncAtmCongPointHardwareType OBJECT-TYPE
        SYNTAX      INTEGER (1..99)
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION 
          "A unique value, 1 to 99, that identifies the current hardware type of
           the congestion point.  See Tables 1 and 2 for further details."
  ::= { nncAtmCongPointInfoEntry 3 }
  
  
-- nncAtmCongStatsRawTable
  nncAtmCongStatsRawTable OBJECT-TYPE 
        SYNTAX      SEQUENCE OF NncAtmCongStatsRawEntry 
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION 
          "The ATM Raw Congestion Statistics table.  This table maintains raw 
           per quality of service statistics since the interface was started."
  ::= { nncAtmCongStatsObjects 2 }
  
  nncAtmCongStatsRawEntry OBJECT-TYPE 
        SYNTAX      NncAtmCongStatsRawEntry 
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION 
          "An entry in the ATM Raw Congestion Statistics table."
        INDEX       { 
          entPhysicalEntityRawIndex, 
          nncAtmCongPointRawIndex, 
          nncAtmCongStatsQOSRawIndex 
        }
  ::= { nncAtmCongStatsRawTable 1 }
  
  NncAtmCongStatsRawEntry ::=
    SEQUENCE {
      entPhysicalEntityRawIndex             PhysicalIndex,
      nncAtmCongPointRawIndex               INTEGER,
      nncAtmCongStatsQOSRawIndex            INTEGER,
      nncAtmCongStatsTotalCellsArrived      NncExtCounter64,
      nncAtmCongStatsTotalCellsDiscarded    NncExtCounter64,
      nncAtmCongStatsClp0CellsDiscarded     NncExtCounter64,
      nncAtmCongStatsLowCongestedSeconds    Counter32,
      nncAtmCongStatsMediumCongestedSeconds Counter32,
      nncAtmCongStatsSevereCongestedSeconds Counter32,
      nncAtmCongStatsTotalSeconds           Counter32,
      nncAtmCongStatsCurrentCongestionState INTEGER,
      nncAtmCongStatsAlarmStatus            INTEGER,
      nncAtmCongStatsVirtualBandwidthAvail  Counter32
    }
    
  entPhysicalEntityRawIndex OBJECT-TYPE
    SYNTAX      PhysicalIndex
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "A unique value that identifies the physical entity, whether it supports
         Congestion Statistics or not.  This value is equivalent to the value
         of PhysicalIndex."
  ::= { nncAtmCongStatsRawEntry 1 }
    
  nncAtmCongPointRawIndex OBJECT-TYPE
    SYNTAX      INTEGER (1..8)
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "A unique value between 1 and 8 that identifies the congestion point on
         an entity which supports Congestion Statistics."
  ::= { nncAtmCongStatsRawEntry 2 }
    
  nncAtmCongStatsQOSRawIndex OBJECT-TYPE
        SYNTAX      INTEGER (1..11)
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION 
          "A number between 1 and 11 that identifies the quality of service on 
           which the set of statistics was collected for an entity.  The 
           following table describes the quality of service categories available
           and their corresponding index.
           
           Quality of Service   Quality of Service Index
           ------------------   ------------------------
                  CBR                      1
                  UBR                      2
                rt-VBR1                    3
                rt-VBR2                    4
               nrt-VBR1-2                  5
                nrt-VBR3                   6
                  ABR                      7
               CBR-rtVBR                   8
              nrt-VBR/UBR                  9
                ABR/UBR                   10
                nrt-VBR                   11
          "  
  ::= { nncAtmCongStatsRawEntry 3 }
 
  nncAtmCongStatsTotalCellsArrived OBJECT-TYPE
        SYNTAX      NncExtCounter64
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION 
          "Total number of cells received."
  ::= { nncAtmCongStatsRawEntry 4 }
  
  nncAtmCongStatsTotalCellsDiscarded OBJECT-TYPE
        SYNTAX      NncExtCounter64
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION 
          "Total number of cells discarded due to congestion."
  ::= { nncAtmCongStatsRawEntry 5 }
  
  nncAtmCongStatsClp0CellsDiscarded OBJECT-TYPE
        SYNTAX      NncExtCounter64
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION 
          "Number of CLP 0 cells discarded due to congestion."
  ::= { nncAtmCongStatsRawEntry 6 }
  
  nncAtmCongStatsLowCongestedSeconds OBJECT-TYPE
        SYNTAX      Counter32
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION 
