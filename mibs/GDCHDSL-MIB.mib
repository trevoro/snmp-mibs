	::= { gdcHdslMaintenanceEntry 16  }

gdcHdslAlarmStatus  OBJECT-TYPE
	SYNTAX	  OCTET STRING (SIZE (1..255))
	ACCESS	  read-only
	STATUS	  mandatory
	DESCRIPTION
		"The current alarms of the unit without the alarm masks."
	::= { gdcHdslMaintenanceEntry 17 }

gdcHdslV54Config	OBJECT-TYPE
	SYNTAX	INTEGER {
			inhibit(1),
			enabled(2)
			}
	ACCESS	  read-write
	STATUS	  mandatory
	DESCRIPTION
		"This object selects the ability of the unit to recognize
         	V.54 pattern. Only applicable to DC730 & DC731 units."
	::= { gdcHdslMaintenanceEntry 18 }

gdcHdslFPRDLConfig	OBJECT-TYPE
	SYNTAX	INTEGER {
			eoc(1),
			v54(2)
			}
	ACCESS	  read-write
	STATUS	  mandatory
	DESCRIPTION
		"This object selects the pattern to be transmitted when 
		RDL test is selected via the front panel RDL switch.
		Only applicable to DC730 & DC731 units."
	::= { gdcHdslMaintenanceEntry 19 }

gdcHdslRLTimeOut	OBJECT-TYPE
	SYNTAX	INTEGER {
			none(1),
			mins10(2)
			}
	ACCESS	  read-write
	STATUS	  mandatory
	DESCRIPTION
		"This object selects the RLTimeOut to be disabled(1)
		or set to 10 minutes(2). Only applicable to SC710D2 units."
	::= { gdcHdslMaintenanceEntry 20 }

gdcHdslRPEnable 	OBJECT-TYPE
	SYNTAX	INTEGER {
			inhibit(1),
			enable(2)
			}
	ACCESS	  read-write
	STATUS	  mandatory
	DESCRIPTION
		"This object selects the Remote power to be enabled(2)
		or disabled(1). Only applicable RP products which are on the shelf."
	::= { gdcHdslMaintenanceEntry 21 }	

gdcHdslLedStatus1 OBJECT-TYPE
	 SYNTAX  OCTET STRING (SIZE(3))
	 ACCESS  read-only
	 STATUS  mandatory
	 DESCRIPTION
		 "Returns a bitwise snapshot of the front panel LED state.
		  This mib object is only used for the new HDSl elements.
		 Octet 1 bit 7 - not used
			 bit 6 - EIA RD
			 bit 5 - EIA CO
			 bit 4 - EIA RS
			 bit 3 - NORM E1/T1 NORM
			 bit 2 - ES E1/T1 ER
			 bit 1 - AL
			 bit 0 - TM

		 Octet 2 bit 7 - not used
			 bit 6 - EIA SD
			 bit 5 - NORM L3
			 bit 4 - ES L3
			 bit 3 - NORM L2
			 bit 2 - ES L2
			 bit 1 - NORM L1
			 bit 0 - ES L1

		 Octet 3 bit 7 - not used
			 bit 6 - not used
			 bit 5 - ST  (GT10xx, GT20xx)
			 bit 4 - RL  (GT10xx, GT20xx)
			 bit 3 - LL  (GT10xx, GT20xx)
			 bit 2 - INS (uas7722) 
			 bit 1 - RPF FAULT
			 bit 0 - RPF NORM	"

	::= { gdcHdslMaintenanceEntry 22  }


-- ********************************************************************
-- The GDC HDSL Configuration Tables
-- ********************************************************************

--                      E1 CONFIGURATION TABLE //  Not applicable for T1 and DTE products                           
--

 gdcHdslE1ConfigTable OBJECT-TYPE
     	SYNTAX SEQUENCE OF GdcHdslE1ConfigEntry
     	ACCESS not-accessible
     	STATUS mandatory
     	DESCRIPTION
     		"The GDC E1 Configuration table.  Information in the entries
     		of this table are used to configure E1 interface."
         ::= { gdcHdslConfiguration 1 }

 gdcHdslE1ConfigEntry OBJECT-TYPE
     	SYNTAX GdcHdslE1ConfigEntry
     	ACCESS not-accessible
     	STATUS mandatory
     	DESCRIPTION
     		"The GDC E1 Configuration table entry.  The information in
     		this entry augments are used to configure E1 interface. "
     	INDEX { gdcHdslE1ConfigIndex }
     	::= { gdcHdslE1ConfigTable 1 }
 
 GdcHdslE1ConfigEntry ::=
     	SEQUENCE {
 		        gdcHdslE1ConfigIndex SCinstance,
 		        gdcHdslE1FramingMode INTEGER,
 		        gdcHdslE1LineUnit INTEGER,
			gdcHdslE1LineCoding INTEGER
 	    }

 gdcHdslE1ConfigIndex OBJECT-TYPE
     	SYNTAX SCinstance
     	ACCESS read-only
     	STATUS mandatory
     	DESCRIPTION
     		"The index value which uniquely identifies the interface to
     		which this entry is applicable.  This has the form of a
     		SCinstance which defines the slot, line, drop, and sub-identifier.
              In which this case is an E1 interface (cc=01)."
     	::= { gdcHdslE1ConfigEntry 1 }
 

 gdcHdslE1FramingMode OBJECT-TYPE
     	SYNTAX INTEGER {
 			framed(1),		
     		unframed(2)
 	    }
     	ACCESS read-write
     	STATUS mandatory
     	DESCRIPTION 
     		" This variable is used to determine E1 framing mode. "
     	::= { gdcHdslE1ConfigEntry 2 }
 
 gdcHdslE1LineUnit OBJECT-TYPE
     	SYNTAX INTEGER {
     			tlu(1),	
 		     	ilu(2)
 	    }
     	ACCESS read-write
     	STATUS mandatory
     	DESCRIPTION
     		" This variable is used to determine operating mode of the
 		  HDSL system with respect to E1 trunk. When this variable
 		  is set to tlu(1), the system operates as a termination line
 		  unit. When this variable is set to ilu(2) the system operates
 		  as an intermediate line unit."
     	::= { gdcHdslE1ConfigEntry 3 }

 gdcHdslE1LineCoding OBJECT-TYPE
           SYNTAX  INTEGER {
                       ami (1),
                       hdb3 (2)
                   }
           ACCESS  read-write
           STATUS  mandatory
           DESCRIPTION
              "This variable describes the  variety  of  Zero
              Code  Suppression  used  on  the link, which in
              turn affects a number of its characteristics.

              E1 links, with or without CRC, use HDB3  or
              AMI.

              AMI refers to a mode wherein no  zero  code
              suppression  is  present  and the line encoding
              does not solve the problem directly.   In  this
              application, the higher layer must provide data
              which meets or exceeds the  pulse  density  re-
              quirements, such as inverting HDLC data."
          ::= { gdcHdslE1ConfigEntry 4 }

 
 
-- *********************************************************************
--                     DTE CONFIGURATION TABLE  //Not applicable for T1 and E1 products
-- *********************************************************************
 gdcHdslDteConfigTable OBJECT-TYPE
     SYNTAX SEQUENCE OF GdcHdslDteConfigEntry
     ACCESS not-accessible
     STATUS mandatory
     DESCRIPTION
     	"The GDC DTE Configuration table.  "
     ::= { gdcHdslConfiguration 2 }
 
 gdcHdslDteConfigEntry OBJECT-TYPE
     SYNTAX GdcHdslDteConfigEntry
     ACCESS not-accessible
     STATUS mandatory
     DESCRIPTION
     	"The GDC DTE Configuration table entry. "
     INDEX { gdcHdslDteConfigIndex }
     ::= { gdcHdslDteConfigTable 1 }

 GdcHdslDteConfigEntry ::= 
     SEQUENCE {
 	        	gdcHdslDteConfigIndex SCinstance,
 	        	gdcHdslDteCtsMode INTEGER,
 	       	    	gdcHdslDteDataRate INTEGER,
                  	gdcHdslDteTxClkSource INTEGER
     		}
 
 gdcHdslDteConfigIndex OBJECT-TYPE
    SYNTAX SCinstance
     ACCESS read-only
     STATUS mandatory
     DESCRIPTION
     	"The index value which uniquely identifies the interface to
     	which this entry is applicable.  This has the form of a
     	SCinstance which defines the slot, line, drop. "
     ::= { gdcHdslDteConfigEntry 1 }
 
 gdcHdslDteCtsMode OBJECT-TYPE
     SYNTAX INTEGER { 
            forcedOn(1),
 			onWithRts(2)
 		}
     ACCESS read-write
     STATUS mandatory
     DESCRIPTION
     	" This variable controls the function of CTS. "
 
     ::= { gdcHdslDteConfigEntry 2 }
 
 gdcHdslDteDataRate OBJECT-TYPE
     	SYNTAX INTEGER  (0..32) 
     	ACCESS read-write
     	STATUS mandatory
     	DESCRIPTION
     		" This variable represents the DTE data Rate in 64K increments."
     ::= { gdcHdslDteConfigEntry 3 }

    gdcHdslDteTxClkSource OBJECT-TYPE
             SYNTAX  INTEGER {
                         externalTiming (1),
                         internalTiming (2),
                         loopTiming (3)
                     }
             ACCESS  read-write
             STATUS  mandatory
             DESCRIPTION
                "The source of Tranmit Clock.

                 'ExternalTiming' indicates that  recovered  re-
                ceive  clock  from another interface is used as
                the transmit clock.

                 'InternalTiming' indicates  that  a  local  clock
                source is used.

                 'loopTiming' indicates that the recovered  re-
                ceive clock is used as the transmit clock."
            ::= {gdcHdslDteConfigEntry 4 }

