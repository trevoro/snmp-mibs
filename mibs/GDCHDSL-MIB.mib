	SYNTAX	INTEGER {
			clearAlarm(1), -- write only
			normalAlarm(2) -- read only
			}
     	ACCESS read-write 
     	STATUS mandatory
         DESCRIPTION " This variable is used to reset alarm per Loop basis."
     	::= { gdcHdslIntervalMaintenanceEntry 4 }
 
gdcHdslResetMinorAlarm	OBJECT-TYPE
	SYNTAX	INTEGER {
			clearAlarm(1), -- write only
			normalAlarm(2) -- read only
			}
     	ACCESS read-write 
     	STATUS mandatory
         DESCRIPTION " This variable is used to reset alarm per Loop basis."
     	::= { gdcHdslIntervalMaintenanceEntry 5 }

