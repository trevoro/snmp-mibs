-- ********************************************************************
-- GDC HDSL Status Tables
-- ********************************************************************

-- Loop Status Table.  This table pertains to the Loop interfaces only.

gdcHdslLoopStatusTable OBJECT-TYPE
	SYNTAX	SEQUENCE OF GdcHdslLoopStatusEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION
		"The table describes the Status objects for 
		the loops and references the loop interfaces (cc = 02-04)."
	::= { gdcHdslStatus 1  }

gdcHdslLoopStatusEntry OBJECT-TYPE
	SYNTAX	GdcHdslLoopStatusEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION
		"An entry in the GDC HDSL Status Configuration Table."
        INDEX   { gdcHdslLoopStatusLineIndex }
	::= { gdcHdslLoopStatusTable 1 }

GdcHdslLoopStatusEntry ::=
	SEQUENCE {
            gdcHdslLoopStatusLineIndex
				SCinstance,
			gdcHdslLoopStartUp
				INTEGER,
			gdcHdslLoopTipRingReversalMode
				INTEGER,
			gdcHdslLoopSignaltoNoiseMargin
				INTEGER,
			gdcHdslLoopSNFrac
				INTEGER,
			gdcHdslLoopSNAttenSense
				INTEGER,
			gdcHdslLoopPulseAtten
				INTEGER,
			gdcHdslLoopPulseAttenFrac
				INTEGER,
			gdcHdslLoopGain 
				INTEGER,
			gdcHdslLoopExchange
				INTEGER
		}

gdcHdslLoopStatusLineIndex OBJECT-TYPE
	SYNTAX	SCinstance
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
		"This object identifies Status parameters associated
		with the managed object."
	::= { gdcHdslLoopStatusEntry 1 }

gdcHdslLoopStartUp 	OBJECT-TYPE
	SYNTAX	INTEGER {
			norm(1),
			start(2),
			cancel(3)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
		" Supports the action of initializing HDSL loop startup sequence.
		When set to start(2), HDSL unit will reinitialize the start up sequence.
		When set to cancel(3), the unit will abort the start up sequence.
		When set to norm(1), the unit will operate normal."

	::= { gdcHdslLoopStatusEntry 2 }

gdcHdslLoopTipRingReversalMode 	OBJECT-TYPE
	SYNTAX	INTEGER {
			norm(1),
			reversed(2)
		}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
                "Signals the loop Tip and Ring status.
		When norm(1), loop operates normal.
		When reverse(2), loop Tip and Ring have been reversed.  "
	::= { gdcHdslLoopStatusEntry 3 }

gdcHdslLoopSignaltoNoiseMargin 	OBJECT-TYPE
	SYNTAX	INTEGER ( 0..127 )
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
              " Represents the loop signal to noise margin in dB units."
	::= { gdcHdslLoopStatusEntry 4 }

gdcHdslLoopSNFrac 	OBJECT-TYPE
	SYNTAX	INTEGER ( 0..5 )
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
              " This variable is used to be used with the 
		gdcHdslLoopSignaltoNoiseMargin variable if the following 
		manor:
		  a) Only the values 0 and 5 are allowed.
		  b) The value of zero means gdcHdslLoopSignaltoNoiseMargin
		     is a whole number.
		  c) The value of five means gdcHdslLoopSignaltoNoiseMargin
		     is a real number and that 0.5 must be added to it."
	::= { gdcHdslLoopStatusEntry 5 }

gdcHdslLoopSNAttenSense		OBJECT-TYPE
	SYNTAX INTEGER {
			positive(1),
			negative(2)
			}
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		" This variable identifies the sense of the
		  gdcHdslLoopSignaltoNoiseMargin variable. "
	::= { gdcHdslLoopStatusEntry 6 }

gdcHdslLoopPulseAtten 	OBJECT-TYPE
	SYNTAX  INTEGER	( 0..100)
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
                " Represents twice the HDSL loop Pulse Attenuation level 
                  in dB units."
	::= { gdcHdslLoopStatusEntry 7 }

gdcHdslLoopPulseAttenFrac 	OBJECT-TYPE
	SYNTAX	INTEGER ( 0..5 )
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
              " This variable is used to be used with the 
		gdcHdslLoopPulseAtten variable if the following 
		manor:
		  a) Only the values 0 and 5 are allowed.
		  b) The value of zero means gdcHdslLoopPulseAtten
		     is a whole number.
		  c) The value of five means gdcHdslLoopPulseAtten
		     is a real number and that 0.5 must be added to it."
	::= { gdcHdslLoopStatusEntry 8 }

gdcHdslLoopGain OBJECT-TYPE
	SYNTAX	INTEGER {
			low(1),
			high(2),
			ok(3)
		}
        ACCESS  read-only
      	STATUS  mandatory
       	DESCRIPTION
                  " Represents the HDSL loop gain setting. "
	::= { gdcHdslLoopStatusEntry 9 }

gdcHdslLoopExchange OBJECT-TYPE
	SYNTAX	INTEGER {
			norm(1),
			exchange(2)
		}
        ACCESS  read-only
      	STATUS  mandatory
       	DESCRIPTION
                    " This variable is used to represent the HDSL loop status.
		When the value is returned to norm(1), HDSL loops are correctly
		connected. 
		When the value is returned as exchange(2), the HDSl loop has been
		interchange by error."
	::= { gdcHdslLoopStatusEntry 10 } 

