	DESCRIPTION
	        "Controls the generation of VT RDI defects on the selected
		 interface.

		 Value		Description
		 on(1)          RDI is continually tramsmitted
		 off(2)         RDI is never transmitted
		 momentary(3)   RDI is transmitted for a short period of time.
		 auto(4)        RDI is generated according to the sonet specification"
	DEFVAL { auto }
	::= { foreSonetVTDiagnosticsEntry 2 }


foreSonetVtDiagnosticsRFI OBJECT-TYPE
        SYNTAX  INTEGER {
	        on(1),
		off(2),
		momentary(3),
		auto(4)
		}
	MAX-ACCESS  read-write
	STATUS  current
	DESCRIPTION
	        "Controls the generation of VT RFI defects on the selected
		 interface.

		 Value		Description
		 on(1)          RFI is continually tramsmitted
		 off(2)         RFI is never transmitted
		 momentary(3)   RFI is transmitted for a short period of time.
		 auto(4)        RFI is generated according to the sonet specification"
	DEFVAL { auto }
	::= { foreSonetVTDiagnosticsEntry 3 }

foreSonetVtDiagnosticsAIS OBJECT-TYPE
        SYNTAX  INTEGER {
	        on(1),
		off(2),
		momentary(3),
		auto(4)
		}
	MAX-ACCESS  read-write
	STATUS  current
	DESCRIPTION
	        "Controls the generation of VT AIS on the selected
		 interface.

		 Value		Description
		 on(1)          AIS is continually tramsmitted
		 off(2)         AIS is never transmitted
		 momentary(3)   AIS is transmitted for a short period of time.
		 auto(4)        AIS is generated according to the sonet specification"
	DEFVAL { auto }
	::= { foreSonetVTDiagnosticsEntry 4 }

foreSonetVtDiagnosticsLOP OBJECT-TYPE
        SYNTAX  INTEGER {
	        on(1),
		off(2),
		momentary(3)
		}
	MAX-ACCESS  read-write
	STATUS  current
	DESCRIPTION
	        "Controls the generation of VT LOP on the selected
		 interface.

		 Value		Description
		 on(1)          LOP is continually tramsmitted
		 off(2)         LOP is never transmitted
		 momentary(3)   LOP is transmitted for a short period of time."
	DEFVAL { off }
	::= { foreSonetVTDiagnosticsEntry 5 }

foreSonetVtDiagnosticsLabel OBJECT-TYPE
        SYNTAX  INTEGER {
	        on(1),
		off(2)
		}
	MAX-ACCESS  read-write
	STATUS  current
	DESCRIPTION
	        "Controls the transmission of generated label values to
		 force PLM-V failures on the remote system.

		 Value		Description
		 on(1)          Use label value in foreSonetVtDiagnosticsLableValue
		 off(2)         Transmit label appropriate for VT type"
	DEFVAL { off }
	::= { foreSonetVTDiagnosticsEntry 6 }

foreSonetVtDiagnosticsLabelValue OBJECT-TYPE
	SYNTAX	INTEGER  {
		unequipped(0),
		equippednonspec(1),
		asynchronous(2),
		bitsynchronous(3),
		bytesynchronous(4)
	}
	MAX-ACCESS  read-write
	STATUS  current
	DESCRIPTION
	        "Selects the  label value to be transmitted in order to
		 force PLM-V failures on the remote system."
	DEFVAL { 1 }
	::= { foreSonetVTDiagnosticsEntry 7 }

foreSonetVtDiagnosticsREI OBJECT-TYPE
        SYNTAX  INTEGER {
	        on(1),
		off(2),
		auto(4)
		}
	MAX-ACCESS  read-write
	STATUS  current
	DESCRIPTION
	        "Controls the generation of VT REI on the selected
		 interface.  REI indicates to the far end that a BIP-2
		 error check failed.

		 Value		Description
		 on(1)          REI is continually tramsmitted
		 off(2)         REI is never transmitted
		 auto(4)        REI is transmitted according to the BIP-2"
	DEFVAL { auto }
	::= { foreSonetVTDiagnosticsEntry 8 }



-- Fore SONET Traps

foreSonetLOSDetected NOTIFICATION-TYPE
	OBJECTS { ifIndex, ifName, trapLogIndex }
	STATUS      current
	DESCRIPTION
		"This trap indicates that the specified SONET port is
		experiencing Loss Of Signal. Bellcore Document
		TA-NWT-000253 Section 6.3.1.1.1 states that... A SONET
		NE shall declare a LOS failure when the LOS defect
		persists for 2.5 (+- .5) seconds, or when a LOS defect
		is present and the criteria for LOF failure
		declaration have been met."
	::= { foreSonetMib 0 1 }

foreSonetLOSCleared NOTIFICATION-TYPE
	OBJECTS { ifIndex, ifName, trapLogIndex }
	STATUS      current
	DESCRIPTION
		"This trap indicates that the LOS condition identified
		by trap foreSonetLOSDetected has been cleared."
	::= { foreSonetMib 0 2 }

foreSonetPathLabelDetected NOTIFICATION-TYPE
	OBJECTS { ifIndex, ifName, trapLogIndex }
	STATUS      current
	DESCRIPTION
		"This trap indicates that the specified SONET port is
		receiving and errored C2 Path Label byte. Reference
		Bellcore Document TA-NWT-000253 Section 3.3.2.3 and
		6.3.1.1.8 the Path Label (C2) byte should have the
		value 0x13 or 0x01."
	::= { foreSonetMib 0 3 }

foreSonetPathLabelCleared NOTIFICATION-TYPE
	OBJECTS { ifIndex,  ifName, trapLogIndex }
	STATUS      current
	DESCRIPTION
		"This trap indicates that the Errored Path Label (C2)
		byte error condition signaled by the
		foreSonetPathLabelDetected trap has been cleared."
	::= { foreSonetMib 0 4 }

foreSonetLineAISDetected NOTIFICATION-TYPE
	OBJECTS { ifIndex, ifName, trapLogIndex }
	STATUS      current
	DESCRIPTION
		"This trap indicates that the specified SONET port is
		receiving a Line level Alarm Indication Signal from the
		far-end equipment."
	::= { foreSonetMib 0 5 }

foreSonetLineAISCleared NOTIFICATION-TYPE
	OBJECTS {  ifIndex, ifName, trapLogIndex }
	STATUS      current
	DESCRIPTION
		"This trap indicates that the Line AIS error condition
		signaled by the foreSonetLineAISDetected trap has been
		cleared."
	::= { foreSonetMib 0 6 }

foreSonetLOFDetected NOTIFICATION-TYPE
	OBJECTS { ifIndex, ifName, trapLogIndex }
	STATUS      current
	DESCRIPTION
		"This trap indicates that the specified SONET port is
		experiencing Loss Of Frame (LOF) failure. An LOF
		failure is declared when the LOF defect persists fore
		a period of 2.5 +/- 0.5 seconds, except when an LOS
		defect or failure is present."
	::= { foreSonetMib 0 7 }

foreSonetLOFCleared NOTIFICATION-TYPE
	OBJECTS { ifIndex, ifName, trapLogIndex }
	STATUS      current
	DESCRIPTION
		"This trap indicates that the LOF failure identified
		by trap foreSonetLOFDetected has been cleared. The LOF
		failure is cleared when the LOS failure is declared,
		or when the LOF defect is absent for 10 +/- 0.5
		seconds."
	::= { foreSonetMib 0 8 }

foreSonetLineRDIDetected NOTIFICATION-TYPE
	OBJECTS { ifIndex, ifName, trapLogIndex }
	STATUS      current
	DESCRIPTION
		"This trap indicates that the specified SONET port is
		experiencing Line Remote Defect Indication (LRDI). A
		Line RDI failure is declared when the incoming Line
		RDI defects lasts for 2.5 +/- 0.5 seconds."
	::= { foreSonetMib 0 9 }

foreSonetLineRDICleared NOTIFICATION-TYPE
	OBJECTS { ifIndex, ifName,  trapLogIndex }
	STATUS      current
	DESCRIPTION
		"This trap indicates that the Line RDI failure
		identified by trap foreSonetLineRDIDetected has been
		cleared.  The Line RDI failure is cleared when no Line
		RDI defects are detected for 10 +/- 0.5 seconds."
	::= { foreSonetMib 0 10 }

foreSonetPathAISDetected NOTIFICATION-TYPE
	OBJECTS { ifIndex, ifName, trapLogIndex }
	STATUS      current
	DESCRIPTION
		"This trap indicates that the specified SONET port is
		experiencing Path Alarm Inidication Signal (PAIS). A Path
		AIS failure is declared when the Path AIS defect
		persists for 2.5 +/- 0.5 seconds."
	::= { foreSonetMib 0 11 }

foreSonetPathAISCleared NOTIFICATION-TYPE
	OBJECTS { ifIndex,  ifName, trapLogIndex }
	STATUS      current
	DESCRIPTION
		"This trap indicates that the PathAIS failure
		identified by trap foreSonetPathAISDetected has been
		cleared.  A PAIS failure is cleared when the PAIS
		defect is absent for 10 +/- 0.5 seconds."
	::= { foreSonetMib 0 12 }

foreSonetPathLOPDetected NOTIFICATION-TYPE
	OBJECTS { ifIndex,  ifName, trapLogIndex }
	STATUS      current
	DESCRIPTION
		"This trap indicates that the specified SONET port is
		experiencing Loss Of Pointer (LOP). A LOP failure is
		declared when the LOP defect persists for a period of
		2.5 +/- 0.5 seconds."
	::= { foreSonetMib 0 13 }

foreSonetPathLOPCleared NOTIFICATION-TYPE
	OBJECTS { ifIndex,  ifName, trapLogIndex }
	STATUS      current
	DESCRIPTION
		"This trap indicates that the LOP failure identified
		by trap foreSonetLOPDetected has been cleared. A LOP
		failure is cleared when the LOP defect is absent for
		10 +/- 0.5 seconds."
	::= { foreSonetMib 0 14 }

foreSonetPathUNEQDetected NOTIFICATION-TYPE
	OBJECTS { ifIndex,  ifName, trapLogIndex }
	STATUS      current
	DESCRIPTION
		"This trap indicates that the specified SONET port is
		experiencing unequiped (UNEQ). A UNEQ failure is
		declared when the UNEQ defect persists for a period of
		2.5 +/- 0.5 seconds."
	::= { foreSonetMib 0 15 }

foreSonetPathUNEQCleared NOTIFICATION-TYPE
	OBJECTS { ifIndex, ifName, trapLogIndex }
	STATUS      current
	DESCRIPTION
		"This trap indicates that the UNEQ failure identified
		by trap foreSonetUNEQDetected has been cleared. A UNEQ
		failure is cleared when the UNEQ defect is absent for
		10 +/- 0.5 seconds."
	::= { foreSonetMib 0 16 }

foreSonetPathRDIDetected NOTIFICATION-TYPE
	OBJECTS { ifIndex, ifName, trapLogIndex }
	STATUS      current
	DESCRIPTION
		"This trap indicates that the specified SONET port is
		experiencing Path Remote Defect Indication (PRDI). A
		Path RDI failure is declared when the incoming Path
		RDI defects lasts for 2.5 +/- 0.5 seconds."
	::= { foreSonetMib 0 17 }

foreSonetPathRDICleared NOTIFICATION-TYPE
	OBJECTS { ifIndex,  ifName, trapLogIndex }
	STATUS      current
	DESCRIPTION
		"This trap indicates that the Path RDI failure
		identified by trap foreSonetPathRDIDetected has been
		cleared.  The Path RDI failure is cleared when no Path
		RDI defects are detected for 10 +/- 0.5 seconds."
	::= { foreSonetMib 0 18 }

foreSonetPathPDIDetected NOTIFICATION-TYPE
	OBJECTS { ifIndex,  ifName, trapLogIndex }
	STATUS      current
	DESCRIPTION
		"This trap indicates that the specified SONET path
		 is experiencing a PDI-P ( Path Defect Indication)
		 condition.  A PDI-P indication is signalled by 
		 a value in the C2 ( path label ) byte in the range
		 0xe0..0xfc."
	::= { foreSonetMib 0 19 }

foreSonetPathPDICleared NOTIFICATION-TYPE
	OBJECTS { ifIndex,  ifName, trapLogIndex }
	STATUS      current
	DESCRIPTION
		"This trap indicates that the PDI falure
		 identified by trap foreSonetPathPDIDetected
		 has been cleared."
	::= { foreSonetMib 0 20 }

foreSonetLineBIPDetected NOTIFICATION-TYPE
	OBJECTS { ifIndex, ifName, trapLogIndex }
	STATUS      current
	DESCRIPTION
		"This trap indicates that the specified SONET port is
		 experiencing Bit Interleaved Parity (BIP) errors. A
		 Line BIP failure is declared when the Line BIP defect
		 persists for a period of 2.5 +/- 0.5 seconds."
	::= { foreSonetMib 0 21 }

foreSonetLineBIPCleared NOTIFICATION-TYPE
	OBJECTS { ifIndex, ifName, trapLogIndex }
	STATUS      current
	DESCRIPTION
		"This trap indicates that the Line BIP failure identified
		 by trap asxSonetAtmLineBIPDetected has been cleared. A Line
		 BIP failure is cleared when the Line BIP defect is absent
		 for 10 +/- 0.5 seconds."
	::= { foreSonetMib 0 22 }

foreSonetVtAISDetected NOTIFICATION-TYPE
        OBJECTS { ifIndex, ifName, trapLogIndex }
	STATUS      current
        DESCRIPTION
                "This trap indicates that the specified Sonet virtual
		 tributary is receiving an AIS indication from the far
		 end.  An AIS-V failure is declared when the AIS-V defect
                 persists for 2.5 +/- 0.5 seconds."
	::= { foreSonetMib 0 23 }

foreSonetVtAISCleared NOTIFICATION-TYPE
        OBJECTS { ifIndex, ifName, trapLogIndex }
	STATUS      current
        DESCRIPTION
                "This trap indicates that the virtual tributary AIS
		 failure condition has been cleared.  An AIS-V failure
		 is cleared when the AIS-V defect is absent for
		 10 +/- 0.5 seconds."
	::= { foreSonetMib 0 24 }

foreSonetVtLOPDetected NOTIFICATION-TYPE
        OBJECTS { ifIndex, ifName, trapLogIndex }
	STATUS      current
        DESCRIPTION
                "This trap indicates that the specified virtual tributary
		 is receiving an LOP-V failure.  An LOP-V failure is declared
		 when an LOP-V defect persists for 2.5 +/- 0.5 seconds."
	::= { foreSonetMib 0 25 }

foreSonetVtLOPCleared NOTIFICATION-TYPE
        OBJECTS { ifIndex, ifName, trapLogIndex }
	STATUS      current
        DESCRIPTION
                "This trap indicates that the specified virtual tributary
		 is no longer experiencing loss-of-pointer failure.  An LOP-V
		 failure is cleared when the LOP-V defect is absent for 10
		 +/- 0.5 seconds"
	::= { foreSonetMib 0 26 }

foreSonetVtUNEQDetected NOTIFICATION-TYPE
        OBJECTS { ifIndex, ifName, trapLogIndex }
	STATUS      current
        DESCRIPTION
                "This trap indicates that the specified virtual tributary
		 is receiving an UNEQ virtual tributary. (V5 byte is 0 )."
	::= { foreSonetMib 0 27 }

foreSonetVtUNEQCleared NOTIFICATION-TYPE
        OBJECTS { ifIndex, ifName, trapLogIndex }
	STATUS      current
        DESCRIPTION
                "This trap indicates that the specified virtual tributary
		 is no longer receving an UNEQ virtual tributary."
	::= { foreSonetMib 0 28 }

foreSonetVtVILMDetected NOTIFICATION-TYPE
        OBJECTS { ifIndex, ifName, trapLogIndex }
	STATUS      current
        DESCRIPTION
                "This trap indicates that the specified virtual tributary
		 is experiencing a VT path label mismatch failure, PLM-V."
	::= { foreSonetMib 0 29 }

foreSonetVtVILMCleared NOTIFICATION-TYPE
        OBJECTS { ifIndex, ifName, trapLogIndex }
	STATUS      current
        DESCRIPTION
                "This trap indicates that the specified virtual tributary
		 is no longer experiencing a PLM-V failure."
	::= { foreSonetMib 0 30 }

foreSonetVtRDIDetected NOTIFICATION-TYPE
        OBJECTS { ifIndex, ifName, trapLogIndex }
	STATUS      current
        DESCRIPTION
                "This trap indicates that the specified virtual tributary
		is experiencing an RDI failure.  An RDI-V failure is declared
		after the interface receives RDI-V defects for 2.5 +/- 0.5
		seconds."
	::= { foreSonetMib 0 31 }

foreSonetVtRDICleared NOTIFICATION-TYPE
        OBJECTS { ifIndex, ifName, trapLogIndex }
	STATUS      current
        DESCRIPTION
                "This trap indicates that the specified virtual tributary
		is no longer experiencing an RDI failure.  An RDI-V failure
		is cleared after the interface receives no RDI-V defects for
		10 +/- 0.5 seconds."
	::= { foreSonetMib 0 32 }

foreSonetVtRFIDetected NOTIFICATION-TYPE
        OBJECTS { ifIndex, ifName, trapLogIndex }
	STATUS      current
        DESCRIPTION
                "This trap indicates that the specified virtual tributary
		 is experiencing an RFI-V failure.  An RFI-V failure is
		 declared within 5 milliseconds of the detection of an
		 incoming RFI-V indication."
	::= { foreSonetMib 0 33 }

foreSonetVtRFICleared NOTIFICATION-TYPE
        OBJECTS { ifIndex, ifName, trapLogIndex }
	STATUS      current
        DESCRIPTION
                "This trap indicates that the specified virtual tributary
		 is no longer experiencing an RFI-V failure.  An RFI-V failure
		 is cleared within 50 milliseconds of the removal of an
		 incoming RFI-V indication."
	::= { foreSonetMib 0 34 }

END

