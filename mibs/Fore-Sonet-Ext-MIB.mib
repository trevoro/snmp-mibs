foreSonetVtDiagnosticsBIP OBJECT-TYPE
    SYNTAX INTEGER {
	on(1),
	off(2),
	momentary(3)
    }		
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
	"Controls the insertion of VT BIP errors in the sonet VT V5 byte
	 overhead. 
	    on:		Generate sonet VT BIP errors continuously.
	    off:	Do not generate sonet VT BIP errors.
	    momentary:	Generate a chip dependent number of sonet frames with
			sonet VT BIP errors."
	DEFVAL { off }
 	::= { foreSonetVTDiagnosticsEntry 1 }

foreSonetVtDiagnosticsRDI OBJECT-TYPE
        SYNTAX  INTEGER {
