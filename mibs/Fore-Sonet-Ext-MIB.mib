foreSonetSectionTotalTable OBJECT-TYPE
	SYNTAX SEQUENCE OF ForeSonetSectionTotalEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION
		"A table of configuration options and statistics
		 that extend the standard Sonet section tables."
	::= { foreSonetSection 2 }

foreSonetSectionTotalEntry OBJECT-TYPE
	SYNTAX ForeSonetSectionTotalEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION
		"A table entry containing Fore specific extensions
		 to the standard SONET section table statistics."
	INDEX { ifIndex }
	::= { foreSonetSectionTotalTable 1 }

ForeSonetSectionTotalEntry ::=
	SEQUENCE {
		foreSonetSectionTotalBIPs		Counter32,
		foreSonetSectionTotalLOSs		Counter32,
		foreSonetSectionTotalLOFs		Counter32
	}

foreSonetSectionTotalBIPs OBJECT-TYPE
	SYNTAX	Counter32
	MAX-ACCESS	read-only
	STATUS	current
	DESCRIPTION
		"The number of Section BIP-8 (Bit Interleaved Parity) errors that have been 
		detected since the last time the port has been reset. The calculated BIP-8 
		code is compared with the BIP-8 code extracted from the B1 byte of the 
		following frame. Differences indicate that a section level bit error has 
		occurred."
	::= { foreSonetSectionTotalEntry 1 }

foreSonetSectionTotalLOSs OBJECT-TYPE
	SYNTAX	Counter32
	MAX-ACCESS	read-only
	STATUS	current
	DESCRIPTION
		"The number of seconds in which Loss Of Signal (LOS) has occurred. A LOS 
		is declared when 20 +/- 3us of all zeros patterns is detected. LOS is cleared 
		when two valid framing words are detected and during the intervening time no 
		LOS condition is detected."
	::= { foreSonetSectionTotalEntry 2 }

foreSonetSectionTotalLOFs OBJECT-TYPE
	SYNTAX	Counter32
	MAX-ACCESS	read-only
	STATUS	current
	DESCRIPTION
		"The number of seconds in which Loss Of Frame (LOF) has occurred. A LOF 
		is declared when an out-of-frame (OOF) condition persists for 3ms. The LOF 
		is cleared when an in-frame condition persists for 3ms. While in-frame the 
		framing bytes (A1, A2) in each frame are compared against the expected 
		pattern. Out-of-frame is declared when four consecutive frames containing 
		one or more framing pattern errors have been received."
	::= { foreSonetSectionTotalEntry 3 }


foreSonetSectionDiagnosticsTable OBJECT-TYPE
	SYNTAX SEQUENCE OF ForeSonetSectionDiagnosticsEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION
		"A table of configuration options and statistics
		 that extend the standard Sonet Section tables."
	::= { foreSonetSection 3 }


foreSonetSectionDiagnosticsEntry OBJECT-TYPE
 	SYNTAX ForeSonetSectionDiagnosticsEntry
 	MAX-ACCESS	not-accessible
 	STATUS		current
 	DESCRIPTION
 		"A table entry containing Fore specific extensions
 		 to the standard SONET Section tables."
 	INDEX { ifIndex }
 	::= { foreSonetSectionDiagnosticsTable 1 }
 
ForeSonetSectionDiagnosticsEntry ::=
 	SEQUENCE {
 		foreSonetSectionDiagnosticsFraming		 INTEGER,
		foreSonetSectionDiagnosticsB1			 INTEGER,
		foreSonetSectionDiagnosticsDisableTxScrambling	 INTEGER,
		foreSonetSectionDiagnosticsDisableRxDescrambling INTEGER,
		foreSonetSectionDiagnosticsLOS			 INTEGER
 	}

foreSonetSectionDiagnosticsFraming OBJECT-TYPE
 	SYNTAX	INTEGER {
		on(1),
		off(2),
		momentary(3)
		}
 	MAX-ACCESS	read-write
 	STATUS	current
 	DESCRIPTION
		"Controls the insertion of framing (A1 A2 byte) errors. 

		off 	   When set to off, transmit Sonet frames 
			   continously with no A1A2 errors (framing errors).
		on 	   When set to on, transmit Sonet frames 
			   continously with bad A1A2 values.
		momentary  When set to momentary, transmit a chip dependent 
			   number of Sonet frames with A1A2 errors."
	DEFVAL { off }	
 	::= { foreSonetSectionDiagnosticsEntry 1 }


foreSonetSectionDiagnosticsB1 OBJECT-TYPE
 	SYNTAX	INTEGER {
		on(1),
		off(2),
		momentary(3)
		}
 	MAX-ACCESS	read-write
 	STATUS	current
 	DESCRIPTION
		"Controls the insertion of BIP errors in the sonet section 
		 B1 byte overhead. 
		off		When set to off, do not generate section
				BIP errors. 
		on		When set to on, generate section BIP errors. 
		momentary	When set to momentary, transmit a chip dependent
				number of Sonet frames with section BIP errors."
	DEFVAL { off }
 	::= { foreSonetSectionDiagnosticsEntry 2 }


foreSonetSectionDiagnosticsDisableTxScrambling OBJECT-TYPE
 	SYNTAX	INTEGER {
		on(1),
		off(2)
		}
 	MAX-ACCESS	read-write
 	STATUS	current
 	DESCRIPTION
		"Controls the scrambling of sonet overhead and SPE of the
		 transmitted Sonet frame.  

		off	When set to off, transmitted Sonet frames are
			scrambled.
		on	When set to on, transmitted Sonet frames are NOT
			scrambled."
	DEFVAL { off }
 	::= { foreSonetSectionDiagnosticsEntry 3 }


foreSonetSectionDiagnosticsDisableRxDescrambling OBJECT-TYPE
 	SYNTAX	INTEGER {
		on(1),
		off(2)
		}
 	MAX-ACCESS	read-write
 	STATUS	current
 	DESCRIPTION
		"Controls the descrambling of sonet overhead and SPE of the
		 received Sonet frame. 

		off	When set to off, received Sonet frames are 
			descrambled.
		on	When set to on, received Sonet frames are NOT 
			descrambled."
	DEFVAL { off }
 	::= { foreSonetSectionDiagnosticsEntry 4 }


foreSonetSectionDiagnosticsLOS OBJECT-TYPE
 	SYNTAX	INTEGER {
		on(1),
		off(2)
		}
 	MAX-ACCESS	read-write
 	STATUS	current
 	DESCRIPTION
		"Controls the insertion of Loss of Signal (LOS) errors in 
		 the transmitted Sonet frame. 

		off	When set to off, do not generate LOS errors.
		on	When set to on, generate LOS errors."

	DEFVAL { off }
 	::= { foreSonetSectionDiagnosticsEntry 5 }

