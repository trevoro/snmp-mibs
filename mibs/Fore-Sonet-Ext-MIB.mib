-- foreSonetSectionDiagnosticsEntry OBJECT-TYPE
-- 	SYNTAX ForeSonetSectionDiagnosticsEntry
-- 	MAX-ACCESS	not-accessible
-- 	STATUS		current
-- 	DESCRIPTION
-- 		"A table entry containing Fore specific extensions
-- 		 to the standard SONET Section tables."
-- 	INDEX { ifIndex }
-- 	::= { foreSonetSectionDiagnosticsTable 1 }
-- 
-- ForeSonetSectionDiagnosticsEntry ::=
-- 	SEQUENCE {
-- 		foreSonetSectionDiagnosticsMode		INTEGER
-- 	}
-- 
-- foreSonetSectionDiagnosticsMode OBJECT-TYPE
-- 	SYNTAX	INTEGER
-- 	MAX-ACCESS	read-write
-- 	STATUS	current
-- 	DESCRIPTION
-- 		"This variable controls the insertion of various types
-- 		of section diagnostic bit patterns in the transmit
-- 		stream.  
-- 
-- 		Under normal operation it should be set to 1
-- 
-- 		1 None : No diagnostics
-- 
-- 		2 DFP : Controls the insertion of single bit errors
-- 		continuously in the most significant bit (bit 1) of
-- 		the A1 section overhead framing byte.  When DFP is on
-- 		the A1 bytes are set to 0x76 instaed of 0xF6
-- 
-- 		4 DBIP8 : Controls the insertion of bit errors
-- 		continuously in the section BIP-8 byte (B1).  When
-- 		DBIB8 is on the B1 byte is inverted.
-- 
-- 		8 DLOS : Controls the insertion of all zeros in the
-- 		stream.  When DLOST is on the transmit stream is
-- 		forced to 0x00.
-- 
-- 		Note that not all section interfaces support all
-- 		diagnostics.
-- 		"
-- 	DEFVAL { 1 }
-- 	::= { foreSonetSectionDiagnosticsEntry 2 }
-- 
-- Fore extensions to the standard SONET Line Tables

foreSonetLineConfigTable OBJECT-TYPE
	SYNTAX SEQUENCE OF ForeSonetLineConfigEntry
	MAX-ACCESS	not-accessible
	STATUS	current
	DESCRIPTION
		"A table of configuration options and statistics that
		 extend the Sonet Line Group in the Standard SONET MIB."
	::= { foreSonetLine 1 }

foreSonetLineConfigEntry OBJECT-TYPE
	SYNTAX ForeSonetLineConfigEntry
	MAX-ACCESS not-accessible
	STATUS current
	DESCRIPTION
		"A table entry containing Fore specific extensions to
		 the standard SONET Line Group Tables."
	INDEX { ifIndex }
	::= { foreSonetLineConfigTable 1 }

ForeSonetLineConfigEntry ::=
	SEQUENCE {
		foreSonetLineBipThrSeconds	Integer32,
		foreSonetLineBipThrErrors	Integer32,
		foreSonetLineBipFailEnable	INTEGER,
		foreSonetLineSignalDegradeBer   Integer32,
		foreSonetLineSignalFailBer      Integer32,
		foreSonetLineBerErrorModel      INTEGER,
		foreSonetLineBerState		INTEGER
	}

foreSonetLineBipThrSeconds OBJECT-TYPE
	SYNTAX	Integer32
	MAX-ACCESS	read-write
	STATUS	current
	DESCRIPTION
		"This variable represents the consecutive number of
		 BAD/GOOD seconds to detect/clear a B2 byte Line Bit
		 Interleaved Parity (BIP) Defect. The range of the 
		 values it can take is between 2 and 10 inclusive."
         DEFVAL { 10 }
	 ::= { foreSonetLineConfigEntry 2 }

foreSonetLineBipThrErrors OBJECT-TYPE
	SYNTAX	Integer32
	MAX-ACCESS	read-write
	STATUS	current
	DESCRIPTION
		"This variable is a threshold for the number of B2 byte
		 Line Bit Interleaved Parity (BIP) Errors per second
		 and is used as a parameter to the Line BIP Defect. If
		 the number of errors exceeds the threshold, the 
		 particular second is declared BAD, otherwise it is
		 declared GOOD."
	DEFVAL { 1 }
	::= { foreSonetLineConfigEntry 3 }

foreSonetLineBipFailEnable OBJECT-TYPE
	SYNTAX	INTEGER {
		enabled(1),
		disabled(2)
		}
	MAX-ACCESS	read-write
	STATUS	current
	DESCRIPTION
		"This variable controls whether declaration of a
		 Line BIP Defect forces the port's operState to Down."
	::= { foreSonetLineConfigEntry 4 }
	
foreSonetLineSignalDegradeBer OBJECT-TYPE
	SYNTAX  Integer32
	MAX-ACCESS  read-write
	STATUS  current
	DESCRIPTION
		"This is the exponent of 10 for the current signal
		 degrade bit error rate (BER) threshold for this port.
		 The value -7, for example, represents a BER of 1E-7.
		 This variable is only applicable when sonetLineBerErrorModel
		 is set to errorModelRandom."
	::= { foreSonetLineConfigEntry 5 }

foreSonetLineSignalFailBer OBJECT-TYPE
	SYNTAX  Integer32
	MAX-ACCESS  read-write
	STATUS  current
	DESCRIPTION
		"This is the exponent of 10 for the current signal
		 fail bit error rate (BER) threshold for this port.
		 The value -4, for example, represents a BER of 1E-4.
		 This variable is only applicable when sonetLineBerErrorModel
		 is set to errorModelRandom."
	::= { foreSonetLineConfigEntry 6 }


foreSonetLineBerErrorModel OBJECT-TYPE
	SYNTAX  INTEGER {
		errorModelNone(0),
		errorModelRandom(1),
		errorModelBurst(2)
		}
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION
		"This is the error distribution model to be used to
		 identify signal degrade and signal fail conditions.
		 errorModelRandom selects a random error distribution
		 and declares signal conditions based on the thresholds
		 set in sonetLineSignalDegradeBer and sonetLineSignalFailBer.
		 errorModelBurst selects a burst error model and declares
		 signal degrade conditions based on the thresholds set in
		 sonetLineBipThrSeconds and sonetLineBipThrErrors.
		 errorModelNone disables detection of signal conditions."
	DEFVAL	{ errorModelRandom }
	::= { foreSonetLineConfigEntry 7 }

foreSonetLineBerState OBJECT-TYPE
	SYNTAX	INTEGER {
		berStateOk(0),
		berStateSigDegrade(1),
		berStateSigFail(2)
		}
	MAX-ACCESS read-only
	STATUS	   current
	DESCRIPTION
		"This value describes the current state of the port as 
		 determined through bit error rate analysis."
	::= { foreSonetLineConfigEntry 8 }

foreSonetLineTotalTable OBJECT-TYPE
	SYNTAX SEQUENCE OF ForeSonetLineTotalEntry
	MAX-ACCESS	not-accessible
	STATUS	current
	DESCRIPTION
		"A table of configuration options and statistics that
		 extend the Sonet Line Group in the Standard SONET MIB."
	::= { foreSonetLine 2 }

foreSonetLineTotalEntry OBJECT-TYPE
	SYNTAX ForeSonetLineTotalEntry
	MAX-ACCESS not-accessible
	STATUS current
	DESCRIPTION
		"A table entry containing Fore specific extensions to
		 the standard SONET Line Group Tables."
	INDEX { ifIndex }
	::= { foreSonetLineTotalTable 1 }

ForeSonetLineTotalEntry ::=
	SEQUENCE {
		foreSonetLineTotalBIPs		Counter32,
		foreSonetLineTotalFEBEs		Counter32,
		foreSonetLineTotalAISs		Counter32,
		foreSonetLineTotalRDIs		Counter32
	}

foreSonetLineTotalBIPs OBJECT-TYPE
	SYNTAX	Counter32
	MAX-ACCESS	read-only
	STATUS	current
	DESCRIPTION
		"The number of Line BIP-24 (Bit Interleaved Parity)
		errors that have been detected since the last time the
		port has been reset. The calculated BIP-24 code is
		based on the line overhead and synchronous payload
		envelope (SPE) of the STS-3c stream. The line BIP-24
		code is a bit interleaved parity calculation using
		even parity. The calculated code is compared with the
		BIP-24 code extracted from the B2 bytes of the
		following frame. Differences indicate that a line
		layer bit error has occurred."
	::= { foreSonetLineTotalEntry 1 }

foreSonetLineTotalFEBEs OBJECT-TYPE
	SYNTAX	Counter32
	MAX-ACCESS	read-only
	STATUS	current
	DESCRIPTION
		"The number of line Far End Block Errors (FEBE) that have been detected since 
		the last time the port has been reset."
	::= { foreSonetLineTotalEntry 2 }

foreSonetLineTotalAISs OBJECT-TYPE
	SYNTAX	Counter32
	MAX-ACCESS	read-only
	STATUS	current
	DESCRIPTION
		"The number of seconds in which line Alarm Indication
		Signal (AIS) has occurred.  A line AIS is asserted
		when a 111 binary pattern is detected in bits 6,7,8 of
		the K2 byte for five consecutive frames. A line AIS is
		removed when any pattern other than 111 is detected in
		these bits for five consecutive frames."
	::= { foreSonetLineTotalEntry 3 }

foreSonetLineTotalRDIs OBJECT-TYPE
	SYNTAX	Counter32
	MAX-ACCESS	read-only
	STATUS	current
	DESCRIPTION
		"The number of seconds in which line Far End Receive
		Failure (RDI) has occurred.  A line RDI is asserted
		when a 110 binary pattern is detected in bits 6,7,8 of
		the K2 byte for five consecutive frames. A line RDI is
		removed when any pattern other than 110 is detected in
		these bits for five consecutive frames."
	::= { foreSonetLineTotalEntry 4 }


foreSonetLineDiagnosticsTable OBJECT-TYPE
	SYNTAX SEQUENCE OF ForeSonetLineDiagnosticsEntry
	MAX-ACCESS	not-accessible
	STATUS	current
	DESCRIPTION
		"A table of configuration options and statistics that
		 extend the Sonet Line Group in the Standard SONET MIB."
	::= { foreSonetLine 3 }


foreSonetLineDiagnosticsEntry OBJECT-TYPE
 	SYNTAX ForeSonetLineDiagnosticsEntry
 	MAX-ACCESS not-accessible
 	STATUS current
 	DESCRIPTION
 		"A table entry containing Fore specific extensions to
 		 the standard SONET Line Group Tables."
 	INDEX { ifIndex }
 	::= { foreSonetLineDiagnosticsTable 1 }


ForeSonetLineDiagnosticsEntry ::=
 	SEQUENCE {
		foreSonetLineDiagnosticsLAIS		INTEGER,
		foreSonetLineDiagnosticsB2		INTEGER,
		foreSonetLineDiagnosticsAPS		INTEGER,
		foreSonetLineDiagnosticsK1channel	INTEGER,
		foreSonetLineDiagnosticsK1request	INTEGER,
		foreSonetLineDiagnosticsK2channel	INTEGER,
		foreSonetLineDiagnosticsK2Architecture	INTEGER,
		foreSonetLineDiagnosticsK2Mode		INTEGER,
		foreSonetLineDiagnosticsLRDI		INTEGER,
		foreSonetLineDiagnosticsLREI		INTEGER
 	}


foreSonetLineDiagnosticsLAIS OBJECT-TYPE
 	SYNTAX	INTEGER {
 		on(1),
 		off(2)
		}
 	MAX-ACCESS	read-write
 	STATUS	current
 	DESCRIPTION
		"Controls the insertion of line alarm indication signal
		 (AIS_L). 

		on	When set to on, all bits of the SONET frame are 
			set to 1 except for the section overhead.
		off	resume normal operation"
	DEFVAL { off }
 	::= { foreSonetLineDiagnosticsEntry 1 }


foreSonetLineDiagnosticsB2 OBJECT-TYPE
 	SYNTAX	INTEGER {
		on(1),
		off(2),
		momentary(3)
		}
 	MAX-ACCESS	read-write
 	STATUS	current
 	DESCRIPTION
		"Controls the insertion of BIP errors in the sonet line 
	         B2 byte overhead. 

		off		When set to off, do not generate line BIP 
				errors.
		on		When set to on, generate line BIP errors.
		momentary	When set to momentary, transmit a chip dependent  
				number of Sonet frame with line BIP errors."
	DEFVAL { off }
 	::= { foreSonetLineDiagnosticsEntry 2 }

 
foreSonetLineDiagnosticsAPS OBJECT-TYPE
 	SYNTAX	INTEGER {
 		on(1),
 		off(2)
		}
 	MAX-ACCESS	read-write
 	STATUS	current
 	DESCRIPTION
 		"Controls the insertion of line APS bytes (K1 and K2)
		that are determined by normal operating rules or by
		diagnostic mibs.

		off	When set to off, resume normal operation.
		on	When set to on, K1 and K2 bytes are calculated
			by contents of diagnostic mibs."
 	DEFVAL { off }
 	::= { foreSonetLineDiagnosticsEntry 3 }


foreSonetLineDiagnosticsK1channel OBJECT-TYPE
 	SYNTAX	INTEGER {
 		null(0),
 		working1(1),
		working2(2),
		working3(3),
		working4(4),
		working5(5),
		working6(6),
		working7(7),
		working8(8),
		working9(9),
		working10(10),
		working11(11),
		working12(12),
		working13(13),
		working14(14),
		extraTraffic(15)
		}
 	MAX-ACCESS	read-write
 	STATUS	current
 	DESCRIPTION
		"Specifies the channel number code, k1 bits 5 to 8.

		* null(0) -  null channel
		Code 0 is used with the Lockout of Protection request.

		* working(1..14) - Working channels
		Codes 1 through n apply in a 1:n architecture
		Only code 1 applies in a 1+1 architecture

		*extraTraffic(15) - Extra traffic channel
		May exist only when provisioned in a 1:n architecture
		NOTE: Refer to GR-253-CORE for more details"
 	DEFVAL { null }
 	::= { foreSonetLineDiagnosticsEntry 4}


foreSonetLineDiagnosticsK1request OBJECT-TYPE
 	SYNTAX	INTEGER {
		lockout(15),
		forcedsw(14),
		sfhp(13),
		sflp(12),
		sdhp(11),
		sdlp(10),
		manualsw(8),
		wtr(6),
		exercise(4),
		rr(2),
		donotrevert(1),
		noreq(0)
		}
 	MAX-ACCESS	read-write
 	STATUS	current
 	DESCRIPTION
		"Defines the various values for K1 byte (1-4) for the 
		 type of request

		 For 1+1 systems, Forced and Manual switch request apply.

	 	 SD and SF requests apply to conditions detected on the 
		 protection line."
 	DEFVAL { noreq }
 	::= { foreSonetLineDiagnosticsEntry 5 }


foreSonetLineDiagnosticsK2channel OBJECT-TYPE
 	SYNTAX	INTEGER {
 		null(0),
 		working1(1),
		working2(2),
		working3(3),
		working4(4),
		working5(5),
		working6(6),
		working7(7),
		working8(8),
		working9(9),
		working10(10),
		working11(11),
		working12(12),
		working13(13),
		working14(14),
		extraTraffic(15)
		}
 	MAX-ACCESS	read-write
 	STATUS	current
 	DESCRIPTION
		"Specifies the channel number code, k2 bits 1 to 4
		* null - null channel
		Code 0 is used with the Lockout of Protection request.

		* working(1..14) - Working channels
		Codes 1 through n apply in a 1:n architecture
		Only code1 applies in a 1+1 architecture

		* extraTraffic(15) - Extra traffic channel
		May exist only when provisioned in a 1:n architecture
                NOTE: Refer to GR-253-CORE for more details"
 	DEFVAL { null }
 	::= { foreSonetLineDiagnosticsEntry 6 }


foreSonetLineDiagnosticsK2Architecture OBJECT-TYPE
 	SYNTAX	INTEGER {
	oneplusone(0),
	onen(1)
	}
 	MAX-ACCESS	read-write
 	STATUS	current
 	DESCRIPTION
		"Bit 5 of the K2 byte shall be set to indicate:
		
		 Code '0' if the provisioned (or only supported) 
		  architecture is 1+1
		 Code '1' if the provisioned (or only supported) 
		  architecture is 1:n"
 	DEFVAL { oneplusone }
 	::= { foreSonetLineDiagnosticsEntry 7 }


foreSonetLineDiagnosticsK2Mode OBJECT-TYPE
 	SYNTAX	INTEGER {
	unidirectional(4),
	bidirectional(5)
	}
 	MAX-ACCESS	read-write
 	STATUS	current
 	DESCRIPTION
		"Bits 6 through 8 of the K2 byte shall be set to indicate:
		 Code '101' if the provisioned mode is bidirectional
		 Code '100' if the provisioned mode is unidirectional
		The codes '011', '010', '001' and '000' in K2 bites 6-8 
		are reserved for future use in 1:n drop and insert(nested) 
		protection switching, and the code '111' is used in 
		detecting an incoming AIS_L."
 	DEFVAL { unidirectional }
 	::= { foreSonetLineDiagnosticsEntry 8 }


foreSonetLineDiagnosticsLRDI OBJECT-TYPE
 	SYNTAX	INTEGER {
 		on(1),
 		off(2),
		auto(4)
		}
 	MAX-ACCESS	read-write
 	STATUS	current
	DESCRIPTION
		"Controls the insertion of line Remote Defect Indication 
		 (Line RDI). Line RDI is inserted by transmitting the code 
		 110 (binary) in bit positions 6, 7 and 8 of the K2 byte 
		 contained in the transmit stream.

		off	When set to off, Line RDI is NOT inserted in the 
			transmit stream even if LOS/LOF is received
		on	When set to on, Line RDI is inserted continuously 
			in the transmit stream.
		auto	When set to auto, Line RDI is inserted as under 
			normal operation"
 	DEFVAL { auto }
 	::= { foreSonetLineDiagnosticsEntry 9 }


foreSonetLineDiagnosticsLREI OBJECT-TYPE
 	SYNTAX	INTEGER {
		on(1),
		off(2),
		auto(4)
 		}
 	MAX-ACCESS	read-write
 	STATUS	current
 	DESCRIPTION
		"Controls the insertion of Line Remote Error Indication
		 (REI_L). The number of BIP errors (B2) detected are encoded
		 in the transmit REI_L (M0/M1) byte. REI_L was previously
		 called Line FEBE (Far End Block Error). 

		off	When set to off, insert 0 for M0/M1 bytes,
			irrespective of the number of B2 errors received.
		on	When set to on, insert 8/8N for M0/M1 bytes 
			continuously.
		auto	When set to auto, resume normal operation."
 	DEFVAL { auto }
 	::= { foreSonetLineDiagnosticsEntry 10 }


-- Fore extensions to the Sonet Path Tables

foreSonetPathConfigTable OBJECT-TYPE
	SYNTAX SEQUENCE OF ForeSonetPathConfigEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION
		"A table of configuration options and statistics
		 that extend the standard SONET Path Tables"
	::= { foreSonetPath 1 }

foreSonetPathConfigEntry OBJECT-TYPE
	SYNTAX 		ForeSonetPathConfigEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION
		"A table entry containing Fore specific extensions
		 to the standard SONET path tables"
	INDEX { ifIndex }
	::= { foreSonetPathConfigTable 1 }

ForeSonetPathConfigEntry ::=
	SEQUENCE {
		foreSonetPathRxSignalLabel	Integer32,
		foreSonetPathTxSignalLabel	Integer32,
		foreSonetPathLoopbackMode	INTEGER
	}

foreSonetPathRxSignalLabel OBJECT-TYPE
	SYNTAX	Integer32
	MAX-ACCESS	read-only
	STATUS	current
	DESCRIPTION
		"This variable contains the path signal label byte
		(C2).  The value in this variable is updated to a new
		path signal label value if the same new value is
		observed for two consecutive frames."
	DEFVAL { 19 }
	::= { foreSonetPathConfigEntry 2 }

foreSonetPathTxSignalLabel OBJECT-TYPE
	SYNTAX	Integer32
	MAX-ACCESS	read-write
	STATUS	current
	DESCRIPTION
		"This variable controls the transmit path payload.
		The default value for ATM is 0x13 (19 decimal). Changing
		the value of this variable attempts to change the payload
		and if successfull will change the transmitted C2 label
		to reflect the new payload.  Use the C2 variable in the
		diagnostics table if you wish to change the C2 value for
		diagnostic purposes without actually changing the
		transmitted payload type."
	DEFVAL { 19 }
	::= { foreSonetPathConfigEntry 3 }
 
foreSonetPathLoopbackMode OBJECT-TYPE
	SYNTAX	INTEGER {
		sonetPathNoLoop(1), 
		sonetPathLineLoop(2),
		sonetPathDiagLoop(3),
		sonetPathOtherLoop(4)
		}
	MAX-ACCESS	read-write
	STATUS	current
	DESCRIPTION
		"This variable represents the current loopback status of the 
		SONET interface.
		The possible values are: 
		sonetPathNoLoop (1) means that the interface is not in a 
		loopback state.
		sonetPathLineLoop (2) means that the receive data 
		is connected to the transmit data.
		sonetPathDiagLoop (3) means that the transmit data 
		stream is looped back to the receiver.
		sonetPathOtherLoop (4) means that the interface is in a 
		loopback that is not defined here."
	DEFVAL { sonetPathNoLoop }
	::= { foreSonetPathConfigEntry 5 }


-- Fore Sonet Path Total Table

foreSonetPathTotalTable OBJECT-TYPE
	SYNTAX SEQUENCE OF ForeSonetPathTotalEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION
		"A table of configuration options and statistics
		 that extend the standard SONET Path Tables"
	::= { foreSonetPath 2 }

foreSonetPathTotalEntry OBJECT-TYPE
	SYNTAX 		ForeSonetPathTotalEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION
		"A table entry containing Fore specific extensions
		 to the standard SONET path tables"
	INDEX { ifIndex }
	::= { foreSonetPathTotalTable 1 }

ForeSonetPathTotalEntry ::=
	SEQUENCE {
		foreSonetPathTotalBIPs		Counter32,
		foreSonetPathTotalLOPs		Counter32,
		foreSonetPathTotalAISs		Counter32,
		foreSonetPathTotalRDIs		Counter32,
		foreSonetPathTotalUNEQs		Counter32,
		foreSonetPathTotalPLMs		Counter32,
		foreSonetPathTotalFEBEs		Counter32
	}

foreSonetPathTotalBIPs OBJECT-TYPE
	SYNTAX	Counter32
	MAX-ACCESS	read-only
	STATUS	current
	DESCRIPTION
		"The number of Path BIP-8 (Bit Interleaved Parity) errors that have been 
		detected since the last time the port has been reset. A path BIP-8 error is 
		detected by comparing the path BIP-8 byte (B3) extracted from the current 
		frame, to the path BIP-8 computed for the previous frame."
	::= { foreSonetPathTotalEntry 1 }

foreSonetPathTotalLOPs OBJECT-TYPE
	SYNTAX	Counter32
	MAX-ACCESS	read-only
	STATUS	current
	DESCRIPTION
		"The number of seconds in which path Loss Of Pointer (LOP) has occurred. A path
		LOP is detected when a 'normal pointer value' is not found in eight consecutive 
		frames. The LOP is cleared when a 'normal pointer value' is detected for three 
		consecutive frames."
	::= { foreSonetPathTotalEntry 2 }

foreSonetPathTotalAISs OBJECT-TYPE
	SYNTAX	Counter32
	MAX-ACCESS	read-only
	STATUS	current
	DESCRIPTION
		"The number of seconds in which path Alarm Indication Signal (AIS) has occurred. 
		A path AIS is asserted when an all-ones pattern is detected in the pointer bytes 
		(H1 and H2) for three consecutive frames. It is cleared when a valid pointer is 
		detected for three consecutive frames. AIS indicates that an upstream failure 
		has been detected."
	::= { foreSonetPathTotalEntry 3 }


foreSonetPathTotalRDIs OBJECT-TYPE
	SYNTAX	Counter32
	MAX-ACCESS	read-only
	STATUS	current
	DESCRIPTION
		"The number of seconds in which path RDI alarm has occurred. A path RDI 
		alarm is detected by extracting bit 5 of the path status byte. If bit 5 is 
		high for ten consecutive frames, a RDI alarm is declared. A RDI alarm 
		is cleared when bit 5 is low for ten consecutive frames. RDI signals are 
		used to alert upstream terminals of a downstream failure in order to initiate 
		trunk conditioning on the failure circuit."
	::= { foreSonetPathTotalEntry 4 }


foreSonetPathTotalUNEQs OBJECT-TYPE
	SYNTAX	Counter32
	MAX-ACCESS	read-only
	STATUS	current
	DESCRIPTION
		"The number of seconds in which a path UNEQ defect has occurred.  A path UNEQ
		defect is detected when the STS Signal label (C2 byte) == 0x00"
	::= { foreSonetPathTotalEntry 5 }


foreSonetPathTotalPLMs OBJECT-TYPE
	SYNTAX	Counter32
	MAX-ACCESS	read-only
	STATUS	current
	DESCRIPTION
		"The number of seconds in which a path PLM defect has occrred.  A path PLM
		defect is detected when the STS Signal lable (C2 bytes) != 0x00, 0x01, 0x13,
		0xFC or 0xFF."
	::= { foreSonetPathTotalEntry 6 }

foreSonetPathTotalFEBEs OBJECT-TYPE
	SYNTAX Counter32
	MAX-ACCESS	read-only
	STATUS	current
	DESCRIPTION
		"The number of path Far End Block Errors (FEBE) that have been detected since 
		the last time the port has been reset. FEBEs are detected by extracting the 
		4-bit FEBE field from the path status byte (G1). The legal range for the 4 bit 
		field is between 0000 and 1000, representing zero to eight errors. Any other 
		value is interpreted as zero errors."
	::= { foreSonetPathTotalEntry 7 } 

-- Fore Sonet Path Current Table
foreSonetPathCurrentTable OBJECT-TYPE
        SYNTAX SEQUENCE OF ForeSonetPathCurrentEntry
        MAX-ACCESS          not-accessible
        STATUS          current
        DESCRIPTION
               "A table of configuration options and statistics
                that extend the standard SONET Path table."
        ::= { foreSonetPath 3 }

foreSonetPathCurrentEntry OBJECT-TYPE
	SYNTAX		ForeSonetPathCurrentEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION
		"A table entry containing fore specific extensions
		 to the current status table for SONET paths."
	INDEX  { ifIndex }
	::= { foreSonetPathCurrentTable 1 }

ForeSonetPathCurrentEntry ::=
	SEQUENCE {
		foreSonetPathCurrentStatus      Integer32,
		foreSonetPathCurrentBBEs	Integer32
        }

foreSonetPathCurrentStatus      OBJECT-TYPE
	SYNTAX Integer32
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"This variable indicates information about the
	         current status of the sonet path.  This variable
                 is encoded as a bit vector so it can represent
                 multiple conditions simultaneously.

                 The various bit positions are:
			1       pathPDI-P detected

				pathPDI-P is set when the incoming
				path C2 byte is set to a value in
				the range 0xe1..0xfc.
		"
	::= { foreSonetPathCurrentEntry 1}

foreSonetPathCurrentBBEs OBJECT-TYPE
        SYNTAX  Integer32
        MAX-ACCESS  read-only
        STATUS current
        DESCRIPTION
                "The counter associated with the number of Background
                 Errors encountered by a
                 SONET/SDH Path in the current 15 minute interval."
        ::= { foreSonetPathCurrentEntry 2 }

-- Fore Sonet Path Diagnostics Table

foreSonetPathDiagnosticsTable OBJECT-TYPE
	SYNTAX SEQUENCE OF ForeSonetPathDiagnosticsEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION
		"A table of diagnostics tests that extend the 
		 standard SONET Path Tables"
	::= { foreSonetPath 4 }


foreSonetPathDiagnosticsEntry OBJECT-TYPE
 	SYNTAX 		ForeSonetPathDiagnosticsEntry
 	MAX-ACCESS	not-accessible
 	STATUS		current
 	DESCRIPTION
 		"A table entry containing Fore specific extensions
 		 to the standard SONET path tables"
 	INDEX { ifIndex }
 	::= { foreSonetPathDiagnosticsTable 1 }
 
ForeSonetPathDiagnosticsEntry ::=
	SEQUENCE {
		foreSonetPathDiagnosticsC2		INTEGER,
		foreSonetPathDiagnosticsC2Value		INTEGER,
		foreSonetPathDiagnosticsB3		INTEGER,
 		foreSonetPathDiagnosticsPREI		INTEGER,
		foreSonetPathDiagnosticsPAIS		INTEGER,
		foreSonetPathDiagnosticsPERDI		INTEGER,
		foreSonetPathDiagnosticsERDIValue	INTEGER,
		foreSonetPathDiagnosticsLOP		INTEGER
	}
 
foreSonetPathDiagnosticsC2 OBJECT-TYPE
 	SYNTAX	INTEGER {
		on(1),
 		off(2)
		}
 	MAX-ACCESS	read-write
 	STATUS	current
 	DESCRIPTION
		"Controls the insertion of Signal label/Payload Defect
		 Indication (PDI_P) into the C2 byte of the Sonet path
		 overhead. The various values for Signal label/PDI_P are
		 defined in GR-253-CORE.

		off	When set to off, the C2 byte is set to previously
			configured C2 value.
		on	When set to on, the foreSonetPathDiagnosticsC2Value
			is inserted in the C2 byte of the tranmsit stream."
 	DEFVAL { off }
 	::= { foreSonetPathDiagnosticsEntry 1 }

foreSonetPathDiagnosticsC2Value OBJECT-TYPE
 	SYNTAX	INTEGER  {
		unequipped(0),
		equippednonspecPL(1),
		vtstructSTSSPE(2),
		lockedVTmode(3),
		asyncDS3mapping(4),
		asyncDS4na(18),
		atmmapping(19),
		dqdbmapping(20),
		asyncfddimapping(21),
		hdlcwithscrambling(22),
		sts1with1vtxPD(225),
		sts1with2vtxPD(226),
		sts1with3vtxPD(227),
		sts1with4vtxPD(228),
		sts1with5vtxPD(229),
		sts1with6vtxPD(230),
		sts1with7vtxPD(231),
		sts1with8vtxPD(232),
		sts1with9vtxPD(233),
		sts1with10vtxPD(234),
		sts1with11vtxPD(235),
		sts1with12vtxPD(236),
		sts1with13vtxPD(237),
		sts1with14vtxPD(238),
		sts1with15vtxPD(239),
		sts1with16vtxPD(240),
		sts1with17vtxPD(241),
		sts1with18vtxPD(242),
		sts1with19vtxPD(243),
		sts1with20vtxPD(244),
		sts1with21vtxPD(245),
		sts1with22vtxPD(246),
		sts1with23vtxPD(247),
		sts1with24vtxPD(248),
		sts1with25vtxPD(249),
		sts1with26vtxPD(250),
		sts1with27vtxPD(251),
		payloaddefect(252)	
		}
 	MAX-ACCESS	read-write
 	STATUS	current
 	DESCRIPTION
		"Contains C2 byte value of the transmit stream"
 	DEFVAL { equippednonspecPL }
 	::= { foreSonetPathDiagnosticsEntry 2 }

foreSonetPathDiagnosticsB3 OBJECT-TYPE
 	SYNTAX	INTEGER {
		on(1),
 		off(2),
		momentary(3)
		}		
 	MAX-ACCESS	read-write
 	STATUS	current
 	DESCRIPTION
		"Controls the insertion of Path BIP errors in the sonet path B3
		 byte overhead. 

		off		When set to off, do not generate path BIP errors.
		on		When set to on, generate path BIP errors.
		momentary	When set to momentary, transmit a chip dependent
			        number of Sonet frames with path BIP errors."
	DEFVAL { off }
 	::= { foreSonetPathDiagnosticsEntry 3 }

foreSonetPathDiagnosticsPREI OBJECT-TYPE
 	SYNTAX	INTEGER {
		on(1),
 		off(2),
		auto(4)
 		}
 	MAX-ACCESS	read-write
 	STATUS	current
 	DESCRIPTION
		"Controls the insertion of Sonet Path Remote Error
		 Indication (REI_P) (previously called Far End Block Error 
		 (FEBE)).

					G1 byte
			 1    2    3    4    5     6    7    8	
			----|----|----|----|----|----|----|----|
		 	|     REI_P        |    RDI_P     | uu |
			----------------------------------------

		REI_P 		Path Remote Error Indication
		RDI_P		Path Remote Defect Indication
		uu  		Unused

		off	When set to off, the value 0 is inserted in bits 
		 	1:4 of G1 byte.
		on 	When set to on, the value 0x8 is inserted in bits 
			1:4 of G1 byte.
		auto	When set to auto, resume normal operation."
	DEFVAL { auto }
 	::= { foreSonetPathDiagnosticsEntry 4 }


foreSonetPathDiagnosticsPAIS OBJECT-TYPE
 	SYNTAX	INTEGER {
		on(1),
 		off(2)
		}
 	MAX-ACCESS	read-write
 	STATUS	current
 	DESCRIPTION
 		"Controls the insertion of STS Path Alarm Indication Signal
		 (AIS_P).  

		off	When set to off, AIS_P is not generated under any
			circumstances.
		on	When set to on, AIS_P is generated."
	DEFVAL { off }
 	::= { foreSonetPathDiagnosticsEntry  5 }


foreSonetPathDiagnosticsPERDI OBJECT-TYPE
 	SYNTAX	INTEGER {
		on(1),
		auto(4)
		}
 	MAX-ACCESS	read-write
 	STATUS	current
 	DESCRIPTION
		"Controls the insertion of Enhanced Remote Defect
		 Indication. Bits 5,6, and 7 of G1 byte is used for the
		 inserting ERDI.

					G1 byte
			 1    2    3    4    5     6    7    8	
			----|----|----|----|----|----|----|----|
		 	|     REI_P        |    RDI_P     | uu |
			----------------------------------------

		REI_P 		Path Remote Error Indication
		RDI_P		Path Remote Defect Indication
		uu  		Unused

		on 	When set to on, foreSonetPathDiagnosticsERDIValue
			is inserted in the bits 5,6,7 of G1 byte.
		auto	When set to auto, resume normal operation using
		        single bit RDI."
 	DEFVAL { auto }
	::= { foreSonetPathDiagnosticsEntry 6 }

foreSonetPathDiagnosticsERDIValue OBJECT-TYPE
 	SYNTAX	INTEGER {
		sonetPathERDIOnebitRDI(4),
		sonetPathERDIserverDefect(5),
		sonetPathERDIconnDefect(6),
		sonetPathERDIpayloadDefect(2),
		sonetPathERDInoDefect(1),
		sonetPathERDInoPrionoDefect(0)
		}
 	MAX-ACCESS	read-write
 	STATUS	current
 	DESCRIPTION
		"Defines the values that may be inserted in the bits
		 5,6 and 7 of G1 byte.

		1xx - One bit RDI
		000 - No RDI_P defect with no priority
		001 - No RDI_P defect
		010 - ERDI_P payload defect
		101 - ERDI_P server defect
		110 - ERDI_P connectivity defect"
 	DEFVAL { sonetPathERDInoDefect }
 	::= { foreSonetPathDiagnosticsEntry 7 }

foreSonetPathDiagnosticsLOP OBJECT-TYPE
 	SYNTAX	INTEGER {
		on(1),
 		off(2)
		}		
 	MAX-ACCESS	read-write
 	STATUS	current
 	DESCRIPTION
		"Controls the insertion of Loss of pointer for path level.

		off	When set to off, loss of pointer is not
			generated.
		on	When set to on, loss of pointer is generated
			in the transmit direction."
	DEFVAL { off }
 	::= { foreSonetPathDiagnosticsEntry 8 }



