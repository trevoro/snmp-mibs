	SYNTAX	Counter	
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	     "Shows the number of times that a request for memory has 
	     failed."
	::= { accL2tpTransportUdpScalars 5 }
    accL2tpUdpStatsInDiscards	OBJECT-TYPE
	SYNTAX	Counter	
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	     "Shows the number of received L2TP packets that were discarded."
	::= { accL2tpTransportUdpScalars 6 }
    accL2tpUdpStatsUnknownTIDs	OBJECT-TYPE
	SYNTAX	Counter	
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	     "Shows the number of packets received from unknown tunnels."
