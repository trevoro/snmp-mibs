               SYNTAX  INTEGER {
                               hdx(1), 
                               fdx(2) 
                               }            
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION "Indicates which current port status is full-duplex
                            or half-duplex.  If it is set to '1', it is 
                            half-duplex. If it is set to '2', it is 
                            full-duplex."
               ::= { mpEtherPortStatusEntry 5 }



