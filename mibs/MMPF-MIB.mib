                                     INTEGER,
                        mpIpFilterExEntSrcIpAddr
                                     IpAddress,
                        mpIpFilterExEntSrcAddrMask
                                     IpAddress,
                        mpIpFilterExEntDestIpAddr
                                     IpAddress,
