        devShowAdaptive-copy                RDFAdaptiveCopy,
        devShowAdaptive-copy-skew           UInt32,
        devShowNum-r1-invalid-tracks        UInt32,
        devShowNum-r2-invalid-tracks        UInt32,
        devShowDev-rdf-state                DeviceStatus,
        devShowRemote-dev-rdf-state         DeviceStatus,
        devShowRdf-status                   DeviceStatus,
        devShowLink-domino                  StateValues,
        devShowPrevent-auto-link-recovery   StateValues,
        devShowLink-config                  RDFLinkConfig,
        devShowSuspend-state                RDDFTransientState,
        devShowConsistency-state            StateValues,
        devShowAdaptive-copy-wp-state       RDDFTransientState,
