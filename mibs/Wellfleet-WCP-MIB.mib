    	    wfHwCompMsaTxExpandedPkts
    		Counter,
    	    wfHwCompStacTxExpandedPkts
    		Counter,
            wfHwCompMsaTxNonCompressedPkts
                Counter,
            wfHwCompStacTxNonCompressedPkts
                Counter,
            wfHwCompMsaRxNonCompressedPkts
                Counter,
            wfHwCompStacRxNonCompressedPkts
                Counter
        }

