	ENTERPRISE  breezecomWB10
    VARIABLES {
	    brzLastAPMacAddress,		-- Old AP mac address
		brzTrapAPMacAddr,			-- New AP mac address
		brzTrapLastRssiQuality,		-- Average RSSI with old AP
		brzTrapRssiQuality			-- Average RSSI with new AP
