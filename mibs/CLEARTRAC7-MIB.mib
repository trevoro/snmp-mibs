                SYNTAX  IpaddrEntry
                ACCESS  not-accessible
                STATUS  mandatory
                DESCRIPTION
                        "This table is an index list of all
                         ip addresses of this unit."
                INDEX   { ipaddrIndex }
                ::= { ipaddrTable 1 }
            
