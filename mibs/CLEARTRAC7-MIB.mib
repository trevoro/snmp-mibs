                ::= { statIflanEntry 7 }
            
            statIflanMeanRx-kbps OBJECT-TYPE
                SYNTAX  Gauge
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                        "This is the average throughput for data received
                        from the LAN."
                ::= { statIflanEntry 8 }
            
            statIflanPeakTx-kbps OBJECT-TYPE
                SYNTAX  Gauge
