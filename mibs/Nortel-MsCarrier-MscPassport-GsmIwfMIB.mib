MscGsmCsV42bisProvEntry ::= SEQUENCE {
   mscGsmCsV42bisCompressionDirection
      INTEGER,
   mscGsmCsV42bisMaximumCodewords
      Unsigned32,
   mscGsmCsV42bisMaximumStringSize
      Unsigned32,
