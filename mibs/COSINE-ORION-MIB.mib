	::= { csOrionFwSmtpConfigEntry 8 }

    csOrionFwSmtpLogSpam OBJECT-TYPE
	SYNTAX  INTEGER {
		none	(1),
		reject	(2)
	}
	MAX-ACCESS  read-write
	STATUS  deprecated
	DESCRIPTION
	    "Logs the rejection of mail spam attempts."
	DEFVAL { none }
	::= { csOrionFwSmtpConfigEntry 9 }

