         MAX-ACCESS   read-write
         STATUS   current
         DESCRIPTION
            "LMI monitored event count. For DCE IF Type.
			 This indicates the expected number of
			 seconds between heartbeat polls."
	    DEFVAL { 15 }
        ::= { csFrDlcmiEntry 22 }

	csFrDlcmiAsyncEnabled OBJECT-TYPE
        SYNTAX   INTEGER    {
                     enable  (1),
                     disable (2)
                     }
        MAX-ACCESS   read-write
        STATUS   current
        DESCRIPTION
            "."
