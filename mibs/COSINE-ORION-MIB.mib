			cenc	(4),	-- content encoding
			clng	(5),	-- content language
			date	(6),	-- date
			exp		(7),	-- expires
			from	(8),	-- from
			imod	(9),	-- if modified since
			lmod	(10),	-- last modified
			lnk		(11),	-- link
			mime	(12),	-- mime version
			prgm	(13),	-- pragma
			refr	(14),	-- referer
			reta	(15),	-- retry after
			serv	(16),	-- server
			titl	(17),	-- title
