	SYNTAX     INTEGER {
		ef  (1),
		af1 (2),
		af2 (3),
		af3 (4),
		af4 (5),
		be  (6),
		none (7),
		ef-low (8),
		ef-high (9)
       }
   MAX-ACCESS    not-accessible
   STATUS        current
   DESCRIPTION
       "The possible queue on a physical VI"
