    csOrionIpSecEventLogArea OBJECT-TYPE
        SYNTAX EvLogArea
        MAX-ACCESS read-write
        STATUS     current
        DESCRIPTION
               "This object is used to set the area of event logs
                for IPSEC/IKE."
        DEFVAL { '00000040'h  }  -- bit 6, ipsec/ike area
        ::= { csOrionIpSecEventLogEntry 2 }

    csOrionEncryptInfoEnd OBJECT-TYPE
	SYNTAX  Integer32
	MAX-ACCESS  read-only
	STATUS  current
	DESCRIPTION
		"End of this group. value undefined"
	::= { csOrionEncryptInfo 15 }

