	SYNTAX	Vm0001StereoPairEntry
    MAX-ACCESS not-accessible
    STATUS     current
    DESCRIPTION
        "An Analog Audio module channel entry."
	INDEX { vm0001StereoPairSlotNumber, vm0001StereoPairNumber }
    ::= { vm0001StereoPairTable 1 } 
		
Vm0001StereoPairEntry   ::= 
