	OBJECTS		{ hicomSQLsessionId, 
				hicomSQLsessionState,
				hicomSQLsessionResultSize,
				hicomSQLsessionResultNoLines }
	STATUS		current
	DESCRIPTION
		"A hicomSQLsessionFinished trap indicates the completion of
		an SQL query. If hicomSQLsessionState indicates an error in
		the query, hicomSQLsessionResultLines will contain the SQL
		error code instead of the number of resulting lines. In this
		case, no entry in the hicomSQLresultTable will be created."
	::=	{ hicomSQLTraps 20 }


-- BEGIN BLOCK discov

hicomDiscovTable	OBJECT-TYPE
	SYNTAX		SEQUENCE OF HicomDiscovEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION	"This table contains information about the master
			discoveries for each managed Hicom. Starting a master
			discoveries will lead to the individual discoveries 
			being started. 
			The termination of these discoveries will be
			signalled through their respective traps."
	::=	{ hicomDiscov 1 }

	hicomDiscovEntry	OBJECT-TYPE
		SYNTAX		HicomDiscovEntry
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION	"Each entry describes the master discovery for 
					a specific hicom."
		INDEX	{ hicomDiscovPabxId }
		::=	{ hicomDiscovTable 1 }

	HicomDiscovEntry ::=
		SEQUENCE {
			hicomDiscovPabxId		Integer32,
			hicomDiscovPabxMnemonic		DisplayString,
			hicomDiscovStatus		DiscoveryStates,
			hicomDiscovMode			DiscoveryModes,
			hicomDiscovTimDat		DisplayString,
			hicomDiscovErrTimDat		DisplayString
		}

	hicomDiscovPabxId	OBJECT-TYPE
		SYNTAX		Integer32
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Unique identifier of the Hicom being discovered."
		::=	{ hicomDiscovEntry 1 }

	hicomDiscovPabxMnemonic	OBJECT-TYPE
		SYNTAX	DisplayString
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Name of the Hicom."
		::=	{ hicomDiscovEntry 2 }

	hicomDiscovStatus	OBJECT-TYPE
		SYNTAX		DiscoveryStates
		MAX-ACCESS	read-write
		STATUS		current
		DESCRIPTION	"Status of master discovery process."
		::=	{ hicomDiscovEntry 3 }

	hicomDiscovMode		OBJECT-TYPE
		SYNTAX		DiscoveryModes
		MAX-ACCESS	read-write
		STATUS		current
		DESCRIPTION	"Mode of master discovery process."
		::=	{ hicomDiscovEntry 4 }

	hicomDiscovTimDat	OBJECT-TYPE
		SYNTAX		DisplayString
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Date and time of the last successful discovery."
		::=	{ hicomDiscovEntry 5 }

	hicomDiscovErrTimDat	OBJECT-TYPE
		SYNTAX		DisplayString
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Date and time of the last failed discovery."
		::=	{ hicomDiscovEntry 6 }

hicomDiscInfo OBJECT IDENTIFIER ::= { hicomDiscov 2 }

	hicomDiscSubagentLastMsgNo	OBJECT-TYPE
		SYNTAX		Integer32
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"The last message, warning or error number 
					issued by the discovery subagent."
	::=	{ hicomDiscInfo 1 }

	hicomDiscSubagentLastMsgText	OBJECT-TYPE
		SYNTAX		OCTET STRING
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"The last message, warning or error text
					issued by the discovery subagent."
	::=	{ hicomDiscInfo 2 }


hicomDiscovGroup	OBJECT-GROUP
	OBJECTS	{
		hicomDiscovPabxId,
		hicomDiscovPabxMnemonic,
		hicomDiscovStatus,
		hicomDiscovMode,
		hicomDiscovTimDat,
		hicomDiscovErrTimDat
	}
	STATUS		current
	DESCRIPTION	"Conformance statement for hicomDiscovGroup."
	::=	{ hicomDiscovTree 1 }

hicomDiscInfoGroup	OBJECT-GROUP
