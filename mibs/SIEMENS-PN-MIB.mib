            SYNTAX        DisplayString
            MAX-ACCESS    read-only
            STATUS        current
            DESCRIPTION
                  "Name of the Hicom proxy agent."
            ::= { hicomControl 1 }

         hicomAgentVersion OBJECT-TYPE
            SYNTAX        DisplayString
            MAX-ACCESS    read-only
            STATUS        current
            DESCRIPTION
                  "Version of the Hicom proxy agent. "
            ::= { hicomControl 2 }

