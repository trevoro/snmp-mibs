        scmNumOfVirtualDrivesOnline OBJECT-TYPE
            SYNTAX INT32withException
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "The Number of Virtual Drives online in the SCM."
            ::= { scmEntry 29 }

        scmNumOfVirtualDrivesOffline OBJECT-TYPE
            SYNTAX INT32withException
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "The Number of Virtual Drives offline in the SCM."
            ::= { scmEntry 30 }

        scmNumOfVirtualDrivesCritical OBJECT-TYPE
            SYNTAX INT32withException
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
