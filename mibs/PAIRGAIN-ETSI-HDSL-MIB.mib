DEFINITIONS ::= BEGIN

-- SECTION 1: Top Level Definitions

IMPORTS
        OBJECT-TYPE
            FROM RFC-1212
        pgainHDSL
            FROM PAIRGAIN-COMMON-HD-MIB;
 
 
-- Define a one byte integer
    Byte ::= INTEGER (0..255)

-- Define a two bytes integer
    Short ::= INTEGER (0..65535)

-- Define a time stamp structure. The time stamp consists of 6 bytes,
-- with the following format :
--     byte 1 : year within century,
--     byte 2 : month of year,
--     byte 3 : day of month,
--     byte 4 : hour (24 hour format)
--     byte 5 : minutes
--     byte 6 : seconds

    PgTimeStamp ::= OCTET STRING (SIZE(6))

    PgAlarmSeverityMode ::=
       INTEGER {
               disable(1),
               minor(2),
               major(3),
               notAvailable(4)
       }

    PgAlarmSeverityProtectionSwitchMode ::=
       INTEGER {
               disable(1),
               minor(2),
               major(3),
               protectionSwitchMajor(4),
               notAvailable(5)
       }

--
-- BER Threshold value
-- 
-- The BER is represented in two bytes, with the first byte represents
-- Mantissa of BER and second byte represents Exponent of BER.
--
--
-- The Mantissa of BER is represented in two BCD digits,
-- with the data point between the two digits."
--
-- Although the exponent value is positive, it shall be treated as a
-- negative value.
--
    PgBERThreshold ::= INTEGER (0..65535)

    ----------------------------------------------------
    -- HDSL 15-minute Performance History for All spans
    ----------------------------------------------------
    hdslSpan15mPerformanceTable  OBJECT-TYPE
        SYNTAX    SEQUENCE OF HdslSpan15mPerformanceEntry
        ACCESS    not-accessible
        STATUS    mandatory
        DESCRIPTION
            "This table provides the 15-minute performance history monitoring
             of the last 24 hours for all HDSL spans Errored Seconds (ES), 
             Severely Errored Seconds (SES) and Unavailable Seconds (UAS)."
        ::= {pgainHDSL 1}

    hdslSpan15mPerformanceEntry  OBJECT-TYPE
        SYNTAX    HdslSpan15mPerformanceEntry
        ACCESS    not-accessible
        STATUS    mandatory
        DESCRIPTION
            "Entries of the HDSL span 15-min performance table "
        INDEX
            {hdslSpan15mSlotId, hdslSpan15mSpanId, hdslSpan15mHistoryId}
        ::= {hdslSpan15mPerformanceTable 1}

    HdslSpan15mPerformanceEntry ::= SEQUENCE {
        hdslSpan15mSlotId
                INTEGER,
        hdslSpan15mSpanId
                INTEGER,
        hdslSpan15mHistoryId
                INTEGER,
        hdsl15mESLoopANet
                Short,
        hdsl15mESLoopBNet
                Short,
        hdsl15mUASLoopANet
                Short,
        hdsl15mUASLoopBNet
                Short,
        hdsl15mSESLoopANet
                Short,
        hdsl15mSESLoopBNet
                Short,
        hdsl15mESLoopACust
                Short,
        hdsl15mESLoopBCust
                Short,
        hdsl15mUASLoopACust
                Short,
        hdsl15mUASLoopBCust
                Short,
        hdsl15mSESLoopACust
                Short,
        hdsl15mSESLoopBCust
                Short
        }

    hdslSpan15mSlotId OBJECT-TYPE
        SYNTAX    INTEGER
        ACCESS    read-only
        STATUS    mandatory
        DESCRIPTION
            "Identifies the HDSL line occupied in the chassis.
             The slots are numbered 1 through 16 from left to right."
        ::= {hdslSpan15mPerformanceEntry 1}

    hdslSpan15mSpanId  OBJECT-TYPE
        SYNTAX    INTEGER
        ACCESS    read-only
        STATUS    mandatory
        DESCRIPTION
            "Identifies the span of the HDSL line we are monitoring.
             Depending on the number of regenerator(s) (REGs)
             present in an HDSL loop, a span will be numbered as follows:
                 0 REG   - only 1 span.
                 1 REG   - span between LTU and REG is identified as 1.
                           span between REG and NTU is identified as 2.
                 2 REGs  - span between LTU and REG #1 is identified as 1.
                           span between REG #1 and REG #2 is identified as 2.
                           span between REG #2 and NTU is identified as 3.
             "
        ::= {hdslSpan15mPerformanceEntry 2}

    hdslSpan15mHistoryId  OBJECT-TYPE
        SYNTAX    INTEGER
        ACCESS    read-only
        STATUS    mandatory
        DESCRIPTION
            "Identify the history ID within the 96 15-minute performance
             history.
            "
        ::= {hdslSpan15mPerformanceEntry 3}

    hdsl15mESLoopANet  OBJECT-TYPE
        SYNTAX    Short
        ACCESS    read-only
        STATUS    mandatory
        DESCRIPTION
            "HDSL 15-min ES count on Loop A's Network side of the span."
        ::= {hdslSpan15mPerformanceEntry 4}

    hdsl15mESLoopBNet  OBJECT-TYPE
        SYNTAX    Short
        ACCESS    read-only
        STATUS    mandatory
        DESCRIPTION
            "HDSL 15-min ES count on Loop B's Network side of the span."
        ::= {hdslSpan15mPerformanceEntry 5}

    hdsl15mUASLoopANet OBJECT-TYPE
