----------------------------------------------------------
--- HDSL Span Alarm Severity Settings.
----------------------------------------------------------
    hdslSpanAlarmSeverityTable  OBJECT-TYPE
        SYNTAX    SEQUENCE OF HdslSpanAlarmSeverityEntry
        ACCESS    not-accessible
        STATUS    mandatory
        DESCRIPTION
            "The HDSL span alarm severity table provides severity settings
             for HDSL alarms reporting and threshold settings for 
             Errored Second(ES), Margin(MAR), Unavailable Seconds(UAS) and
             Bit Error Rate(BER)."
        ::= {pgainHDSL 7}

    hdslSpanAlarmSeverityEntry OBJECT-TYPE
        SYNTAX    HdslSpanAlarmSeverityEntry
        ACCESS    not-accessible
        STATUS    mandatory
        DESCRIPTION
            "Entry in the hdslSpanAlarmSeverityTable"
        INDEX
            {hdslSpanAlarmSeveritySlotId, hdslSpanAlarmSeveritySpanId}
        ::= { hdslSpanAlarmSeverityTable 1 }

    HdslSpanAlarmSeverityEntry ::= SEQUENCE {
        hdslSpanAlarmSeveritySlotId
            INTEGER,
        hdslSpanAlarmSeveritySpanId 
            INTEGER,
        hdslSpanLOSWAlarmSetting
            PgAlarmSeverityProtectionSwitchMode,
        hdslSpanPFOAlarmSetting
            PgAlarmSeverityMode,
        hdslSpanPFSAlarmSetting
            PgAlarmSeverityMode,
        hdslSpanMarginThreshold
            Byte,
        hdslSpanMarginAlarmSetting
            PgAlarmSeverityProtectionSwitchMode,
        hdslSpanESThreshold
            Byte,
        hdslSpanESAlarmSetting
            PgAlarmSeverityProtectionSwitchMode,
        hdslSpanUASThreshold
            Byte,
        hdslSpanUASAlarmSetting
            PgAlarmSeverityMode,
        hdslSpanBERThreshold
            PgBERThreshold,
        hdslSpanBERAlarmSetting
            PgAlarmSeverityMode
    }

    hdslSpanAlarmSeveritySlotId OBJECT-TYPE
        SYNTAX    INTEGER
        ACCESS    read-only
        STATUS    mandatory
        DESCRIPTION
            "The slot number of the HDSL loop whose
             alarm severity or threshold we are setting.
             The slots are numbered 1 through 16 from left to right."
        ::= {hdslSpanAlarmSeverityEntry 1}

    hdslSpanAlarmSeveritySpanId OBJECT-TYPE
        SYNTAX    INTEGER
        ACCESS    read-only
        STATUS    mandatory
        DESCRIPTION
            "The span number of the HDSL loop whose
             alarm severity or threshold we are setting.
             Depending on the number of regenerator(s) (REGs) 
             present in an HDSL loop, a span will be numbered as follows:
                 0 REG   - only 1 span.
                 1 REG   - span between LTU and REG is identified as 1.
                           span between REG and NTU is identified as 2.
                 2 REGs  - span between LTU and REG #1 is identified as 1.
                           span between REG #1 and REG #2 is identified as 2.
                           span between REG #2 and NTU is identified as 3.
             "
            ::= {hdslSpanAlarmSeverityEntry 2}

    hdslSpanLOSWAlarmSetting OBJECT-TYPE
        SYNTAX    PgAlarmSeverityProtectionSwitchMode
        ACCESS    read-write
        STATUS    mandatory
        DESCRIPTION
            "Sets the severity for Loss of Sync Word alarm."
        ::= {hdslSpanAlarmSeverityEntry 3}

    hdslSpanPFOAlarmSetting OBJECT-TYPE
        SYNTAX    PgAlarmSeverityMode
        ACCESS    read-write
        STATUS    mandatory
        DESCRIPTION
            "Sets the severity for Power Feed Open alarm."
        ::= {hdslSpanAlarmSeverityEntry 4}

    hdslSpanPFSAlarmSetting OBJECT-TYPE
        SYNTAX    PgAlarmSeverityMode
        ACCESS    read-write
        STATUS    mandatory
        DESCRIPTION
            "Sets the severity for Power Feed Short alarm."
        ::= {hdslSpanAlarmSeverityEntry 5}

    hdslSpanMarginThreshold  OBJECT-TYPE
        SYNTAX    Byte
        ACCESS    read-write
        STATUS    mandatory
        DESCRIPTION
            "Sets the HDSL Margin threshold value."
        ::= {hdslSpanAlarmSeverityEntry 6}

    hdslSpanMarginAlarmSetting OBJECT-TYPE
        SYNTAX    PgAlarmSeverityProtectionSwitchMode
        ACCESS    read-write
        STATUS    mandatory
        DESCRIPTION
            "Sets the severity for Margin threshold exceeded alarm."
        ::= {hdslSpanAlarmSeverityEntry 7}

    hdslSpanESThreshold OBJECT-TYPE
        SYNTAX    Byte
        ACCESS    read-write
        STATUS    mandatory
        DESCRIPTION
            "Sets the HDSL Errored Seconds threshold value."
        ::= {hdslSpanAlarmSeverityEntry 8}

    hdslSpanESAlarmSetting OBJECT-TYPE
        SYNTAX    PgAlarmSeverityProtectionSwitchMode
        ACCESS    read-write
        STATUS    mandatory
        DESCRIPTION
            "Sets the severity for Errored Seconds threshold exceeded alarm."
        ::= {hdslSpanAlarmSeverityEntry 9}

    hdslSpanUASThreshold OBJECT-TYPE
        SYNTAX    Byte
        ACCESS    read-write
        STATUS    mandatory
        DESCRIPTION
            "Sets the HDSL Unavailable Seconds threshold value."
        ::= {hdslSpanAlarmSeverityEntry 10}
 
    hdslSpanUASAlarmSetting OBJECT-TYPE
        SYNTAX    PgAlarmSeverityMode
        ACCESS    read-write
        STATUS    mandatory
        DESCRIPTION
            "Sets the severity for UAS threshold exceeded alarm."
        ::= {hdslSpanAlarmSeverityEntry 11}

    hdslSpanBERThreshold OBJECT-TYPE
            SYNTAX    PgBERThreshold 
            ACCESS    read-write
            STATUS    mandatory
            DESCRIPTION
                "Sets the HDSL BER threshold value."
            ::={hdslSpanAlarmSeverityEntry 12}
 
    hdslSpanBERAlarmSetting OBJECT-TYPE 
        SYNTAX    PgAlarmSeverityMode 
        ACCESS    read-write    
        STATUS    mandatory 
        DESCRIPTION 
            "Sets the severity for HDSL BER threshold exceeded alarm."
        ::= {hdslSpanAlarmSeverityEntry 13} 

END


