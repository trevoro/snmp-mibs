					}

		logSysLogIndex		OBJECT-TYPE
				SYNTAX	INTEGER
				ACCESS	read-only
				STATUS	mandatory
				DESCRIPTION
					"The sequence number of the event."
				::= { logSysLogEntry 1 }

		logSysLogDate		OBJECT-TYPE
				SYNTAX	OCTET STRING
				ACCESS	read-only
				STATUS	mandatory
				DESCRIPTION
					"The date the event was logged at the terminal."
				::= { logSysLogEntry 2 }

		logSysLogTime		OBJECT-TYPE
				SYNTAX	OCTET STRING
				ACCESS	read-only
				STATUS	mandatory
				DESCRIPTION
					"The time of day the event was logged at the terminal."
				::= { logSysLogEntry 3 }

		logSysLogUser		OBJECT-TYPE
				SYNTAX	OCTET STRING
				ACCESS	read-only
				STATUS	mandatory
				DESCRIPTION
					"The user name in context of which the
					application logging the event is running."
				::= { logSysLogEntry 4 }

		logSysLogComputer	OBJECT-TYPE
				SYNTAX	OCTET STRING
				ACCESS	read-only
				STATUS	mandatory
				DESCRIPTION
					"The (NetBIOS/Windows computer) name of the
					terminal where the event was logged."
				::= { logSysLogEntry 5 }

		logSysLogEventID	OBJECT-TYPE
				SYNTAX	INTEGER
				ACCESS	read-only
				STATUS	mandatory
				DESCRIPTION
					"A numeric identification of the event."
				::= { logSysLogEntry 6 }

