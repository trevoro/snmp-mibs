		NTSeverity ::= INTEGER { error(1), warning(2), information(4), auditSuccess(8), auditFailure(16) }

-- log MIB

		logSysLog	OBJECT IDENTIFIER ::= { log 1 }

		logSysLogTable	OBJECT-TYPE
