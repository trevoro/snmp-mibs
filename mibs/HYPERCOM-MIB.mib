    -- This is the Private MIB module for Hypercom IEN Systems.
    -- The following groups describe the variables within the Hypercom
    -- product lines. Specific groups may or may not be present
    -- depending on the actual installed card types and the software
    -- options present in the managed IEN device.




    --Table of hypIpTable
    --hypIpTable description (Table format)
    hypSysTableTable OBJECT-TYPE
        SYNTAX   SEQUENCE OF HypSysTableEntry
        ACCESS   not-accessible
        STATUS   mandatory
        DESCRIPTION
        "A table containing information about
         the Hypercom System"
        ::= { hypSystem 1 }

        hypSysTableEntry OBJECT-TYPE
        SYNTAX   HypSysTableEntry
        ACCESS   not-accessible
        STATUS   mandatory
        DESCRIPTION
        "Hypercom System Table"
        INDEX { hypSysTableIndex }
        ::= { hypSysTableTable 1 }

        HypSysTableEntry ::=
        SEQUENCE {
            hypSysBusAddr
                INTEGER,
            hypSysSwName
                DisplayString,
            hypPortMode
                DisplayString,
            hypSysStatus
                DisplayString,
            hypSysCuAddress
                INTEGER,
            hypSysModeSwitch1
                INTEGER,
            hypSysModeSwitch2
                INTEGER,
            hypSysUpTime
                INTEGER,
            hypSysDownTime
                INTEGER,
            hypSysRestart
                INTEGER
            }


    -- The SYSTEM Group
    -- Implementation of this group is mandatory for all
    -- Hypercom IEN Systems although some specific members
    -- may or may not apply to all Hypercom IEN devices.

    hypSysBusAddr  OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Card BUS Address switch setting"
        ::= { hypSysTableEntry 1 }

    hypSysSwName  OBJECT-TYPE
        SYNTAX DisplayString
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Name and version number of the SW module  (SIZE (0..20)"
        ::= { hypSysTableEntry 2 }

    hypPortMode  OBJECT-TYPE
        SYNTAX DisplayString
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Port Card Operating mode ('E'=EPROM, 'R'=RAM)"
        ::= { hypSysTableEntry 3 }

    hypSysStatus  OBJECT-TYPE
        SYNTAX DisplayString
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Card Display status display values"
        ::= { hypSysTableEntry 4 }

    hypSysCuAddress  OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Card CU Address switch setting"
        ::= { hypSysTableEntry 5 }

    hypSysModeSwitch1  OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Card Mode switch 1 setting"
        ::= { hypSysTableEntry 6 }

    hypSysModeSwitch2  OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Card Mode switch 2 setting"
        ::= { hypSysTableEntry 7 }

    hypSysUpTime  OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "System Up time in minutes"
        ::= { hypSysTableEntry 8 }

    hypSysDownTime  OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "System Down time in minutes"
        ::= { hypSysTableEntry 9 }

    hypSysRestart  OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "System Restart"
        ::= { hypSysTableEntry 10 }




    -- The IP Group
    -- The IP Group contains information about the IP
    -- in a Hypercom IEN.
    -- For clearing counters see the hypSystem group object hypClearStatCounters



    --Table of hypIpTable
    --hypIpTable description (Table format)
    hypIpTableTable OBJECT-TYPE
        SYNTAX   SEQUENCE OF HypIpTableEntry
        ACCESS   not-accessible
        STATUS   mandatory
        DESCRIPTION
        "A table containing information about
         IP Table"
        ::= { hypIp 1 }

        hypIpTableEntry OBJECT-TYPE
        SYNTAX   HypIpTableEntry
        ACCESS   not-accessible
        STATUS   mandatory
        DESCRIPTION
        "IP Table"
        INDEX { hypIpTableIndex }
        ::= { hypIpTableTable 1 }

        HypIpTableEntry ::=
        SEQUENCE {
            hypIpIfIndex
                INTEGER,
            hypIpIn
                Counter,
            hypIpOut
                Counter,
            hypIpAverage
                Gauge,
            hypIpInSec
                Gauge,
            hypIpInMax
                Gauge,
            hypUnknownFrame
                Counter,
            hypTimeToLive
                INTEGER,
            hypMsgIn
                Counter,
            hypMsgOut
                Counter,
            hypUnreachIn
                Counter,
            hypUnreachOut
                Counter,
            hypPingIn
                Counter,
            hypPingOut
                Counter,
            hypDataIn
                Counter,
            hypDataOut
                Counter,
            hypBadFrames
                Counter,
            hypUnknownPort
                Counter
            }

        hypIpIfIndex  OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "The ifIndex  (IP Interface Instance)"
        ::= { hypIpTableEntry 1 }

        hypIpIn  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "The number of IP packet received since last reset"
        ::= { hypIpTableEntry 2 }

        hypIpOut  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "The number of IP packet sent since last reset"
        ::= { hypIpTableEntry 3 }

        hypIpAverage  OBJECT-TYPE
        SYNTAX Gauge
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Average number of IP frame received/sec since last reset"
        ::= { hypIpTableEntry 4 }

        hypIpInSec  OBJECT-TYPE
        SYNTAX Gauge
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Currently number of IP frame received/sec"
        ::= { hypIpTableEntry 5 }

        hypIpInMax  OBJECT-TYPE
        SYNTAX Gauge
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Maximum number of IP frame received/sec since last reset"
        ::= { hypIpTableEntry 6 }

        hypUnknownFrame  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Unknown protocol frame received"
        ::= { hypIpTableEntry 7 }

        hypTimeToLive  OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "IP Time To Live (TTL) value"
        ::= { hypIpTableEntry 8 }

    hypMsgIn  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Messages received"
        ::= { hypIpTableEntry 9 }

    hypMsgOut  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Messages sent"
        ::= { hypIpTableEntry 10 }

    hypUnreachIn  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Destination unreachable received"
        ::= { hypIpTableEntry 11 }

    hypUnreachOut  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Destination unreachable sent"
        ::= { hypIpTableEntry 12 }

    hypPingIn  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Pings received"
        ::= { hypIpTableEntry 13 }

    hypPingOut  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Pings sent"
        ::= { hypIpTableEntry 14 }

    hypDataIn  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Datagrams in"
        ::= { hypIpTableEntry 15 }

    hypDataOut  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Datagrams out"
        ::= { hypIpTableEntry 16 }

    hypBadFrames  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Bad frames in"
        ::= { hypIpTableEntry 17 }

    hypUnknownPort  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Unknown port received"
        ::= { hypIpTableEntry 18 }

    --End of Table hypIpTable  IP Table




