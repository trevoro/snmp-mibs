    -- The tcp Group
    -- The tcp Group contains information about the
    -- tcp Parameters in a Hypercom IEN.

    hypCntConnections  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "number of active TCP connections"
        ::= { hypTcp 1 }



    --Table of hypTcpConnections
    --hypTcpConnections description (Table format)
    hypTcpConnectionsTable OBJECT-TYPE
        SYNTAX   SEQUENCE OF HypTcpConnectionsEntry
        ACCESS   not-accessible
        STATUS   mandatory
        DESCRIPTION
        "A table containing information about
         "
        ::= { hypTcp 2 }

        hypTcpConnectionsEntry OBJECT-TYPE
        SYNTAX   HypTcpConnectionsEntry
        ACCESS   not-accessible
        STATUS   mandatory
        DESCRIPTION
        ""
        INDEX { hypTcpConnectionsIndex }
        ::= { hypTcpConnectionsTable 1 }

        HypTcpConnectionsEntry ::=
        SEQUENCE {
            hypTcpConnectionsIndex
                INTEGER,
            hypFramesIn
                Counter,
            hypFramesOut
                Counter,
            hypTcpPort
                INTEGER,
            hypTcpStatus
                INTEGER,
            hypIpAddr
                NetworkAddress
            }

        hypTcpConnectionsIndex  OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "The ifIndex Value of the corresponding ifEntry."
        ::= { hypTcpConnectionsEntry 1 }

        hypFramesIn  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Frames received"
        ::= { hypTcpConnectionsEntry 2 }

        hypFramesOut  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Frames sent"
        ::= { hypTcpConnectionsEntry 3 }

        hypTcpPort  OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "TCP port number"
        ::= { hypTcpConnectionsEntry 4 }

        hypTcpStatus  OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Connection status: Idle(0), ARPsent(1), SYNsent(2), Active(3)"
        ::= { hypTcpConnectionsEntry 5 }

        hypIpAddr  OBJECT-TYPE
        SYNTAX NetworkAddress
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
        "Remote IP address"
        ::= { hypTcpConnectionsEntry 6 }




    --End of Table hypTcpConnections






