foam              OBJECT IDENTIFIER ::= { oem 3 }


-- Card operational state
CardOpState ::= INTEGER {
    cardOpStateUnknown(0),
    cardOpStateNotPresent(1),
    cardOpStateStandby(2),
    cardOpStateActive(3),
    cardOpStateDown(4)
}

-- Card temperature status
CardTempStatus ::= INTEGER {
    tempUnknown(0),
    tempNominal(1),
    tempHigh(2),
    tempSevere(3),
    tempCritical(4)
}

-- Card voltage status
CardVoltageStatus ::= INTEGER {
    voltageUnknown(0),
    voltageNominal(1),
    voltageLow(2),
    voltageHigh(3)
}



