    ::= { wvLinkSettings 11 }

wvFecReceiveCorrection OBJECT-TYPE
    SYNTAX      INTEGER {
                    on(1),
                    off(2)
                    }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Enables/disables Automatic Error Correction on receive."
