    DESCRIPTION
        "Sets the terminal radio modulation.
        Not all values apply to all models."
    DEFVAL { cp4fsk }
    ::= { wvLinkSettings 9 }

wvForceAis OBJECT-TYPE
    SYNTAX      INTEGER {
                    never(1),
                    onBerWarning(2),
                    onBerError(3),
                    always(4)
                    }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Defines whether to force Alarm Indication Signal (all 1's) on the
        link upon either occurence of a link BER Warning or BER Error alarm or
        unconditionally."
    DEFVAL { never }
    ::= { wvLinkSettings 10 }

wvFecEnabled OBJECT-TYPE
    SYNTAX      INTEGER {
                    on(1),
                    off(2)
                    }
