          configRS232ModemStatus OBJECT-TYPE
              SYNTAX  INTEGER {
                      modemDetected(1),
                      autobauding(2),
                      noModemDetected(3)
              }
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The current status of the modem port connection."
              ::= { configRS232PortEntry 4 }

          configRS232ConnectionStatus OBJECT-TYPE
              SYNTAX  INTEGER {
                      connected(1),
                      connectionWait(2),
                      unknown(3)
              }
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The current status of the modem data connection."
              ::= { configRS232PortEntry 5 }

          configRS232ModemConfigString OBJECT-TYPE
              SYNTAX  DisplayString (SIZE (0..40))
              ACCESS  read-write
              STATUS  mandatory
              DESCRIPTION
                      "A user-configurable ASCII string used to initialize
                       a modem on this port. If this string is set to Z, the 
                       Star-Tek Agent sends ATZ<CR> to the modem during 
                       configuration."
              ::= { configRS232PortEntry 6 }

          configRS232OutputData OBJECT-TYPE
              SYNTAX  OCTET STRING (SIZE (0..42))
              ACCESS  read-write
              STATUS  mandatory
              DESCRIPTION
                      "Raw data to be sent out this port. Data can only 
                       be sent if the ConnectionStatus is connectionWait."
              ::= { configRS232PortEntry 7 }

          configRS232InputData OBJECT-TYPE
              SYNTAX  OCTET STRING (SIZE (0..100))
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The response, if any, to the output data."
