--
-- This is the beginning of the Star-Tek enterprise specific MIB.
-- This is implemented by all Star-Tek products which support
-- management via SNMP.
-- 
--         STARTEK-MIB DEFINTIONS ::= BEGIN
--

           managers      OBJECT IDENTIFIER ::= { maintenance 1 }

          -- Manager section

          mgrNumber OBJECT-TYPE
              SYNTAX  INTEGER
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The number of managers in the manager table."
              ::= {  managers 1  }

          mgrAutoAdd OBJECT-TYPE
              SYNTAX  INTEGER { enabled(1), disabled(2)  }
              ACCESS  read-write
              STATUS  mandatory
              DESCRIPTION
                      "This variable indicates whether any manager that
                       communicates with the agent should be added to the
                       manager table."
              ::= {  managers 2  }

          mgrAutoAge OBJECT-TYPE
              SYNTAX  INTEGER { enabled(1), disabled(2)  }
              ACCESS  read-write
              STATUS  mandatory
              DESCRIPTION
                      "When mgrAutoAdd is enabled, this variable then 
                       indicates whether manager entries should 
                       be deleted after 'mgrAgeInterval' ticks.  This
                       variable has no effect when mgrAutoAdd is disabled."
              ::= {  managers 3  }

          mgrAgeInterval OBJECT-TYPE
              SYNTAX  TimeTicks
              ACCESS  read-write
              STATUS  mandatory
              DESCRIPTION
                      "This is the aging interval."
              ::= {  managers 4  }


          -- the Manager table

          mgrTable OBJECT-TYPE
              SYNTAX  SEQUENCE OF MgrEntry
              ACCESS  not-accessible
              STATUS  mandatory
              DESCRIPTION
                      "A list of managers."
              ::= {  managers 5  }

          mgrEntry OBJECT-TYPE
              SYNTAX  MgrEntry
              ACCESS  not-accessible
              STATUS  mandatory
              DESCRIPTION
                      "A manager entry."
              INDEX   { mgrIndex  }
              ::= {  mgrTable 1  }

          MgrEntry ::=
              SEQUENCE {
                  mgrIndex      
                      INTEGER,
                  mgrIpAddr
                      IpAddress,
                  mgrCommunity
                      DisplayString,
                  mgrTimeStamp
                      TimeTicks,
                  mgrStatus
                      INTEGER,
                  mgrPhysAddr
                      OCTET STRING (SIZE(6..6)),
                  mgrInterfaceIndex
                      INTEGER
               }

          mgrIndex OBJECT-TYPE
              SYNTAX  INTEGER 
              ACCESS  read-write
              STATUS  mandatory
              DESCRIPTION
                      "The Index number of the Manager Entry."
              ::= {  mgrEntry 1  }

          mgrIpAddr OBJECT-TYPE
              SYNTAX  IpAddress
              ACCESS  read-write
              STATUS  mandatory
              DESCRIPTION
                      "The IP address of the manager."
              ::= {  mgrEntry 2  }

          mgrCommunity OBJECT-TYPE
              SYNTAX  DisplayString     (SIZE (0..255))
              ACCESS  read-write
              STATUS  mandatory
              DESCRIPTION
                      "The Community of the manager."
              ::= {  mgrEntry 3  }

          mgrTimeStamp OBJECT-TYPE
              SYNTAX  TimeTicks
              ACCESS  read-write
              STATUS  mandatory
              DESCRIPTION
                      "Time of last communication with the manager."
              ::= {  mgrEntry 4  }

          mgrStatus OBJECT-TYPE
              SYNTAX  INTEGER { valid(1), invalid(2)  }
              ACCESS  read-write
              STATUS  mandatory
              DESCRIPTION
                      "The Status of this Entry."
              ::= {  mgrEntry 5  }

          mgrPhysAddr OBJECT-TYPE
              SYNTAX    OCTET STRING (SIZE (6..6))
              ACCESS  read-write
              STATUS  mandatory
              DESCRIPTION
                      "The physical address of the manager entry."
              ::= {  mgrEntry 6  }

          mgrInterfaceIndex OBJECT-TYPE
              SYNTAX  INTEGER 
              ACCESS  read-write
              STATUS  mandatory
              DESCRIPTION
                      "The Interface Index of the manager entry."
              ::= {  mgrEntry 7  }



--
-- This is the beginning of the Star-Tek enterprise specific MIB.
