          STARTEK-PRIVATE-MIB DEFINITIONS ::= BEGIN

	      IMPORTS
    		enterprises, OBJECT-TYPE, TimeTicks, Opaque, Counter, IpAddress
     			FROM RFC1155-SMI
			DisplayString FROM SNMPv2-TC;


	    startek       OBJECT IDENTIFIER ::= { enterprises 260 }
            products      OBJECT IDENTIFIER ::= { startek 1 }
            maintenance   OBJECT IDENTIFIER ::= { startek 2 }
            fSeries       OBJECT IDENTIFIER ::= { products 2 }
            stackable     OBJECT IDENTIFIER ::= { products 3 }




           m828Series    OBJECT IDENTIFIER ::= { products 1 }

           m828Hubs      OBJECT IDENTIFIER ::= { m828Series 1 }

           -- m828Hubs Group

          m828HubNumber OBJECT-TYPE
              SYNTAX  INTEGER
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The number of items in hub table."
              ::= { m828Hubs 1 }

          m828HubTable OBJECT-TYPE
              SYNTAX  SEQUENCE OF M828hubTableEntry
              ACCESS  not-accessible
              STATUS  mandatory
              DESCRIPTION     
                      "A table which maintains a list of all of the intelligent
                       m828 series hubs on the ring being managed by the 
                       addressed agent. This table is indexed by the RS485 OOB 
                       address of the m828 series intelligent hubs."
              ::= { m828Hubs 2 }

          m828HubTableEntry        OBJECT-TYPE
              SYNTAX  M828hubTableEntry
              ACCESS  not-accessible
              STATUS  mandatory
              DESCRIPTION
                     "An entry in the m828HubTable."
              INDEX   { m828HubTableIndex }
              ::= { m828HubTable 1 }

          M828hubTableEntry ::=
              SEQUENCE  {
                      m828HubTableIndex        INTEGER,
                      m828HubModel             INTEGER,
                      m828Hub485Address        OCTET STRING,
                      m828HubRiFTolSw          INTEGER,
                      m828HubRiFTolState       INTEGER,
                      m828HubRiMask            INTEGER,
                      m828HubRiPhDet           INTEGER,
                      m828HubRiNsrtStatus      INTEGER,
                      m828HubRiBcnRemStatus    INTEGER,
                      m828RiNeighborType       INTEGER,
                      m828RiNeighborAddr       OCTET STRING,
                      m828RiTimeStamp          TimeTicks,
                      m828HubRoFTolSw          INTEGER,
                      m828HubRoFTolState       INTEGER,
                      m828HubRoMask            INTEGER,
                      m828HubRoPhDet           INTEGER,
                      m828HubRoNsrtStatus      INTEGER,
                      m828HubRoBcnRemStatus    INTEGER,
                      m828RoNeighborType       INTEGER,
                      m828RoNeighborAddr       OCTET STRING,
                      m828RoTimeStamp          TimeTicks,
                      m828HubDataRateSwitch    INTEGER,
                      m828HubDataRateState     INTEGER,
                      m828HubDataRateTimeStamp TimeTicks,
                      m828HubMgmtStatus        INTEGER,
                      m828HubProxyAgentID      OCTET STRING,
                      m828HubCodeVersion       OCTET STRING,
                      m828HubRawCommandData    OCTET STRING
                      }

          m828HubTableIndex        OBJECT-TYPE
              SYNTAX  INTEGER
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The index for this hub entry."
              ::= { m828HubTableEntry 1 }

          m828HubModel     OBJECT-TYPE
              SYNTAX  INTEGER {
                      model-4(1),     -- Star-Tek m828AT model 4  
                      model-5(2),     -- Star-Tek m828AT model 5 
                      model-6(3),     -- Star-Tek m828AT model 6
                      model-7(4)      -- Star-Tek m828AT model 7
                      }
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION     
                      "The model number of the m828 series hub being managed."
              ::= { m828HubTableEntry 2 }

          m828Hub485Address        OBJECT-TYPE
              SYNTAX  OCTET STRING (SIZE (2..2))
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The 4 hex digit address used on the RS485 serial OOB 
                       management channel."
              ::=     { m828HubTableEntry 3 }

          m828HubRiFTolSw  OBJECT-TYPE
              SYNTAX  INTEGER {
                          off(1),
                          on(2)
                  }
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The status of the Ring In fault tolerance switch on the 
                       front panel of the hub." 
              ::= { m828HubTableEntry 4 }

          m828HubRiFTolState       OBJECT-TYPE
              SYNTAX  INTEGER {
                          off(1),
                          on(2)
                  }
              ACCESS  read-write
              STATUS  mandatory
              DESCRIPTION
                      "This parameter controls the Ring In (RI) fault tolerance 
                       behavior. The default value for this parameter is the 
                       same setting as is present for m828HubRiFTolSw. SETTING 
                       this parameter overrides the value derived from 
                       m828HubRiFTolSw.  Off(1) means that phantom currentis not
                       used on the corresponding RI interface as a condition for
                       main ring link insertion. On (2) means that phantom 
                       current is used on the corresponding RI interface as a 
                       condition for main ring link insertion. The preffered 
                       setting for this parameter is on (2)."
              ::= { m828HubTableEntry 5 }

          m828HubRiMask    OBJECT-TYPE
              SYNTAX  INTEGER {
                          isolate(1),
                          enable(2)
                  }
              ACCESS  read-write
              STATUS  mandatory
              DESCRIPTION
                      "Administrative status for this Ring In port. By
                       setting this parameter, a remote manager can control
                       the partitioning of this port."
              ::= { m828HubTableEntry 6 }

          m828HubRiPhDet   OBJECT-TYPE
              SYNTAX  INTEGER {
                          noPhantom(1),
                          phantDetected(2)
                  }
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "This parameter describes the phantom detect status for
                       this Ring In port."
              ::= { m828HubTableEntry 7 }

          m828HubRiNsrtStatus  OBJECT-TYPE
              SYNTAX  INTEGER {
                          bypassed(1),
                          inserted(2)
                  }
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "Insertion status for this Ring In port."
              ::= { m828HubTableEntry 8 }

          m828HubRiBcnRemStatus    OBJECT-TYPE
              SYNTAX  INTEGER {
                          ok(1),
                          autoIsolated(2)
                  }
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "This parameter describes the result of any automatic 
                       beacon removal operations performed by the agent on this
                       port. Automatic beacon removal operations consist of 
                       agent-initiated isolation of a lobe port in order to 
                       remove the cause of a persistent beaconing condition." 
              ::= { m828HubTableEntry 9 }

          m828RiNeighborType    OBJECT-TYPE
              SYNTAX  INTEGER {
                          series828(1),
                          fSeries(2),
                          tokenRingMAC(3),
                          none(4),
                          stackable(5)
                  }
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                     "This parameter indicates the type of nearest neighbor
                      on the Ring In port."
              ::= { m828HubTableEntry 10 }

          m828RiNeighborAddr    OBJECT-TYPE
              SYNTAX  OCTET STRING (SIZE (2..6))
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                     "The address of the nearest neighbor on the Ring In port."
              ::= { m828HubTableEntry 11 }

          m828RiTimeStamp    OBJECT-TYPE
              SYNTAX  TimeTicks
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The time the status of Ring In (was) last changed."
              ::= { m828HubTableEntry 12 }

          m828HubRoFTolSw  OBJECT-TYPE
              SYNTAX  INTEGER {
                          off(1),
                          on(2)
                   }
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The state of the Ring Out Fault Tolerance switch on
                       the front panel of the unit."
              ::= { m828HubTableEntry 13 }

          m828HubRoFTolState       OBJECT-TYPE
              SYNTAX  INTEGER {
                          off(1),
                          on(2)
                   }
              ACCESS  read-write
              STATUS  mandatory
              DESCRIPTION
                      "This parameter controls the Ring Out(RO) fault tolerance 
                       behavior. The default value for this parameter is the 
                       same setting as is present for m828HubRoFTolSw. SETTING 
                       this parameter overrides the value derived from 
                       m828HubRoFTolSw.  Off(1) means that phantom currentis not
                       used on the corresponding RO interface as a condition for
                       main ring link insertion. On (2) means that phantom 
                       current is used on the corresponding RO interface as a 
                       condition for main ring link insertion. The preffered 
                       setting for this parameter is on (2)."
              ::= { m828HubTableEntry 14 }

          m828HubRoMask    OBJECT-TYPE
              SYNTAX  INTEGER {
                          isolate(1),
                          enable(2)
                   }
              ACCESS  read-write
              STATUS  mandatory
              DESCRIPTION
                      "Administrative status for this Ring Out port. By
                       setting this parameter, a remote manager can control
                       the partitioning of this port."
              ::= { m828HubTableEntry 15 }

          m828HubRoPhDet   OBJECT-TYPE
              SYNTAX  INTEGER {
                          wireFault(1),
                          phantOk(2)      --corresponds to 
                                          --forward ok
                   }
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "This parameter describes the phantom detect status for
                       this Ring Out port."
              ::= { m828HubTableEntry 16 }

          m828HubRoNsrtStatus      OBJECT-TYPE
              SYNTAX  INTEGER {
                          bypassed(1),
                          inserted(2)
                   }
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "Insertion status for this Ring Out port."
              ::= { m828HubTableEntry 17 }

          m828HubRoBcnRemStatus    OBJECT-TYPE
              SYNTAX  INTEGER {
                          ok(1),
                          autoIsolated(2)
                   }
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "This parameter describes the result of any automatic 
                       beacon removal operations performed by the agent on this
                       port. Automatic beacon removal operations consist of 
                       agent-initiated isolation of a lobe port in order to 
                       remove the cause of a persistent beaconing condition." 
              ::= { m828HubTableEntry 18 }

          m828RoNeighborType    OBJECT-TYPE
              SYNTAX  INTEGER {
                          series828(1),
                          fSeries(2),
                          tokenRingMAC(3),
                          none(4),
                          stackable(5)
                  }
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "This parameter indicates the type of nearest neighbor
                     on the Ring Out port."
              ::= { m828HubTableEntry 19 }

          m828RoNeighborAddr    OBJECT-TYPE
              SYNTAX  OCTET STRING (SIZE (2..6))
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The address of the nearest neighbor on the Ring Out
                       port."
              ::= { m828HubTableEntry 20 }

          m828RoTimeStamp    OBJECT-TYPE
              SYNTAX  TimeTicks
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The time the status of Ring Out (was) last changed."
              ::= { m828HubTableEntry 21 }

          m828HubDataRateSwitch    OBJECT-TYPE
              SYNTAX  INTEGER {
                          s4Mbps(1),
                          s16Mbps(2),
                          noSwitch(3)
                   }
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "Setting of this parameter changes the data rate of
                       this unit.  A value of noSwitch(3) indicates that the
                       setting of the data rate is not available on this unit."
              ::= { m828HubTableEntry 22 }

          m828HubDataRateState     OBJECT-TYPE
              SYNTAX  INTEGER {
                          s4Mbps(1),
                          s16Mbps(2),
                          autoConfig(3)
                   }
              ACCESS  read-write
              STATUS  mandatory
              DESCRIPTION
                      "This parameter indicates the current data rate of
                     this unit."
              ::= { m828HubTableEntry 23 }

          m828HubDataRateTimeStamp    OBJECT-TYPE
              SYNTAX  TimeTicks
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The time the data rate status (was) last changed."
              ::= { m828HubTableEntry 24 }

          m828HubMgmtStatus     OBJECT-TYPE
              SYNTAX  INTEGER {
                          operational(1),
                          down(2)
                   }
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The operational status of this unit."
              ::= { m828HubTableEntry 25 }

          m828HubProxyAgentID      OBJECT-TYPE
              SYNTAX  OCTET STRING (SIZE (6..6))  -- the MAC address of the
                                                  -- TR interface in the agent
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The physical layer MAC Address of the agent."
              ::= { m828HubTableEntry 26 }

          m828HubCodeVersion       OBJECT-TYPE
              SYNTAX  OCTET STRING (SIZE (2..2))
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The firmware version number of this unit."
              ::= { m828HubTableEntry 27 }

          m828HubRawCommandData    OBJECT-TYPE
              SYNTAX  OCTET STRING (SIZE (1..1))
              ACCESS  read-write
              STATUS  mandatory
              DESCRIPTION
                      "This is used to send raw 485 serial data to
                       this unit."
              ::= { m828HubTableEntry 28 }

          m828HubLobeNumber OBJECT-TYPE
              SYNTAX  INTEGER
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The number of items in the hub lobe table."
              ::= { m828Hubs 3 }


          -- Connections table for m828 hubs

          m828HubLobeTable OBJECT-TYPE
              SYNTAX  SEQUENCE OF M828hubLobeEntry
              ACCESS  not-accessible
              STATUS  mandatory
              DESCRIPTION     
                      "A table which contains the control and status 
                       information for the lobes."
              ::= { m828Hubs 4 }

          m828HubLobeEntry OBJECT-TYPE
              SYNTAX  M828hubLobeEntry
              ACCESS  not-accessible
              STATUS  mandatory
              DESCRIPTION
                      "A collection of control and status parameters kept for
                       each lobe."
              INDEX   { m828HubIndex, m828HubLobePort  }
              ::= { m828HubLobeTable 1 }

          M828hubLobeEntry ::=
                  SEQUENCE  {
                          m828HubIndex             INTEGER,
                          m828HubLobePort          INTEGER,
                          m828HubLobePhDet         INTEGER,
                          m828HubLobeMask          INTEGER,
                          m828HubLobeNsrtStatus    INTEGER,
                          m828HubLobeMacAddr       OCTET STRING,
                          m828HubLobeBcnRemStatus  INTEGER,
                          m828HubLobeTimeStamp     TimeTicks
                   }

          m828HubIndex OBJECT-TYPE
              SYNTAX  INTEGER
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The index into the m828HubTable of the mau this lobe is
                       on."
              ::=  { m828HubLobeEntry 1 }

          m828HubLobePort OBJECT-TYPE
              SYNTAX  INTEGER
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The port of the mau this lobe is on."
              ::=  { m828HubLobeEntry 2 }

          m828HubLobePhDet  OBJECT-TYPE
              SYNTAX  INTEGER {
                                  noPhantom(1),
                                  phantDetected(2)
                           }
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "Phantom current status for this lobe port. A value of 1 
                       corresponds to no phantom current detected. A value of 2 
                       corresponds to phantom presence." 
              ::=  { m828HubLobeEntry 3 }

          m828HubLobeMask    OBJECT-TYPE
              SYNTAX  INTEGER {
                                  isolated(1),
                                  enabled(2)
                           }
              ACCESS  read-write
              STATUS  mandatory
              DESCRIPTION
                      "Administrative status for this lobe port. By setting 
                       this parameter, a remote manager can control access to 
                       the lobe port." 
              ::=  { m828HubLobeEntry 4 }

          m828HubLobeNsrtStatus    OBJECT-TYPE
              SYNTAX  INTEGER {
                                  bypassed(1),
                                  inserted(2)
                           }
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "Actual insertion status for this lobe port. A 1 value 
                       means that the corresponding station is not connected to 
                       the ring. A 2 value means that the corresponding station 
                       is connected to the ring." 
              ::=  { m828HubLobeEntry 5 }

          m828HubLobeMacAddr       OBJECT-TYPE
              SYNTAX  OCTET STRING (SIZE (6..6))
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The MAC layer address of the station which is attached 
                       to the corresponding lobe port."
              ::=  { m828HubLobeEntry 6 }

          m828HubLobeBcnRemStatus  OBJECT-TYPE
              SYNTAX  INTEGER {
                          ok(1),
                          autoIsolated(2),
                          unknown(3)
                   }
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "This parameter describes the result of any automatic 
                       beacon removal operations performed by the agent on this
                       port. Automatic beacon removal operations consist of 
                       agent-initiated isolation of a lobe port in order to 
                       remove the cause of a persistent beaconing condition." 
              ::=  { m828HubLobeEntry 7 }

          m828HubLobeTimeStamp      OBJECT-TYPE
              SYNTAX  TimeTicks
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The time the status of this lobe (was) last changed."
              ::=  { m828HubLobeEntry 8 }



--      The fSeries group encompasses the portion of the StarTek MIB which 
--      applies to the StarTek Focus Series intelligent modular wiring hub 
--      product line.


                  fChassis      OBJECT IDENTIFIER ::= { fSeries 1 }
                  fTRMainRing   OBJECT IDENTIFIER ::= { fSeries 2 }
                  fTRLobe       OBJECT IDENTIFIER ::= { fSeries 3 }
                  fEthernet     OBJECT IDENTIFIER ::= { fSeries 4 }
                  fFddi         OBJECT IDENTIFIER ::= { fSeries 5 }
                  fTwx          OBJECT IDENTIFIER ::= { fSeries 6 }

          --              fchassis Group

          fChassisNumber       OBJECT-TYPE
              SYNTAX  INTEGER
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The number of chassis in table"
              ::= { fChassis 1  }

          fChassisTable     OBJECT-TYPE
              SYNTAX  SEQUENCE OF FchassisTableEntry
              ACCESS  not-accessible
              STATUS  mandatory
              DESCRIPTION
                      "A table which describes which modules are plugged into 
                       which slots of the chassis."
              ::= { fChassis 2 }

          fChassisTableEntry        OBJECT-TYPE
              SYNTAX  FchassisTableEntry
              ACCESS  not-accessible
              STATUS  mandatory
              INDEX   { fChassisIndex  }
              ::= {  fChassisTable 1 }

          FchassisTableEntry ::=
          SEQUENCE {
                          fChassisIndex     INTEGER,
                          fChassisModel     INTEGER,
                          fChassisPowStatus INTEGER,
                          fChassisLocation  INTEGER
                   }

          fChassisIndex       OBJECT-TYPE
              SYNTAX  INTEGER
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "Index of this entry"
              ::= {  fChassisTableEntry 1  }

          fChassisModel   OBJECT-TYPE     
              SYNTAX  INTEGER {
                                  twoslot-10100(1),                       
                                  fiveslot-10101(2),               
                                  fiveslotredundant-10102(3),              
                                  reserved4(4),    
                                  reserved5(5),             
                                  reserved6(6),
                                  reserved7(7),             
                                  reserved8(8)     
                           }
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The model number of the f series chassis in which the 
                       addressed management agent is installed."
              ::= {  fChassisTableEntry 2  }

          fChassisPowStatus       OBJECT-TYPE     
              SYNTAX  INTEGER {
                                  singleSupOK(1),                 
                                  dualSupOK(2),            
                                  dualSup-BSupBAD(3),
                                  dualSup-ASupBAD(4),
                                  illegal(5)
                           }
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The status of the power supply or supplies in the 
                       chassis."
              ::= {  fChassisTableEntry 3  }

          fChassisLocation       OBJECT-TYPE
              SYNTAX  INTEGER {
                                  local(1),
                                  external(2)
                           }
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The location of the chassis."
              ::= {  fChassisTableEntry 4  }


          --              fTokenRing Group

          fTRMainRingNumber OBJECT-TYPE
              SYNTAX  INTEGER
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The number of the ring."
              ::= { fTRMainRing 1 }

          fTRMainRingTable OBJECT-TYPE
              SYNTAX  SEQUENCE OF FTRMainRingTableEntry
              ACCESS  not-accessible
              STATUS  mandatory
              DESCRIPTION     
                      "A table which maintains a list of all of the intelligent
                       Focus series hubs on the ring being managed by the 
                       addressed agent. This table is INDEXed by the RS485 OOB 
                       address of the Focus series intelligent hubs."
              ::= { fTRMainRing 2 }

          fTRMainRingTableEntry        OBJECT-TYPE
              SYNTAX  FTRMainRingTableEntry
              ACCESS  not-accessible
              STATUS  mandatory
              DESCRIPTION
                      "An entry in the fTRMainRingTable"
              INDEX   { fTRMainRingTableIndex  }
              ::= { fTRMainRingTable 1 }

          FTRMainRingTableEntry ::=
                  SEQUENCE  {
                          fTRMainRingTableIndex         INTEGER,
                          fTRMainRingModel              INTEGER,
                          fTRMainRing485Address         OCTET STRING,
                          fTRMainRingSlotIndex          INTEGER,
                          fTRMainRingChassisIndex       INTEGER,
                          fTRMainRingRiFTolState        INTEGER,
                          fTRMainRingRiMask             INTEGER,
                          fTRMainRingRiPhDet            INTEGER,
                          fTRMainRingRiNsrtStatus       INTEGER,
                          fTRMainRingRiBcnRemStatus     INTEGER,
                          fTRMainRingRiCableType        INTEGER,
                          fTRMainRingRiNghbrType        INTEGER,
                          fTRMainRingRiNghbrAddr        OCTET STRING,
                          fTRMainRingRiTimeStamp        TimeTicks,
                          fTRMainRingRoFTolState        INTEGER,
                          fTRMainRingRoMask             INTEGER,
                          fTRMainRingRoPhDet            INTEGER,
                          fTRMainRingRoNsrtStatus       INTEGER,
                          fTRMainRingRoBcnRemStatus     INTEGER,
                          fTRMainRingRoCableType        INTEGER,
                          fTRMainRingRoNghbrType        INTEGER,
                          fTRMainRingRoNghbrAddr        OCTET STRING,
                          fTRMainRingRoTimeStamp        TimeTicks,
                          fTRMainRingDataRateState      INTEGER,
                          fTRMainRingDataRateTimeStamp  TimeTicks,
                          fTRMainRingBkPlnNsrtState     INTEGER,
                          fTRMainRingBkPlnTimeStamp     TimeTicks,
                          fTRMainRingLobePhDet          INTEGER,
                          fTRMainRingLobeMask           INTEGER,
                          fTRMainRingLobeNsrtStatus     INTEGER,
                          fTRMainRingLobeMacAddr        OCTET STRING,
                          fTRMainRingLobeBcnRemStatus   INTEGER,
                          fTRMainRingLobeTimeStamp      TimeTicks,
                          fTRMainRingMgmtStatus         INTEGER,
                          fTRMainRingProxyAgentID       OCTET STRING,
                          fTRMainRingAttachedAgentType  INTEGER,
                          fTRMainRingAttachedAgentState INTEGER,
                          fTRMainRingTemperatureStatus  INTEGER,
                          fTRMainRingAutoRecovControl   INTEGER,
                          fTRMainRingAutoRecovRetries   INTEGER,
                          fTRMainRingCodeVersion        OCTET STRING,
                          fTRMainRingAutoRecovState     INTEGER,
                          fTRMainRingRawCommandData     OCTET STRING,
                          fTRMainRingPowerStatus        INTEGER,
                          fTRMainRingCageType           INTEGER,
                          fTRMainRingAutoRecovMode      INTEGER
                       }

          fTRMainRingTableIndex        OBJECT-TYPE
              SYNTAX  INTEGER
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The index for this ring entry."
              ::= { fTRMainRingTableEntry 1 }

          fTRMainRingModel     OBJECT-TYPE
              SYNTAX  INTEGER {
                                  f10154RiRoExpModule(1),
                                  f10155RiRoFiberExpModule(2),
                                  f10150RiRoExpModuleAgent(3),
                                  f10151RiRoFiberExpModuleAgent(4)
                           }
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION     
                      "The model number of the Focus series hub being managed."
              ::= {  fTRMainRingTableEntry 2 }

          fTRMainRing485Address        OBJECT-TYPE
              SYNTAX  OCTET STRING (SIZE (3..3))
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The 6 hex digit address used on the RS485 serial OOB
                       management channel."
              ::=  {  fTRMainRingTableEntry 3 }

          fTRMainRingSlotIndex       OBJECT-TYPE
              SYNTAX  INTEGER
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The slot id."
              ::= { fTRMainRingTableEntry 4 }

          fTRMainRingChassisIndex       OBJECT-TYPE
              SYNTAX  INTEGER
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The index of the chassis the card is in."
              ::= { fTRMainRingTableEntry 5 }

          fTRMainRingRiFTolState       OBJECT-TYPE
              SYNTAX  INTEGER {
                          off(1),
                          on(2)
                   }
              ACCESS  read-write
              STATUS  mandatory
              DESCRIPTION
                      "This parameter controls the Ring In (RI) fault tolerance 
                       behavior. Off(1) means that phantom current is not used
                       on the corresponding RI interface as a condition for main
                       ring link insertion. On (2) means that phantom current
                       is used on the corresponding RI interface as a condition
                       for main ring link insertion. The preferred setting for
                       this parameter is on (2)."
              ::= { fTRMainRingTableEntry 6 }

