                ACCESS  read-write
                STATUS  mandatory
                ::=  { trapDestEntry 2 }
 
        trapDestEntryType OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-write
                STATUS  mandatory
                ::=  { trapDestEntry 3 }
 
        trapDestAction OBJECT-TYPE
