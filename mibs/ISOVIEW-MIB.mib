                ACCESS  read-only
                STATUS  mandatory
                ::=  { lapbEntry 3 }
 
        lapbManualT1value OBJECT-TYPE
                SYNTAX  INTEGER (10..5000)
                ACCESS  read-write
                STATUS  mandatory
                ::=  { lapbEntry 4 }
 
        lapbWindow OBJECT-TYPE
                SYNTAX  INTEGER (7..127)
                ACCESS  read-only
                STATUS  mandatory
                ::=  { lapbEntry 5 }
 
        lapbPolarity OBJECT-TYPE
