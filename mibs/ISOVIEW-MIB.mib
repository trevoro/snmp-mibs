         SnmpAccessPolicyEntry ::= SEQUENCE {
                accessPolicyIndex
                        INTEGER,
                communityName
                        OCTET STRING (SIZE(0..10)),
                accessMode
                        INTEGER,
                snmpAccessPolicyType
                        INTEGER
                }
 
