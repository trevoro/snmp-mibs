                lapbIndex
                        INTEGER,
                lapbModeT1
                        INTEGER (0..1),
                lapbAutoT1value
                        INTEGER,
                lapbManualT1value
                        INTEGER (10..5000),
                lapbWindow
                        INTEGER (7..127),
                lapbPolarity
                        INTEGER (0..1),
                lapbResetCount
                        Counter,
                lapbSendFrameCount
                        Counter,
                lapbRcvFrameCount
                        Counter,
                lapbSendOctetCount
                        Counter,
                lapbRcvOctetCount
                        Counter,
                lapbAbortCount
                        Counter,
                lapbCrcErrorCount
                        Counter,
                lapbState
