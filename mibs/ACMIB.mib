cardProtectionStatus    OBJECT-TYPE
                SYNTAX INTEGER  {
                                none(1),
                                protected(2),
                                wrongType(3)
                                }
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Current protection status of the card: 
                        none if the card is un-protected; protected
                        if it is under the protection of another card; 
                        wrongType if the card types of protection card
                        and protected card are different."
                DEFVAL { none }
                ::= { cardEntry 3 }
 
cardOperStatus OBJECT-TYPE
                SYNTAX INTEGER  {
                                primary(1),
                                standby(2),
                                unknown(3)
                                }
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Current operational status of the card.
                        For I/O card, it is primary if configured, 
                        it is unknown if unconfigured, or removed.
                        For CPU, it is primary if the CPU is master
                        CPU, or standby if the CPU is slave CPU."
                DEFVAL { unknown }
                ::= { cardEntry 4 }

cardAdminStatus OBJECT-TYPE
                SYNTAX INTEGER  {
                                switchProtection(1)
                                }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "administrative status"
                ::= { cardEntry 5 }
 
cardAlarmStatus  OBJECT-TYPE
                SYNTAX INTEGER  {
                                noAlarm(1),
                                wrongCardType(2),
                                lineFailed(3),
                                cardRemoved(4),
                                referenceClockFailed(5),
                                compositeClockFailed(6),
                                overload(7),
                                plus5vFailed(8),
                                plus120vFailed(9),
                                minus48vFailed(10),
                                unknownAlarm(11),
                                completeClockFailed(12),
                                backplaneCircuitryFailed(13),
                                powerFailed(14),
                                inputPortClkFailed(15)
                                }
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "last alarm occurred"
                DEFVAL { noAlarm }
                ::= { cardEntry 6 }

cardCleiCode        OBJECT-TYPE
                SYNTAX DisplayString (SIZE(10))
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "CLEI code"
                ::= { cardEntry 7}

cardRevision        OBJECT-TYPE
                SYNTAX Integer32 
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Card Revision number"
                ::= { cardEntry 8}

cardSerialNumber OBJECT-TYPE
                SYNTAX DisplayString (SIZE(12))
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Serial Number"
                ::= { cardEntry 9}

cardOperatingMode        OBJECT-TYPE
                SYNTAX INTEGER  {
                                none(1),
                                dS3(2), 
                                e3(3),  
                                sTS1e(4),
                                oC12(5),
                                sTM4(6)
                                } 
                MAX-ACCESS read-write
                STATUS current 
                DESCRIPTION "Mode of operation for a card. This is 
                applicable
                to those card types which can be configured in a dual mode.
                e.g. unstDS3E3-3P can be operational as either a 3 port 
                DS3 or
                E3 card. All ports on the card will be of the same type
                depending on the card's operating mode."
        DEFVAL { none }
                ::= { cardEntry 10 }

protectionConfigMode          OBJECT-TYPE
                    SYNTAX INTEGER  {
                              unprotected(1),
                              protection(2),
                              working(3)
                              }
                    MAX-ACCESS read-write
                    STATUS current
                    DESCRIPTION "Protection Configuration Mode of a Card
                       unprotected: Card is not in protection or protected state
                       protection: Card is in protection state
                       working: Card is in protected/working state and this 
                               state is read only and cannot be set by the user"
                    DEFVAL { unprotected }
                    ::= { cardEntry 11 }

protectionStatus             OBJECT-TYPE
                    SYNTAX INTEGER  {
                              none(1),
                              inservice-Active(2),
                              inservice-Standby(3),
                              outOfService-Active(4),
                              outOfService-Standby(5)
                              }
                    MAX-ACCESS read-only
                    STATUS current
                    DESCRIPTION "Protection Status of a card. Inservice and 
                              OutofService indicates if the card has failed or 
                              still operational.
                              Active or Standby state indicates if it is 
                              carrying active traffic or not."
                    DEFVAL { none }
                    ::= { cardEntry 12 }

