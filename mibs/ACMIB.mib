casConfigTable  OBJECT-TYPE
                  SYNTAX SEQUENCE OF CasConfigEntry 
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Table for CAS trunk/line configuration."
                ::= { casConfig 1 }

casConfigEntry  OBJECT-TYPE
                SYNTAX CasConfigEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Row definition for CAS trunk/line configuration."
                INDEX { casIfIndex } 
                ::= { casConfigTable 1 }

CasConfigEntry ::= SEQUENCE {
                            casIfIndex          IntfIndex,
                            casAdminStatus            INTEGER,
                            casOperStatus              INTEGER,
                            casProtocolType           INTEGER,
                            casDirection          INTEGER,
                            casAddressingType         INTEGER,
                            casBitPatternType         INTEGER,
                            casGroupId          INTEGER
                            }

casIfIndex OBJECT-TYPE
                  SYNTAX IntfIndex
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "The channel ID (slot/port/channel) in SSPPCCC 
                        format associated with the CAS trunk/line."
                  ::= { casConfigEntry 1 }

casAdminStatus  OBJECT-TYPE
                  SYNTAX INTEGER {
                                outOfService(1),
                                inService(2),
                                delete(3)
                                }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "The administrative action:
                        outOfService-take the CAS trunk/line out of service, 
                        inService-take the CAS trunk/line in service,
                         delete-remove/deconfigure the CAS trunk/line."
                  DEFVAL { outOfService }
                ::= { casConfigEntry 2 }

casOperStatus OBJECT-TYPE
                SYNTAX INTEGER {
                                outOfService(1),
                                inService(2)
                                }
                  MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "The operational status of the CAS trunk/line:
                        outOfService-the CAS trunk/line is out of service,
                        inService-the CAS trunk/line is in service."
                DEFVAL { outOfService }
                  ::= { casConfigEntry 3 }

casProtocolType OBJECT-TYPE
                  SYNTAX INTEGER {
                                wink-start(1),
                                loop-start-network(2),
                                loop-start-user(3),
                                ground-start-network(4),
                                ground-start-user(5),
                                itu-t-r2(6)
                                }
                  MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "The type of CAS protocol to be used. 
                        The protocol must match the one used on the 
                        other end of the trunk/line."
                DEFVAL { wink-start }
                  ::= { casConfigEntry 4 }

casDirection  OBJECT-TYPE
                SYNTAX INTEGER {
                                both(1), 
                                incoming(2),
                                outgoing(3)
                                }
                  MAX-ACCESS read-write
                  STATUS current
                  DESCRIPTION "The direction of traffic flow on this CAS
                        trunk/line."
                DEFVAL { both }
                  ::= { casConfigEntry 5 }

casAddressingType OBJECT-TYPE
                SYNTAX INTEGER {
                                dtmf(1), 
                                r1-mf(2),
                                r2-mf(3) 
                                }
                  MAX-ACCESS read-write
                  STATUS current
                  DESCRIPTION "The addressing type used for the CAS trunk/line.
                                dtmf  -- dual tone multifrequency
                                r1-mf -- R1 multifrequency,
                                r2-mf -- R2 multifrequency.
                            Type of 're-mf' is valid only when casProtocolType 
                            is 'wink start', while 'r2-mf' is valid only when
                            casProtocolType is 'itu-t r2' ."
                  DEFVAL { dtmf }
                  ::= { casConfigEntry 6 }

casBitPatternType OBJECT-TYPE
                  SYNTAX INTEGER {
                                ab(1),
                                abcd(2)
                                }
                  MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "The type of bit pattern used for supervisory 
                         signaling. The value is AB when SF framing is
                              used, ABCD when ESF framing is used for DS1.
                        In case of E1, the value is always ABCD."
                  ::= { casConfigEntry 7 }

casGroupId OBJECT-TYPE
                  SYNTAX INTEGER (0..255)
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "The identifier to associate a group of CAS
                        trunks/lines which have same reachability.
                        When SG is disabled, it must be specified with a
                        non-zero value. When SG is enabled, the value should
                        be set to zero which indicate no routing performed
                        based on the value."
                DEFVAL { 0 }
                ::= { casConfigEntry 8 }


