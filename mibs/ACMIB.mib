-- Alarm card configuration group

alarmStatusInputBitmap OBJECT-TYPE
                SYNTAX OCTET STRING (SIZE(2))
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Remote status from external device.
                             bit = 0  closed,
                             bit = 1  open,
                
                             LSB 15 bits represent 15 external inputs.
                             MSB bit 15 is always 0."
                ::= { alarmConfig 1 }

alarmStatusOutputBitmap OBJECT-TYPE
                SYNTAX OCTET STRING (SIZE(1))
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Control output for alarm card.
                             bit = 0  deactivated,
                             bit = 1  activated,
                
                             LSB 4 bits represent 4 external outputs.
                             Bit 0 through bit 3 is output 1 through 4
                             Control output 4 will be activated upon
                             power failure."
                DEFVAL { 0 }
                ::= { alarmConfig 2 }

alarmSeverityLevelBitmap OBJECT-TYPE
                SYNTAX OCTET STRING (SIZE(1))
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Alarm severity level.
                             bit = 0  noAlarm,
                             bit = 1  Alarm present,
                
                             LSB 3 bits represent severity levels.
                             bit 0 = Minor Alarm,
                             bit 1 = Major Alarm,
                             bit 2 = Critical Alarm."
                DEFVAL { 0 }
                ::= { alarmConfig 3 }

alarmAcoStatusBitmap OBJECT-TYPE
                SYNTAX OCTET STRING (SIZE(1))
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Audio alarm status.
                             bit = 0  No alarm cutoff,
                             bit = 1  Alarm cutoff,
                
                             LSB 3 bits represent audio alarms.
                             bit 0 = Local alarm cutoff,
                             bit 1 = remote alarm cutoff,
                             bit 2 = composite alarm cutoff status."
                ::= { alarmConfig 4 }


