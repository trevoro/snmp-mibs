              SEQUENCE {
                  appIpRouteDest
                      IpAddress,
                  appIpRouteMask
                      IpAddress,
                  appIpRouteMetric1
                      INTEGER,
                  appIpRouteNextHop
                      IpAddress,
                  appIpRouteState
                     INTEGER
              }

appIpRouteDest OBJECT-TYPE
              SYNTAX  IpAddress
              MAX-ACCESS  not-accessible
              STATUS  current
              DESCRIPTION
                      "The destination IP address of this route.  An
                      entry with a value of 0.0.0.0 is not valid
                      route. "
              ::= { appIpRouteEntry 1 }

appIpRouteMask OBJECT-TYPE
              SYNTAX  IpAddress
              MAX-ACCESS not-accessible
              STATUS  current
              DESCRIPTION
                      "Mask for Ip route, usefull only for SENS stack 
                       VxWorks"
              ::= { appIpRouteEntry 2 }


appIpRouteMetric1 OBJECT-TYPE
              SYNTAX  INTEGER (1..255)
              MAX-ACCESS not-accessible
              STATUS  current
              DESCRIPTION
                      "The primary routing metric for this route.  The
                      semantics of this metric are determined by the
                      routing-protocol specified in the routes
                      appIpRouteProto value.  If this metric is not used,
                      its value should be set to 1."
              ::= { appIpRouteEntry 3 }

appIpRouteNextHop OBJECT-TYPE
              SYNTAX  IpAddress
              MAX-ACCESS  read-write
              STATUS  current
              DESCRIPTION
                      "The IP address of the next hop of this route.
                      (In the case of a route bound to an interface
                      which is realized via a broadcast media, the value
                      of this field is the agent's IP address on that
                      interface.)"
              ::= { appIpRouteEntry 4 }

appIpRouteState OBJECT-TYPE
                SYNTAX INTEGER {
                                invalid(0),
                                inuse(1),
                                notinuse(2),
                                error(3)
                               }
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Current state of route"
                ::= { appIpRouteEntry  5 }

