atmSpvcVccRetryInterval OBJECT-TYPE
                SYNTAX INTEGER (0..3600)
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "The time to wait before attempting to establish 
                             the SVC after a previous failed call attempt.  A 
                             value of 0 indicates retry will be attempted immediately."
                ::= { atmSpvcVccActiveSvcEntry 5 }

atmSpvcVccRetryLimit OBJECT-TYPE
                SYNTAX INTEGER (0..65535)
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "A maximum limit on how many retries should be 
                             attempted.  A value of 0 indicates no limit."
                ::= { atmSpvcVccActiveSvcEntry 6 }

atmSpvcVccRestart OBJECT-TYPE
                SYNTAX INTEGER  {
                                restart(1),
                                noop(2)
                                }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "The active SVC is released, if necessary, and a 
