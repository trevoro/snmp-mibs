                }

imaGrpChannelId OBJECT-TYPE
                SYNTAX IntfIndex
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "interface index - SSPPCCC"
                ::= { imaGrpChannelConfigEntry 1 }

imaGrpChannelIfType OBJECT-TYPE
                SYNTAX INTEGER  {
                                unconfigured(1),
                                atmUni3-0(2),
                                atmUni3-1(3),
                                iispUser(4),
                                iispNetwork(5),
                                atmPnni1-0(6),
                                atmUni4-0(7)
                                }
                MAX-ACCESS read-write
