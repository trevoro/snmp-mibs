tasCmprsAnnceId OBJECT-TYPE
                SYNTAX INTEGER (1..100)
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Announcement ID for this announcement file
                            to be compressed."
                ::= { toneAnnceControl 31 }

tasCmprsAnnceReplaceId OBJECT-TYPE
                SYNTAX INTEGER (1..100)
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Announcement ID of the announcement file
                             to be replaced from the announcement
                             table by this file."
                ::= { toneAnnceControl 32 }

tasCmprsAnnceFilename OBJECT-TYPE
                SYNTAX DisplayString (SIZE(79))
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "File name for this announcement which is 
                        to be compressed."
                ::= { toneAnnceControl 33 }

tasCmprsAnnceDescription OBJECT-TYPE
                SYNTAX DisplayString (SIZE(255))
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Description for this announcement which is to
                             to be compressed."
                ::= { toneAnnceControl 34 }

tasCmprsAnnceCmprsType OBJECT-TYPE
               SYNTAX INTEGER {
                                uncompressed(1),
                                compress8k(2),
                                compress16k(3),
                                compress32k(4)
                               }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "The type of compression used for
                             storing this announcement."
                ::= { toneAnnceControl 35 }

tasCmprsAnnceType OBJECT-TYPE
                SYNTAX INTEGER {
                                voice(1),
                                toneVoice(2),
                                userTones(3),
                                music(4),
                                musicVoice(5),
                                toneMusic(6)
                               }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "The type of this announcement."
                ::= { toneAnnceControl 36 }

tasCmprsAnnceIntfIndex OBJECT-TYPE
                SYNTAX IntfIndex
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Interface index in SSPPCCC format of TASM
                        card on which this announcement is to be 
                        compressed."
                ::= { toneAnnceControl 37 }


tasCmprsAction  OBJECT-TYPE
                SYNTAX INTEGER  {
                                 compress(1),
                                 delete(2),
                                 store(3) 
                                }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Action to be performed on this announcement."
                ::= { toneAnnceControl 38 }

--  Fields for initiating  the playing of the specified 
--  announcement on the specified interface.

tasTstAnnceId OBJECT-TYPE
                SYNTAX INTEGER (1..100)
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Announcement ID to be played for testing."
                ::= { toneAnnceControl 39 }

tasTstAnnceIntfIndex OBJECT-TYPE
                SYNTAX IntfIndex
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Interface index in SSPPCCC format of 
                        CE card on which this announcement
                        is to be played for testing."
