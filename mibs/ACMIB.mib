cardTable       OBJECT-TYPE
                SYNTAX SEQUENCE OF CardEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Table of card configuration data"
                ::= { equipConfig 1 }
 
cardEntry       OBJECT-TYPE
                SYNTAX CardEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Row definition for ac120 card"
                INDEX { cardSlot }
                ::= { cardTable 1 }
 
CardEntry       ::= SEQUENCE {
                cardSlot                INTEGER,
                cardType                INTEGER,
                cardProtectionStatus    INTEGER,
                cardOperStatus          INTEGER,
                cardAdminStatus         INTEGER,
                cardAlarmStatus         INTEGER,
                cardCleiCode            DisplayString,
                cardRevision            Integer32,
                cardSerialNumber        DisplayString,
                cardOperatingMode       INTEGER,
                protectionConfigMode    INTEGER,
                protectionStatus        INTEGER,
                protectionSlot          INTEGER 
                }
 
