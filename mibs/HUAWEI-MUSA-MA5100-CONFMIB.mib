        DESCRIPTION
        " ADL port Transfer cells. "
    ::= { hwMusaAdlbPortEntry 5 }

    adlbPortRxCell	   		OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
        " ADL port Receive cells. "
