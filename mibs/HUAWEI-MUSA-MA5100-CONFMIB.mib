HUAWEI-MUSA-MA5100-CONFMIB DEFINITIONS ::= BEGIN
IMPORTS
        IpAddress,Counter
                        FROM RFC1155-SMI
        OBJECT-TYPE
                        FROM RFC-1212                
        musa
                        FROM HUAWEI-MIB;
    
    DisplayString ::= OCTET STRING                  
    
    hwMa5100Mib     OBJECT IDENTIFIER ::= { musa 5 }
    hwMusaSysMib    OBJECT IDENTIFIER ::= { hwMa5100Mib 1 } 
    hwMusaEndOfMib  OBJECT IDENTIFIER ::= { hwMa5100Mib 100 } 



    -- -------------------------------------------------------------------------
    --  机架、框、槽描述表
    -- ------------------------------------------------------------------------- 
    hwMusaSlotGroup OBJECT IDENTIFIER ::= { hwMusaSysMib  6 }
    hwMusaFrame     OBJECT IDENTIFIER ::= { hwMusaSlotGroup 2 }
    hwMusaSlot      OBJECT IDENTIFIER ::= { hwMusaSlotGroup 3 }
    
    
   -- ----------------------------------------------------------------------------
   -- 机框信息表
   -- ----------------------------------------------------------------------------
   hwMusaFrameNumber    OBJECT-TYPE
        SYNTAX      INTEGER
        ACCESS      read-only
        STATUS      mandatory
    DESCRIPTION  
    "Musa frame numbers."
    ::= { hwMusaFrame 1 }
    
    hwMusaFrameConfTable    OBJECT-TYPE
        SYNTAX      SEQUENCE OF HwMusaFrameConfEntry
        ACCESS      not-accessible
        STATUS      mandatory
    DESCRIPTION  
    "This table contains Musa Frame configuration parameters."
    ::= { hwMusaFrame 2 }

    hwMusaFrameConfEntry    OBJECT-TYPE
        SYNTAX         HwMusaFrameConfEntry
        ACCESS         not-accessible
        STATUS         mandatory
    DESCRIPTION
    "This list contains Musa Frame configuration parameters and status variables.
    MUSA设备机框配置描述表."
    INDEX { hwMusaShelfIndex,
            hwMusaFrameIndex }
    ::= { hwMusaFrameConfTable  1}
    
   HwMusaFrameConfEntry    ::= SEQUENCE  {
            hwMusaFrameIndex                    INTEGER
            }
            
   hwMusaFrameIndex OBJECT-TYPE
        SYNTAX     INTEGER
        ACCESS     not-accessible
        STATUS     mandatory
    DESCRIPTION
    "the Musa Frame number. 
    框号索引"
    ::= { hwMusaFrameConfEntry 1}
    
    hwMusaSlotConfTable    OBJECT-TYPE
        SYNTAX      SEQUENCE OF HwMusaSlotConfEntry
        ACCESS      not-accessible
        STATUS      mandatory
    DESCRIPTION  
    "This table contains Musa slot configuration
     parameters, one entry per Musa slot."
    ::= { hwMusaSlot 2 }

   hwMusaSlotConfEntry    OBJECT-TYPE
        SYNTAX         HwMusaSlotConfEntry
        ACCESS         not-accessible
        STATUS         mandatory
    DESCRIPTION
    "This list contains Musa slot configuration parameters and status variables.
    MUSA设备槽位配置描述表."
    INDEX { 
            hwMusaFrameIndex,
            hwMusaSlotIndex }
    ::= { hwMusaSlotConfTable  1}
    
    HwMusaSlotConfEntry    ::= SEQUENCE  {
            hwMusaSlotIndex             INTEGER
            				}
            				
    hwMusaSlotIndex  OBJECT-TYPE
        SYNTAX     INTEGER(0..15)
        ACCESS     not-accessible
        STATUS     mandatory
    DESCRIPTION
    "the Musa slot number. 槽位号"
    ::= { hwMusaSlotConfEntry 1 }            				
    
    -- ------------------------------------------------------------------------
    -- 小区PVC表 (adl-lan)
    -- ------------------------------------------------------------------------
    hwMusaRegionPvcConfTable    OBJECT-TYPE
        SYNTAX      SEQUENCE OF HwMusaRegionPvcConfEntry
        ACCESS      not-accessible
        STATUS      mandatory
    DESCRIPTION
    "Musa region pvc configuration table."
    ::= { hwMusaSysMib 15 }

   hwMusaRegionPvcConfEntry    OBJECT-TYPE
        SYNTAX         HwMusaRegionPvcConfEntry
        ACCESS         not-accessible
        STATUS         mandatory
    DESCRIPTION
    "This list contains Musa region pvc Configuration
     parameters and variables."
    INDEX       { 
                hwMusaFrameIndex, 
                hwMusaSlotIndex,
                hwMusaVlanId,
                hwMusaVlanIciIndex
                }
    ::= { hwMusaRegionPvcConfTable  1 }

   HwMusaRegionPvcConfEntry    ::= SEQUENCE  {
            hwMusaVlanId                INTEGER,
            hwMusaVlanIciIndex          INTEGER,
            hwMusaAdlFrameId            INTEGER,
            hwMusaAdlSlotId             INTEGER,
            hwMusaAdlPortId             INTEGER,
            hwMusaAdlVpi                INTEGER,
            hwMusaAdlVci                INTEGER,
            hwMusaToLanTrafficId        INTEGER,
            hwMusaFromLanTrafficId      INTEGER,
            hwMusaAdlPortOperat         INTEGER
          }

    hwMusaVlanId    OBJECT-TYPE
        SYNTAX          INTEGER(1..63)
        ACCESS          read-only
        STATUS          mandatory
    DESCRIPTION
    "Musa region VLan Id.
    本小区VLAN ID。"
    ::= { hwMusaRegionPvcConfEntry  1 }   
    
    hwMusaVlanIciIndex    OBJECT-TYPE
        SYNTAX          INTEGER
        ACCESS          read-only
        STATUS          mandatory
    DESCRIPTION
    "Musa region VLanIciIndex.
    本小区VLAN 内部索引。"
    ::= { hwMusaRegionPvcConfEntry  2 }   
    
    hwMusaAdlFrameId    OBJECT-TYPE
        SYNTAX          INTEGER(0..4)
        ACCESS          read-write
        STATUS          mandatory
    DESCRIPTION
    "Musa region ADL frame ID.
    小区PVC的ADL框号"
    ::= { hwMusaRegionPvcConfEntry  4 }       
    
    hwMusaAdlSlotId    OBJECT-TYPE
        SYNTAX          INTEGER(1..15)
        ACCESS          read-write
        STATUS          mandatory
    DESCRIPTION
    "Musa region ADL slot ID.
    小区PVC的ADL槽号"
    ::= { hwMusaRegionPvcConfEntry  5 }
    
    hwMusaAdlPortId    OBJECT-TYPE
        SYNTAX          INTEGER(0..15)
        ACCESS          read-write
        STATUS          mandatory
    DESCRIPTION
    "Musa region ADL port ID.
    小区PVC的ADL端口号"
    ::= { hwMusaRegionPvcConfEntry  6 }                           

    hwMusaAdlVpi    OBJECT-TYPE
        SYNTAX          INTEGER(0..127)
        ACCESS          read-only
        STATUS          mandatory
    DESCRIPTION
    "Musa region VPI.
    小区PVC的VPI"
    ::= { hwMusaRegionPvcConfEntry  7 }       

    hwMusaAdlVci    OBJECT-TYPE
        SYNTAX          INTEGER(32..127)
        ACCESS          read-only
        STATUS          mandatory
    DESCRIPTION
    "Musa region VCI.
    小区PVC的VCI"
    ::= { hwMusaRegionPvcConfEntry  8 }       

    hwMusaToLanTrafficId    OBJECT-TYPE
        SYNTAX          INTEGER
        ACCESS          read-write
        STATUS          mandatory
    DESCRIPTION
    "Musa region to lan board traffic index.
    往LAN板的流量表号"
    ::= { hwMusaRegionPvcConfEntry  9 }       

    hwMusaFromLanTrafficId    OBJECT-TYPE
        SYNTAX          INTEGER
        ACCESS          read-write
        STATUS          mandatory
    DESCRIPTION
    "Musa region from lan board traffic index.
    来自LAN板的流量表号"
    ::= { hwMusaRegionPvcConfEntry  10 }  

    hwMusaAdlPortOperat    OBJECT-TYPE
        SYNTAX          INTEGER
                        {
                        add(0),
                        del(1)
                        }
        ACCESS          read-write
        STATUS          mandatory
    DESCRIPTION
    "Musa region ADL Operat.
    小区模式ADL端口的操作。"
    ::= { hwMusaRegionPvcConfEntry  11 }   
    
   -- ******************************************************************
   -- 总PVC表
   -- ******************************************************************

   hwMusaAllPvcConfTable   OBJECT-TYPE
       SYNTAX      SEQUENCE OF HwMusaAllPvcConfEntry
       ACCESS      not-accessible
       STATUS      mandatory
   DESCRIPTION
    "The  config talbe of all kinds of PVC."
   ::= { hwMusaSysMib 22 }
   
   hwMusaAllPvcConfEntry   OBJECT-TYPE
       SYNTAX      HwMusaAllPvcConfEntry
       ACCESS      not-accessible
       STATUS      mandatory
   DESCRIPTION
    "This lists config variables of all kinds of PVC."
   INDEX {  hwMusaTypeOfPvcPvpindex,
            hwMusaCidIndex }
   ::= { hwMusaAllPvcConfTable 1 }
   
   HwMusaAllPvcConfEntry   ::= SEQUENCE  {
             hwMusaCidIndex                  Counter,
             hwMusaSrcFrameId                INTEGER,
             hwMuasSrcSlotId                 INTEGER,
             hwMusaSrcPortVlanVccId          INTEGER,
             hwMusaSrcBoardVpi               INTEGER,
             hwMusaSrcBoardVci               INTEGER,
             hwMusaSrcUpcEpdPpd              INTEGER,
             hwMusaDestFrameId               INTEGER,
             hwMusaDestSlotId                INTEGER,
             hwMusaDestPortVlanVccId         INTEGER,
             hwMusaDestBoardVpi              INTEGER,
             hwMusaDestBoardVci              INTEGER,
             hwMusaDestUpcEpdPpd             INTEGER,
             hwMusaSrcToDestTraffic          INTEGER,
             hwMusaDestToSrcTraffic          INTEGER,
             hwMusaAllPvcOperater            INTEGER,
             hwMusaTypeOfPvcPvpindex         INTEGER,
             hwMusaPvcPvpState               INTEGER
             }
   
    hwMusaCidIndex    OBJECT-TYPE
        SYNTAX          Counter
        ACCESS          read-only
        STATUS          mandatory
    DESCRIPTION
    "The CID index."
    ::= { hwMusaAllPvcConfEntry  1 }
    
    hwMusaSrcFrameId    OBJECT-TYPE
        SYNTAX          INTEGER
        ACCESS          read-write
        STATUS          mandatory
    DESCRIPTION
    "The Frame id of source board."
    ::= { hwMusaAllPvcConfEntry  2 }
    
    hwMuasSrcSlotId    OBJECT-TYPE
        SYNTAX          INTEGER
        ACCESS          read-write
        STATUS          mandatory
    DESCRIPTION
    "The slot id of source board."
    ::= { hwMusaAllPvcConfEntry  3 }
    
    hwMusaSrcPortVlanVccId    OBJECT-TYPE
        SYNTAX          INTEGER
        ACCESS          read-write
        STATUS          mandatory
    DESCRIPTION
    "The Port/VlanID/VCC Index of source board."
    ::= { hwMusaAllPvcConfEntry  4 }
    
    hwMusaSrcBoardVpi    OBJECT-TYPE
        SYNTAX          INTEGER
        ACCESS          read-write
        STATUS          mandatory
    DESCRIPTION
    "Src board Port vpi Index."
    ::= { hwMusaAllPvcConfEntry  6 }   

    hwMusaSrcBoardVci    OBJECT-TYPE
        SYNTAX          INTEGER
        ACCESS          read-write
        STATUS          mandatory
    DESCRIPTION
    "Src board VCI Index."
    ::= { hwMusaAllPvcConfEntry  7 }
        
   hwMusaSrcUpcEpdPpd  OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
        "The flow control parameter of connection."
    ::= { hwMusaAllPvcConfEntry 16 } 
    
   hwMusaDestFrameId    OBJECT-TYPE
        SYNTAX          INTEGER
        ACCESS          read-write
        STATUS          mandatory
    DESCRIPTION
    "Destination frame Index."
    ::= { hwMusaAllPvcConfEntry  20 }
   
   hwMusaDestSlotId    OBJECT-TYPE
        SYNTAX          INTEGER
        ACCESS          read-write
        STATUS          mandatory
    DESCRIPTION
    "Destination slot Index."
    ::= { hwMusaAllPvcConfEntry  21 }
    
   hwMusaDestPortVlanVccId    OBJECT-TYPE
        SYNTAX          INTEGER
        ACCESS          read-write
        STATUS          mandatory
    DESCRIPTION
    "Destination port Index."
    ::= { hwMusaAllPvcConfEntry  22 }
    
    hwMusaDestBoardVpi    OBJECT-TYPE
        SYNTAX          INTEGER
        ACCESS          read-write
        STATUS          mandatory
    DESCRIPTION
    "Destination Port vpi Index."
    ::= { hwMusaAllPvcConfEntry  24 }   

    hwMusaDestBoardVci    OBJECT-TYPE
        SYNTAX          INTEGER
        ACCESS          read-write
        STATUS          mandatory
    DESCRIPTION
    "Destination VCI Index."
    ::= { hwMusaAllPvcConfEntry  25 }
    
   hwMusaDestUpcEpdPpd    OBJECT-TYPE
        SYNTAX          INTEGER
        ACCESS          read-write
        STATUS          mandatory
    DESCRIPTION
    "The flow control parameter of connection."
   ::= { hwMusaAllPvcConfEntry  34 }
   
   hwMusaSrcToDestTraffic  OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
        "The source board to destination traffic."
    ::= { hwMusaAllPvcConfEntry 38 }
    
   hwMusaDestToSrcTraffic  OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
        "The destination to source board traffic."
    ::= { hwMusaAllPvcConfEntry 39 }
       
   hwMusaAllPvcOperater    OBJECT-TYPE
        SYNTAX          INTEGER
                        {
                        add(0),
                        del(1)
                        }
        ACCESS          read-write
        STATUS          mandatory
    DESCRIPTION
    "Add or delete PVC."
    ::= { hwMusaAllPvcConfEntry  40}

   hwMusaTypeOfPvcPvpindex    OBJECT-TYPE
       SYNTAX          INTEGER
                        {
                        pvc(0),
                        pvp(1)
                        }
        ACCESS          read-write
        STATUS          mandatory
    DESCRIPTION
    "Identified type of Pvc or Pvp"
    ::= { hwMusaAllPvcConfEntry  41}
    
    hwMusaPvcPvpState  OBJECT-TYPE
       SYNTAX          INTEGER
                        {
                        normal(1),
                        invalid(2),
                        delete(3) 
                        }
        ACCESS          read-only
        STATUS          mandatory
    DESCRIPTION
    "Pvc or Pvp  state "
    ::= { hwMusaAllPvcConfEntry  42}
    
    
-- ADLB板
    hwMusaAdlb             OBJECT IDENTIFIER ::= { hwMa5100Mib 6 }
-- ADLB板描述组 Adlb Board
    hwMusaAdlbBoard        OBJECT IDENTIFIER ::= { hwMusaAdlb 1 }
-- ADLB套片管理组 Adlb Chipset
    hwMusaAdlbChipset      OBJECT IDENTIFIER ::= { hwMusaAdlb 2 }
-- ADLB端口配置管理组 Adlb Port Config
    hwMusaAdlbConf         OBJECT IDENTIFIER ::= { hwMusaAdlb 3 }
-- ADLB端口性能管理组 Adlb Port Performance
    hwMusaAdlbPfm          OBJECT IDENTIFIER ::= { hwMusaAdlb 4 }
-- ADLB维测管理组 Adlb Port Maintenance
    hwMusaAdlbTest         OBJECT IDENTIFIER ::= { hwMusaAdlb 5 }

    
    
-- ***************************************************************
--     |    |－ ADLB板描述组
--     |    |   |－板与套片（MODEM）描述表
--     |    |   |   |－#框号
--     |    |   |   |－#槽位号
--     |    |   |   |－产品代号ID（R）
--     |    |   |   |－用户线类型:(0:未知类型 1:ISDN 2:POTS)（R）
--     |    |   |   |－MODEM固化软件的版本（R）
--     |    |   |   |－MODEM固化软件的生成日期（R）       
--     |    |   |   |－OAM软件模块的版本（R）
--     |    |   |   |－OAM软件模块的生成日期（R）
--     |    |   |   |－DIAG CODE软件模块的版本（R）
--     |    |   |   |－MODEM CODE软件模块的版本（R）
--     |    |   |   |－供应商ID，只有在T1.413方式下才有（R）
--     |    |   |   |－供应商的版本号，在T.1413和G.HS方式下用（R）
--     |    |   |   |－ITU-T标准的国家代码号，在G.HS方式用（R）
--     |    |   |   |－ITU-T标准的供应商代码号，在G.HS方式用（R）
-- ***************************************************************       
     
    -- 板与套片（MODEM）描述表
    hwMusaAdlbBoardInfoTable    OBJECT-TYPE
        SYNTAX      SEQUENCE OF HwMusaAdlbBoardInfoEntry
        ACCESS      not-accessible
        STATUS      mandatory
    DESCRIPTION
    "Musa adlb board description table."
    ::= { hwMusaAdlbBoard 1 }

   hwMusaAdlbBoardInfoEntry    OBJECT-TYPE
        SYNTAX         HwMusaAdlbBoardInfoEntry
        ACCESS         not-accessible
        STATUS         mandatory
    DESCRIPTION
    "This list contains Musa adlb board description variables."
    INDEX { hwMusaFrameIndex,
            hwMusaSlotIndex }
    ::= { hwMusaAdlbBoardInfoTable  1 }

   HwMusaAdlbBoardInfoEntry    ::= SEQUENCE  {
            adlbProductId      		INTEGER,
            adlbCustomId      		INTEGER,
            adlbAtucRomSwVer   		DisplayString,
            adlbAtucRomSwBuildDate  DisplayString,
            adlbAtucOamSwVer  		DisplayString,
            adlbAtucOamSwBuildDate	DisplayString,
  			adlbByDiagCodeVer 		DisplayString,
    		adlbByModemCodeVer		DisplayString,
            adlbAtucVendorId        INTEGER,
            adlbAtucVersionNo       INTEGER,
            adlbItuCountryCode     	INTEGER,
            adlbItuProviderCode    	DisplayString
            }            
        
    adlbProductId  			OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
        " 产品代号ID "
    ::= { hwMusaAdlbBoardInfoEntry 1 }

    adlbCustomId      		OBJECT-TYPE
        SYNTAX  INTEGER		{
                            unknown(0), 
                            isdn(1),
                            pots(2)
                       }
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
        " 用户线类型(0:未知类型 1:ISDN 2:POTS) "
    ::= { hwMusaAdlbBoardInfoEntry 2 }
    
    adlbAtucRomSwVer   		OBJECT-TYPE
        SYNTAX  DisplayString
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
        " MODEM固化软件的版本 "
    ::= { hwMusaAdlbBoardInfoEntry 3 }
    
    adlbAtucRomSwBuildDate  OBJECT-TYPE
        SYNTAX  DisplayString
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
        " MODEM固化软件的生成日期 "
    ::= { hwMusaAdlbBoardInfoEntry 4 }
    
    adlbAtucOamSwVer  		OBJECT-TYPE
        SYNTAX  DisplayString
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
        " OAM软件模块的版本 "
    ::= { hwMusaAdlbBoardInfoEntry 5 }
    
    adlbAtucOamSwBuildDate	OBJECT-TYPE
        SYNTAX  DisplayString
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
        " OAM软件模块的生成日期 "
    ::= { hwMusaAdlbBoardInfoEntry 6 }
    
	adlbByDiagCodeVer 		OBJECT-TYPE
        SYNTAX  DisplayString
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
        " DIAG CODE软件模块的版本 "
    ::= { hwMusaAdlbBoardInfoEntry 7 }
    
	adlbByModemCodeVer		OBJECT-TYPE
        SYNTAX  DisplayString
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
        " MODEM CODE软件模块的版本 "
    ::= { hwMusaAdlbBoardInfoEntry 8 }
    
    adlbAtucVendorId        OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
        " 供应商ID ,只有在T1.413方式下才有 "
    ::= { hwMusaAdlbBoardInfoEntry 9 }
    
    adlbAtucVersionNo       OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
        " 供应商的版本号，在T.1413和G.HS方式下用 "
    ::= { hwMusaAdlbBoardInfoEntry 10 }
    
    adlbItuCountryCode     	OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
        " ITU-T标准的国家代码号，在G.HS方式用 "
    ::= { hwMusaAdlbBoardInfoEntry 11 }
    
    adlbItuProviderCode        	OBJECT-TYPE
        SYNTAX  DisplayString
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
        " ITU-T标准的供应商代码号，在G.HS方式用 "
    ::= { hwMusaAdlbBoardInfoEntry 12 }
    
-- ***************************************************************       
--      |    |－ ADLB套片管理组
--      |    |   |－套片维护表
--      |    |   |   |－#框号
--      |    |   |   |－#槽位号
--      |    |   |   |－#套片索引号 
--      |    |   |   |－套片操作（复位）（W）
-- ***************************************************************       

    -- 套片维护表
    hwMusaAdlbChipsetMtTable    OBJECT-TYPE
        SYNTAX      SEQUENCE OF HwMusaAdlbChipsetMtEntry
        ACCESS      not-accessible
        STATUS      mandatory
    DESCRIPTION
    "Musa adlb board description table."
    ::= { hwMusaAdlbChipset 1 }

   hwMusaAdlbChipsetMtEntry    OBJECT-TYPE
        SYNTAX         HwMusaAdlbChipsetMtEntry
        ACCESS         not-accessible
        STATUS         mandatory
    DESCRIPTION
    "This list contains Musa chipset maintenance variables."
    INDEX { hwMusaFrameIndex,
            hwMusaSlotIndex,
            adlbChipsetIndex }
    ::= { hwMusaAdlbChipsetMtTable  1 }

   HwMusaAdlbChipsetMtEntry    ::= SEQUENCE  {
            adlbChipsetIndex	INTEGER,
            adlbChipsetOper     INTEGER
            }            
        
    adlbChipsetIndex			OBJECT-TYPE
        SYNTAX  INTEGER ( 0..3 )
        ACCESS  not-accessible
        STATUS  mandatory
        DESCRIPTION
        " Adlb套片号 "
    ::= { hwMusaAdlbChipsetMtEntry 1 }
	
	adlbChipsetOper				OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  write-only
        STATUS  mandatory
        DESCRIPTION
        " reset chipset 复位套片. "
    ::= { hwMusaAdlbChipsetMtEntry 2 }

-- ***************************************************************       
--     |    |－ ADLB端口配置管理组
--     |    |   |－端口远端设备描述表
--     |    |   |   |－#框号
--     |    |   |   |－#槽位号
--     |    |   |   |－#端口索引号 
--     |    |   |   |－ATUR的供应商ID号（R）
--     |    |   |   |－ATUR的版本号（R）
--     |    |   |   |－ATUR的国家代码（R）	     
--     |    |   |   |－ATUR的供应商代码（R）	 
--     |    |   |   |－ATUR的ADSL能力（R）	 
-- ***************************************************************       

    -- 端口远端设备描述表表
    hwMusaAdlbPortDeviceTable    OBJECT-TYPE
        SYNTAX      SEQUENCE OF HwMusaAdlbPortDeviceEntry
        ACCESS      not-accessible
        STATUS      mandatory
    DESCRIPTION
    "Musa adlb port remote device table."
    ::= { hwMusaAdlbConf 1 }

   hwMusaAdlbPortDeviceEntry    OBJECT-TYPE
        SYNTAX         HwMusaAdlbPortDeviceEntry
        ACCESS         not-accessible
        STATUS         mandatory
    DESCRIPTION
    "This list contains Musa adlb port remote device variables."
    INDEX { hwMusaFrameIndex,
            hwMusaSlotIndex,
            adlbPortIndex }
    ::= { hwMusaAdlbPortDeviceTable  1 }

   HwMusaAdlbPortDeviceEntry    ::= SEQUENCE  {
            adlbPortIndex			INTEGER,
            adlbAturVendorId   		INTEGER,
            adlbAturVersion			INTEGER,
            adlbAturCountryCode		INTEGER,
	    	adlbAturProviderCode	INTEGER,
    		adlbAturAdslCapability	INTEGER
            }            
        
    adlbPortIndex			OBJECT-TYPE
        SYNTAX  INTEGER ( 0..15 )
        ACCESS  not-accessible
        STATUS  mandatory
        DESCRIPTION
        " Adlb端口号 "
    ::= { hwMusaAdlbPortDeviceEntry 1 }
	
    adlbAturVendorId   		OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
        " ATUR的供应商ID号 "
    ::= { hwMusaAdlbPortDeviceEntry 2 }
    
    adlbAturVersion			OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
        " ATUR的版本号 "
    ::= { hwMusaAdlbPortDeviceEntry 3 }
    
    adlbAturCountryCode		OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
        " ATUR供应商的国家代码 "
    ::= { hwMusaAdlbPortDeviceEntry 4 }
    
	adlbAturProviderCode	OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
        " ATUR的供应商代码 "
    ::= { hwMusaAdlbPortDeviceEntry 5 }
    
	adlbAturAdslCapability	OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
        " ATUR的ADSL能力 
           0:not support，1:support;
	       bit0--T1.413 issue2
	       bit2--g992.1 POTS non-overlapped spectrum
	       bit3--g992.1 POTS overlapped specturm
	       bit4--g992.1 ISDN non-overlapped spectrum
	       bit5--g992.1 ISDN overlapped spectrum
	       bit6--g992.1 operation in conjunction with TCM-ISDN non-overlapped spectrum
	       bit7--g992.1 operation in conjunction with TCM-ISDN overlapped spectrum
	       bit8--g992.2 POTS non-overlapped spectrum
	       bit9--g992.2 POTS overlapped specturm
	       bit10--g992.2 operation in conjunction with TCM-ISDN non-overlapped spectrum
	       bit11--g992.2 operation in conjunction with TCM-ISDN overlapped spectrum "
    ::= { hwMusaAdlbPortDeviceEntry 6 }
    
   
