-- ***************************************************************       

    -- 端口状态表
    hwMusaAdlbPortTable    OBJECT-TYPE
        SYNTAX      SEQUENCE OF HwMusaAdlbPortEntry
        ACCESS      not-accessible
        STATUS      mandatory
    DESCRIPTION
    "Musa adlb port table."
    ::= { hwMusaAdlbConf 2 }

   hwMusaAdlbPortEntry    OBJECT-TYPE
        SYNTAX         HwMusaAdlbPortEntry
        ACCESS         not-accessible
        STATUS         mandatory
    DESCRIPTION
    "This list contains Musa adlb port query and operation variables."
    INDEX { hwMusaFrameIndex,
            hwMusaSlotIndex,
            adlbPortIndex }
    ::= { hwMusaAdlbPortTable  1 }

   HwMusaAdlbPortEntry    ::= SEQUENCE  {
            adlbPortState   		INTEGER,
            adlbPortTrainStandard  	INTEGER,
            adlbPortOper	   		INTEGER,
            adlbPortTemplateId      INTEGER,
            adlbPortTxCell      INTEGER,
            adlbPortRxCell      INTEGER
            }            
        
    adlbPortState   		OBJECT-TYPE
        SYNTAX  INTEGER     {
                            active(0), 
                            deactive(1),
                            bad(2), 
                            activating(3),
                            deactivating(4),
                            block(5),
                            unblock(6),     --  只是用来给网管显示用，实际中没有的状态
                            other(255)
                       }

        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
        " 端口状态 (0:active 1:deactive 2:bad 3:activating 4:deactivating 5:block 255:other) "
    ::= { hwMusaAdlbPortEntry 1 }
	
    adlbPortTrainStandard  	OBJECT-TYPE
        SYNTAX  INTEGER	{ none(0), dmt(1), glite(2), t1413(3) }
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
        " 端口训练过程所遵循的标准 ( 0:none 1:G.DMT 2:G.LITE 3:T1.413 ) "
    ::= { hwMusaAdlbPortEntry 2 }
    
    adlbPortOper	   		OBJECT-TYPE
        SYNTAX  INTEGER		{
                            deact(0), 
                            block(1),
                            unblock(2),
                            clearCellStat(3)
                       }
        ACCESS  write-only
        STATUS  mandatory
        DESCRIPTION
        " 端口操作（0:关闭/去激活 1:阻塞 2:解阻塞） "
    ::= { hwMusaAdlbPortEntry 3 }

    adlbPortTemplateId	   		OBJECT-TYPE
        SYNTAX  INTEGER(1..99)
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
        " activate ADL port by one template. 激活端口 "
    ::= { hwMusaAdlbPortEntry 4 }

    adlbPortTxCell	   		OBJECT-TYPE
