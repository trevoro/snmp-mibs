			    oriLinkTestOurMinNoiseLevel			INTEGER,
			    oriLinkTestOurMinSNR				INTEGER,
			    oriLinkTestOurMaxSignalLevel		INTEGER,
			    oriLinkTestOurMaxNoiseLevel			INTEGER,
			    oriLinkTestOurMaxSNR				INTEGER,
			    oriLinkTestOurLowFrameCount			INTEGER,
			    oriLinkTestOurStandardFrameCount	INTEGER,
			    oriLinkTestOurMediumFrameCount		INTEGER,
			    oriLinkTestOurHighFrameCount		INTEGER,
			    oriLinkTestHisCurSignalLevel		INTEGER,
			    oriLinkTestHisCurNoiseLevel			INTEGER,
			    oriLinkTestHisCurSNR				INTEGER,
			    oriLinkTestHisMinSignalLevel		INTEGER,
			    oriLinkTestHisMinNoiseLevel			INTEGER,
			    oriLinkTestHisMinSNR				INTEGER,
			    oriLinkTestHisMaxSignalLevel		INTEGER,
			    oriLinkTestHisMaxNoiseLevel			INTEGER,
			    oriLinkTestHisMaxSNR				INTEGER,
		    	oriLinkTestHisLowFrameCount			INTEGER,
			    oriLinkTestHisStandardFrameCount	INTEGER,
			    oriLinkTestHisMediumFrameCount		INTEGER,
			    oriLinkTestHisHighFrameCount		INTEGER,
			    oriLinkTestInterface                INTEGER,
			    oriLinkTestRadioType                INTEGER	    
		}

	oriLinkTestTableIndex    OBJECT-TYPE
	    SYNTAX INTEGER
	    ACCESS read-only
	    STATUS mandatory
	    DESCRIPTION
			"This parameter represents a unique value for each stattion. The 
			value for each station must remain constant at least from one 
			explore to the next."
	    ::= { oriLinkTestTableEntry 1 }

	oriLinkTestInProgress    OBJECT-TYPE
	    SYNTAX INTEGER
	--	    {
	--			nolinktestinprogress(1),
	--			linktestinprogress(2)
