    psIpxClientNetworkNumber OBJECT-TYPE
        SYNTAX OCTET STRING
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "The IPX Node Number assigned by the Novell Server to 
            this IPX client."
        ::= { psASClientEntry 2 }

    psIpxClientMAC OBJECT-TYPE
        SYNTAX PhysAddress
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "The Wireless MAC of this IPX Client."
        ::= { psASClientEntry 3 }

