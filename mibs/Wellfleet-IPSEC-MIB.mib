        DEFVAL { tunnel }
        ::= { wfIpsecSaStatsEntry 15 }

    wfIpsecSaStatsPfs OBJECT-TYPE
        SYNTAX  INTEGER {
                    true(1),
                    false(2)
                }
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
