        DEFVAL { true }
        ::= { wfIpsecSaStatsEntry 16 }

    wfIpsecSaStatsExpiryType OBJECT-TYPE
        SYNTAX  INTEGER {
                    seconds(1),
                    kilobytes(2),
                    none(3)
                }
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
                "The units used to interpret the expiry value. The SA's
