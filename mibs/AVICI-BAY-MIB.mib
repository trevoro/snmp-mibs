     	SYNTAX INTEGER { 
     		critical(1), 
     		major(2),    
     		minor(3)   
     	} 
     	MAX-ACCESS read-only 
     	STATUS current 
     	DESCRIPTION 
     		"The severity level of this central office alarm." 
        ::= { aviciBayCoAlarmEntry 2 }

aviciBayCoAlarmDescr  OBJECT-TYPE
        SYNTAX      DisplayString (SIZE (0..255))
        MAX-ACCESS  read-only
