mscDpnGateFwdStatsStorageType OBJECT-TYPE
   SYNTAX     StorageType 
   ACCESS     read-only
   STATUS     mandatory
   DESCRIPTION
     "This variable represents the storage type value for the
      mscDpnGateFwdStats tables."
::= { mscDpnGateFwdStatsRowStatusEntry 4 }

mscDpnGateFwdStatsIndex OBJECT-TYPE
   SYNTAX     NonReplicated 
   ACCESS     not-accessible
   STATUS     mandatory
   DESCRIPTION
     "This variable represents the index for the mscDpnGateFwdStats tables."
::= { mscDpnGateFwdStatsRowStatusEntry 10 }

mscDpnGateFwdStatsOperTable OBJECT-TYPE
   SYNTAX     SEQUENCE OF MscDpnGateFwdStatsOperEntry
   ACCESS     not-accessible
   STATUS     mandatory
   DESCRIPTION
     "This operational group counts the Trunk or DpnGateway packet 
      forwarding traffic statistics for operator console display."
::= { mscDpnGateFwdStats 10 }

mscDpnGateFwdStatsOperEntry OBJECT-TYPE
   SYNTAX     MscDpnGateFwdStatsOperEntry
   ACCESS     not-accessible
   STATUS     mandatory
   DESCRIPTION
     "An entry in the mscDpnGateFwdStatsOperTable."
   INDEX {
      mscDpnGateIndex,
      mscDpnGateFwdStatsIndex }
::= { mscDpnGateFwdStatsOperTable 1 }

MscDpnGateFwdStatsOperEntry ::= SEQUENCE {
   mscDpnGateFwdStatsFwdPktFromIf
      PassportCounter64,
   mscDpnGateFwdStatsFwdDiscUnforwardFromIf
      PassportCounter64,
   mscDpnGateFwdStatsFwdOctetFromIf
      PassportCounter64 }

mscDpnGateFwdStatsFwdPktFromIf OBJECT-TYPE
   SYNTAX     PassportCounter64 
   ACCESS     read-only
   STATUS     mandatory
   DESCRIPTION
     "This attribute counts the number of forwarding packets received by 
      the Trunk or DpnGateway. This counter wraps around to zero when 
      the maximum value is exceeded."
::= { mscDpnGateFwdStatsOperEntry 1 }

mscDpnGateFwdStatsFwdDiscUnforwardFromIf OBJECT-TYPE
   SYNTAX     PassportCounter64 
