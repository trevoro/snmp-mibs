	STATUS mandatory

	DESCRIPTION

		"The slot number of this port's module."

	::= { olPortEntry 1 }



olPortIndex OBJECT-TYPE

	SYNTAX INTEGER

	ACCESS read-only

	STATUS mandatory

	DESCRIPTION

		"The port number of this port within this module."

	::= { olPortEntry 2 }



olPortType OBJECT-TYPE

	SYNTAX INTEGER {

