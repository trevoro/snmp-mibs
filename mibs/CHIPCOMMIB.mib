	::= { olPortEntry 5 }



olPortAdminState OBJECT-TYPE

	SYNTAX INTEGER {

		enabled(1),	      -- only valid value for class 2&4 modules

		disabled(2),

		redundant-primary(3), -- use olSpecMods to set

