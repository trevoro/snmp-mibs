	   HELP = ;
	   CAPABILITIES = ;"
              INDEX   { mcmBrgMacFilterPortIndex, mcmBrgMacFilterNumber }
              ::= { mcmBrgMacFilterTable 1 }

          McmBrgMacFilterEntry ::=
