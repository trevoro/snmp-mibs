        DESCRIPTION
                "per LEC Operation Config objects - wfAtmLecOperConfigCct corresponds to
                Wellfleet circuit number "
        INDEX	{ wflecOperConfigCct }
        ::= { wfAtmLecOperConfigTable 1 }

