-- QWES.com qproprietaryMib2 { oem 1 }
--
-- This group contains the MIB variables that correspond to the
-- standard Internet Approved Managed Object MIBS (i.e. RFC-1213
-- RFC 1406, RFC-1695, RFC-2358, etc.).

--    qsystem	        OBJECT IDENTIFIER ::= {qproprietaryMib2  1}
--    qinterfaces	OBJECT IDENTIFIER ::= {qproprietaryMib2  2}
--    qat		OBJECT IDENTIFIER ::= {qproprietaryMib2  3}
--    qip		OBJECT IDENTIFIER ::= {qproprietaryMib2  4}
    qsnmp		OBJECT IDENTIFIER ::= {qproprietaryMib2 11}
    qigmp		OBJECT IDENTIFIER ::= {qip 7}

-- SNMP Community Table
   	qsnmpCommunityTable 		OBJECT-TYPE
		SYNTAX		   	SEQUENCE OF QsnmpCommunityEntry
		ACCESS			not-accessible
		STATUS			mandatory
		DESCRIPTION		"A list of SNMP Community entries.  The
					maximum number of SNMP Communities are
					five."
		::= { qsnmp 1 }

   	qsnmpCommunityEntry 		OBJECT-TYPE
		SYNTAX			QsnmpCommunityEntry
		ACCESS			not-accessible
		STATUS			mandatory
		DESCRIPTION		"One entry in the table."
	        INDEX   { qsnmpCommunityIndex }
   		::= { qsnmpCommunityTable 1 }

	QsnmpCommunityEntry ::=
		SEQUENCE {
		        qsnmpCommunityIndex
		            INTEGER,
		        qsnmpCommunityRights
		            INTEGER,
		        qsnmpCommunityName
		            DisplayString
			}

-- SNMP Community Entry fields
	qsnmpCommunityIndex 	OBJECT-TYPE
		SYNTAX			INTEGER (0..4)
		ACCESS			read-only
		STATUS			mandatory
		DESCRIPTION		"A unique value which identifies each
					SNMP Community."
		::= { qsnmpCommunityEntry 1 }

	qsnmpCommunityRights 	OBJECT-TYPE
		SYNTAX			INTEGER {
						readonly  (0),
						readwrite (1)
						}
		ACCESS			read-only
		STATUS			optional
                DESCRIPTION             "The first and third
					qsnmpCommunityIndex have read-write
                                        permission while the others have
					read-only privilege. Values 0 =
					ReadOnly, and 1 = ReadWrite"
                ::= { qsnmpCommunityEntry 2 }

	qsnmpCommunityName 	OBJECT-TYPE
		SYNTAX			DisplayString (SIZE (0..255))
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"The SNMP Community Name used to access
					the unit with the associated permission
					from the qsnmpCommunityRights."
		::= { qsnmpCommunityEntry 3 }

-- SNMP NMS Table
   	qsnmpNMSTable 		OBJECT-TYPE
		SYNTAX		   	SEQUENCE OF QsnmpNMSEntry
		ACCESS			not-accessible
		STATUS			mandatory
		DESCRIPTION		"A list of SNMP Network Management
					System (NMS) entries."
		::= { qsnmp 2 }

   	qsnmpNMSEntry 		OBJECT-TYPE
		SYNTAX			QsnmpNMSEntry
		ACCESS			not-accessible
		STATUS			mandatory
		DESCRIPTION		"One entry in the table."
	        INDEX   { qsnmpNMSIndex }
   		::= { qsnmpNMSTable 1 }

	QsnmpNMSEntry ::=
		SEQUENCE {
		        qsnmpNMSIndex
		            INTEGER,
		        qsnmpNMSIpAddress
		            IpAddress
			}

-- SNMP NMS Address Entry fields
	qsnmpNMSIndex 		OBJECT-TYPE
		SYNTAX			INTEGER (0..4)
		ACCESS			read-only
		STATUS			mandatory
		DESCRIPTION		"A unique value which identifies each
					NMS."
		::= { qsnmpNMSEntry 1 }

	qsnmpNMSIpAddress 	OBJECT-TYPE
		SYNTAX			IpAddress
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"The IP Address of a Network Management
					System, which is allowed to access MIB
					variables."
		::= { qsnmpNMSEntry 2 }

-- SNMP Trap IP Address Table
   	qsnmpTrapTable 		OBJECT-TYPE
		SYNTAX		   	SEQUENCE OF QsnmpTrapEntry
		ACCESS			not-accessible
		STATUS			mandatory
		DESCRIPTION		"A list of IP Address entries
					associated with traps."
		::= { qsnmp 3 }

   	qsnmpTrapEntry 		OBJECT-TYPE
		SYNTAX			QsnmpTrapEntry
		ACCESS			not-accessible
		STATUS			mandatory
		DESCRIPTION		"One entry in the table."
	        INDEX   { qsnmpTrapIndex }
   		::= { qsnmpTrapTable 1 }

	QsnmpTrapEntry ::=
		SEQUENCE {
		        qsnmpTrapIndex
		            INTEGER,
		        qsnmpTrapIpAddress
		            IpAddress
			}

-- SNMP Trap IP Address Entry fields
	qsnmpTrapIndex 		OBJECT-TYPE
		SYNTAX			INTEGER (0..4)
		ACCESS			read-only
		STATUS			mandatory
		DESCRIPTION		"A unique value which identifies each
					IP Address."
		::= { qsnmpTrapEntry 1 }

	qsnmpTrapIpAddress 	OBJECT-TYPE
		SYNTAX			IpAddress
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"The IP Address of an IP entity, which
					is allowed to receive SNMP Trap
					events."
		::= { qsnmpTrapEntry 2 }

	qsnmpForwardTrap 	OBJECT-TYPE
		SYNTAX			INTEGER {
						enable (1),
						disable (2)
						}
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"The flag which enables(1)/disables(2)
					SNMP Trap events to be transmitted."
		::= { qsnmp 4 }

-- QWES.com qproducts { oem 2 }
--
-- Each QWES.com product family is Assigned an Object Identifier.
-- This is the identifier stored in the MIB-II system group
-- item sysObjectID to identify the product to a network manager.
-- If product family specific items need to be defined they
-- should be defined in the appropriate product family subtree


-- QWES.com qslot { oem 3 }
--


-- QWES.com qFlowClass { oem 4 }
--
-- This group contains the definitions for all MIB objects used to
-- manage all traffic flow through Flow Classification, etc.  Chris
-- will expand.

-- ClassDefinition entry count

	qClassDefinitionMaxEntries 	OBJECT-TYPE
		SYNTAX			Counter
		ACCESS			read-only
		STATUS			mandatory
		DESCRIPTION		"The number of Class Definition
					entries."
		::= { qFlowClass 1 }

-- ClassDefinition table

   	qClassDefinitionTable	OBJECT-TYPE
		SYNTAX		   	SEQUENCE OF QClassDefinitionEntry
		ACCESS			not-accessible
		STATUS			mandatory
		DESCRIPTION		"A list of Class Definition entries.
					The number of entries is given by the
					value of qClassDefinitionMaxEntries."
		::= { qFlowClass 2 }

   	qClassDefinitionEntry 	OBJECT-TYPE
		SYNTAX			QClassDefinitionEntry
		ACCESS			not-accessible
		STATUS			mandatory
		DESCRIPTION		"One entry in the table."
	        INDEX   { qClassDefinitionIndex }
   		::= { qClassDefinitionTable 1 }

	QClassDefinitionEntry ::=
		SEQUENCE {
		        qClassDefinitionIndex
		            INTEGER,
		        qClassDefinitionAlias
		            DisplayString,
		        qClassDefinitionParentClassID
		            INTEGER,
		        qClassDefinitionFlowType
		            INTEGER,
		        qClassDefinitionSourceIPMask
		            OCTET STRING,
		        qClassDefinitionDestIPMask
		            IpAddress,
		        qClassDefinitionSourceIPLowerBound
		            IpAddress,
		        qClassDefinitionSourceIPUpperBound
		            IpAddress,
		        qClassDefinitionDestIPLowerBound
		            IpAddress,
		        qClassDefinitionDestIPUpperBound
		            IpAddress,
		        qClassDefinitionDSMask
		            INTEGER,
		        qClassDefinitionDSLowerBound
		            INTEGER,
		        qClassDefinitionDSUpperBound
		            INTEGER,
		        qClassDefinitionWellKnownApplication
		            INTEGER,
		        qClassDefinitionProtocol
		            INTEGER,
		        qClassDefinitionSourcePortLowerBound
		            INTEGER,
		        qClassDefinitionSourcePortUpperBound
		            INTEGER,
		        qClassDefinitionDestPortLowerBound
		            INTEGER,
		        qClassDefinitionDestPortUpperBound
		            INTEGER,
                        qClassDefinitionFlowAgingTime
		            INTEGER
		  }

-- Class Definition Entry fields
	qClassDefinitionIndex 	OBJECT-TYPE
		SYNTAX			INTEGER (2..1024)
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"A unique value for each class
					definition. Values 0 and 1 are reserved
					for internal use."
		::= { qClassDefinitionEntry 1 }

	qClassDefinitionAlias 	OBJECT-TYPE
		SYNTAX			DisplayString (SIZE (0..255))
		ACCESS			read-write
		STATUS			optional
		DESCRIPTION		"A textual string containing user
					defined information about the Class
					Definition."
		::= { qClassDefinitionEntry 2 }

	qClassDefinitionParentClassID 	OBJECT-TYPE
		SYNTAX			INTEGER (0..1024)
		ACCESS			read-write
		STATUS			optional
		DESCRIPTION		"The qClassDefinitionIndex value of the
					parent for this Class Definition."
		::= { qClassDefinitionEntry 3 }

	qClassDefinitionFlowType 	OBJECT-TYPE
		SYNTAX			INTEGER {
						normal (1),
						dsingres (2),
						dsegress (3),
						dsnode (4),
						invalid (5)
						}
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"The type of Class Definition being
					created.  INVALID causes the current
					Class Definition Entry to be removed."
		::= { qClassDefinitionEntry 4 }

	qClassDefinitionSourceIPMask 	OBJECT-TYPE
		SYNTAX			IpAddress
		ACCESS			read-write
		STATUS			optional
		DESCRIPTION		"The IP address mask for the source."
		::= { qClassDefinitionEntry 5 }

	qClassDefinitionSourceIPLowerBound 	OBJECT-TYPE
		SYNTAX			IpAddress
		ACCESS			read-write
		STATUS			optional
		DESCRIPTION		"The first source IP address which
					matches the class (after masking)."
		::= { qClassDefinitionEntry 6 }

	qClassDefinitionSourceIPUpperBound 	OBJECT-TYPE
		SYNTAX			IpAddress
		ACCESS			read-write
		STATUS			optional
		DESCRIPTION		"The last source IP address which
					matches the class (after masking)."
		::= { qClassDefinitionEntry 7 }

	qClassDefinitionDestIPMask 	OBJECT-TYPE
		SYNTAX			IpAddress
		ACCESS			read-write
		STATUS			optional
		DESCRIPTION		"The IP address mask for the
					destination."
		::= { qClassDefinitionEntry 8 }

	qClassDefinitionDestIPLowerBound 	OBJECT-TYPE
		SYNTAX			IpAddress
		ACCESS			read-write
		STATUS			optional
		DESCRIPTION		"The first destination IP address which
					matches the class (after masking)."
		::= { qClassDefinitionEntry 9 }

	qClassDefinitionDestIPUpperBound 	OBJECT-TYPE
		SYNTAX			IpAddress
		ACCESS			read-write
		STATUS			optional
		DESCRIPTION		"The last destination IP address which
					matches the class (after masking)."
		::= { qClassDefinitionEntry 10 }

	qClassDefinitionDSMask	OBJECT-TYPE
		SYNTAX			INTEGER (0..255)
		ACCESS			read-write
		STATUS			optional
		DESCRIPTION		"The DS/TOS byte mask value."
		::= { qClassDefinitionEntry 11 }

	qClassDefinitionDSLowerBound 	OBJECT-TYPE
		SYNTAX			INTEGER (0..255)
		ACCESS			read-write
		STATUS			optional
		DESCRIPTION		"The first DS/TOS byte value which
					matches the class (after masking)."
		::= { qClassDefinitionEntry 12 }

	qClassDefinitionDSUpperBound 	OBJECT-TYPE
		SYNTAX			INTEGER (0..255)
		ACCESS			read-write
		STATUS			optional
		DESCRIPTION		"The last DS/TOS byte value which
					matches the class (after masking)."
		::= { qClassDefinitionEntry 13 }

	qClassDefinitionWellKnownApplication OBJECT-TYPE
		SYNTAX			INTEGER {
						unspecified (0), -- none of the following:
								 -- following
						realaudio (1),
						netmeeting (2),
						echo (3),
						discard (4),
						rip (5),
						daytime (5),
						quoteofday (6),
						msp (7),
						ftp (8),
						telnet (9),
						smtp (10),
						msg (11),
						time (12),
						rap (13),
						rlp (14),
						whois (15),
						login (16),
						remotemail (17),
						xns (18),
						dns (19),
						sql (20),
						bootp (21),
						tftp (22),
						gopher (23),
						finger (24),
						http (25),
						kerberos (26),
						npp (27),
						hostname (28),
						isotsap (29),
						remotetelnet (30),
						sna (31),
						pop3 (32),
						rpc (33),
						authentication (34),
						audionews (35),
						sftp (36),
						uucp (37),
						networktime (38),
						pwdgen (39),
						ciscofna (40),
						ciscotna (41),
						ciscosys (42),
						netbios (43),
						news (44),
						management (45),
						send (46),
						appletalk (47),
						ipx (48),
						performancedata (49),
						ldap (50),
						novell (51),
						microsoft (52),
						doom (53),
						lotusnotes (54),
