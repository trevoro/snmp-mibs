-- Policy Definition Entry fields
	qPolicyDefinitionIndex 	OBJECT-TYPE
		SYNTAX			INTEGER (2..1024)
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"A unique value for each policy
					definition. Values 0 and 1 are
					reserved for internal use."
		::= { qPolicyDefinitionEntry 1 }

	qPolicyDefinitionAlias 	OBJECT-TYPE
		SYNTAX			DisplayString (SIZE (0..255))
		ACCESS			read-write
		STATUS			optional
		DESCRIPTION		"A textual string containing user
					defined information about the Policy
					Definition."
		::= { qPolicyDefinitionEntry 2 }

	qPolicyDefinitionType 	OBJECT-TYPE
		SYNTAX			INTEGER {
						normal (1), -- normal policy
						dsingress (2), -- diffserv
							       -- ingress node
						dsnode (3), -- diffserv node
						dsegress (4), -- diffserv egree
							      -- node
						discard (5), -- discard policy
							     -- ... all pdus go
							     -- into the bit
							     -- bucket
						delete (6) -- delete policy
							   -- descriptor
						}
		ACCESS			read-write
		STATUS			optional
		DESCRIPTION		"The type of Policy Definition. DELETE
					causes the current definition to be
					removed."
		::= { qPolicyDefinitionEntry 3 }

        qPolicyDefinitionLP     OBJECT-TYPE
		SYNTAX			INTEGER (0..1024)
		ACCESS			read-write
		STATUS			optional
		DESCRIPTION		"The logical interface this policy
					relates to."
		::= { qPolicyDefinitionEntry 4 }

	qPolicyDefinitionStartTime 	OBJECT-TYPE
		SYNTAX			TimeDateString
		ACCESS			read-write
		STATUS			optional
		DESCRIPTION		"The time of day this policy takes
					effect. NULL = always in effect."
		::= { qPolicyDefinitionEntry 5 }

	qPolicyDefinitionEndTime 	OBJECT-TYPE
		SYNTAX			TimeDateString
		ACCESS			read-write
		STATUS			optional
		DESCRIPTION		"The time of day this policy stops
					taking effect. NULL = always in
					effect."
		::= { qPolicyDefinitionEntry 6 }

	qPolicyDefinitionDayofWeek 	OBJECT-TYPE
		SYNTAX			INTEGER (0..255)
		ACCESS			read-write
		STATUS			optional
		DESCRIPTION		"Bit flags which indicate the day of
					week. Bit 0 = Monday, 1 = Tuesday,
					2 = Weds, 4 = Thurs, 8 = Fri, 16 = Sat,
					32 = Sun, 64 = Holidays."
		::= { qPolicyDefinitionEntry 7 }

	qPolicyDefinitionPipeID 	OBJECT-TYPE
		SYNTAX			INTEGER (0..1024)
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"The pipe definition this policy
					references."
		::= { qPolicyDefinitionEntry 8 }

	qPolicyDefinitionClassID 	OBJECT-TYPE
		SYNTAX			INTEGER (0..1024)
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"The class definition this policy
					references."
		::= { qPolicyDefinitionEntry 9 }

	qPolicyDefinitionChannelized 	OBJECT-TYPE
		SYNTAX			INTEGER (0..65535)
		ACCESS			read-write
		STATUS			optional
		DESCRIPTION		"Bit flags indicating which Classing range is used to create
					unique channels. 0 = off, 1 = Source IP, 2 = Dest IP
					4 = Source Port, 8 = Dest Port, 16 = Protocol, 32 = DS.
					The maximum number of channels (1-255) is set into the upper
					byte of this value. If a channel is selected, the default 
					maximum is 16."
		::= { qPolicyDefinitionEntry 10 }

	qPolicyDefinitionOutboundDSValue 	OBJECT-TYPE
		SYNTAX			INTEGER (0..256)
		ACCESS			read-write
		STATUS			optional
		DESCRIPTION		"The DS/TOS byte value to set in
					outbound PDUs. A value of 256 indicates
					no replacement."
		::= { qPolicyDefinitionEntry 11 }

	qPolicyDefinitionPriority 	OBJECT-TYPE
		SYNTAX			INTEGER (0..255)
		ACCESS			read-write
		STATUS			optional
		DESCRIPTION		"The relative priority of this policy."
		::= { qPolicyDefinitionEntry 12 }

	qPolicyDefinitionEvent OBJECT-TYPE
		SYNTAX			INTEGER {
						static (1), -- the policy is
							    -- always in effect.
						time (2), -- time of day.
						reducedbw (3), -- reduced
							       -- bandwidth on
							       -- lif/vcc.
						increasedbw (4) -- increased
								-- bandwidth on
								-- lif/vcc.
			               		}
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"The type of event which causes this
					policy to come into effect."
		::= { qPolicyDefinitionEntry 13 }

-- qPipeDefinitionMaxEntries entry count

	qPipeDefinitionMaxEntries  OBJECT-TYPE
