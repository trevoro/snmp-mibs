						udp(2),
						tcp(3),
						icmp(4),
						arp(5),
						rsvp(6),
						egp(7),
						ipinip(8),
						rdp(9),
						isotp4(10),
						idrp(11),
						pvp(12),
						isoip(13),
						vines(14),
						ttp(15),
						nsfnetigp(16),
						igrp(17),
						ospfigp(18)
			               }
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"The IP protocol which matches the
					class."
		::= { qClassDefinitionEntry 15 }

	qClassDefinitionSourcePortLowerBound OBJECT-TYPE
		SYNTAX			INTEGER (0..65535)
		ACCESS			read-write
		STATUS			optional
