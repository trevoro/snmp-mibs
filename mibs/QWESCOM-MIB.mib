-- the IP group

qipCircExtTable OBJECT-TYPE
	SYNTAX SEQUENCE OF QipCircExtEntry
	ACCESS not-accessible
	STATUS mandatory
        DESCRIPTION "Table of IP circuit parameters"
	::= {qipExt 1}

qipCircExtEntry OBJECT-TYPE
	SYNTAX QipCircExtEntry
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION "A circuits IP parameters"
	INDEX {qipCircExtIndex}
	::= {qipCircExtTable 1}

QipCircExtEntry ::=
	SEQUENCE {
		qipCircExtIndex
			INTEGER,
		qipCircExtIfIndex
			INTEGER,
		qipCircExtAdminState
			INTEGER,
		qipCircExtOperState
			INTEGER
		}

qipCircExtIndex OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION "An index value which uniquely identifies
		this circuit within the router. This may or may
		not be related to the physical interface index."
	::= {qipCircExtEntry 1}

qipCircExtIfIndex OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION "An index value which uniquely identifies
		the local physical interface which this circuit
		uses. This value identifies the same interface
		identified by ifIndex in MIB-2."
	::= {qipCircExtEntry 2}

qipCircExtAdminState OBJECT-TYPE
	SYNTAX INTEGER{
			disabled(1),	-- operation disabled
			enabled(2)	-- operation enabled
		  }
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION "Enable or disable the operation of IP
		on the specified interface."
	::= {qipCircExtEntry 3}

qipCircExtOperState OBJECT-TYPE
	SYNTAX INTEGER{
			off(1),	-- circuit not up
			on(2)	-- circuit up
		  }
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION "The operational state of the circuit. May
		or may not be the same as administrative state."
	::= {qipCircExtEntry 4}

-- the ARP group

qarpExtTtl OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION "The lifetime of an ARP entry within the
		router."
