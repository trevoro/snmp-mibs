	VERILINK-ENTERPRISE-NCMM13-MIB

        -- set up MIB-wide defaults
        -- all functions can use %n for object name and
        -- %t for object type, %p for parent name,
        -- %d for name of default-bearing node and %% for %

          DEFINITIONS ::= BEGIN

     IMPORTS
                Gauge
                        FROM RFC1155-SMI
            	DisplayString
                     FROM RFC1213-MIB
        	OBJECT-TYPE
                     FROM RFC-1212
        	ncm-m13
             FROM VERILINK-ENTERPRISE-NCMALARM-MIB;


     --  This MIB module uses the extended OBJECT-TYPE macro as
     --  defined in RFC 1212.
     --  This MIB module uses the TRAP-TYPE macro as
     --  defined in RFC 1215.


     --   verilink                    OBJECT IDENTIFIER ::= { enterprises 321 }
     --   as2000                      OBJECT IDENTIFIER ::= { verilink 1 }
     --   ncm-m13                     OBJECT IDENTIFIER ::= { as2000 3037 }



	--
     --ncm m13 DS3 table
     --  
     ncmm13DS3PortTable OBJECT-TYPE
          SYNTAX SEQUENCE OF Ncmm13DS3PortEntry
          ACCESS not-accessible
          STATUS mandatory
          DESCRIPTION
               "The m13 DS3 Port table."
                --DEFAULT test-function   m13-test
              --DEFAULT set-function    null-set-proc
                --DEFAULT next-function   m13-next
          ::= { ncm-m13 14000 }

     ncmm13DS3PortEntry OBJECT-TYPE
          SYNTAX Ncmm13DS3PortEntry
          ACCESS not-accessible
          STATUS mandatory
          DESCRIPTION
               "An entry in the m13 information table."
          INDEX   { ncmm13DS3PortNIDIndex, ncmm13DS3PortIndex }
          ::= { ncmm13DS3PortTable 1 }

     Ncmm13DS3PortEntry ::= SEQUENCE {
             ncmm13DS3PortNIDIndex
                INTEGER,
             ncmm13DS3PortIndex
                INTEGER,
             ncmm13DS3ModeControl
                INTEGER,
             ncmm13DS3LineType
                INTEGER,
	     ncmm13DS3PerfControl
		INTEGER,
	     ncmm13DS3AlarmControl
		INTEGER,
	     ncmm13DS3FarEndControl
                INTEGER,
	     ncmm13DS3BusType
                INTEGER,
             ncmm13DS3InbandControl
                INTEGER,
	     ncmm13DS3CableLength
		INTEGER,
             ncmm13DS3EquipCode
                 DisplayString,
             ncmm13DS3LocationIDCode
                 DisplayString,
             ncmm13DS3FrameIDCode
                 DisplayString,
             ncmm13DS3UnitCode
                 DisplayString,
             ncmm13DS3FacilityIDCode
                 DisplayString,
             ncmm13DS3PortIdCode
                 DisplayString,
             ncmm13DS3GenIDCode
                 DisplayString
          }


	ncmm13DS3PortNIDIndex OBJECT-TYPE
          SYNTAX  INTEGER (1..'7fffffff'h)
          ACCESS  read-only
          STATUS  mandatory
          DESCRIPTION
                "This variable indicates the node id value of the node."
            ::= { ncmm13DS3PortEntry 1 }
 
     	ncmm13DS3PortIndex OBJECT-TYPE
          SYNTAX  INTEGER (1..65535)
          ACCESS  read-only
          STATUS  mandatory
          DESCRIPTION
               "The index value which uniquely identifies  the
                DS3  interface  to which this entry is applica-
                ble.  The interface identified by a  particular
                value  of  this  index is the same interface as
                identified by the same value as a dsx1LineIndex
                object instance."
            ::= { ncmm13DS3PortEntry 2 }
 
     	ncmm13DS3ModeControl OBJECT-TYPE
          SYNTAX  INTEGER {
                c-bit(1),
                m23-bit(2)
          }
          ACCESS  read-write
          STATUS  mandatory
          DESCRIPTION
                "The Mode Control Bit for M13."
            ::= { ncmm13DS3PortEntry 3 }
 
	 ncmm13DS3LineType  OBJECT-TYPE
          SYNTAX  INTEGER{
                bi-polar(1),
                uni-polar(2)
          }
          ACCESS  read-write
          STATUS  mandatory
          DESCRIPTION
               "The Line Type for M13."
            ::= { ncmm13DS3PortEntry 4 }
 
	 ncmm13DS3PerfControl OBJECT-TYPE
          SYNTAX  INTEGER { 
		disabled (1),
                enabled (2)
          }
          ACCESS  read-write
          STATUS  mandatory
          DESCRIPTION
                "Performance Control Enabled/Disabled for M13."
           ::= { ncmm13DS3PortEntry 5 }


	ncmm13DS3AlarmControl OBJECT-TYPE
	SYNTAX INTEGER { 
		disabled (1),
                enabled (2)
          }
          ACCESS  read-write
          STATUS  mandatory
          DESCRIPTION
                "Alarm Control Enabled/Disabled for M13."
           ::= { ncmm13DS3PortEntry 6 }

	ncmm13DS3FarEndControl OBJECT-TYPE
	SYNTAX INTEGER { 
		disabled (1),
                enabled (2)
          }
          ACCESS  read-write
          STATUS  mandatory
          DESCRIPTION
                "Far End Control Enabled/Disabled for M13."
           ::= { ncmm13DS3PortEntry 7 }

	ncmm13DS3BusType	OBJECT-TYPE
	SYNTAX INTEGER { 
		c-bus (1),
                a-bus(2)
          }
          ACCESS  read-write
          STATUS  mandatory
          DESCRIPTION
                "Inband Control Enabled/Disabled for M13."
           ::= { ncmm13DS3PortEntry 8 }



	
	ncmm13DS3InbandControl	OBJECT-TYPE
	SYNTAX INTEGER { 
		disabled (1),
                enabled (2)
          }
          ACCESS  read-write
          STATUS  mandatory
          DESCRIPTION
                "Inband Control Enabled/Disabled for M13."
           ::= { ncmm13DS3PortEntry 9 }


	
	ncmm13DS3CableLength	OBJECT-TYPE
	SYNTAX INTEGER { 
		short (1),
                long (2)
          }
          ACCESS  read-write
          STATUS  mandatory
          DESCRIPTION
                "Cable length for M13."
           ::= { ncmm13DS3PortEntry 10 }

	 
     ncmm13DS3EquipCode OBJECT-TYPE
          SYNTAX  DisplayString (SIZE (0..10))
          ACCESS  read-write
          STATUS  mandatory
          DESCRIPTION
               "This is the Far End Equipment Identification code
                that describes the specific piece of equipment.
                It is sent within the Path
                Identification Message."
          ::= { ncmm13DS3PortEntry 11 }
 
     ncmm13DS3LocationIDCode OBJECT-TYPE
          SYNTAX  DisplayString (SIZE (0..11))
          ACCESS  read-write
          STATUS  mandatory
          DESCRIPTION
               "This is the Far End Location Identification code
                that describes the specific location of the
                equipment.  It is sent within the Path
                Identification Message."
          ::= { ncmm13DS3PortEntry 12 }
 
     ncmm13DS3FrameIDCode OBJECT-TYPE
          SYNTAX  DisplayString (SIZE (0..10))
          ACCESS  read-write
          STATUS  mandatory
          DESCRIPTION
               "This is the Far End Frame Identification code
                that identifies where the equipment is located
                within a building at a given location.
                It is sent within the Path Identification Message."
          ::= { ncmm13DS3PortEntry 13 }


	ncmm13DS3UnitCode OBJECT-TYPE
          SYNTAX  DisplayString (SIZE (0..6))
          ACCESS  read-write
          STATUS  mandatory
          DESCRIPTION
               "This is the Far End code
                that identifies the equipment location within a bay.
                It is sent within the Path
                Identification Message."

          ::= { ncmm13DS3PortEntry 14 }
 
     ncmm13DS3FacilityIDCode OBJECT-TYPE
          SYNTAX  DisplayString (SIZE (0..38))
          ACCESS  read-write
          STATUS  mandatory
          DESCRIPTION
               "This code identifies a specific Far End DS3 path.
                It is sent within the Path Identification Message."
          ::= { ncmm13DS3PortEntry 15 }
 
     ncmm13DS3PortIdCode OBJECT-TYPE
          SYNTAX  DisplayString (SIZE (0..38))
          ACCESS read-write
          STATUS mandatory
          DESCRIPTION
               "Port ID."
          ::= { ncmm13DS3PortEntry 16 }
 
 
     ncmm13DS3GenIDCode OBJECT-TYPE
          SYNTAX  DisplayString (SIZE (0..38))
          ACCESS read-write
          STATUS mandatory
          DESCRIPTION
               "Test Signal Generator ID."
          ::= { ncmm13DS3PortEntry 17 }


	-- ncm m13 Status Table

 

	ncmm13StatTable OBJECT-TYPE
