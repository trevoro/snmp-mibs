                appletalk(16),    -- Apple Talk
                route(17),        -- Internal Routing Protocol
                link(18),         -- Link layer interface
                xtpPseudo(19),    -- eXpress Transfer Protocol (no AF)
                intf(20),         -- Debugging use only
