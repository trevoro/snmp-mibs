}

smMlmAliasState OBJECT-TYPE
        SYNTAX  INTEGER {
                enabled(3),   -- enabled alias entry
                invalid(2),   -- an invalidated alias entry
                disabled(1)   -- disabled alias entry
                }
        ACCESS  read-write
