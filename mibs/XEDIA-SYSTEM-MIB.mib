    STATUS      current
    DESCRIPTION
        "The index of the table."
    ::= { xsysBootImageEntry 1 }

xsysBootImageBootPriority OBJECT-TYPE
    SYNTAX      Unsigned32 (0..10)
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
