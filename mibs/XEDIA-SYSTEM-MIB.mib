xsysBootImageLocalAddr OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The local IP address used during a boot TFTP."
    DEFVAL      { 0 }
