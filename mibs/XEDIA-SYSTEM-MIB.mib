xsysBootImageMask OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The IP address mask used during a boot TFTP."
    DEFVAL      { 0 }
    ::= { xsysBootImageEntry 9 }

xsysBootImageDefaultRouter OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  read-create
