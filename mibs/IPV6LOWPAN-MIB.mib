        MODULE-IDENTITY, OBJECT-TYPE, Unsigned32, mib-2
            FROM SNMPv2-SMI
        LowpanIEEEEUI64Address, LowpanShortAddress
            FROM LOWPAN-TC-MIB;

    lowpanMIB    MODULE-IDENTITY
