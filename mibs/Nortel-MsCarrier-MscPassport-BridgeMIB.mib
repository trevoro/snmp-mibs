   mscVrPpFddiETBTranslateIpx
      INTEGER,
   mscVrPpFddiETBFragmentIp
      INTEGER,
   mscVrPpFddiETBServiceClass
      INTEGER,
   mscVrPpFddiETBConvertArpMacAddress
      INTEGER,
