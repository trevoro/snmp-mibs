IwrGroupEntry ::=
	SEQUENCE {
	    iwrGroupId
		INTEGER,
	    iwrOperState
		INTEGER,
	    iwrPMState
		INTEGER,
	    iwrIOMState
		INTEGER,
	    iwrEthernetMode
		INTEGER,
	    iwrPrimaryFDDIInsert 
		INTEGER,
	    iwrSecondaryFDDIInsert 
		INTEGER
	    }

