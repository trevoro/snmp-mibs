	SYNTAX		DmiString
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION	"Additional message reported by the verification instrumentation."
	REFERENCE "'DMTF|Verification|001' 5"
	::= { dmtfVerificationEntry 5 }

