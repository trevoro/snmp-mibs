	SYNTAX		Dmi-Date
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION	"The date and time when the last verification test was completed."
	REFERENCE "'DMTF|Verification|001' 4"
	::= { dmtfVerificationEntry 4 }

