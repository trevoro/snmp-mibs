-- Introduction
--
-- This document defines the SNMP Management Information Base (MIB)
-- variables that are implemented in the MegaPAC suite of "Switch-Router"
-- software products, manufactured by Sat.  This MIB*
-- includes all Gateway Emulation, Terminal Emulation, Bridge / Router,
-- Protocol Translator and serial / packet Switching functions.
-- The document relies upon the Structure of Management Information
-- (SMI), RFC1155.  It is presented in a format described in RFC1212,
-- the Concise MIB document.
--
--      MIB Version ................... 25    (Fevrier , 1999)
--

SAT-MEGA-MIB --{ iso org(3) dod(6) internet(1) private(4) enterprises(1) 1038 4 }
   DEFINITIONS ::= BEGIN
   IMPORTS
		DisplayString FROM SNMPv2-TC
           OBJECT-TYPE, Counter, enterprises
                   FROM RFC1155-SMI
		TRAP-TYPE FROM RFC-1215;

        sat             OBJECT IDENTIFIER ::= { enterprises 1038 }
        mega            OBJECT IDENTIFIER ::= { sat 4 }

        products        OBJECT IDENTIFIER ::= { mega 1 }
        base            OBJECT IDENTIFIER ::= { mega 2 }
        controller      OBJECT IDENTIFIER ::= { mega 3 }
        channel         OBJECT IDENTIFIER ::= { mega 4 }
        x25File         OBJECT IDENTIFIER ::= { mega 5 }
        nameFile        OBJECT IDENTIFIER ::= { mega 6 }
        passFile        OBJECT IDENTIFIER ::= { mega 7 }
        macFile         OBJECT IDENTIFIER ::= { mega 8 }
        ilanFile        OBJECT IDENTIFIER ::= { mega 9 }
        elogFile        OBJECT IDENTIFIER ::= { mega 10 }
        billFile        OBJECT IDENTIFIER ::= { mega 11 }

-- The Products Group
-- The product group defined below identifies each model of MegaPAC
-- processor. Each model has a unique object identifier allocated
-- from this section which is referenced by the sysObjectID variable
-- from RFC1156. New products will be added at the end of this list.

        megaPAC-E-cpu-C         OBJECT IDENTIFIER ::= { products 1 }
        megaPAC-ESL             OBJECT IDENTIFIER ::= { products 2 }
        megaPAC-E-cpu-D-E       OBJECT IDENTIFIER ::= { products 3 }
        megaPAC-V-cpu-8         OBJECT IDENTIFIER ::= { products 4 }
        megaPAC-V-cpu-16        OBJECT IDENTIFIER ::= { products 5 }
        megaPAC-V-cpu-68040     OBJECT IDENTIFIER ::= { products 6 }
        megaPAC-V-cpu-68060     OBJECT IDENTIFIER ::= { products 7 }

-- The BASE Group
-- The BASE Group describes the global attributes of the MegaPAC.

        baseVersion     OBJECT-TYPE
            SYNTAX      DisplayString (SIZE (4))
            ACCESS      read-only
            STATUS      mandatory
            DESCRIPTION
                "Software Version.
                The software version currently running. Defined as YMMR (i.e.
                Year, Month, Revision"
            ::= { base 1 }

        baseRestartTime OBJECT-TYPE
            SYNTAX      DisplayString (SIZE (20))
            ACCESS      read-only
            STATUS      mandatory
            DESCRIPTION
                "BASE Restart Time.
                The time and data of the last system restart (i.e. power-on
                 or last BOOT)"
            ::= { base 2 }

        baseMaxPacketSize OBJECT-TYPE
            SYNTAX      INTEGER
            ACCESS      read-only
            STATUS      mandatory
            DESCRIPTION
                "Node Maximum Packet Size.
                The maximum single buffer size available on the MegaPAC."
            ::= { base 3 }

        baseBuffPoolMax OBJECT-TYPE
            SYNTAX      INTEGER
            ACCESS      read-only
            STATUS      mandatory
            DESCRIPTION
                "Node Buffer Pool Maximum.
                The maximum number of buffers available temporary storage of
                transitting data."
            ::= { base 4 }

        baseBuffPoolNow OBJECT-TYPE
            SYNTAX      INTEGER
            ACCESS      read-only
            STATUS      mandatory
            DESCRIPTION
                "Node Current Buffer Pool.
                The current number of free buffers available temporary storage of
                transitting data."
            ::= { base 5 }

        baseBufferPoolLowest OBJECT-TYPE
            SYNTAX      INTEGER
            ACCESS      read-only
            STATUS      mandatory
            DESCRIPTION
                "Node Buffer Pool Lowest.
                The lowest value the buffer pool has ever reached."
            ::= { base 6 }

        baseBufferPoolEmpty OBJECT-TYPE
            SYNTAX      INTEGER
            ACCESS      read-only
            STATUS      mandatory
            DESCRIPTION
                "Node Buffer Pool Empty.
                The number of times that the free buffer pool has fallen below
                the minimum buffer threshold 'baseBuffThreshold'."
            ::= { base 7 }

        baseStatBufferThreshold OBJECT-TYPE
            SYNTAX      INTEGER (5..1000)
            ACCESS      read-only
            STATUS      mandatory
            DESCRIPTION
                "Node Buffer Threshold.
                When the number of buffers in the free buffer pool reaches
                this threshold, flow control will occur."
            ::= { base 8 }

        baseMemorySize OBJECT-TYPE
            SYNTAX      INTEGER
            ACCESS      read-only
            STATUS      mandatory
            DESCRIPTION
                "Node Memory Size.
                The size of the system memory installed in the MegaPAC."
            ::= { base 9 }

        baseDataFramesIn OBJECT-TYPE
            SYNTAX      Counter
            ACCESS      read-only
            STATUS      mandatory
            DESCRIPTION
                "Node Frames In.
                The total number of frames received by the MegaPAC."
            ::= { base 10 }

        baseDataFramesOut OBJECT-TYPE
            SYNTAX      Counter
            ACCESS      read-only
            STATUS      mandatory
            DESCRIPTION
                "Node Frames Out.
                The total number of frames transmitted by the MegaPAC."
            ::= { base 11 }

        baseDataFrameRateIn OBJECT-TYPE
            SYNTAX      Counter
            ACCESS      read-only
            STATUS      mandatory
            DESCRIPTION
                "Node Frame Rate In.
                The rate measured in frames per second that the MegaPAC
                is currently receiving frames."
            ::= { base 12 }

        baseDataFrameRateOut OBJECT-TYPE
            SYNTAX      Counter
            ACCESS      read-only
            STATUS      mandatory
            DESCRIPTION
                "Node Frames Rate Out.
                The rate measured in frames per second that the MegaPAC
                is currently transmitting frames."
            ::= { base 13 }

        baseTotalCallSetUps OBJECT-TYPE
            SYNTAX      Counter
            ACCESS      read-only
            STATUS      mandatory
            DESCRIPTION
                "Node Total Call Set ups.
                The total number of calls successfully established by the
                MegaPAC."
            ::= { base 14 }

        baseCurrentCalls OBJECT-TYPE
            SYNTAX      Counter
            ACCESS      read-only
            STATUS      mandatory
            DESCRIPTION
                "Current calls.
                The number of calls currently active on the MegaPAC."
            ::= { base 15 }

        baseRetransmissions OBJECT-TYPE
            SYNTAX      Counter
            ACCESS      read-only
            STATUS      mandatory
            DESCRIPTION
                "Node Retransmissions.
                The total number of frames which have been retransmitted
                by the MegaPAC."
            ::= { base 16 }

        baseRejects OBJECT-TYPE
            SYNTAX      Counter
            ACCESS      read-only
            STATUS      mandatory
            DESCRIPTION
                "Node Rejects.
                The total number of frames that have been rejected by the
                MegaPAC."
            ::= { base 17 }

        baseTotalTransportCalls OBJECT-TYPE
            SYNTAX      Counter
            ACCESS      read-only
            STATUS      mandatory
            DESCRIPTION
                "Node Total Transport calls.
                The total number of transport calls established by the
                MegaPAC."
            ::= { base 18 }

        baseCurrentTransportCalls OBJECT-TYPE
            SYNTAX      Counter
            ACCESS      read-only
            STATUS      mandatory
            DESCRIPTION
                "Current Transport Calls.
                The number of transport calls currently active on the
                MegaPAC."
            ::= { base 19 }

        baseRunStatus OBJECT-TYPE
            SYNTAX      INTEGER { prom(1), ram(2) }
            ACCESS      read-only
            STATUS      mandatory
            DESCRIPTION
                "Node Run Status.
                This parameter indicates whether the operating system has
                been 'Booted' from RAM (TELELOAD) or EPROM (default)."
            ::= { base 20 }

        baseReports OBJECT-TYPE
            SYNTAX      DisplayString (SIZE (4))
            ACCESS      read-write
            STATUS      mandatory
            DESCRIPTION
                "Node Reporting channel.
                The name of the logical resource (channel) that the system
                reports will be directed to."
            ::= { base 21 }

        baseSessionStatistics OBJECT-TYPE
            SYNTAX      DisplayString (SIZE (4))
            ACCESS      read-write
            STATUS      mandatory
            DESCRIPTION
                "Node Session Statistics reporting channel.
                The name of the logical resource (channel) that the system
                session statistics will be directed to."
            ::= { base 22 }

        baseBufferThreshold OBJECT-TYPE
            SYNTAX      INTEGER (5..1000)
            ACCESS      read-write
            STATUS      mandatory
            DESCRIPTION
                "Node Buffer Threshold.
                The threshold before flow control is invoked"
            ::= { base 23 }

        baseInitialPresentationTimer OBJECT-TYPE
            SYNTAX      INTEGER (0..300)
            ACCESS      read-write
            STATUS      mandatory
            DESCRIPTION
                "Node Initial Presentation Timer.
                The maximum time allowed between initial connection to the
                presentation service and the user entering a valid resource.
                The call will be disconnected if this timer elapses."
            ::= { base 24 }

        baseSecondaryPresentationTimer OBJECT-TYPE
            SYNTAX      INTEGER (0..300)
            ACCESS      read-write
            STATUS      mandatory
            DESCRIPTION
                "Node Secondary Presentation Timer.
                After a call has been cleared by the Host DTE the user
                has the option to use the presentation service to
                establish another call, failure to do so within this time
                period will result in the initial call being cleared."
            ::= { base 25 }

        baseInactivityDetectTimer OBJECT-TYPE
            SYNTAX      INTEGER (0..30)
            ACCESS      read-write
            STATUS      mandatory
            DESCRIPTION
                "Node Inactivity Timer.
                This timer will cause calls to be cleared if the IDLE option
                has been configured on the channel."
            ::= { base 26 }

        baseSegmentAccounting OBJECT-TYPE
            SYNTAX      INTEGER { no(1), yes(2) }
            ACCESS      read-write
            STATUS      mandatory
            DESCRIPTION
                "Node Segment Accounting.
                This parameter causes the frames in/out parameters to
                be displayed in terms of 64 byte SEGMENTS rather than frames."
            ::= { base 27 }

        baseTerminalEmulationBuffers OBJECT-TYPE
            SYNTAX      INTEGER (0..640)
            ACCESS      read-write
            STATUS      mandatory
            DESCRIPTION
                "Terminal Emulation Buffers.
                The number of system buffers allocated for Terminal Emulation
                purposes."
            ::= { base 28 }

        baseTransportBuffers OBJECT-TYPE
            SYNTAX      INTEGER (0..2048)
            ACCESS      read-write
            STATUS      mandatory
            DESCRIPTION
                "Transport Buffers.
                The number of transport buffers allocated for TP 0,2,3,4
                sessions."
            ::= { base 29 }

        baseCountrySettings OBJECT-TYPE
            SYNTAX      INTEGER { none(1), japn(2) }
            ACCESS      read-write
            STATUS      mandatory
            DESCRIPTION
                "Country Setting.
                Invokes the ASCII representation of the Japanese
                character set."
            ::= { base 30 }

        baseTeleloadEnabled OBJECT-TYPE
            SYNTAX      INTEGER { no(1), yes(2), k256(3), k160(129) }
            ACCESS      read-write
            STATUS      mandatory
            DESCRIPTION
                "Teleload enable.
                If set to yes the MegaPAC software may be downloaded by
                ASCII file transfer."
            ::= { base 31 }

        baseDefaultToPromCompatible OBJECT-TYPE
            SYNTAX      INTEGER { no(1), yes(2) }
            ACCESS      read-write
            STATUS      mandatory
            DESCRIPTION
                "Cold Boot From PROM.
                If set to yes the MegaPAC software may be downloaded by
                ASCII file transfer."
            ::= { base 32 }

-- The Controller Group (Statistics Section)
-- The Controller Group described below relates to MegaPAC protocol
-- interfaces that support a physical or virtual interface
-- connection.  The controller Group defines all parameters available
-- in "flat table format", and as such, certain object types may or
-- may not be presented depending on the controller type being used.

        ctrlNumberConfigured OBJECT-TYPE
            SYNTAX      INTEGER
            ACCESS      read-only
            STATUS      mandatory
            DESCRIPTION
                "Total Number of Controllers Configured.
                The number of controllers in the MegaPAC which have been
                configured."
            ::= { controller 1 }

        ctrlTotalNumber OBJECT-TYPE
            SYNTAX      INTEGER
            ACCESS      read-only
            STATUS      mandatory
            DESCRIPTION
                "Total Number of Controllers.
                The TOTAL number of controllers in the MegaPAC including
                unconfigured (NULL) controllers."
            ::= { controller 2 }

        ctrlStatTable OBJECT-TYPE
            SYNTAX      SEQUENCE OF CtrlStatEntry
            ACCESS      not-accessible
            STATUS      mandatory
            ::= { controller 3 }

        ctrlStatEntry OBJECT-TYPE
            SYNTAX      CtrlStatEntry
            ACCESS      not-accessible
            STATUS      mandatory
            DESCRIPTION
                ""
            INDEX       { ctrlStatIndex }
            ::= { ctrlStatTable 1 }

        CtrlStatEntry ::= SEQUENCE { ctrlStatIndex
                                     INTEGER,
                                     ctrlStatName
                                     DisplayString,
                                     ctrlStatType
                                     INTEGER,
                                     ctrlStatSubType
                                     INTEGER,
                                     ctrlStatNumberChannels
                                     INTEGER,
                                     ctrlStatFirstChannelIndex
                                     INTEGER,
                                     ctrlStatCRCErrors
                                     Counter,
                                     ctrlStatTotalCallSetUps
                                     Counter,
                                     ctrlStatCurrentCalls
                                     INTEGER,
                                     ctrlStatDataIn
                                     Counter,
                                     ctrlStatDataOut
                                     Counter,
                                     ctrlStatDataFramesIn
                                     Counter,
                                     ctrlStatDataFramesOut
                                     Counter,
                                     ctrlStatDataFrameRateIn
                                     INTEGER,
                                     ctrlStatDataFrameRateOut
                                     INTEGER,
                                     ctrlStatCallsReceived
                                     Counter,
                                     ctrlStatCallsSent
                                     Counter,
                                     ctrlStatClearsReceived
                                     Counter,
                                     ctrlStatRetransmissions
                                     Counter,
                                     ctrlStatRejects
                                     Counter,
                                     ctrlStatState
                                     INTEGER,
                                     ctrlStatSubState
                                     INTEGER,
                                     ctrlStatNoTries
                                     Counter,
                                     ctrlStatOptions
                                     INTEGER,
                                     ctrlStatLCGN
                                     INTEGER,
                                     ctrlStatResetsReceived
                                     Counter,
                                     ctrlStatInterruptsReceived
                                     Counter,
                                     ctrlStatOutputQueueLength
                                     INTEGER,
                                     ctrlStatStationAddress
                                     DisplayString,
                                     ctrlStatDestinationAddress
                                     DisplayString,
                                     ctrlStatEIA
                                     DisplayString,
                                     ctrlStatSdlcOptions
                                     INTEGER,
                                     ctrlStatArpRequestRec
                                     INTEGER,
                                     ctrlStatArpRequestSent
                                     INTEGER,
                                     ctrlStatArpResponsesRec
                                     INTEGER,
                                     ctrlStatFRTxSeqLost
                                     INTEGER,
                                     ctrlStatFRRxSeqLost
                                     INTEGER,
                                     ctrlStatFRCurTxSeq
                                     INTEGER,
                                     ctrlStatFRCurRxSeq
                                     INTEGER,
                                     ctrlStatFRRcvBecn
                                     INTEGER,
                                     ctrlStatFRRcvFecn
                                     INTEGER,
                                     ctrlStatFRThroughput
                                     INTEGER,
                                     ctrlStatFRLMI
                                     INTEGER,
                                     ctrlStatFRrespLmi
                                     INTEGER,
                                     ctrlStatFRCurSndSeq
                                     INTEGER,
                                     ctrlStatFRCurRcvSeq
                                     INTEGER,
                                     ctrlStatFRRcvDe
                                     INTEGER }

        ctrlStatIndex OBJECT-TYPE
            SYNTAX      INTEGER
            ACCESS      read-only
            STATUS      mandatory
            DESCRIPTION
                "Controller index.
                The position of this controller in relation to 'BASE'.
                 i.e.   BASE = 0
                        NMSC = 1
                        PS01 = 2
                        ..."
            REFERENCE
                "ALL"
            ::={ ctrlStatEntry 1 }

        ctrlStatName OBJECT-TYPE
            SYNTAX      DisplayString (SIZE(4))
            ACCESS      read-only
            STATUS      mandatory
            DESCRIPTION
                "Controller name.
                The unique 4 character name by which this controller is
                known."
            REFERENCE
                "ALL"
            ::={ ctrlStatEntry 2 }

        ctrlStatType OBJECT-TYPE
            SYNTAX      INTEGER { x25(1),    mux(2),   null(3),  epci(4),  hdlc(8),  qllc(9),
                                  xlan(10),  ilan(11), term(12), lan(13),  clus(14), mult(15),
                                  sdlc(16),  ip(17),   sru(18),  v25b(19), cons(20), nmsc(21),
                                  dlc(22),   map(23),  tlan(24), snmp(25), nfrm(26), xfrm(27),
                                  tip(28),   rout(29), ppp(30),  ipx(31),  tipx(32), esis(34),
                                  tesis(35), vfrm(36), smdx(37), dlcx(38), edlc(39) }
            ACCESS      read-only
            STATUS      mandatory
            DESCRIPTION
                "Controller Type.
                The type of controller this is.
                Applicable controllers: PPP, X.25, XFRM, NFRM VFRM, CONS,
                IP, TIP, IPX, TIPX, ESIS, DLC, LAN, TLAN, ILAN, XLAN
                SDLC, TERM, QLLC AND HDLC"
            REFERENCE
                "PPP X25 XFRM NFRM VFRM CONS IP TIP IPX TIPX ESIS
                DLC LAN TLAN ILAN XLAN SDLC TERM QLLC HDLC"
            ::={ ctrlStatEntry 3 }

        ctrlStatSubType OBJECT-TYPE
            SYNTAX      INTEGER { none(1), sio(2), xio(3),
                                  xio-sio(4), generic(5),scc(17), vio(33) }
            ACCESS      read-only
            STATUS      mandatory
            DESCRIPTION
                "Sub Type.
                The serial I/O board installed."
            REFERENCE
                "PPP X25 XFRM NFRM VFRM SDLC QLLC HDLC"
            ::={ ctrlStatEntry 4 }

        ctrlStatNumberChannels OBJECT-TYPE
            SYNTAX      INTEGER
            ACCESS      read-only
            STATUS      mandatory
            DESCRIPTION
                "Channel Count.
                The number of channels assigned to a controller.
                Applicable controllers: PPP, X.25, XFRM, NFRM VFRM, CONS,
                IP, TIP, IPX, TIPX, ESIS, DLC, LAN, TLAN, ILAN, XLAN
                SDLC, TERM, QLLC AND HDLC"
            REFERENCE
                "PPP X25 XFRM NFRM VFRM CONS IP TIP IPX TIPX ESIS
                DLC LAN TLAN ILAN XLAN SDLC TERM QLLC HDLC"
            ::={ ctrlStatEntry 5 }

        ctrlStatFirstChannelIndex OBJECT-TYPE
            SYNTAX      INTEGER
            ACCESS      read-only
            STATUS      mandatory
            DESCRIPTION
                "First Channnel.
                The 1st channel index number used by the controller
                : PPP, X.25, XFRM, NFRM VFRM, CONS, IP, TIP, IPX, TIPX,
                ESIS, DLC, LAN, TLAN, ILAN, XLAN SDLC, TERM, QLLC & HDLC"
            REFERENCE
                "PPP X25 XFRM NFRM VFRM CONS IP TIP IPX TIPX ESIS
                DLC LAN TLAN ILAN XLAN SDLC TERM QLLC HDLC"
            ::={ ctrlStatEntry 6 }

        ctrlStatCRCErrors OBJECT-TYPE
            SYNTAX      Counter
            ACCESS      read-only
            STATUS      mandatory
            DESCRIPTION
                "CRC Errors.
                The number of packets received for which the checksum
                was not valid.
                Applicable controllers: PPP, X.25, XFRM, NFRM VFRM, CONS,
                IP, TIP, IPX, TIPX, ESIS, DLC, LAN, TLAN, ILAN, XLAN
                SDLC, TERM, QLLC AND HDLC"
            REFERENCE
                "PPP X25 XFRM NFRM VFRM CONS IP TIP IPX TIPX ESIS
                DLC LAN TLAN ILAN XLAN SDLC TERM QLLC HDLC"
            ::= { ctrlStatEntry 7 }

        ctrlStatTotalCallSetUps OBJECT-TYPE
            SYNTAX      Counter
            ACCESS      read-only
            STATUS      mandatory
            DESCRIPTION
                "Total Call Set ups.
                The total number of successful calls established on this
                link.
                Applicable controllers: PPP, X.25, XFRM, NFRM VFRM, CONS,
                IP, TIP, IPX, TIPX, ESIS, DLC, LAN, TLAN, ILAN, XLAN
                SDLC, TERM, QLLC AND HDLC"
            REFERENCE
                "PPP X25 XFRM NFRM VFRM CONS IP TIP IPX TIPX ESIS
                DLC LAN TLAN ILAN XLAN SDLC TERM QLLC HDLC"
            ::= { ctrlStatEntry 8 }

        ctrlStatCurrentCalls OBJECT-TYPE
            SYNTAX      INTEGER
            ACCESS      read-only
            STATUS      mandatory
            DESCRIPTION
                "Current Calls.
                The number of calls currently active on this link.
                Applicable controllers: PPP, X.25, XFRM, NFRM VFRM, CONS,
                IP, TIP, IPX, TIPX, ESIS, DLC, LAN, TLAN, ILAN, XLAN
                SDLC, TERM, QLLC AND HDLC"
            REFERENCE
                "PPP X25 XFRM NFRM VFRM CONS IP TIP IPX TIPX ESIS
                DLC LAN TLAN ILAN XLAN SDLC TERM QLLC HDLC"
            ::= { ctrlStatEntry 9 }

        ctrlStatDataIn OBJECT-TYPE
            SYNTAX      Counter
            ACCESS      read-only
            STATUS      mandatory
            DESCRIPTION
                "Data In.
                The total number of data bytes received on this link.
                Applicable controllers: PPP, X.25, XFRM, NFRM VFRM, CONS,
                IP, TIP, IPX, TIPX, ESIS, DLC, LAN, TLAN, ILAN, XLAN
                SDLC, TERM, QLLC AND HDLC"
            REFERENCE
                "PPP X25 XFRM NFRM VFRM CONS IP TIP IPX TIPX ESIS
                DLC LAN TLAN ILAN XLAN SDLC TERM QLLC HDLC"
            ::= { ctrlStatEntry 10 }

        ctrlStatDataOut OBJECT-TYPE
            SYNTAX      Counter
            ACCESS      read-only
            STATUS      mandatory
            DESCRIPTION
                "Data Out.
                The total number of data bytes transmitted on this link.
                Applicable controllers: PPP, X.25, XFRM, NFRM VFRM, CONS,
                IP, TIP, IPX, TIPX, ESIS, DLC, LAN, TLAN, ILAN, XLAN
                SDLC, TERM, QLLC AND HDLC"
            REFERENCE
                "PPP X25 XFRM NFRM VFRM CONS IP TIP IPX TIPX ESIS
                DLC LAN TLAN ILAN XLAN SDLC TERM QLLC HDLC"
            ::= { ctrlStatEntry 11 }

        ctrlStatDataFramesIn OBJECT-TYPE
            SYNTAX      Counter
            ACCESS      read-only
            STATUS      mandatory
            DESCRIPTION
                "Data Frames In.
                The total number of data frames received on this link.
                Applicable controllers: PPP, X.25, XFRM, NFRM VFRM, CONS,
                IP, TIP, IPX, TIPX, ESIS, DLC, LAN, TLAN, ILAN, XLAN
                SDLC, TERM, QLLC AND HDLC"
            REFERENCE
                "PPP X25 XFRM NFRM VFRM CONS IP TIP IPX TIPX ESIS
                DLC LAN TLAN ILAN XLAN SDLC TERM QLLC HDLC"
            ::= { ctrlStatEntry 12 }

        ctrlStatDataFramesOut OBJECT-TYPE
            SYNTAX      Counter
            ACCESS      read-only
            STATUS      mandatory
            DESCRIPTION
                "Data Frames Out.
                The total number of data frames transmitted on this
                link.
                Applicable controllers: PPP, X.25, XFRM, NFRM VFRM, CONS,
                IP, TIP, IPX, TIPX, ESIS, DLC, LAN, TLAN, ILAN, XLAN
                SDLC, TERM, QLLC AND HDLC"
            REFERENCE
                "PPP X25 XFRM NFRM VFRM CONS IP TIP IPX TIPX ESIS
                DLC LAN TLAN ILAN XLAN SDLC TERM QLLC HDLC"
            ::= { ctrlStatEntry 13 }

        ctrlStatDataFrameRateIn OBJECT-TYPE
            SYNTAX      INTEGER
            ACCESS      read-only
            STATUS      mandatory
            DESCRIPTION
                "Data Frame Rate In.
                The rate that frames are arriving on this link,
                specified in frames per second.
                Applicable controllers: PPP, X.25, XFRM, NFRM VFRM, CONS,
                IP, TIP, IPX, TIPX, ESIS, DLC, LAN, TLAN, ILAN, XLAN
                SDLC, TERM, QLLC AND HDLC"
            REFERENCE
                "PPP X25 XFRM NFRM VFRM CONS IP TIP IPX TIPX ESIS
                DLC LAN TLAN ILAN XLAN SDLC TERM QLLC HDLC"
            ::= { ctrlStatEntry 14 }

        ctrlStatDataFrameRateOut OBJECT-TYPE
            SYNTAX      INTEGER
            ACCESS      read-only
            STATUS      mandatory
            DESCRIPTION
                "Data Frame Frate Out.
                The rate that frames are being transmitted on this link,
                specified in frames per second.
                Applicable controllers: PPP, X.25, XFRM, NFRM VFRM, CONS,
                IP, TIP, IPX, TIPX, ESIS, DLC, LAN, TLAN, ILAN, XLAN
                SDLC, TERM, QLLC AND HDLC"
            REFERENCE
                "PPP X25 XFRM NFRM VFRM CONS IP TIP IPX TIPX ESIS
                DLC LAN TLAN ILAN XLAN SDLC TERM QLLC HDLC"
            ::= { ctrlStatEntry 15 }

        ctrlStatCallsReceived OBJECT-TYPE
            SYNTAX      Counter
            ACCESS      read-only
            STATUS      mandatory
            DESCRIPTION
                "Calls Received.
                The total number of incoming call requests seen on this
                link.
                Applicable controllers: PPP, X.25, XFRM, NFRM VFRM, CONS,
                DLC, SDLC, TERM, QLLC, HDLC, IPX, TIPX, ESIS,
                LAN, TLAN, ILAN AND XLAN"
            REFERENCE
                "PPP X25 XFRM NFRM VFRM CONS DLC SDLC TERM QLLC
                HDLC IPX TIPX ESIS LAN TLAN ILAN XLAN"
            ::= { ctrlStatEntry 16 }

        ctrlStatCallsSent OBJECT-TYPE
            SYNTAX      Counter
            ACCESS      read-only
            STATUS      mandatory
            DESCRIPTION
                "Calls Sent.
                The total number of call requests sent out on this link.
                Applicable controllers: PPP, X.25, XFRM, NFRM VFRM, CONS,
                DLC, SDLC, TERM, QLLC, HDLC, IPX, TIPX, ESIS,
                LAN, TLAN, ILAN AND XLAN"
            REFERENCE
                "PPP X25 XFRM NFRM VFRM CONS DLC SDLC TERM QLLC
                HDLC IPX TIPX ESIS LAN TLAN ILAN XLAN"
            ::= { ctrlStatEntry 17 }

        ctrlStatClearsReceived OBJECT-TYPE
            SYNTAX      Counter
            ACCESS      read-only
            STATUS      mandatory
            DESCRIPTION
                "Clears Received.
                The total number of call clear requests received on this
