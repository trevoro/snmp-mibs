            SYNTAX      INTEGER { ibm(1), none(2) }
            ACCESS      read-write
            STATUS      mandatory
            DESCRIPTION
                "Printer Type.
                If set to IBM the logical channel will act as an
                appication system printer, otherwise it operates in
                screen emulation mode. Only applicable to channels (2)
                to (n).
                Applicable: CLUS controller type only"
            REFERENCE
                "CLUS"
            ::={ chanParamEntry 47 }

        chanParamScreenPrinter OBJECT-TYPE
            SYNTAX      INTEGER { yes(1), no(2) }
            ACCESS      read-write
            STATUS      mandatory
            DESCRIPTION
                "Screen Printer.
                If set to YES, screen printer requests are to be processed
                and sent to this LU address (in this mode, the terminal
                attaching to this channel MUST have a printer attached). If
                set to NO, screen printing is supported via the LOCAL PRINTER
                or the DEFAULT LOCAL PRINTER parameters.
                Applicable: CLUS controller type only"
            REFERENCE
                "CLUS"
            ::={ chanParamEntry 48 }

        chanParamLocalPrinter OBJECT-TYPE
            SYNTAX      INTEGER (0..128)
            ACCESS      read-write
            STATUS      mandatory
            DESCRIPTION
                "Local Printer.
                This parameter specifies the channel to which screen printing
                should be directed. It takes precedence over the value defined
                by the DEFAULT LOCAL PRINTER at controller level. This parameter
                may only be applied to channels (2) to (n).
                Applicable: CLUS controller type only"
            REFERENCE
                "CLUS"
            ::={ chanParamEntry 49 }

        chanParamBnnDsap OBJECT-TYPE
            SYNTAX      INTEGER (0..254)
            ACCESS      read-write
            STATUS      mandatory
            DESCRIPTION
                "DSAP.
                The Destination Service Access Point.
                Applicable: NFRM controller type with SNA protocol encapsulation."
            REFERENCE
                " NFRM "
            ::={ chanParamEntry 50 }

        chanParamBnnSsap OBJECT-TYPE
            SYNTAX      INTEGER (0..254)
            ACCESS      read-write
            STATUS      mandatory
            DESCRIPTION
                "SSAP.
                The Source Service Access Point.
                Applicable: NFRM controller type with SNA protocol encapsulation."
            REFERENCE
                " NFRM "
            ::={ chanParamEntry 51 }

        chanParamBnnPuTyp OBJECT-TYPE
            SYNTAX      INTEGER { none(0),    pu2-0(130),
                                  pu2-1(131)  }
            ACCESS      read-write
            STATUS      mandatory
            DESCRIPTION
                "PU TYPE.
                IBM PU type.
                Applicable: NFRM controller type with SNA protocol encapsulation."
            REFERENCE
                "NFRM"
            ::={ chanParamEntry 52 }

        chanParamPriority OBJECT-TYPE
            SYNTAX      INTEGER (0..7)
            ACCESS      read-write
            STATUS      mandatory
            DESCRIPTION
                "Channel Priority.
                The internal priority which will be assigned to
                the channel. Data from the channel with the highest priority i.e. 1
                will overtake data of from less priority channels on the same DLCI.
                Applicable: NFRM controller type."
            REFERENCE
                "NFRM"
            ::={ chanParamEntry 53 }

-- The X.25 File
-- The X.25 File defines the X.25 address file used for switching /
-- translation of X.25 call packets.  This file applies to controller
-- types ; X25 and XFRM.  Note, where complex decision making
-- routines are required for switching, translating and or alternate
-- routing, this file can be complemented (or replaced) by the
-- MegaPAC MACro file.

        x25FileSize OBJECT-TYPE
            SYNTAX      INTEGER
            ACCESS      read-only
            STATUS      mandatory
            DESCRIPTION
                "X25 file size.
                The number of entries configured into the X25 file."
            ::= { x25File 1 }

        x25Table OBJECT-TYPE
            SYNTAX      SEQUENCE OF X25Entry
            ACCESS      not-accessible
            STATUS      mandatory
            DESCRIPTION
                "A table of X.25 routing entries"
            ::= { x25File 2 }

        x25Entry OBJECT-TYPE
            SYNTAX      X25Entry
            ACCESS      not-accessible
            STATUS      mandatory
            INDEX       { x25Index }
            ::= { x25Table 1 }

        X25Entry ::= SEQUENCE { x25Index         INTEGER,
                                x25Name          DisplayString,
                                x25AddressGroup  INTEGER,
                                x25Address       DisplayString,
                                x25UserData      DisplayString }

        x25Index OBJECT-TYPE
            SYNTAX      INTEGER
            ACCESS      read-only
            STATUS      mandatory
            DESCRIPTION
                "Index.
                The index to an individual X.25 entry"
            ::={ x25Entry 1 }

        x25Name OBJECT-TYPE
            SYNTAX      DisplayString (SIZE(4))
            ACCESS      read-write
            STATUS      mandatory
            DESCRIPTION
                "Name.
                A 4 character name which is used to match on the 'NAME'
                parameter of the channel on which a call will be routed
                over the X.25 network."
            ::={ x25Entry 2 }

        x25AddressGroup OBJECT-TYPE
            SYNTAX      INTEGER (0..15)
            ACCESS      read-write
            STATUS      mandatory
            DESCRIPTION
                "Addr Grp.
                An integer used to partition the X.25 routing tables.
                Any match on a channel name will only take effect if
                the channel 'AddressGroup' parameter is set to the same
                value."
            ::={ x25Entry 3 }

        x25Address OBJECT-TYPE
            SYNTAX      DisplayString (SIZE(0..16))
            ACCESS      read-write
            STATUS      mandatory
            DESCRIPTION
                "Address.
                The numeric address which is used to match on the
                incoming X.25 address in a call request packet."
            ::={ x25Entry 4 }

        x25UserData OBJECT-TYPE
            SYNTAX      DisplayString (SIZE(0..30))
            ACCESS      read-write
            STATUS      mandatory
            DESCRIPTION
                "User Data
                A text field in which Call User Data or Facilities may
                be placed. The user data will be inserted into call
                request packets generated by the MegaPAC."
            ::={ x25Entry 5 }

-- The Name File
-- The Name File is used to associate up to 8 characters service
-- names (known as to the ASCII user) with 2 character resource names
-- used by the MegaPAC for internal routing. The Name File can also
-- be used to invoke terminal gateway emulation, such as,
-- IBM 3270, ICL 7561, TCP-IP Telnet etc.

        nameFileSize OBJECT-TYPE
            SYNTAX      INTEGER
            ACCESS      read-only
            STATUS      mandatory
            DESCRIPTION
                "Name File Size.
                The number of entries configured into the NAME file."
            ::= { nameFile 1 }

        nameTable OBJECT-TYPE
            SYNTAX      SEQUENCE OF NameEntry
            ACCESS      not-accessible
            STATUS      mandatory
            DESCRIPTION
                "A table of 'Name' entries."
            ::= { nameFile 2 }

        nameEntry OBJECT-TYPE
            SYNTAX      NameEntry
            ACCESS      not-accessible
            STATUS      mandatory
            INDEX       { nameIndex }
            ::= { nameTable 1 }

        NameEntry ::= SEQUENCE { nameIndex            INTEGER,
                                 nameName             DisplayString,
                                 nameDRCMask          DisplayString,
                                 nameCallPriority     INTEGER,
                                 nameBilling          INTEGER,
                                 nameOptions          INTEGER }

        nameIndex OBJECT-TYPE
            SYNTAX      INTEGER
            ACCESS      read-only
            STATUS      mandatory
            DESCRIPTION
                "Index.
                The index into the name table."
            ::={ nameEntry 1 }

        nameName OBJECT-TYPE
            SYNTAX      DisplayString (SIZE(0..8))
            ACCESS      read-write
            STATUS      mandatory
            DESCRIPTION
                "Name.
                A string of up to 8 characters in length which is
                typed in by the network user in order to establish
                a connection to a specific resource."
            ::={ nameEntry 2 }

        nameDRCMask OBJECT-TYPE
            SYNTAX      DisplayString (SIZE(4))
            ACCESS      read-write
            STATUS      mandatory
            DESCRIPTION
                "DRC Mask.
                The four character resource (channel) name to which
                the call will be directed."
            ::={ nameEntry 3 }

        nameCallPriority OBJECT-TYPE
            SYNTAX      INTEGER (0..15)
            ACCESS      read-write
            STATUS      mandatory
            DESCRIPTION
                "Call Priority.
                The internal call priority which will be assigned to
                the call."
            ::={ nameEntry 4 }

        nameBilling OBJECT-TYPE
            SYNTAX      INTEGER { enabled(1), disabled(2) }
            ACCESS      read-write
            STATUS      mandatory
            DESCRIPTION
                "Billing
                If enabled, statistics relating to the call will be
                sent to the billing file."
            ::={ nameEntry 5 }

        nameOptions OBJECT-TYPE
            SYNTAX      INTEGER { none(1),     iso(2), prof(5),  icl7561(9),
                                  teln(65), rev-teln(129), ppp(33), ppp-pap(49) }
            ACCESS      read-write
            STATUS      mandatory
            DESCRIPTION
                "Options
                Enables the appropriate terminal emulation options."
            ::={ nameEntry 6 }

-- The Password File (NOT IMPLEMENTED CURRENTLY)
-- The Password File is used to password protect ASCII resources
-- i.e. either on input to a switched network, or as a outbound
-- security gateway.  For security reasons, only the size of the
-- Password File can be displayed.  In order to display or modify
-- this file the MegaPAC's VCP must be accessed directly.

        passFileSize OBJECT-TYPE
            SYNTAX      INTEGER
            ACCESS      read-only
            STATUS      mandatory
            DESCRIPTION
                "Pass File Size.
                The number of entries configured into the PASSWORD
                file."
            ::= { passFile 1 }

-- The Macro File
-- The Macro File is a general purpose file that is used to define
-- complex routing, filtering functions, and or emulation functions.

        macFileSize OBJECT-TYPE
            SYNTAX      INTEGER
            ACCESS      read-only
            STATUS      mandatory
            DESCRIPTION
                "Mac File Size.
                The number of entries configured into the MAC file."
            ::= { macFile 1 }

        macTable OBJECT-TYPE
            SYNTAX      SEQUENCE OF MacEntry
            ACCESS      not-accessible
            STATUS      mandatory
            DESCRIPTION
                "A table of MAC entries"
            ::= { macFile 2 }

        macEntry OBJECT-TYPE
            SYNTAX      MacEntry
            ACCESS      not-accessible
            STATUS      mandatory
            INDEX       { macIndex }
            ::= { macTable 1 }

        MacEntry ::= SEQUENCE { macIndex         INTEGER,
                                macName          DisplayString,
                                macFunction      INTEGER,
                                macOptions       INTEGER,
                                macSelector      DisplayString,
                                macGenerator     DisplayString }

        macIndex OBJECT-TYPE
            SYNTAX      INTEGER
            ACCESS      read-only
            STATUS      mandatory
            DESCRIPTION
                "Index.
                An index to an individual entry in the MAC table."
            ::={ macEntry 1 }

        macName OBJECT-TYPE
            SYNTAX      DisplayString (SIZE(4))
            ACCESS      read-write
            STATUS      mandatory
            DESCRIPTION
                "Name.
                The level 2 controller name to which this entry
                applies."
            ::={ macEntry 2 }

        macFunction OBJECT-TYPE
            SYNTAX      INTEGER { nop(1),      cad(2),      dad(3),      cud(4),
                                  else(5),     fac(6),      inse(16386), dele(16387),
                                  copy(16388), posn(16389), mark(16390), data(16391),
                                  eof(16392),  ibeg(32769), iend(32770), obeg(32771),
                                  oend(32772), pu(32773),   xid(32774),  list(32775),
                                  mask(32776), sync(32777), tab(32778),  rout(32781),
                                  nvmt(32782), add(32783),  bar(32784),  dhcp(32786),
                                  time(32785), dpr(8),      cpr(7) }
            ACCESS      read-write
            STATUS      mandatory
            DESCRIPTION
                "Function.
                The function field determines what action should be
                taken if their is a match on this entry."
            ::={ macEntry 3 }

        macOptions OBJECT-TYPE
            SYNTAX      INTEGER { tlat(99),     rej (2),   tcal(3),  iso (4),
                                  cad (5),      thru(6),   talk(7),  bill(8),
                                  ccit-1980(9), expe(10),  jump(11), copy(12),
                                  none(1),      wipx(161), sap(133), y13(65),
                                  disc(8193),   not(129)}
            ACCESS      read-write
            STATUS      mandatory
            DESCRIPTION
                "Options.
                Specific actions that may be envoked in conjunction
                with the function field."
            ::={ macEntry 4 }

        macSelector OBJECT-TYPE
            SYNTAX      DisplayString (SIZE(0..32))
            ACCESS      read-write
            STATUS      mandatory
            DESCRIPTION
                "Selector.
                This field can contain a string of between 0 and 32
                Hexadecimal characters, that must be matched by the
                Function DAD, CAD, CUD or FAC."
            ::={ macEntry 5 }

        macGenerator OBJECT-TYPE
            SYNTAX      DisplayString (SIZE(0..32))
            ACCESS      read-write
            STATUS      mandatory
            DESCRIPTION
                "Generator.
                This field can contain a string of between 0 and 32
                Hexadecimal characters, and can have different purposes
                depending on the Function or Options fields."
            ::={ macEntry 6 }

-- The ILAN File
-- The ILAN File is used OSI TP0,2,3,4 applications where address
-- mapping of TSAPs (Transport Service Access Points) is required
-- e.g. mapping between TP4 to X25 or IBM 3270 LU sessions etc.

        ilanFileSize OBJECT-TYPE
            SYNTAX      INTEGER
            ACCESS      read-only
            STATUS      mandatory
            DESCRIPTION
                "Ilan File Size.
                The number of entries configured into the ILAN file."
            ::= { ilanFile 1 }

        ilanTable OBJECT-TYPE
            SYNTAX      SEQUENCE OF IlanEntry
            ACCESS      not-accessible
            STATUS      mandatory
            DESCRIPTION
                "A table of ILAN entries"
            ::= { ilanFile 2 }

        ilanEntry OBJECT-TYPE
            SYNTAX      IlanEntry
            ACCESS      not-accessible
            STATUS      mandatory
            INDEX       { ilanIndex }
            ::= { ilanTable 1 }

        IlanEntry ::= SEQUENCE { ilanIndex           INTEGER,
                                 ilanName            DisplayString,
                                 ilanOptions         INTEGER,
                                 ilanEthernetAddress DisplayString,
                                 ilanTSAP            DisplayString }

        ilanIndex OBJECT-TYPE
            SYNTAX      INTEGER
            ACCESS      read-only
            STATUS      mandatory
            DESCRIPTION
                "Index.
                An index to an individual entry in the ILAN table."
            ::={ ilanEntry 1 }

        ilanName OBJECT-TYPE
            SYNTAX      DisplayString (SIZE(4))
            ACCESS      read-write
            STATUS      mandatory
            DESCRIPTION
                "Name.
                The Resource which this entry applies to."
            ::={ ilanEntry 2 }

        ilanOptions OBJECT-TYPE
            SYNTAX      INTEGER { iso(1), expe(2), none(3), y13(16), dbit(4), talk(8) }
            ACCESS      read-write
            STATUS      mandatory
            DESCRIPTION
                "Options
                ISO     Allows transport calls to be set up on the outgoing
                        channel.
                EXPE    Allows expedited data to be sent."
            ::={ ilanEntry 3 }

        ilanEthernetAddress OBJECT-TYPE
            SYNTAX      DisplayString (SIZE(12))
            ACCESS      read-write
            STATUS      mandatory
            DESCRIPTION
                "Ethernet Address.
                The destination MAC address of the named host."
            ::={ ilanEntry 4 }

        ilanTSAP OBJECT-TYPE
            SYNTAX      DisplayString (SIZE(0..20))
            ACCESS      read-write
            STATUS      mandatory
            DESCRIPTION
                "TSAP.
                The Transport Service Access Point required on the
                destination Host."
            ::={ ilanEntry 5 }

