        ::= { syDirEntry 1 }
 
syDirName OBJECT-TYPE
        SYNTAX OCTET STRING (SIZE(1..13))
        ACCESS read-only
        STATUS mandatory
        ::= { syDirEntry 2 }
 
syDirCompressSize OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        ::= { syDirEntry 3 }
 
syDirVersion OBJECT-TYPE
        SYNTAX OCTET STRING (SIZE(1..128))
