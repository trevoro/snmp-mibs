           INDEX { l2tpv3SessionMapIfIndex }
           ::= { l2tpv3SessionMapTable 1 }

   L2tpv3SessionMapEntry ::=
           SEQUENCE {
               l2tpv3SessionMapIfIndex     InterfaceIndex,
               l2tpv3SessionMapCtrlIfIndex InterfaceIndex,
               l2tpv3SessionMapLocalID     Unsigned32,
