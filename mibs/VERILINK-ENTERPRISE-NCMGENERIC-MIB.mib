          SYNTAX INTEGER {
 
                       low(1),
                       high(2)
               }
          ACCESS read-write
          STATUS mandatory
          DESCRIPTION
          "The variable specifies the bit 2 status of control line;
             where 0=low, 1=high; Status is RL if Mode = DTE while is TM if Mode = DCE"
 
          ::= { ncmDataPortConfigEntry 8 }


        ncmDataPortConfigLOS OBJECT-TYPE
          SYNTAX INTEGER {
 
                       disable(1),
                       enable(2)
               }
          ACCESS read-write
          STATUS mandatory
          DESCRIPTION
          "The variable specifies the bit 7 status of control line;
             where 0=disable, 1=enable; Status is LOS Mode"
 
          ::= { ncmDataPortConfigEntry 9 }                                                 
 

