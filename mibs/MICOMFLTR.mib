        SYNTAX SEQUENCE OF NvmIpSysFltrEntry
        ACCESS not-accessible
        STATUS mandatory    
	DESCRIPTION
	  "NAME = ;
	   DESC = Information about the IP forwarding \
		  system filter entry;
	   HELP = ;
           CAPABILITIES = NET_CFG, VPN_NONE;"
        ::= { mcmfilter 18 }
 
    nvmIpSysFltrEntry OBJECT-TYPE
        SYNTAX  NvmIpSysFltrEntry
        ACCESS  not-accessible
        STATUS  mandatory    
	DESCRIPTION
	  "NAME = ;
	   DEST = IP forwarding system filter entry;
	   HELP = ;
           CAPABILITIES = ;"
        INDEX   {
		nvmIpSysFltrDstMask,
		nvmIpSysFltrDstAddr,
