	SYNTAX	INTEGER {
		    clpequal0 (2),
		    clpequal0or1 (3)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"For PNNI, indicates whether any advertised GCAC parameters apply for CLP=0 traffic or for CLP=0+1 traffic."
	::= { mibpnniRouteNodeConfigEntry 9 }

    pnniRouteNodeConfig-FwdMetricAw	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"The cumulative administrative weight calculated for the forward direction of this route. 0xFFFFFFFF means the metric is not used."
	::= { mibpnniRouteNodeConfigEntry 10 }

    pnniRouteNodeConfig-FwdMetric1	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"An alternate routing parameter for the forward direction of this route. For information learned from PNNI nodes, this is the maximum possible cell rate (in cells per second) for the forward direction of the route. 0xFFFFFFFF means the metric is not used."
	::= { mibpnniRouteNodeConfigEntry 11 }

    pnniRouteNodeConfig-FwdMetric2	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"An alternate routing parameter for the forward direction of this route. For information learned from PNNI nodes, this is the Available cell rate (in cells per second) for the forward direction of the route. 0xFFFFFFFF means the metric is not used."
	::= { mibpnniRouteNodeConfigEntry 12 }

    pnniRouteNodeConfig-FwdMetric3	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"An alternate routing parameter for the forward direction of this route. For information learned from PNNI nodes, this is the cumulative Max. Cell Transfer Delay (in microseconds) for the forward direction of the route. 0xFFFFFFFF means the metric is not used."
	::= { mibpnniRouteNodeConfigEntry 13 }

    pnniRouteNodeConfig-FwdMetric4	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"An alternate routing parameter for the forward direction of this route. For information learned from PNNI nodes, this is the cumulative Cell Delay Variation (in microseconds)for forward direction of the route. 0xFFFFFFFF means the metric is not used."
	::= { mibpnniRouteNodeConfigEntry 14 }

    pnniRouteNodeConfig-FwdMetric5	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"An alternate routing parameter for the forward direction of this route. For information learned from PNNI nodes, this is the cumulative Cell Loss Ratio for CLP=0 traffic for the forward direction of the route. Cell loss ratio value is computed as 10**(-n) where 'n' is the value returned in this variable.0xFFFFFFFF means the metric is not used."
	::= { mibpnniRouteNodeConfigEntry 15 }

    pnniRouteNodeConfig-FwdMeteric6	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"An alternate routing parameter for the forward direction of this route. For information learned from PNNI nodes, this is the cumulative Cell Loss Ratio for CLP=0+1 traffic for the forward direction of the route. Cell loss ratio value is computed as 10**(-n) where 'n' is the returned in this variable.0xFFFFFFFF means the metric is not used."
	::= { mibpnniRouteNodeConfigEntry 16 }

    pnniRouteNodeConfig-FwdMeteric7	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"An alternate routing parameter for the forward direction of this route. For information learned from PNNI nodes, this is the Cell Rate Margin (in cells per second) for the forward direction of the route. 0xFFFFFFFF means the metric is not used."
	::= { mibpnniRouteNodeConfigEntry 17 }

    pnniRouteNodeConfig-FwdMeteric8	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"An alternate routing parameter for the forward direction of this route. For information learned from PNNI nodes, this is the Variance Factor (in units of 2**(-8)) for the forward direction of the route. 0xFFFFFFFF means the metric is not used."
	::= { mibpnniRouteNodeConfigEntry 18 }

    pnniRouteNodeConfig-BwdMetricAw	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"The administrative weight calculated for the backward direction of this route. 0xFFFFFFFF means the metric is not used."
	::= { mibpnniRouteNodeConfigEntry 19 }

    pnniRouteNodeConfig-BwdMetric1	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Alternate routing parameter for the backward direction of this route. For information learned from PNNI nodes, this is the maximum possible cell rate(in cells per second)for the backward direction of the route. 0xFFFFFFFF means the metric is not used."
	::= { mibpnniRouteNodeConfigEntry 20 }

    pnniRouteNodeConfig-BwdMetric2	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Alternate routing parameter for the backward direction of this route. For information learned from PNNI nodes, this is the Available cell rate (in cells per second) for the backward direction of the route. Further information on available bandwidth may be obtainable by reference to the nodal advertisements of the nodes in the path. 0xFFFFFFFF means the metric is not used."
	::= { mibpnniRouteNodeConfigEntry 21 }

    pnniRouteNodeConfig-BwdMetric3	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Alternate routing parameter for the backward direction of this route. For information learned from PNNI nodes, this is the cumulative Max Cell Transfer Delay (in microseconds) for the backward direction of the route.0xFFFFFFFF means the metric is not used."
	::= { mibpnniRouteNodeConfigEntry 22 }

    pnniRouteNodeConfig-BwdMetric4	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Alternate routing parameter for the backward direction of this route. For information learned from PNNI nodes, this is the cumulative Cell Delay Variation (in microseconds) for the backward direction of the route. 0xFFFFFFFF means the metric is not used."
	::= { mibpnniRouteNodeConfigEntry 23 }

    pnniRouteNodeConfig-BwdMetric5	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Alternate routing parameter for the backward direction of this route. For information learned from PNNI nodes, this is the cumulative Cell Loss Ratio for CLP=0 traffic for the backward direction of the route. Cell loss ratio value is computed as 10**(-n) where 'n' is the value returned in this variable.0xFFFFFFFF means the metric isn't used."
	::= { mibpnniRouteNodeConfigEntry 24 }

    pnniRouteNodeConfig-BwdMetric6	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Alternate routing parameter for the backward direction of this route. For information learned from PNNI nodes, this is the cumulative Cell Loss Ratio for CLP=0+1 traffic for the backward direction of the route Cell loss ratio value is computed as 10**(-n) where 'n' is the value returned in this variable.0xFFFFFFFF means the metric isn't used."
	::= { mibpnniRouteNodeConfigEntry 25 }

    pnniRouteNodeConfig-BwdMetric7	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Alternate routing parameter for the backward direction of this route. For information learned from PNNI nodes, this is the Cell Rate Margin (in cells per second) for the backward direction of the route. 0xFFFFFFFF means the metric isn't used."
	::= { mibpnniRouteNodeConfigEntry 26 }

    pnniRouteNodeConfig-BwdMetric8	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Alternate routing parameter for the backward direction of this route. For information learned from PNNI nodes, this is the Variance Factor (in units of 2**(-8)) for the backward direction of the route. 0xFFFFFFFF means the metric isn't used."
	::= { mibpnniRouteNodeConfigEntry 27 }

    pnniRouteNodeConfig-VpCapability	OBJECT-TYPE
	SYNTAX	INTEGER {
		    true (2),
		    false (3)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Attribute indicates whether a VPC setup on this route is possible."
	::= { mibpnniRouteNodeConfigEntry 28 }

    pnniRouteNodeConfig-Active	OBJECT-TYPE
	SYNTAX	INTEGER {
		    no (1),
		    yes (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"The current state of this route node entry."
	::= { mibpnniRouteNodeConfigEntry 29 }

    pnniRouteNodeConfig-Action-o	OBJECT-TYPE
	SYNTAX	INTEGER {
		    noAction (1),
		    createProfile (2),
		    deleteProfile (3)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	""
	::= { mibpnniRouteNodeConfigEntry 30 }


	-- The mibpnniRouteAddrConfig table
	--
	-- This table contains parameters for the mibpnniRouteAddrConfig profile
	--

    mibpnniRouteAddrConfigTable	OBJECT-TYPE
	SYNTAX		SEQUENCE OF MibpnniRouteAddrConfigEntry
	ACCESS		not-accessible
	STATUS		mandatory
	DESCRIPTION	"A list of mibpnniRouteAddrConfig profile entries."
	::= { mibpnniRouteAddrConfig 1 }

    mibpnniRouteAddrConfigEntry	OBJECT-TYPE
	SYNTAX		MibpnniRouteAddrConfigEntry
	ACCESS		not-accessible
	STATUS		mandatory
	DESCRIPTION	"A mibpnniRouteAddrConfig entry containing objects that
			 maps to the parameters of mibpnniRouteAddrConfig profile."
	INDEX		{ pnniRouteAddrConfig-Name }
	::= { mibpnniRouteAddrConfigTable 1 }

    MibpnniRouteAddrConfigEntry ::=
	SEQUENCE {
	    pnniRouteAddrConfig-Name
		DisplayString,
	    pnniRouteAddrConfig-AddrIndex-NodeIndex
		INTEGER,
	    pnniRouteAddrConfig-AddrIndex-Address
		DisplayString,
	    pnniRouteAddrConfig-AddrIndex-PrefixLen
		INTEGER,
	    pnniRouteAddrConfig-AddrIndex-Index
		INTEGER,
	    pnniRouteAddrConfig-IfIndex
		INTEGER,
	    pnniRouteAddrConfig-AdvNodeId
		DisplayString,
	    pnniRouteAddrConfig-AdvPortId
		INTEGER,
	    pnniRouteAddrConfig-Type
		INTEGER,
	    pnniRouteAddrConfig-Proto
		INTEGER,
	    pnniRouteAddrConfig-PnniScope
		INTEGER,
	    pnniRouteAddrConfig-VpCapability
		INTEGER,
	    pnniRouteAddrConfig-MetricsTag
		INTEGER,
	    pnniRouteAddrConfig-PtseIdPtseId
		INTEGER,
	    pnniRouteAddrConfig-OriginateAdvert
		INTEGER,
	    pnniRouteAddrConfig-Info
		DisplayString,
	    pnniRouteAddrConfig-OperStatus
		INTEGER,
	    pnniRouteAddrConfig-TimeStamp
		INTEGER,
	    pnniRouteAddrConfig-Active
		INTEGER,
	    pnniRouteAddrConfig-Action-o
		INTEGER
	}

    pnniRouteAddrConfig-Name	OBJECT-TYPE
	SYNTAX	DisplayString
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"The name of this profile which is corresponding to the value of the addr-index."
	::= { mibpnniRouteAddrConfigEntry 1 }

    pnniRouteAddrConfig-AddrIndex-NodeIndex	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Index that identifies a logical PNNI node."
	::= { mibpnniRouteAddrConfigEntry 2 }

    pnniRouteAddrConfig-AddrIndex-Address	OBJECT-TYPE
	SYNTAX	DisplayString
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"The value of the ATM End System Address prefix."
	::= { mibpnniRouteAddrConfigEntry 3 }

    pnniRouteAddrConfig-AddrIndex-PrefixLen	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"The prefix length to be applied to the ATM End System Address prefix."
	::= { mibpnniRouteAddrConfigEntry 4 }

    pnniRouteAddrConfig-AddrIndex-Index	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"An index into the set of listings of connectivity to a given address prefix from a given local node."
	::= { mibpnniRouteAddrConfigEntry 5 }

    pnniRouteAddrConfig-IfIndex	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"The local interface over which the reachable address can be reached. The value zero indicates an unknown interface or reachability through a remote node."
	::= { mibpnniRouteAddrConfigEntry 6 }

    pnniRouteAddrConfig-AdvNodeId	OBJECT-TYPE
	SYNTAX	DisplayString
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"The node ID of a node advertising reachability to the address prefix. If the local node index is zero, then the advertising node ID must be set to all zeros."
	::= { mibpnniRouteAddrConfigEntry 7 }

    pnniRouteAddrConfig-AdvPortId	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"The port identifier used from the advertising node to reach the given address prefix."
	::= { mibpnniRouteAddrConfigEntry 8 }

    pnniRouteAddrConfig-Type	OBJECT-TYPE
	SYNTAX	INTEGER {
		    other (2),
		    reject (3),
		    internal (4),
		    exterior (5)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"The type (e.g. internal or exterior) of reachability from the advertising node to the address prefix."
	::= { mibpnniRouteAddrConfigEntry 9 }

    pnniRouteAddrConfig-Proto	OBJECT-TYPE
	SYNTAX	INTEGER {
		    other (2),
		    local (3),
		    mgmt (4),
		    pnni (5)
		}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"The routing mechanism via which the connectivity from the advertising node to the reachable address prefix was learned."
	::= { mibpnniRouteAddrConfigEntry 10 }

    pnniRouteAddrConfig-PnniScope	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"The PNNI scope of advertisement (i.e. level of PNNI hierarchy) of the reachability from the advertising node to the address prefix."
	::= { mibpnniRouteAddrConfigEntry 11 }

    pnniRouteAddrConfig-VpCapability	OBJECT-TYPE
	SYNTAX	INTEGER {
		    true (2),
		    false (3)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Indicates whether VPCs can be established from the advertising node to the reachable address prefix."
	::= { mibpnniRouteAddrConfigEntry 12 }

    pnniRouteAddrConfig-MetricsTag	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"The index into the pnniMetricsTable for the traffic parameter values that apply for the connectivity from the advertising node to the reachable address prefix. There will be one or more entries in the pnniMetricsTable whose first instance identifier matches the value of this variable."
	::= { mibpnniRouteAddrConfigEntry 13 }

    pnniRouteAddrConfig-PtseIdPtseId	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"For reachable addresses learned via PNNI, this attribute contains the value of the PTSE Identifier for the PTSE being originated by the originating node which contains the information group(s) describing the reachable address. For reachable addresses learned by means other than PNNI, this attribute is set to zero."
	::= { mibpnniRouteAddrConfigEntry 14 }

    pnniRouteAddrConfig-OriginateAdvert	OBJECT-TYPE
	SYNTAX	INTEGER {
		    true (2),
		    false (3)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Whether or not the reachable address specified by this entry is to be advertised by the local node into its PNNI routing domain."
	::= { mibpnniRouteAddrConfigEntry 15 }

    pnniRouteAddrConfig-Info	OBJECT-TYPE
	SYNTAX	DisplayString
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"A reference to MIB definitions specific to the particular routing protocol which is responsible for this reachable address prefix, as determined by the value specified in the route's pnniRouteAddrProto value. Currently not supportted and is set to 0.0."
	::= { mibpnniRouteAddrConfigEntry 16 }

    pnniRouteAddrConfig-OperStatus	OBJECT-TYPE
	SYNTAX	INTEGER {
		    inactive (2),
		    active (3),
		    advertised (4)
		}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Indicates whether the reachable address prefix is operationally valid and whether it is being advertised by this node."
	::= { mibpnniRouteAddrConfigEntry 17 }

    pnniRouteAddrConfig-TimeStamp	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Indicates when the connectivity from the advertising node to the reachable address prefix became known to the local node."
	::= { mibpnniRouteAddrConfigEntry 18 }

    pnniRouteAddrConfig-Active	OBJECT-TYPE
	SYNTAX	INTEGER {
		    no (1),
		    yes (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"The current state of this route address entry."
	::= { mibpnniRouteAddrConfigEntry 19 }

    pnniRouteAddrConfig-Action-o	OBJECT-TYPE
	SYNTAX	INTEGER {
		    noAction (1),
		    createProfile (2),
		    deleteProfile (3)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	""
	::= { mibpnniRouteAddrConfigEntry 20 }


	-- The mibpnniMetricsConfig table
	--
	-- This table contains parameters for the mibpnniMetricsConfig profile
	--

    mibpnniMetricsConfigTable	OBJECT-TYPE
	SYNTAX		SEQUENCE OF MibpnniMetricsConfigEntry
	ACCESS		not-accessible
	STATUS		mandatory
	DESCRIPTION	"A list of mibpnniMetricsConfig profile entries."
	::= { mibpnniMetricsConfig 1 }

    mibpnniMetricsConfigEntry	OBJECT-TYPE
	SYNTAX		MibpnniMetricsConfigEntry
	ACCESS		not-accessible
	STATUS		mandatory
	DESCRIPTION	"A mibpnniMetricsConfig entry containing objects that
			 maps to the parameters of mibpnniMetricsConfig profile."
	INDEX		{ pnniMetricsConfig-MetricsIndex-NodeIndex, pnniMetricsConfig-MetricsIndex-MetricsTag, pnniMetricsConfig-MetricsIndex-MetricsDirection, pnniMetricsConfig-MetricsIndex-MetricsIndex }
	::= { mibpnniMetricsConfigTable 1 }

    MibpnniMetricsConfigEntry ::=
	SEQUENCE {
	    pnniMetricsConfig-MetricsIndex-NodeIndex
		INTEGER,
	    pnniMetricsConfig-MetricsIndex-MetricsTag
		INTEGER,
	    pnniMetricsConfig-MetricsIndex-MetricsDirection
		INTEGER,
	    pnniMetricsConfig-MetricsIndex-MetricsIndex
		INTEGER,
	    pnniMetricsConfig-MetricsClasses
		INTEGER,
	    pnniMetricsConfig-MetricsGcacClp
		INTEGER,
	    pnniMetricsConfig-MetricsAdminWeight
		INTEGER,
	    pnniMetricsConfig-Metrics1
		INTEGER,
	    pnniMetricsConfig-Metrics2
		INTEGER,
	    pnniMetricsConfig-Metrics3
		INTEGER,
	    pnniMetricsConfig-Metrics4
		INTEGER,
	    pnniMetricsConfig-Metrics5
		INTEGER,
	    pnniMetricsConfig-Metrics6
		INTEGER,
	    pnniMetricsConfig-Metrics7
		INTEGER,
	    pnniMetricsConfig-Metrics8
		INTEGER,
	    pnniMetricsConfig-Active
		INTEGER,
	    pnniMetricsConfig-Action-o
		INTEGER
	}

    pnniMetricsConfig-MetricsIndex-NodeIndex	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Index that identifies a logical PNNI node."
	::= { mibpnniMetricsConfigEntry 1 }

    pnniMetricsConfig-MetricsIndex-MetricsTag	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"An arbitrary integer that is used to associate a set of traffic parameters that are always advertised together.Within this set, the parameters are distinguished by the service categories and direction to which a set of parameters apply."
	::= { mibpnniMetricsConfigEntry 2 }

    pnniMetricsConfig-MetricsIndex-MetricsDirection	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"The direction, with respect to the advertising node,in which the parameters in this entry apply."
	::= { mibpnniMetricsConfigEntry 3 }

    pnniMetricsConfig-MetricsIndex-MetricsIndex	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"An index into the set of parameters associated with the given tag and direction."
	::= { mibpnniMetricsConfigEntry 4 }

    pnniMetricsConfig-MetricsClasses	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"The service categories to which this set of parameters applies.This is an integer used as a bit mask with each bit that is set representing a single service category for which the resources indicated are avail- able.Bit 5 represents CBR, bit 4 represents real-time VBR, bit 3 represents non-real-time VBR, bit 2 represents ABR, and bit 1 (LSB) represents UBR."
	::= { mibpnniMetricsConfigEntry 5 }

    pnniMetricsConfig-MetricsGcacClp	OBJECT-TYPE
	SYNTAX	INTEGER {
		    clpequal0 (2),
		    clpequal0or1 (3)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Indicates whether the advertised GCAC parameters apply for CLP=0 traffic or for CLP=0+1 traffic."
	::= { mibpnniMetricsConfigEntry 6 }

    pnniMetricsConfig-MetricsAdminWeight	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"The administrative weight from the advertising node to the remote end of the PNNI entity or to the reachable address or transit network, for the specified service categories."
	::= { mibpnniMetricsConfigEntry 7 }

    pnniMetricsConfig-Metrics1	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"An alternate routing parameter from the advertising node to the remote end of the PNNI entity or to the reachable address or transit network, for the specified service categories."
	::= { mibpnniMetricsConfigEntry 8 }

    pnniMetricsConfig-Metrics2	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"An alternate routing parameter from the advertising node to the remote end of the PNNI entity or to the reachable address or transit network, for the specified service categories."
	::= { mibpnniMetricsConfigEntry 9 }

    pnniMetricsConfig-Metrics3	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"An alternate routing parameter from the advertising node to the remote end of the PNNI entity or to the reachable address or transit network, for the specified service categories."
	::= { mibpnniMetricsConfigEntry 10 }

    pnniMetricsConfig-Metrics4	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"An alternate routing parameter from the advertising node to the remote end of the PNNI entity or to the reachable address or transit network, for the specified service categories."
	::= { mibpnniMetricsConfigEntry 11 }

    pnniMetricsConfig-Metrics5	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"An alternate routing parameter from the advertising node to the remote end of the PNNI entity or to the reachable address or transit network, for the specified service categories."
	::= { mibpnniMetricsConfigEntry 12 }

    pnniMetricsConfig-Metrics6	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"An alternate routing parameter from the advertising node to the remote end of the PNNI entity or to the reachable address or transit network, for the specified service categories."
	::= { mibpnniMetricsConfigEntry 13 }

    pnniMetricsConfig-Metrics7	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"An alternate routing parameter from the advertising node to the remote end of the PNNI entity or to the reachable address or transit network, for the specified service categories."
	::= { mibpnniMetricsConfigEntry 14 }

    pnniMetricsConfig-Metrics8	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"An alternate routing parameter from the advertising node to the remote end of the PNNI entity or to the reachable address or transit network, for the specified service categories."
	::= { mibpnniMetricsConfigEntry 15 }

    pnniMetricsConfig-Active	OBJECT-TYPE
	SYNTAX	INTEGER {
		    no (1),
		    yes (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"The current state of this metrics entry"
