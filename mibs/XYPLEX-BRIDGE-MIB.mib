		ACCESS	read-only
		STATUS	mandatory
		::= { lksTblEnt 7 }

lksEntUtilizationHigh	OBJECT-TYPE
		SYNTAX	Gauge
		ACCESS	read-only
		STATUS	mandatory
		::= { lksTblEnt 8 }

lksEntUtilizationAvg	OBJECT-TYPE
		SYNTAX	Gauge
		ACCESS	read-only
		STATUS	mandatory
