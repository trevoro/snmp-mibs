lksEntFilteringCur	OBJECT-TYPE
		SYNTAX	Gauge
		ACCESS	read-only
		STATUS	mandatory
		::= { lksTblEnt 13 }

lksEntFilteringHigh	OBJECT-TYPE
		SYNTAX	Gauge
		ACCESS	read-only
		STATUS	mandatory
		::= { lksTblEnt 14 }

lksEntFilteringAvg	OBJECT-TYPE
		SYNTAX	Gauge
		ACCESS	read-only
		STATUS	mandatory
		::= { lksTblEnt 15 }

lksEntOutputQCur	OBJECT-TYPE
		SYNTAX	Gauge
		ACCESS	read-only
		STATUS	mandatory
		::= { lksTblEnt 16 }

lksEntOutputQHigh	OBJECT-TYPE
		SYNTAX	Gauge
		ACCESS	read-only
		STATUS	mandatory
		::= { lksTblEnt 17 }

lksEntOutputQTotal	OBJECT-TYPE
		SYNTAX	Gauge
		ACCESS	read-only
		STATUS	mandatory
		::= { lksTblEnt 18 }

lksEntTransmitLinkSpeed	OBJECT-TYPE
		SYNTAX	Gauge
		ACCESS	read-only
		STATUS	mandatory
		::= { lksTblEnt 19 }

lksEntReceiveLinkSpeed	OBJECT-TYPE
		SYNTAX	Gauge
		ACCESS	read-only
		STATUS	mandatory
		::= { lksTblEnt 20 }

