st2StreamFlowDesPduRate OBJECT-TYPE
    SYNTAX  INTEGER (0..'FFFF'h)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "flowspec desired pdu rate field (16 bits)"
    ::= { st2StreamEntry 28 }

st2StreamOptionPtp OBJECT-TYPE
