      ::= { agnSigProfileTable 1}

AgnSigProfileEntry ::=
      SEQUENCE {
      agnSigProfileCnfgIdx INTEGER,
      agnSigProfileIdx     INTEGER,
      agnSigProfileRxTx    INTEGER,
      agnSigProfileABCD    OCTET STRING
      }


agnSigProfileCnfgIdx OBJECT-TYPE
      SYNTAX  INTEGER (1..255)
      ACCESS  read-only
      STATUS  mandatory
