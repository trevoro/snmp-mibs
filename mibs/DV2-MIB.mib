sdhClearCounts	OBJECT-TYPE
	SYNTAX INTEGER {
		no(2),
		yes(1)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
	"Clear Error Counts and Performance Parameters"
	::= { sdhCEntry 9 }

sdhReconfig 	OBJECT-TYPE
	SYNTAX INTEGER {
		no(2),
		yes(1)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
	"Perform Reconfiguration for Link"
	::= { sdhCEntry 10 }

sdhCLpbkTimeout	OBJECT-TYPE
	SYNTAX	INTEGER (1..604800)
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	"This parameter is used to set the loopback timeout in minutes."
	::= { sdhCEntry 11 }

sdhStmSts       OBJECT-TYPE
