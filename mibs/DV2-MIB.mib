-- The SDHSonet config table

sdhCTable OBJECT-TYPE
	SYNTAX SEQUENCE OF SdhCEntry
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION
	"This table is used to control sdh/Sonet configuration."
	::= { dv2SDH 1 }

sdhCEntry OBJECT-TYPE
	SYNTAX	SdhCEntry
	ACCESS	not-accessible
	STATUS	mandatory
        INDEX   { sdhCLinkNo }
	::= { sdhCTable 1 }

SdhCEntry ::=
	SEQUENCE {
		sdhCLinkNo
			INTEGER,
		sdhCLinkEnab
			INTEGER,
		sdhCLoopTimer
			INTEGER,
		sdhCLoopDiag
			INTEGER,
		sdhCLoopLine
			INTEGER,
		sdhCScrambler
			INTEGER,
		sdhC0x55
			INTEGER,
		sdhSonetSdh
			INTEGER,
		sdhClearCounts
			INTEGER,
		sdhReconfig
			INTEGER,
		sdhCLpbkTimeout
			INTEGER,
		sdhStmSts    
			INTEGER,
		sdhAPSOnOff
			INTEGER,
		sdhCIdleCell
			INTEGER
}


sdhCLinkNo	OBJECT-TYPE
	SYNTAX INTEGER (0..1)
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
	"The link number for this configuration."
	::= { sdhCEntry 1 }

sdhCLinkEnab	OBJECT-TYPE
	SYNTAX INTEGER {
		yes(1),
		no(2)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
	"Set to yes(1) to enable the link."
	::= { sdhCEntry 2 }

sdhCLoopTimer	OBJECT-TYPE
	SYNTAX INTEGER {
		no(2),
		yes(1)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
	"Set to yes(1) to enable loop timing for this link."
	::= { sdhCEntry 3 }

sdhCLoopDiag OBJECT-TYPE
	SYNTAX INTEGER {
		no(2),
		yes(1)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
	"Set to yes(1) to enable diagnostic loopback."
	::= { sdhCEntry 4 }

sdhCLoopLine OBJECT-TYPE
	SYNTAX INTEGER {
		no(2),
		yes(1)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
	"Set to yes(1) to enable line loopback."
	::= { sdhCEntry 5 }

sdhCScrambler	OBJECT-TYPE
	SYNTAX INTEGER {
		yes(1),
		no(2)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
	"Set to no(2) to disable scramble mode for this link."
	::= { sdhCEntry 6 }

sdhC0x55	OBJECT-TYPE
	SYNTAX INTEGER {
		yes(1),
		no(2)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
	"If set to no(2) then HCS is XORed with 0x55"
	::= { sdhCEntry 7 }

sdhSonetSdh	OBJECT-TYPE
	SYNTAX INTEGER {
		sonet(1),
		sdh(2)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
	"If set to sdh(2) then sdh header is used"
