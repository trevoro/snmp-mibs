        ACCESS  not-accessible
        STATUS  mandatory
        INDEX   { oamLinkNo }
        ::= { oamTable 1 }

OamEntry ::=
	SEQUENCE {
		oamLinkNo
                        INTEGER,
		oamSegEndPoint
			INTEGER,
		oamVcEndPoint
			INTEGER,
		oamAisRdiGeneration
			INTEGER,
		oamLastRxType
			INTEGER,
		oamLastRxVpi
			VirtualPathIdentifier,
		oamLastRxVci
			VirtualCircuitIdentifier,
		oamRxCount
			Counter,
		oamLastTxType
			INTEGER,
		oamLastTxVpi
			VirtualPathIdentifier,
		oamLastTxVci
			VirtualCircuitIdentifier,
		oamTxCount
			Counter,
		oamClearStats
			INTEGER
		}

oamLinkNo       OBJECT-TYPE
        SYNTAX INTEGER (0..3)
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "The link number for this configuration."
        ::= { oamEntry 1 }

oamSegEndPoint  OBJECT-TYPE
        SYNTAX INTEGER {
                yes(1),
                no(2)
        }
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
        "Set to yes(1) to configure the link as segment end point."
        DEFVAL { yes }
        ::= { oamEntry 2 }

oamVcEndPoint  OBJECT-TYPE
        SYNTAX INTEGER {
                no(2),
                yes(1)
        }
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
        "Set to yes(1) to configure the link as connection end point."
        DEFVAL { no }
        ::= { oamEntry 3 }

oamAisRdiGeneration        OBJECT-TYPE
        SYNTAX INTEGER {
                no(2),
                yes(1)
        }
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
        "Set to yes(1) to generate AIS/RDI up on physical failure."
        DEFVAL { no }
        ::= { oamEntry 4 }

oamLastRxType	OBJECT-TYPE
	SYNTAX INTEGER {
		ais(1),
		rdi(2),
		loopback(3),
		continuity-check(4),
		performance-monitoring(5),
		unsupported(6),
		none(10)
		}
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
	"Type of the last received OAM cell."
	DEFVAL {none }
	::= { oamEntry 5 }

oamLastRxVpi	OBJECT-TYPE
	SYNTAX VirtualPathIdentifier
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
	"VPI value of the last received OAM cell."
	::= { oamEntry 6 }

oamLastRxVci	OBJECT-TYPE
	SYNTAX VirtualCircuitIdentifier
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
	"VCI value of the last received OAM cell."
	::= { oamEntry 7 }

oamRxCount	OBJECT-TYPE
	SYNTAX Counter
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
	"Sum of received OAM cells."
	::= { oamEntry 8 }

oamLastTxType	OBJECT-TYPE
	SYNTAX INTEGER {
		ais(1),
		rdi(2),
		loopback(3),
		continuity-check(4),
		performance-monitoring(5),
		unsupported(6),
		none(10)
		}
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
	"Type of the last transmitted OAM cell."
	DEFVAL {none }
	::= { oamEntry 9 }

oamLastTxVpi	OBJECT-TYPE
	SYNTAX VirtualPathIdentifier
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
	"VPI value of the last transmitted OAM cell."
	::= { oamEntry 10 }

oamLastTxVci	OBJECT-TYPE
	SYNTAX VirtualCircuitIdentifier
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
	"VCI value of the last transmitted OAM cell."
	::= { oamEntry 11 }

oamTxCount	OBJECT-TYPE
	SYNTAX Counter
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
	"Sum of transmitted OAM cells."
	::= { oamEntry 12 }

oamClearStats	OBJECT-TYPE
	SYNTAX INTEGER {
		no(1),
		yes(2)
		}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
	"Set to yes(2) to clear OAM Tx and Rx Statistics."
	::= { oamEntry 13 }

oamLoopbackTable OBJECT-TYPE
        SYNTAX SEQUENCE OF OamLoopbackEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION
        "This table is used to configure and control loopback test.
         Change Test Status to Start Test(1) to initiate loopback test."
        ::= { dv2Oam 2 }

oamLoopbackEntry OBJECT-TYPE
        SYNTAX  OamLoopbackEntry
        ACCESS  not-accessible
        STATUS  mandatory
        INDEX   { oamLoopbackLinkNo }
        ::= { oamLoopbackTable 1 }

OamLoopbackEntry ::=
        SEQUENCE {
                oamLoopbackLinkNo
                        INTEGER,
                oamLoopbackVpi
                        VirtualPathIdentifier,
                oamLoopbackVci
                        VirtualCircuitIdentifier,
                oamLoopbackSpan
                        INTEGER,
                oamLoopbackMaxNumCells
                        INTEGER,
                oamLoopbackNumCellsSent
                        INTEGER,
                oamLoopbackTimeOut
                        INTEGER,
                oamLoopbackActive
                        INTEGER,
                oamLoopbackResult
                        INTEGER,
                oamLoopbackLocId
                        INTEGER
}


oamLoopbackLinkNo       OBJECT-TYPE
        SYNTAX INTEGER (0..1)
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "The link number for this configuration."
        ::= { oamLoopbackEntry 1 }

oamLoopbackVpi  OBJECT-TYPE
        SYNTAX VirtualPathIdentifier
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
        "VPI value for the loopback cells to send."
        DEFVAL { 0 }
        ::= { oamLoopbackEntry 2 }

oamLoopbackVci  OBJECT-TYPE
        SYNTAX VirtualCircuitIdentifier
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
        "VCI value for the loopback cells to send."
        DEFVAL { 5 }
        ::= { oamLoopbackEntry 3 }

oamLoopbackSpan OBJECT-TYPE
        SYNTAX INTEGER {
                segment(4),
                end-to-end(5),
                location-id(6)
        }
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
        "Flow span for the loopback cells to send."
        DEFVAL { segment }
        ::= { oamLoopbackEntry 4 }

oamLoopbackMaxNumCells     OBJECT-TYPE
        SYNTAX INTEGER (1..20)
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
        "Maximum number of loopback cells to send."
        DEFVAL { 2 }
        ::= { oamLoopbackEntry 5 }

oamLoopbackNumCellsSent     OBJECT-TYPE
        SYNTAX INTEGER (0..20)
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Number of loopback cells sent."
        DEFVAL { 0 }
        ::= { oamLoopbackEntry 6 }

oamLoopbackTimeOut      OBJECT-TYPE
        SYNTAX INTEGER (1..10)
        ACCESS read-write
