-- The PLPP config table

plppCTable OBJECT-TYPE
	SYNTAX SEQUENCE OF PlppCEntry
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION
	"This table is used to control PLPP configuration."
	::= { dv2PLPP 1 }

plppCEntry OBJECT-TYPE
	SYNTAX	PlppCEntry
	ACCESS	not-accessible
	STATUS	mandatory
        INDEX   { plppCLinkNo }
	::= { plppCTable 1 }

PlppCEntry ::=
	SEQUENCE {
		plppCLinkNo
			INTEGER,
		plppCLinkEnab
			INTEGER,
		plppCCPLCPMode
			INTEGER,
		plppCDS3Framer
			INTEGER,
		plppCScrambler
			INTEGER,
		plppC0x55
			INTEGER,
		plppCCLine
			INTEGER,
		plppCLoopback
			INTEGER,
--		plppCLoopTimer
--			INTEGER,
--		plppCLoopDiag
--			INTEGER,
--		plppCLoopLine
--			INTEGER,
		plppCSmdsMode
			INTEGER,
		plppCPloadTypeLab
			INTEGER,
		plppCTxTrlTrcIdLen
			INTEGER,
		plppCTxTrcId
			PhysAddress,
		plppCRxExTrcId
			PhysAddress
}


plppCLinkNo	OBJECT-TYPE
	SYNTAX INTEGER (0..1)
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
	"The link number for this configuration."
	::= { plppCEntry 1 }

plppCLinkEnab	OBJECT-TYPE
	SYNTAX INTEGER {
		yes(1),
		no(2)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
	"Set to yes(1) to enable the link."
	::= { plppCEntry 2 }

plppCCPLCPMode	OBJECT-TYPE
	SYNTAX INTEGER {
		none(1),
		e1(2),
		g751-e3(3),
		ds1(4),
		ds3(5),
		g832-e3(6)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
	"The PLCP mode for link."
	::= { plppCEntry 3 }

plppCDS3Framer	OBJECT-TYPE
	SYNTAX INTEGER {
		no(2),
		yes(1)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
	"Set to yes(1) to enable the DS3 framer mode for this link."
	::= { plppCEntry 4 }

plppCScrambler	OBJECT-TYPE
	SYNTAX INTEGER {
		yes(1),
		no(2)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
	"Set to no(2) to disable the scrambler mode for this link."
	::= { plppCEntry 5 }

plppC0x55	OBJECT-TYPE
	SYNTAX INTEGER {
		yes(1),
		no(2)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
	"If set to no(2) then HCS is not XORed with 0x55"
	::= { plppCEntry 6 }

plppCCLine	OBJECT-TYPE
	SYNTAX INTEGER {
		processed(1),
		ignored(2)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
	"Control line processing for this link."
	::= { plppCEntry 7 }

plppCLoopback	OBJECT-TYPE
	SYNTAX INTEGER {
		no(2),
		yes(1)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
	"Set to yes(1) to enable loopback mode for this link."
	::= { plppCEntry 8 }

