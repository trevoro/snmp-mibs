vcDocTestVal OBJECT-TYPE
	SYNTAX Hex
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
	"This is the test value to use when doing the pattern-send test."
	::= { vcDocEntry 11 }

vcDocPriority OBJECT-TYPE
	SYNTAX INTEGER (0..1)
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
	"This indicates that cells will be sent with the priority bit set."
	::= { vcDocEntry 12 }

vcDocSuba OBJECT-TYPE
	SYNTAX INTEGER (0..3)
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
	"This is the target link number for the VC."
	::= { vcDocEntry 13 }

vcDocSaddr OBJECT-TYPE
	SYNTAX INTEGER (0..15)
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
	"This is the target slot number for the VC."
	::= { vcDocEntry 14 }

vcDocClockFix OBJECT-TYPE
	SYNTAX INTEGER
	{
		no(1),
		yes(2)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
