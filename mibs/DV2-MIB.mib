-- link table for the slot

sysLinkTable OBJECT-TYPE
	SYNTAX SEQUENCE OF SysLinkEntry
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION
	"This table contains information about each link on this slot controller."
	::= { dv2System 15 }

sysLinkEntry OBJECT-TYPE
	SYNTAX	SysLinkEntry
	ACCESS	not-accessible
	STATUS	mandatory
        INDEX   { sysLkLinkNo }
	::= { sysLinkTable 1 }

SysLinkEntry ::=
	SEQUENCE {
		sysLkLinkNo
			INTEGER,
		sysLkKillLink
			INTEGER,
		sysLkState
			INTEGER,
		sysLkTxCells
			Counter,
		sysLkRxCells
			Counter,
		sysLkTxRate
			Gauge,
		sysLkRxRate
			Gauge,
		sysLkTxMaxRate
			Gauge,
		sysLkRxMaxRate
			Gauge,
		sysLkResetStats
			INTEGER,
		sysLkClockSrc
			INTEGER,
		sysLkResetResrc
            		INTEGER,
		sysLKEgCLPThreshold
		    INTEGER,
		sysLKIgCLPThreshold
		    INTEGER,
		sysLKEgEFCIThreshold
		    INTEGER,
		sysLKIgEFCIThreshold
		    INTEGER,
		sysLKEgLogBuffer
		    INTEGER,
		sysLKIgLogBuffer
		    INTEGER,
		sysLKEgFIFODepth
		    INTEGER,
		sysLKIgFIFODepth
		    INTEGER,
		sysLKEgMemBlockId
		    INTEGER,
		sysLKIgMemBlockId
		    INTEGER
}

