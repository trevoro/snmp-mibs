			INTEGER,
		vcDocSuba
			INTEGER,
		vcDocSaddr
			INTEGER,
		vcDocClockFix
			INTEGER
}

vcDocLinkNo OBJECT-TYPE
	SYNTAX INTEGER (0..127)
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
	"This is the target link number for the PVC."
	::= { vcDocEntry 1 }

vcDocEnable OBJECT-TYPE
	SYNTAX INTEGER {
			yes(1),
			no(2)
		}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
	"Setting to yes(1) enables the VCDOC channel."
	::= { vcDocEntry 2 }

vcDocVpi OBJECT-TYPE
	SYNTAX VirtualPathIdentifier
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
	"VPI of the VC being used for cell transfers."
	::= { vcDocEntry 3 }

vcDocVci OBJECT-TYPE
	SYNTAX VirtualCircuitIdentifier
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
	"VCI of the VC being used for cell transfers."
	::= { vcDocEntry 4 }

vcDocLinkSpeed OBJECT-TYPE
	SYNTAX INTEGER {
			s32kbps(64),
			s64kbps(128),
			s128kbps(192),
			s256kbps(224),
			s512kbps(240),
			s1024kbps(248),
			s2048kbps(252)
		}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
	"This sets the speed at which the link is to run."
	::= { vcDocEntry 5 }

vcDocCLines OBJECT-TYPE
	SYNTAX INTEGER {
			process(1),
			ignore(2)
		}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
	"This sets how control lines should be processed. If Ignore(2) is set then the port is always online."
	::= { vcDocEntry 6 }

vcDocFifoSize OBJECT-TYPE
	SYNTAX INTEGER (10..100)
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
	"This is the size of the software emulated FIFO."
	::= { vcDocEntry 7 }

vcDocLowWater OBJECT-TYPE
	SYNTAX INTEGER (0..50)
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
	"This is the point at which the transmit clock is switched to low frequency. This must be between 0 and FIFO Size/2"
	::= { vcDocEntry 8 }

vcDocHighWater OBJECT-TYPE
	SYNTAX INTEGER (5..100)
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
	"This is the point at which the transmit clock is switched to high frequency. This must be between FIFO size and FIFO Size/2"
	::= { vcDocEntry 9 }


vcDocTestMode OBJECT-TYPE
	SYNTAX INTEGER {
		normal(1),
		local-loop(2),
		cable-loop(3),
		remote-loop(4),
