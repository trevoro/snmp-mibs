               lmi-seq-num-mismatch-net          (62),
               line-failure                      (63),
               line-in-service                   (64),
               connected                         (65),
               connect-failure                   (66),
               incoming-call                     (67),
               disconnected                      (68),
               bpv-threshold-exceeded            (69),
