          ::= { dfrap 6 }

dfrapDLCINum     OBJECT-TYPE
          SYNTAX      INTEGER
          ACCESS      read-only
          STATUS      mandatory
          DESCRIPTION
             "The DLCI number for the most recent DLCI TRAP generated."
          ::= { dfrap 7 }

dfrapInterface     OBJECT-TYPE
          SYNTAX      INTEGER
          {
             dte   (1),
             dds   (2)
          }
          ACCESS      read-only
          STATUS      mandatory
