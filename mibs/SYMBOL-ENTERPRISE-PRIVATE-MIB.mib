	--
       	-- Wireless AP Statistics Group (2.11)
	--

 	apNbrOfWLAPItfs  OBJECT-TYPE
            	SYNTAX  Counter
          	ACCESS  read-only
               	STATUS  mandatory
               	DESCRIPTION
                     	"Indicates number of WLAP interfaces established."
               	::= { apWLAPInfo 1 }

 	apWLAPState  OBJECT-TYPE
            	SYNTAX  INTEGER {
                    	initializing(1),    
	              	sending-probe(2),  
                    	send-assoc-req(3),    
                    	send-cfg-bpdu(4),    
                    	wait-for-probe(5),    
                    	send-Probe-rsp(6),    
                    	send-assoc-rsp(7),    
                    	send-cfg-rsp(8),    
                    	received-root-rsp(10),    
