trafficDescriptorAbrNrm OBJECT-TYPE
        SYNTAX  INTEGER { nrm2(1),
                          nrm4(2),
                          nrm8(3),
                          nrm16(4),
                          nrm32(5),
                          nrm64(6),
                          nrm128(7),
                          nrm256(8) }

        ACCESS  read-write
        STATUS  mandatory
