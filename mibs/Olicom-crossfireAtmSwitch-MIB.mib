        SEQUENCE {
            pvpSetupIndex
                INTEGER,
            pvpSetupApplication
                INTEGER,
            pvpSetupLowEPortIndex
                INTEGER,
            pvpSetupLowVpi
                INTEGER,
            pvpSetupHighEPortIndex
                INTEGER,
            pvpSetupHighVpi
                INTEGER,
            pvpSetupVpCrossConnectIndex
