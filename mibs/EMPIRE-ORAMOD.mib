                ::= {  oramodLatchTable 1 }

        OramodLatchEntry ::= SEQUENCE {
                oramodLatchSIDINDX
                        INTEGER,
                oramodLatchLATCHINDX
                        INTEGER,
                oramodLatchLATCHNUM
                        INTEGER,
                oramodLatchLATCHNAME
                        DisplayString,
                oramodLatchGETS
