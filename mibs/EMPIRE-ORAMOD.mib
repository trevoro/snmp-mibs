EMPIRE-ORAMOD DEFINITIONS ::= BEGIN
	IMPORTS
                OBJECT-TYPE
			FROM RFC-1212
                TRAP-TYPE
			FROM RFC-1215
                DisplayString 
			FROM RFC1213-MIB
                enterprises, TimeTicks, Counter, Gauge
                        FROM RFC1155-SMI;

	empire 	   	OBJECT IDENTIFIER ::= { enterprises 546 }
        applications    OBJECT IDENTIFIER ::= { empire 16 }
        oracledb        OBJECT IDENTIFIER ::= { applications 4 }

-- Total Footprint information
        oramodTtlFootprt  OBJECT IDENTIFIER ::= { oracledb 12 }

-- Configuration information
        oramodCfg       OBJECT IDENTIFIER ::= { oracledb 10 }

	oramodDbCfg          OBJECT IDENTIFIER ::= { oramodCfg 1 }
        oramodCfgDf    	     OBJECT IDENTIFIER ::= { oramodCfg 2 }
	oramodCfgLf          OBJECT IDENTIFIER ::= { oramodCfg 3 }
	oramodCfgSga         OBJECT IDENTIFIER ::= { oramodCfg 4 }

-- Performance information
        oramodPerf     OBJECT IDENTIFIER ::= { oracledb 11 }
        oramodFootprint OBJECT IDENTIFIER ::= { oramodPerf 1 }
        oramodMetrics OBJECT IDENTIFIER ::= { oramodPerf 2 }
        oramodSGA OBJECT IDENTIFIER ::= { oramodPerf 3 }
        oramodRedoLogBuf OBJECT IDENTIFIER ::= { oramodPerf 4 }
        oramodRollBack OBJECT IDENTIFIER ::= { oramodPerf 5 }
        oramodWaits OBJECT IDENTIFIER ::= { oramodPerf 6 }
        oramodExpSql OBJECT IDENTIFIER ::= { oramodPerf 7 }
        oramodTblsp OBJECT IDENTIFIER ::= { oramodPerf 8 }
        oramodLocks OBJECT IDENTIFIER ::= { oramodPerf 9 }
        oramodBackups OBJECT IDENTIFIER ::= { oramodPerf 10 }
        oramodArchive OBJECT IDENTIFIER ::= { oramodPerf 11 }

      -- Footprint Descriptions
      oramodFootprt OBJECT IDENTIFIER ::= { oramodFootprint 1 }
      -- File Footprint Description
      oramodFFootprt OBJECT IDENTIFIER ::= { oramodFootprint 2 }
      -- SGA Tables
      oramodSgaDddAgg OBJECT IDENTIFIER ::= { oramodSGA 1 }
      oramodSgaDlci OBJECT IDENTIFIER ::= { oramodSGA 2 }
      -- Locks
      oramodLock OBJECT IDENTIFIER ::= { oramodLocks 1 }
      oramodLatch OBJECT IDENTIFIER ::= { oramodLocks 2 }
      -- Physical Backups
      oramodBackup OBJECT IDENTIFIER ::= { oramodBackups 1 }
      -- Archive 
      oramodArcDest OBJECT IDENTIFIER ::= { oramodArchive 1 }
--
-- Types and textual conventions used within our oracledb mib spec
-- 

   --DateAndTime ::= OCTET STRING (SIZE (8))

   --        A date-time specification for the local time of day.
   --        This data type is intended to provide a consistent
   --        method of reporting date information.
   --
   --            field  octets  contents                  range
   --            _____  ______  ________                  _____
   --              1      1-2   year                      0..65536
   --                           (in network byte order)
   --              2       3    month                     1..12
   --              3       4    day                       1..31
   --              4       5    hour                      0..23
   --              5       6    minutes                   0..59
   --              6       7    seconds                   0..60
   --                           (use 60 for leap-second)
   --              7       8    deci-seconds              0..9
   --              8       9    direction from UTC        "+" / "-"
   --                           (in ascii notation)


--
-- Main MIB branch contains module-related items like version,
-- licensing info, etc.
-- 

	oraModVersion OBJECT-TYPE
		SYNTAX DisplayString
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
		   "Description and version number of this installation."
		::= { oracledb 1 }


        oraModMode OBJECT-TYPE
                SYNTAX INTEGER {
                    fullMode(1),
                    restrictedMode(2)
                }
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
                   "This object indicates if the Oracle Module is in
                    full-mode or restricted-mode.  Restricted-mode
                    is entered, when SystemEDGE 4.0 and higher,
                    fails to find a valid license.  Once
                    a valid license has been allocated and
                    set, the module will then transition to
                    fullMode(1)"
                ::= { oracledb 2 }

        oraFlag OBJECT-TYPE
                SYNTAX DisplayString
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
                   "Performance  flags for this installation."
                ::= { oracledb 3 }


-- Total Footprint of all of the Oracle "Instances" in terms of resource 
-- consumption (e.g. CPU, disk, memory)

        --oramodTtlFootprt  OBJECT IDENTIFIER ::= { oracledb 12 }

        oramodTtlCPUTIME OBJECT-TYPE
		SYNTAX INTEGER
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
                   "CPU time, in seconds, accumulated by Oracle
                    which includes all its core components and
                    instance.  Because core components can
                    come and go, the accumulated cpu time can
                    fluctuate therefore making this variable
                    unsuitable as a counter." 
		::= { oramodTtlFootprt 1 }

        oramodTtlPERCENTCPU OBJECT-TYPE
		SYNTAX INTEGER (0..100)
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
                   "The percentage of CPU utilization, by Oracle,
                    over the last sample interval.  Since fractions
                    are not supported by SNMP, the value reported is
                    the percentage multipled by 100.  It is the sum of
                    the CPU utilization of all the Oracle core
                    services and components.  Its gives a
                    good indication of the CPU resource consumption by
                    the entire Oracle application'"
		::= { oramodTtlFootprt 2 }

        oramodTtlMEMSIZE OBJECT-TYPE
		SYNTAX Gauge
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
 		    "The combined size of Oracle's text, data,
                     and stack segments in KBytes.  It is the
                     summation of the process sizes for all the core
                     Oracle services and optional connectors." 
		::= { oramodTtlFootprt 3 }

        oramodTtlRSS OBJECT-TYPE
		SYNTAX Gauge
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
		    "The real memory (resident set) size of Oracle
                     in KBytes.  It is the summation of the process
                     RSS for all the core Oracle services and
                     optional connectors."  
		::= { oramodTtlFootprt 4 }

        oramodTtlPERCENTMEM OBJECT-TYPE
		SYNTAX INTEGER (0..100)
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
		   "The percentage (0-100) of real memory used by
		    the Oracle Application which includes core
                    services and optional connectors." 
		::= { oramodTtlFootprt 5 }

        oramodTtlTHREADS OBJECT-TYPE
		SYNTAX Gauge
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
                   "The total number of threads operating within the
                    respective Oracle components as seen by the
                    operating system.  Monitoring the number of
                    threads can help with performance tuning resource
                    contention issues that may be brought about by too
                    many threads or can help identify if too few
                    threads have too great of a workload." 
		::= { oramodTtlFootprt 6 }

        oramodTtlINBLKS OBJECT-TYPE
		SYNTAX Gauge
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
			" The number of blocks of data input by the processes."
		::= { oramodTtlFootprt 7 }

        oramodTtlOUTBLKS OBJECT-TYPE
		SYNTAX Gauge
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
			" The number of blocks of data put by the processes."
		::= { oramodTtlFootprt 8 }

        oramodTtlMSGSSENT OBJECT-TYPE
		SYNTAX Counter
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
			" The number of messages sent by the processes."
		::= { oramodTtlFootprt 9 }

        oramodTtlMSGSRCVD OBJECT-TYPE
		SYNTAX Counter
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
		" The number of messages received by the processes."
		::= { oramodTtlFootprt 10 }

        oramodTtlSYSCALLS OBJECT-TYPE
		SYNTAX Counter
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
		" The number of system calls invoked by the processes."
		::= { oramodTtlFootprt 11 }

        oramodTtlMINORPGFLTS OBJECT-TYPE
		SYNTAX Counter
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
		" The number of minor page faults incurred by the process.
	 	Minor faults do not require I/O to retrieve the page."
		::= { oramodTtlFootprt 12 }

        oramodTtlMAJORPGFLTS OBJECT-TYPE
		SYNTAX Counter
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
		" The number of major page faults incurred by the process.
	 	Minor faults require I/O to retrieve the page."
		::= { oramodTtlFootprt 13 }

        oramodTtlNUMSWAPS OBJECT-TYPE
		SYNTAX Counter
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
		" The number of times the processes have been swapped."
		::= { oramodTtlFootprt 14 }

        oramodTtlVOLCNTX OBJECT-TYPE
		SYNTAX Counter
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
		" The number of voluntary context switches incurred by the 
		processes. Voluntary contest switches occur when the processor
		voluntary gives up the CPU (e.g. waiting on I/O)."
		::= { oramodTtlFootprt 15 }

        oramodTtlINVOLCNTX OBJECT-TYPE
                SYNTAX Counter
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
                   "The number of involuntary context switches
                    incurred by the processes.  An involuntary context
                    switch occurs when the process has executed for
                    the duration of its time quantum and is switched
                    out by the process scheduler."
		::= { oramodTtlFootprt 16 }

        oramodTtlDISKSIZE OBJECT-TYPE
                SYNTAX INTEGER 
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
                   "The size, in KBytes of the Oracle root directory
                    and all the Database Disk storage areas."
		::= { oramodTtlFootprt 17 }


-- This is the master index of databases.
-- Contains the most important scalar creation information from
-- v$database and v$parameter.

        oramodDbCfgTable OBJECT-TYPE
                SYNTAX SEQUENCE OF OramodDbCfgEntry
                ACCESS not-accessible
                STATUS mandatory
                DESCRIPTION
                   "This table describes the instance names, index numbers
		    and initial startup information."
                ::= { oramodDbCfg 1 }

        oramodDbCfgEntry OBJECT-TYPE
                SYNTAX OramodDbCfgEntry
                ACCESS not-accessible
                STATUS mandatory
                DESCRIPTION
                   "An entry in the Oracle Configuration table."
                INDEX { oramodDbCfgSIDINDX }
                ::= {  oramodDbCfgTable 1 }

        OramodDbCfgEntry ::= SEQUENCE{
                oramodDbCfgSIDINDX
                        INTEGER,
                oramodDbCfgSID
                        DisplayString,
		oramodDbCfgVERSION 
                        DisplayString,
		oramodDbCfgHOME 
			DisplayString,
		oramodDbCfgBASE 
			DisplayString,
                oramodDbCfgID
                        INTEGER,
                oramodDbCfgCRTDT
                        DisplayString,
                oramodDbCfgLOGMODE
                        DisplayString,
                oramodDbCfgCTRLFILETYPE
                        DisplayString,
                oramodDbCfgOPENMODE
                        DisplayString,
                oramodDbCfgMAXPROCESS
                        INTEGER,
                oramodDbCfgMAXSESSION
                        INTEGER,
                oramodDbCfgTIMEDSTATISTICS
                        DisplayString,
                oramodDbCfgCPUCNT
                        INTEGER,
                oramodDbCfgSHAREDPOOLSIZE
                        INTEGER,
                oramodDbCfgSHAREDPOOLRSSIZE
                        INTEGER,
                oramodDbCfgLARGEPOOLSIZE
                        INTEGER,
                oramodDbCfgJAVAPOOLSIZE
                        INTEGER,
                oramodDbCfgCNTRLFILES
                        DisplayString,
                oramodDbCfgDBBLKBUFF
                        INTEGER,
                oramodDbCfgDBBLKSIZE
                        INTEGER,
                oramodDbCfgCKPTINTRVL
                        INTEGER,
                oramodDbCfgDBFILES
                        INTEGER,
                oramodDbCfgSORTAREASIZE
                        INTEGER,
                oramodDbCfgOPENCURSORS
                        INTEGER,
                oramodDbCfgTRNSACTNS
                        INTEGER,
                oramodDbCfgTRNSACTNSPERSEG
                        INTEGER,
                oramodDbCfgMAXROLLSEG
                        INTEGER
        }

        oramodDbCfgSIDINDX OBJECT-TYPE
                SYNTAX INTEGER (1..2147483647)
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "Sid's Index Number."
                ::= { oramodDbCfgEntry 1 }

	oramodDbCfgSID OBJECT-TYPE
		SYNTAX DisplayString
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
		   "Database Name."
		::= { oramodDbCfgEntry 2 }

	oramodDbCfgVERSION OBJECT-TYPE
		SYNTAX DisplayString
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
		   "Description and version of this Oracle installation
		    from v$version"
		::= { oramodDbCfgEntry 3 }

        oramodDbCfgHOME OBJECT-TYPE
                SYNTAX DisplayString 
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
                   "The Installation Directory for Oracle or ORACLEHOME."
		::= { oramodDbCfgEntry 4 }

        oramodDbCfgBASE OBJECT-TYPE
                SYNTAX DisplayString 
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
                   "The Base Directory for Oracle or ORACLEBASE."
		::= { oramodDbCfgEntry 5 }

        oramodDbCfgID OBJECT-TYPE
		SYNTAX INTEGER 
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
                   "The database ID calculated when the database is created
		and stored in all file headers (521382036)."
		::= { oramodDbCfgEntry 6 }

        oramodDbCfgCRTDT OBJECT-TYPE
		SYNTAX DisplayString
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
                   "The original creation date and time of the database."
		::= { oramodDbCfgEntry 7 }

        oramodDbCfgLOGMODE OBJECT-TYPE
		SYNTAX DisplayString
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
                   "The Archive log mode: NOARCHIVELOG or ARCHIVELOG."
		::= { oramodDbCfgEntry 8 }

        oramodDbCfgCTRLFILETYPE OBJECT-TYPE
		SYNTAX DisplayString
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
                   "Control File Type. Can be the following types : 
		CURRENT,STANDBY,CLONE,BACKUP,CREATED."
		::= { oramodDbCfgEntry 9 }

        oramodDbCfgOPENMODE OBJECT-TYPE
		SYNTAX DisplayString
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
                   "Shows whether the database is set to READ WRITE 
		or READ ONLY (NEW TO 8i.)."
		::= { oramodDbCfgEntry 10 } 

        oramodDbCfgMAXPROCESS OBJECT-TYPE
		SYNTAX INTEGER (1..2147483647) 
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
                   "For a multiple-process operation, this parameter 
		specifies the maximum number of user processes that can 
		simultaneously connect to an Oracle server. The PROCESSES 
		parameter is used to derive the SESSIONS INIT.ORA parameter.
		The default is 50. Typical range 6 - OS/dependent" 
		::= { oramodDbCfgEntry 11 }

        oramodDbCfgMAXSESSION OBJECT-TYPE
		SYNTAX INTEGER (1..2147483647) 
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
                   "Maximum number of User and System Sessions." 
		::= { oramodDbCfgEntry 12 }

        oramodDbCfgTIMEDSTATISTICS OBJECT-TYPE
		SYNTAX DisplayString
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
                   "TRUE/FALSE: sets timing on. Should be false for 
		maximum performance." 
		::= { oramodDbCfgEntry 13 }

        oramodDbCfgCPUCNT OBJECT-TYPE
		SYNTAX INTEGER (1..2147483647) 
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
                   "The maximum number of CPU's for this instance." 
		::= { oramodDbCfgEntry 14 }

        oramodDbCfgSHAREDPOOLSIZE OBJECT-TYPE
		SYNTAX INTEGER (1..2147483647) 
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
                   "Shared Pool Size Size in Kbytes. Controls the size of the
		memory area used for parsing and executing SQL statements.
		The default is 3.5 Mbytes. Typical Range 300 Kbytes. It is O/S
		dependent. " 
		::= { oramodDbCfgEntry 15 }

        oramodDbCfgSHAREDPOOLRSSIZE OBJECT-TYPE
		SYNTAX INTEGER  (1..2147483647) 
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
                   "Shared Pool Reserved Size Kbytes." 
		::= { oramodDbCfgEntry 16 }

        oramodDbCfgLARGEPOOLSIZE OBJECT-TYPE
		SYNTAX INTEGER (1..2147483647) 
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
                   "Large Pool Reserved Size Kbytes." 
		::= { oramodDbCfgEntry 17 }

        oramodDbCfgJAVAPOOLSIZE OBJECT-TYPE
		SYNTAX INTEGER (1..2147483647) 
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
                   "Java Pool Size in Kbytes Default is (20000K) " 
		::= { oramodDbCfgEntry 18 }

        oramodDbCfgCNTRLFILES OBJECT-TYPE
		SYNTAX DisplayString
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
                   "Name and path of control files. String of file names." 
		::= { oramodDbCfgEntry 19 }

        oramodDbCfgDBBLKBUFF OBJECT-TYPE
		SYNTAX INTEGER (1..2147483647)  
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
                   "Number of Database Block Buffers in blocks. Controls 
		how much memory is available in the buffer cache for data 
		blocks. Default is (26240)" 
		::= { oramodDbCfgEntry 20 }

        oramodDbCfgDBBLKSIZE OBJECT-TYPE
		SYNTAX INTEGER (1024..8192)
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
                   "Size of Database Blocks. O/S dependent" 
		::= { oramodDbCfgEntry 21 }

        oramodDbCfgCKPTINTRVL OBJECT-TYPE
		SYNTAX INTEGER (1..2147483647)  
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
                   "Redo Log Checkpoint Interval." 
		::= { oramodDbCfgEntry 22 }

        oramodDbCfgDBFILES OBJECT-TYPE
		SYNTAX INTEGER (1..2147483647)  
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
                   "Maximum Number of Database Files." 
		::= { oramodDbCfgEntry 23 }

        oramodDbCfgSORTAREASIZE OBJECT-TYPE
		SYNTAX INTEGER (1..2147483647)  
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
                   "Maximum Sort Area Size. Controls the amount of memory
		allocated to each process in the Process Global Area for
		any sorting activity (for multithreaded server this is 
		allocated in the shared pool). When a process needs more 
		memory than is available for a sort, disk space must be 
		allocated in the temporary tablespace. Tuning is based on 
		the transaction mix of the application. The default 
		(O/S dependent) is adequate for most DB operations.  " 
		::= { oramodDbCfgEntry 24 }

        oramodDbCfgOPENCURSORS OBJECT-TYPE
		SYNTAX INTEGER (1..2147483647) 
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
                   "Maximum open cursors a single-user process can have 
		at once. Typical Range (1 - 2147483647)." 
		::= { oramodDbCfgEntry 25 }

        oramodDbCfgTRNSACTNS OBJECT-TYPE
		SYNTAX INTEGER (1..2147483647)  
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
                   "Maximum number of concurrent transactions allowed at 
		one time." 
		::= { oramodDbCfgEntry 26 }

        oramodDbCfgTRNSACTNSPERSEG OBJECT-TYPE
		SYNTAX INTEGER (1..2147483647)  
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
                   "Maximum number of concurrent transactions per segment.
		Default is 30. O/S dependent." 
		::= { oramodDbCfgEntry 27 }

        oramodDbCfgMAXROLLSEG OBJECT-TYPE
		SYNTAX INTEGER (1..65536) 
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
                   "Maximum number of rollback segments." 
		::= { oramodDbCfgEntry 28 }

--
-- Oracle configuration information from v$datafile and v$datafileheader
--        oramodCfgDf    OBJECT IDENTIFIER ::= { oramodCfg 3 }

        oramodCfgDfTable OBJECT-TYPE
                SYNTAX SEQUENCE OF OramodCfgDfEntry
                ACCESS not-accessible
                STATUS mandatory
                DESCRIPTION
                   "This table describes the database files for the database."
                ::= { oramodCfgDf 1 }

        oramodCfgDfEntry OBJECT-TYPE
                SYNTAX OramodCfgDfEntry
                ACCESS not-accessible
                STATUS mandatory
                DESCRIPTION
                   "An entry in the Oracle v$datafile table."
                INDEX { oramodCfgDfSIDINDX, oramodCfgDfFILENUM }
                ::= {  oramodCfgDfTable 1 }

        OramodCfgDfEntry ::= SEQUENCE{
                oramodCfgDfSIDINDX
                        INTEGER,
                oramodCfgDfFILENUM
                        INTEGER,
                oramodCfgDfSTATUS
                        DisplayString,
                oramodCfgDfENABLED
                        DisplayString,
                oramodCfgDfUNRCVRBLECHG
                        INTEGER,
                oramodCfgDfUNRCVRBLETIME
                        INTEGER,
                oramodCfgDfKBYTES
                        INTEGER,
                oramodCfgDfCRTKBYTES
                        INTEGER,
                oramodCfgDfFNAME
                        DisplayString,
                oramodCfgDfCRTTIME
                        DisplayString,
                oramodCfgDfTBLESPACENUM
                        INTEGER,
                oramodCfgDfTBLESPACERFILENUM
                        INTEGER,
                oramodCfgDfBLOCKS
                        INTEGER,
                oramodCfgDfBLOCKSIZE
                        INTEGER,
                oramodCfgDfERROR
                        INTEGER,
                oramodCfgDfRECOVER
                        DisplayString,
                oramodCfgDfRSTLOGSCHGNUM
                        INTEGER,
                oramodCfgDfRSTLOGSTIME
			DisplayString
        }

        oramodCfgDfSIDINDX OBJECT-TYPE
                SYNTAX INTEGER (1..2147483647)
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "Sid Index Number."
                ::= { oramodCfgDfEntry 1 }

        oramodCfgDfFILENUM OBJECT-TYPE
                SYNTAX INTEGER
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "File Identification Number."
                ::= { oramodCfgDfEntry 2 }

        oramodCfgDfSTATUS OBJECT-TYPE
                SYNTAX DisplayString
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "Type of file (system or user) and its status.
		Values: OFFLINE, ONLINE, SYSTEM, RECOVER, SYSOFF(an offline
		file from the SYSTEM tablespace)."
                ::= { oramodCfgDfEntry 3 }

        oramodCfgDfENABLED OBJECT-TYPE
                SYNTAX DisplayString
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "Describes how accessible the file is from SQL. Can be
		DISABLED, READ ONLY, READ WRITE and UNKNOWN."
                ::= { oramodCfgDfEntry 4 }

        oramodCfgDfUNRCVRBLECHG OBJECT-TYPE
                SYNTAX INTEGER (1..2147483647)
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "The last unrecoverbale change number made to this datafile.
		Always updated when an unrecoverable operation completes."
                ::= { oramodCfgDfEntry 5 }

        oramodCfgDfUNRCVRBLETIME OBJECT-TYPE
                SYNTAX INTEGER (1..2147483647)
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "The last unrecoverbale change time made to this datafile.
		Always updated when an unrecoverable operation completes."
                ::= { oramodCfgDfEntry 6 }

        oramodCfgDfKBYTES OBJECT-TYPE
                SYNTAX INTEGER (1..2147483647)
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "Current size in kbytes; 0 if unaccessible."
                ::= { oramodCfgDfEntry 7 }

        oramodCfgDfCRTKBYTES OBJECT-TYPE
                SYNTAX INTEGER (1..2147483647)
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "Size when created."
                ::= { oramodCfgDfEntry 8 }

        oramodCfgDfFNAME OBJECT-TYPE
                SYNTAX DisplayString
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "Name of the file."
                ::= { oramodCfgDfEntry 9 }

        oramodCfgDfCRTTIME OBJECT-TYPE
                SYNTAX DisplayString 
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "Time at which the datafile was created."
                ::= { oramodCfgDfEntry 10 }

        oramodCfgDfTBLESPACENUM OBJECT-TYPE
                SYNTAX INTEGER (1..2147483647)
                ACCESS read-only
                STATUS mandatory
