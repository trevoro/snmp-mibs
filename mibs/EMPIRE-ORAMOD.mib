-- Rollback Segment information
	-- This table records cumulative statistics about all rollback segment
	-- usage since the database was last started. To simplify the
	-- management of multiple rollback segment entries within a rollback
	-- segment, try to size the rollback segment so that each of its 
	-- extents is large enough to support a typical transaction.
 
        --oramodRollBack OBJECT IDENTIFIER ::= { oramodPerf 5 }

        oramodRollBackTable OBJECT-TYPE
                SYNTAX SEQUENCE OF OramodRollBackEntry
                ACCESS not-accessible
                STATUS mandatory
                DESCRIPTION
                  "This table describes any rollback segments."
                ::= { oramodRollBack 1 }

        oramodRollBackEntry OBJECT-TYPE
                SYNTAX OramodRollBackEntry
                ACCESS not-accessible
                STATUS mandatory
                DESCRIPTION
                   "An entry in the rollstat table."
                INDEX { oramodRollBackSIDINDX, oramodRollBackSEGINDX }
                ::= {  oramodRollBackTable 1 }

        OramodRollBackEntry ::= SEQUENCE {
                oramodRollBackSIDINDX
                        INTEGER,
                oramodRollBackSEGINDX
                        INTEGER,
                oramodRollBackSEGNAME
                        DisplayString,
                oramodRollBackEXTENTS
                        Counter,
                oramodRollBackRSSIZE
                        Counter,
                oramodRollBackWRITES
                        Counter,
                oramodRollBackGETS
                        Counter,
                oramodRollBackWAITS
                        Counter,
                oramodRollBackOPTSIZE
                        INTEGER,
                oramodRollBackSHRINKS
                        Counter,
                oramodRollBackWRAPS
                        Counter,
                oramodRollBackEXTENDS
                        Counter,
                oramodRollBackAVESHRINK
                        INTEGER,
                oramodRollBackAVEACTIVE
                        Counter,
                oramodRollBackSTATUS
                        INTEGER
        }

        oramodRollBackSIDINDX OBJECT-TYPE
                SYNTAX INTEGER (1..2147483647)
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "SID Index Number."
                ::= { oramodRollBackEntry 1 }


        oramodRollBackSEGINDX OBJECT-TYPE
                SYNTAX INTEGER
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "Undo Segment Number used as an index."
                ::= { oramodRollBackEntry 2 }

        oramodRollBackSEGNAME OBJECT-TYPE
                SYNTAX DisplayString
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "Rollback Segment Name."
                ::= { oramodRollBackEntry 3 }

        oramodRollBackEXTENTS OBJECT-TYPE
                SYNTAX Counter
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "The total number of Segment Extends since instance
		startup."
                ::= { oramodRollBackEntry 4 }

        oramodRollBackRSSIZE OBJECT-TYPE
                SYNTAX Counter
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "The size of the Rollback Segment."
                ::= { oramodRollBackEntry 5 }

        oramodRollBackWRITES OBJECT-TYPE
                SYNTAX Counter
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "The total number of Segment Writes since instance startup."
                ::= { oramodRollBackEntry 6 }

        oramodRollBackGETS OBJECT-TYPE
                SYNTAX Counter
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "The total number of Rollback Segment Gets since 
		instance startup."
                ::= { oramodRollBackEntry 7 }

        oramodRollBackWAITS OBJECT-TYPE
                SYNTAX Counter
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "The total number of Segment Waits since instance startup."
                ::= { oramodRollBackEntry 8 }

        oramodRollBackOPTSIZE OBJECT-TYPE
                SYNTAX INTEGER
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "The Optimal Size of the Rollback Segment. This size should
		be set based on application usage to minimize space management
		issues."
                ::= { oramodRollBackEntry 9 }

        oramodRollBackSHRINKS OBJECT-TYPE
                SYNTAX Counter
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "The total number of Rollback Segment Shrinks since
		instance startup. Rollback Segments shrink back to their
		optimal size after extends have occurred."
                ::= { oramodRollBackEntry 10 }

        oramodRollBackWRAPS OBJECT-TYPE
                SYNTAX Counter
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   " The total number of Rollback Segment wraps since instance
		startup. When a transaction's rollback segment entry cannot be
		stored within a single extent, the entry wraps into a second
		extent within the rollback segment. If this value is non-zero,
		try recreating the rollback segments with larger extent sizes."
                ::= { oramodRollBackEntry 11 }

        oramodRollBackEXTENDS OBJECT-TYPE
                SYNTAX Counter
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "Rollback Segment extends. Extensions indicate that the
		rollback segments are handling larger transactions than they
		were designed for; and if the entire rollback segment cannot
		handle a transaction's rollback information, a single extent 
		will not be able to hold it either. Thus, rollback segments
		that extend will frequently have a high number of wraps."
                ::= { oramodRollBackEntry 12 }

        oramodRollBackAVESHRINK OBJECT-TYPE
                SYNTAX INTEGER
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "The average shrink size of the Rollback Segment since
		this instance startup."
                ::= { oramodRollBackEntry 13 }

        oramodRollBackAVEACTIVE OBJECT-TYPE
                SYNTAX Counter
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "Segment average active."
                ::= { oramodRollBackEntry 14 }

        oramodRollBackSTATUS OBJECT-TYPE
                SYNTAX INTEGER
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "Segment status."
                ::= { oramodRollBackEntry 15 }

