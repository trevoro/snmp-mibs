		of spincounts has been reached the prcess will back off 
		and sleep."
                ::= { oramodLatchEntry 11 }

-- Backup - A table of physical backup information.  

        oramodBackupTable OBJECT-TYPE
                SYNTAX SEQUENCE OF OramodBackupEntry
                ACCESS not-accessible
                STATUS mandatory
                DESCRIPTION
                  "Backup Information"
                ::= { oramodBackup 1 }

        oramodBackupEntry OBJECT-TYPE
                SYNTAX OramodBackupEntry
                ACCESS not-accessible
                STATUS mandatory
                DESCRIPTION
                   "An entry from the backup table."
                INDEX { oramodBackupSIDINDX, oramodBackupFILENUM }
                ::= {  oramodBackupTable 1 }

        OramodBackupEntry ::= SEQUENCE {
                oramodBackupSIDINDX
                        INTEGER,
                oramodBackupFILENUM
                        INTEGER,
                oramodBackupSTATUS
                        DisplayString,
                oramodBackupCHANGENUM
                        INTEGER,
                oramodBackupDATE
                        DisplayString
        }

        oramodBackupSIDINDX OBJECT-TYPE
                SYNTAX INTEGER
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "Sid Index Number."
                ::= { oramodBackupEntry 1 }

        oramodBackupFILENUM OBJECT-TYPE
                SYNTAX INTEGER
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "Backup File Index Number."
                ::= { oramodBackupEntry 2 }

        oramodBackupSTATUS OBJECT-TYPE
                SYNTAX DisplayString
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "Status of the backup."
                ::= { oramodBackupEntry 3 }

        oramodBackupCHANGENUM OBJECT-TYPE
                SYNTAX INTEGER
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "The change number of the backup file."
                ::= { oramodBackupEntry 4 }

        oramodBackupDATE OBJECT-TYPE
                SYNTAX DisplayString
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "Date of the Backup."
                ::= { oramodBackupEntry 5 }


-- Archive - A table of archive information. 

        oramodArcDestTable OBJECT-TYPE
                SYNTAX SEQUENCE OF OramodArcDestEntry
                ACCESS not-accessible
                STATUS mandatory
                DESCRIPTION
                  "Archive Information."
                ::= { oramodArcDest 1 }

        oramodArcDestEntry OBJECT-TYPE
                SYNTAX OramodArcDestEntry
                ACCESS not-accessible
                STATUS mandatory
                DESCRIPTION
                   "An entry from the archive destination table."
                INDEX { oramodArcDestSIDINDX, oramodArcDestARCINDX  }
                ::= {  oramodArcDestTable 1 }

        OramodArcDestEntry ::= SEQUENCE {
                oramodArcDestSIDINDX
                        INTEGER,
                oramodArcDestARCINDX
                        INTEGER,
                oramodArcDestARCMODE
                        DisplayString,
                oramodArcDestSTATUS
                        DisplayString,
                oramodArcDestDEST
                        DisplayString
        }

        oramodArcDestSIDINDX OBJECT-TYPE
                SYNTAX INTEGER
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "Sid Index Number."
                ::= { oramodArcDestEntry 1 }

        oramodArcDestARCINDX OBJECT-TYPE
                SYNTAX INTEGER
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "Index of Archive log."
                ::= { oramodArcDestEntry 2 }

        oramodArcDestARCMODE OBJECT-TYPE
                SYNTAX DisplayString
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "Mode of Archive log."
                ::= { oramodArcDestEntry 3 }

        oramodArcDestSTATUS OBJECT-TYPE
                SYNTAX DisplayString
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "Status of Archive Log File."
                ::= { oramodArcDestEntry 4 }

        oramodArcDestDEST OBJECT-TYPE
                SYNTAX DisplayString
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "Archive Log File Destination."
                ::= { oramodArcDestEntry 5 }

END
