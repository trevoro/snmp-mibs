        oramodExpSqlSORTS OBJECT-TYPE
                SYNTAX Gauge
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "Number of Sorts."
                ::= { oramodExpSqlEntry 7 }

       oramodExpSqlSQL OBJECT-TYPE
                SYNTAX DisplayString 
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "Sql Statement truncated."
                ::= { oramodExpSqlEntry 8 }


