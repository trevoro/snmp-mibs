                ::= {  oramodExpSqlTable 1 }

        OramodExpSqlEntry ::= SEQUENCE {
                oramodExpSqlSIDINDX
                        INTEGER,
                oramodExpSqlSQLINDX
                        INTEGER,
                oramodExpSqlEXECUTION
                        Gauge,
                oramodExpSqlDISKREAD
                        Gauge,
                oramodExpSqlPARSECNT
                        Gauge,
                oramodExpSqlBUFFGETS
                        Gauge,
                oramodExpSqlSORTS
                        Gauge,
                oramodExpSqlSQL
                        DisplayString 
        }

