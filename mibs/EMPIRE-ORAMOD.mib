        oramodLatchLATCHINDX OBJECT-TYPE
                SYNTAX INTEGER
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "Latch Index Number."
                ::= { oramodLatchEntry 2 }

        oramodLatchLATCHNUM OBJECT-TYPE
                SYNTAX INTEGER
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "Latch Number."
                ::= { oramodLatchEntry 3 }

        oramodLatchLATCHNAME OBJECT-TYPE
                SYNTAX DisplayString
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "Latch Name."
