--  **********************************************************************
--  **********************************************************************
--
--  Traps (20)
--              This section defines the traps that the HUB issues.
--
--      This section defines the traps which will be sent to an SNMP 
--      management console.  The HUB SNMP Agent will generate a trap for 
--      each alert message received from the Storage Hub.  
--
--      The message will have one of 4 severity types : Informational, Warning, 
--      Serious or Fatal. Each trap message will be identified by its severity.
--
--      The trap definitions use the MIB variable 'xioHubTvMessage' 
--      to specify the message text.        
--
--  **********************************************************************
--  **********************************************************************

    xioHubUnknownEvent   TRAP-TYPE
                 ENTERPRISE  xioHubTraps
                 VARIABLES  { 
						xioHubTvEventNum,
						xioHubTvTimestamp,
						xioHubTvType,
						xioHubTvExtType,
						xioHubTvFlags,
						xioHubTvLedStatus,
						xioHubTvMessage,
						xioHubTvAddress,
						xioHubTvHubName
						}
                 DESCRIPTION
                        "A Magnitude unknown event; the severity is invalid."
                 ::= 2

    xioHubInfoEvent   TRAP-TYPE
                 ENTERPRISE  xioHubTraps
                 VARIABLES  { 
						xioHubTvEventNum,
						xioHubTvTimestamp,
						xioHubTvType,
						xioHubTvExtType,
						xioHubTvFlags,
						xioHubTvLedStatus,
						xioHubTvMessage,
						xioHubTvAddress,
						xioHubTvHubName
						}
                 DESCRIPTION
                        "A Magnitude Informational event."
                 ::= 5

    xioHubWarningEvent   TRAP-TYPE
                 ENTERPRISE  xioHubTraps
                 VARIABLES  { 
						xioHubTvEventNum,
						xioHubTvTimestamp,
						xioHubTvType,
						xioHubTvExtType,
						xioHubTvFlags,
						xioHubTvLedStatus,
						xioHubTvMessage,
						xioHubTvAddress,
						xioHubTvHubName
						}
                 DESCRIPTION
                        "A Magnitude Warning event."
                 ::= 6

    xioHubSeriousEvent   TRAP-TYPE
                 ENTERPRISE  xioHubTraps
                 VARIABLES  { 
						xioHubTvEventNum,
						xioHubTvTimestamp,
						xioHubTvType,
						xioHubTvExtType,
						xioHubTvFlags,
						xioHubTvLedStatus,
						xioHubTvMessage,
						xioHubTvAddress,
						xioHubTvHubName
						}
                 DESCRIPTION
                        "A Magnitude Serious event."
                 ::= 7

    xioHubFatalEvent   TRAP-TYPE
                 ENTERPRISE  xioHubTraps
                 VARIABLES  { 
						xioHubTvEventNum,
						xioHubTvTimestamp,
						xioHubTvType,
						xioHubTvExtType,
						xioHubTvFlags,
						xioHubTvLedStatus,
						xioHubTvMessage,
						xioHubTvAddress,
						xioHubTvHubName
						}
                 DESCRIPTION
                        "A Magnitude Fatal event."
                 ::= 8



