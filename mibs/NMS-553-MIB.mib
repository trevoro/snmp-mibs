nms553AlarmData	              OBJECT IDENTIFIER ::= { nms553Alarms 1 }

nms553NoResponseAlm           OBJECT IDENTIFIER ::= { nms553AlarmData 1 }
nms553DiagRxErrAlm            OBJECT IDENTIFIER ::= { nms553AlarmData 2 }
