     	ncmhdmStatDTEReady OBJECT-TYPE
          SYNTAX INTEGER {
                       oFF (1),
                       oN (2)
               } 
          ACCESS read-only
          STATUS mandatory
          DESCRIPTION
               "Hssi Port DTE Ready OFF/ON. "
          ::= { ncmhdmHssiStatEntry  4 }
 
 
     	ncmhdmStatLpbkA OBJECT-TYPE
          SYNTAX INTEGER {
                       oFF (1),
                       oN (2)
               } 
          ACCESS read-only
          STATUS mandatory
          DESCRIPTION
               "Hssi Port - Loopback A OFF/ON."
          ::= { ncmhdmHssiStatEntry  5 }
 
 
     	ncmhdmStatLpbkB OBJECT-TYPE
          SYNTAX INTEGER {
                       oFF (1),
                       oN (2)
               } 
          ACCESS read-only
          STATUS mandatory
          DESCRIPTION
               "Hssi Port - Loopback B OFF/ON."
          ::= { ncmhdmHssiStatEntry  6 }


	ncmhdmStatTestMode OBJECT-TYPE
          SYNTAX INTEGER {
                       oFF (1),
                       oN (2)
               } 
          ACCESS read-only
          STATUS mandatory
          DESCRIPTION
               "Hssi Port Test Mode OFF/ON."
          ::= { ncmhdmHssiStatEntry  7  }
 
 
     	ncmhdmStatPortState OBJECT-TYPE
          SYNTAX INTEGER {
                       oUT-OF-SERVICE(1),
                       iN-SERVICE(2)
               } 
          ACCESS read-only
          STATUS mandatory
          DESCRIPTION
               "Hssi Port - In-Service or Out of Service."
          ::= { ncmhdmHssiStatEntry  8 }
 
 
     	ncmhdmStatCIMType OBJECT-TYPE
          SYNTAX INTEGER {
                       cim-2080 (44),
                       cdm-2080 (59),
		       cdm-2182 (63)
               } 
          ACCESS read-only
          STATUS mandatory
          DESCRIPTION
               "Ds3 Cim Type."
          ::= { ncmhdmHssiStatEntry 9 }
 
     	ncmhdmStatDiagStatus OBJECT-TYPE
          SYNTAX INTEGER {
                       oFF (1),
                       oN (2)
               } 
          ACCESS read-only
