            ::= { ncmhdmHssiStatEntry 2 }
 
 
     	ncmhdmStatDCEReady OBJECT-TYPE
          SYNTAX INTEGER {
                       oFF (1),
                       oN (2)
               } 
          ACCESS read-only
          STATUS mandatory
          DESCRIPTION
               "Hssi Port DCE Ready OFF/ON. "
