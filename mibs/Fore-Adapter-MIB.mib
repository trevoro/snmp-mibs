aal0TransmittedCells OBJECT-TYPE
	SYNTAX	Counter32
	MAX-ACCESS	read-only
	STATUS	current
	DESCRIPTION
		"The number of AAL type 0 cells transmitted."
	::= { aal0Entry 2 }

aal0ReceivedCells OBJECT-TYPE
	SYNTAX	Counter32
	MAX-ACCESS	read-only
	STATUS	current
	DESCRIPTION
		"The number of AAL type 0 cells received."
