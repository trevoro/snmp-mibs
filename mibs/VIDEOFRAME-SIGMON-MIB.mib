			FROM SNMPv2-CONF
		vfProducts, vfProductsReg, vfMIBModules
			FROM VIDEOFRAME-REGISTRATIONS-MIB
		vfBoxId
			FROM VIDEOFRAME-GENERIC-MIB;

	videoframeSigmonMIB MODULE-IDENTITY
		LAST-UPDATED "0101190000Z"
		ORGANIZATION "Videoframe Systems"
		CONTACT-INFO
