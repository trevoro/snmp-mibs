   ::= { sysPortCtrlTable 1 }

TabPortCtrlEntry ::=
   SEQUENCE {
      sysPortCtrlIndex
         Integer32,
      sysPortCtrlMode
         INTEGER,
      sysPortCtrlDuplex
         INTEGER,
      sysPortCtrlSpeed
         INTEGER,
      sysPortCtrlFlowControl
         INTEGER,
      sysPortCtrlMDI
         INTEGER,
      issJumboFramePerPortEnable
         INTEGER,
      issJumboFramePerPortMtu
         Integer32,
	  sysPortCtrlEAPPassThrough
		 INTEGER,
	  sysPortCtrlBPDUPassThrough
         INTEGER
  }

sysPortCtrlIndex OBJECT-TYPE
   SYNTAX      Integer32 (1..65535)
   MAX-ACCESS  not-accessible
   STATUS      current
   DESCRIPTION
      "Interface index of the port for which the configuration
       in this entry applies."
   ::= { sysPortCtrlEntry 1 }

sysPortCtrlMode OBJECT-TYPE
   SYNTAX      INTEGER {
                 auto          (1),
                 noNegotiation (2)
              }
   MAX-ACCESS  read-write
   STATUS      current
   DESCRIPTION
      "Speficies the mode in which the speed, duplex modes and flow control
       of the interface is determined.

       If set as 'auto', the hardware senses speed and negotiates with the port
       on the other end of the link for data transfer operation as
       'full-duplex' or 'half-duplex' and about flow contol.

