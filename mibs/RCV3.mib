		"Number of pending Secure Associations."
	::= { rc3Stat 2 }

rc3SigFailCount OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"Indicates number of times the signature failed."
	::= { rc3Stat 3 }

-- =======================
-- STATUS Messages Table
-- =======================

rc3StatusMsgTable OBJECT-TYPE
	SYNTAX SEQUENCE OF Rc3StatusMsgEntry
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION
		"The table containing the status messages for this device.  This
		entire table is read-only."
	::= { rc3Stat 4 }

rc3StatusMsgEntry OBJECT-TYPE
	SYNTAX Rc3StatusMsgEntry
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION
		"An entry in the status message table.  For status messages
		that repeat, only one entry is used, and the rc3EventCodeRepetitions
		filed identifies the number of consecutive messages of that type
		that have occurred.  This should prevent the table from being wiped
		out by a series of consecutive messages of the same type.  The
		entire table can be cleared using rc3Reset."
	INDEX { rc3StatusMsgIndex }
	::= { rc3StatusMsgTable 1 }

Rc3StatusMsgEntry ::= SEQUENCE {
	rc3StatusMsgIndex		INTEGER,
	rc3FirstInTimeStamp		INTEGER,
	rc3LastInTimeStamp		INTEGER,
	rc3EventCode				INTEGER,
	rc3EventCodeRepetitions	INTEGER,
	rc3EventSpecificDescr1	OCTET STRING,
	rc3EventSpecificDescr2	OCTET STRING
	}

rc3StatusMsgIndex OBJECT-TYPE
    SYNTAX INTEGER
	ACCESS not-accessible
--	ACCESS read-only
    STATUS mandatory
    DESCRIPTION
		"This value is used as a unique identifier for each entry."
	::= { rc3StatusMsgEntry 1 }

rc3FirstInTimeStamp OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
		"The time when this message was first generated."
	::= { rc3StatusMsgEntry 2 }

rc3LastInTimeStamp OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
		"The time when this message was last generated."
	::= { rc3StatusMsgEntry 3 }

rc3EventCode OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
		"An identifier of the event caused the status message to be generated."
	::= { rc3StatusMsgEntry 4 }

rc3EventCodeRepetitions OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
		"The number of times in a row this event occurred."
	::= { rc3StatusMsgEntry 5 }

rc3EventSpecificDescr1 OBJECT-TYPE
    SYNTAX OCTET STRING
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
		"If the event code maps to a string that contains '%1', this value will
		be substituted for the '%1' when the string is displayed."
	::= { rc3StatusMsgEntry 6 }

rc3EventSpecificDescr2 OBJECT-TYPE
    SYNTAX OCTET STRING
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
		"If the event code maps to a string that contains '%2', this value will
		be substituted for the '%2' when the string is displayed."
	::= { rc3StatusMsgEntry 7 }

rc3SysLogServerIP OBJECT-TYPE
    SYNTAX IpAddress
    ACCESS read-write
    STATUS deprecated
    DESCRIPTION
		"IP Address of the syslog server."
    ::= { rc3Stat 5 }

rc3SysLogPortNum OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-write
    STATUS deprecated
    DESCRIPTION
		"Port number for the syslog service."
    ::= { rc3Stat 6 }

rc3SysLogMsgLevel OBJECT-TYPE
    SYNTAX INTEGER {
    critical(1),
    severe(2),
    error(3),
    warning(4),
    normal(5),
    debug(6),
    invalid(7)
    }
    ACCESS read-write
    STATUS deprecated
    DESCRIPTION
		"Specify how verbose the message logging should be."
    ::= { rc3Stat 7 }

-- =======================
-- SYSLOG SERVER Table
-- =======================

rc3SysLogServerTable OBJECT-TYPE
	SYNTAX SEQUENCE OF Rc3SysLogServerEntry
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION
		"The table containing the list of syslog server IP Address, 
        syslog port, and priority of syslog message it will receive."
