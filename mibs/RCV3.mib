rc3WriteCommunityString OBJECT-TYPE
    SYNTAX DisplayString (SIZE(1..20))
    ACCESS read-write
    STATUS deprecated
    DESCRIPTION
		"This object is used to change Write Community string private.
		For SNMP set operations the string is encrypted. The key for
		encrypt/Decrypt is generated by hashing the password and a 
		selector value. This object will return NULL for get/getnext."
    ::= { rc3Snmp 2 }

-- =======================
-- TRAP recipient SNMP Managers
-- =======================

rc3TrapRcvrTable OBJECT-TYPE
    SYNTAX SEQUENCE OF Rc3TrapRcvrEntry
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION
		"A Table to register SNMP Manager IP Address and
		community string to receive TRAP's from Ravlin Unit. 
		This table can have a maximum of 5 entries."
    ::= { rc3Snmp 3 }

rc3TrapRcvrEntry OBJECT-TYPE
    SYNTAX Rc3TrapRcvrEntry
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION
		"A row in the Trap Receiver Table. Rows in this
		table can be created or Deleted."
	INDEX { rc3TrapRcvrIpAddr }
	::= { rc3TrapRcvrTable 1 }

Rc3TrapRcvrEntry ::= SEQUENCE {
	rc3TrapRcvrIpAddr	IpAddress,
	rc3TrapRcvrComm		OCTET STRING,
    rc3TrapRcvrType      INTEGER,
	rc3TrapRcvrStatus	RowStatus
}

rc3TrapRcvrIpAddr OBJECT-TYPE
    SYNTAX IpAddress
	ACCESS not-accessible
--	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"The IP Address of the SNMP Manager that would receive
		TRAP."
	::= { rc3TrapRcvrEntry 1 }
	
rc3TrapRcvrComm OBJECT-TYPE
    SYNTAX OCTET STRING (SIZE(0..20))
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"Community string used by SNMP Manager."
	::= { rc3TrapRcvrEntry 2 }

rc3TrapRcvrType OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"Specifies the types of TRAPS that this manager should received."
	::= { rc3TrapRcvrEntry 3 }

rc3TrapRcvrStatus OBJECT-TYPE
    SYNTAX RowStatus
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"This object is used to create/delete rows from the TRAP
		Receiver Table. See the definition for RowStatus for a
		range of valid values used. On read status indicates ACTIVE."
	::= { rc3TrapRcvrEntry 4 }

rc3SnmpErrorCode OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
		"This object will return the reason for the last SNMP SET error.
		Since SNMPv1 has a limited number of error codes, this object 
		provides an extension to report specific enterprise errors."
	::= { rc3Snmp 4 }

rc3ReadCommunityStringFips OBJECT-TYPE
    SYNTAX PbeShaEncryptedObject
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
		"Available for release 3.30 and up. Object used to set read community
        string for this device. Refer to the textual convention of PbeShaEncryptedObject,
        for the process used to set this object. The default ReadCommunityString is 
        'public'. On read an octet string of zero length is returned.
        read community string SIZE(1..20)"
    ::= { rc3Snmp 5 }

rc3WriteCommunityStringFips OBJECT-TYPE
    SYNTAX PbeShaEncryptedObject
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
		"Available for release 3.30 and up. Object used to set write community
        string for this device. Refer to the textual convention of PbeShaEncryptedObject,
        for the process used to set this object. The default WriteCommunityString is 
        'private'. On read an octet string of zero length is returned.
        write community string SIZE(1..20)"
    ::= { rc3Snmp 6 }

-- =======================
-- Client Configuration Group
-- =======================

rc3ClientAuthentication OBJECT-TYPE
	SYNTAX INTEGER {
		enableRadius(1),
		disableAuthentication(2),
        enableLocal (3)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
        "Enable or Disable client Authentication. There are two Authentication methods.
        Either RADIUS or LOCAL. Client Authentication is not avaliable on the 
        personal Ravlin."
	::= { rc3ClientCfg 1 }

rc3ActiveRadiusServer OBJECT-TYPE
	SYNTAX IpAddress
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"Specify the current active RADIUS server by its IP Address. If this
		RADIUS server is not responding, then the agent will direct RADIUS
		requests to the next preferred RADIUS server in the list, and
		accordingly change the value of this object to indicate the active server."
	::= { rc3ClientCfg 2 }

rc3RadiusPriority OBJECT-TYPE
	SYNTAX OCTET STRING (SIZE(1..3))
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"Specify the priority of the Radius servers in the table.  Each octet
		will specify one of the entries in order of preference."
	::= { rc3ClientCfg 3 }

-- =======================
-- Radius Server Table
-- =======================

rc3RadiusAuthServerTable OBJECT-TYPE
	SYNTAX SEQUENCE OF Rc3RadiusAuthServerEntry
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION
		"Table, to setup Primary and/or Secondary Authentication Server.
		This is a fixed sized table. RADIUS packets will be routed to
		their destination using the Route Table."
	::= { rc3ClientCfg 4 }

rc3RadiusAuthServerEntry OBJECT-TYPE
	SYNTAX Rc3RadiusAuthServerEntry
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION
		"RADIUS Auth Server entry containing all the attributes
		required to configure Auth Server. There will be a
        maximum of three entries in the table. If 
        rc3RadiusAuthServerIP is 0.0.0.0, then that row is not used."
	INDEX  { rc3RadiusAuthServerEntryIndx }
	::= { rc3RadiusAuthServerTable 1 }

Rc3RadiusAuthServerEntry ::= SEQUENCE {
    rc3RadiusAuthServerEntryIndx         INTEGER,
	rc3RadiusAuthServerIP				IpAddress,
	rc3RadiusAuthServerPort				INTEGER,
    rc3RadiusAttributeOffset             INTEGER,
	rc3RadiusAuthServerSharedSecret		OCTET STRING,
	rc3RadiusAuthServerRetries			INTEGER,
    rc3RadiusAuthServerSharedSecretFips PbeShaEncryptedObject
	}

rc3RadiusAuthServerEntryIndx OBJECT-TYPE
	SYNTAX INTEGER (1..3)
	ACCESS not-accessible
--	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"A unique identifier for the entry in this table.  Valid values
		are 1-3."	
	::= { rc3RadiusAuthServerEntry 1 }

rc3RadiusAuthServerIP OBJECT-TYPE
	SYNTAX IpAddress
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"The IP Address (Network Address), of the RADIUS Authentication
		server."	
	::= { rc3RadiusAuthServerEntry 2 }

rc3RadiusAuthServerPort OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"Specifies the UDP port number used by the RADIUS service on the
		Authentication Server."
	::= { rc3RadiusAuthServerEntry 3 }

rc3RadiusAttributeOffset OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
		"If zero, then use vendor specific attributes. Otherwise this object
		specifies the first offset to use for the RedCreek attribute values."
    ::= { rc3RadiusAuthServerEntry 4 }

rc3RadiusAuthServerSharedSecret OBJECT-TYPE
	SYNTAX OCTET STRING
	ACCESS read-write
	STATUS deprecated
	DESCRIPTION
		"This is the shared secret between the Ravlin and Radius Server.
		For SNMP set operations the string is encrypted. The key for
		encrypt/Decrypt is generated by hashing the password and a 
		selector value. This object will return NULL for get/getnext."
	::= { rc3RadiusAuthServerEntry 5 }

rc3RadiusAuthServerRetries OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"This limits the number of retries with the Authentication Server.
		Default is 3. Maximum is 5. Min is 1"
	::= { rc3RadiusAuthServerEntry 6 }

rc3RadiusAuthServerSharedSecretFips OBJECT-TYPE
	SYNTAX PbeShaEncryptedObject
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"Available for release 3.30 and up. Object used to set shared secret
        key between the Ravlin and Radius Server. Refer to the textual convention of 
        PbeShaEncryptedObject, for the process used to set this object.
        If the preshared key exists, then on read a dummy string is 
        returned. shared secret key length is (0..32)"
	::= { rc3RadiusAuthServerEntry 7 }

-- =======================
-- Local Authentication Table.
-- =======================
rc3LocalAuthTable OBJECT-TYPE
	SYNTAX SEQUENCE OF Rc3LocalAuthEntry
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION
		"Table to maintain the database of user name, password and static
        virtual IP Address and Mask."
	::= { rc3ClientCfg 5 }

rc3LocalAuthEntry OBJECT-TYPE
	SYNTAX Rc3LocalAuthEntry
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION
		"Entries in this table can be added and deleted."
	INDEX  { rc3LocalAuthId }
	::= { rc3LocalAuthTable 1 }

Rc3LocalAuthEntry ::= SEQUENCE {
    rc3LocalAuthId           INTEGER,
    rc3LocalAuthName         DisplayString,
	rc3LocalAuthPassword     PbeShaEncryptedObject,
	rc3LocalAuthIpAddr       IpAddress,				
    rc3LocalAuthIpMask       IpAddress,
    rc3LocalAuthRowStatus    RowStatus
	}

rc3LocalAuthId OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS not-accessible
    STATUS mandatory
	DESCRIPTION
		"A unique value for each entry."
	::= { rc3LocalAuthEntry 1 }

rc3LocalAuthName OBJECT-TYPE
    SYNTAX DisplayString (SIZE(1..16))
	ACCESS read-write
    STATUS mandatory
    DESCRIPTION
		"User name, used to logon and get authenticated. When password is
        read it will return a NULL string."
    ::= { rc3LocalAuthEntry 2 }

rc3LocalAuthPassword OBJECT-TYPE
    SYNTAX PbeShaEncryptedObject
	ACCESS read-write
    STATUS mandatory
    DESCRIPTION
		"Password for the user, to authenticated with"
    ::= { rc3LocalAuthEntry 3 }

rc3LocalAuthIpAddr OBJECT-TYPE
    SYNTAX IpAddress
	ACCESS read-write
    STATUS mandatory
    DESCRIPTION
		"Static virtual IP address."
    ::= { rc3LocalAuthEntry 4 }

rc3LocalAuthIpMask OBJECT-TYPE
    SYNTAX IpAddress
	ACCESS read-write
    STATUS mandatory
    DESCRIPTION
		"Static virtual Mask"
    ::= { rc3LocalAuthEntry 5 }

rc3LocalAuthRowStatus OBJECT-TYPE
    SYNTAX RowStatus
	ACCESS read-write
    STATUS mandatory
    DESCRIPTION
		"This object is used to create/delete rows from the IP
		Route Table. See the definition for RowStatus for a range
		of valid values used. On read status indicates ACTIVE."
    ::= { rc3LocalAuthEntry 6 }

-- =======================
-- Software Download Group
-- =======================

rc3ImageSize OBJECT-TYPE
    SYNTAX INTEGER 
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
	    "Size of Image in bytes. SNMP manager must send this value before
		it can do a software download. On receipt of this object the AGENT 
		will allocate the memory of rc3ImageSize, and start a 60 second timer.
		If the Agent does not receive the actual software block within the 
		timeout period, it will assume the software download is aborted."
	::= { rc3Download 1 }

rc3SoftwareBlock OBJECT-TYPE
    SYNTAX OCTET STRING
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
	    "A block of software image to be downloaded to the flash. AGENT uses
		a 60 second timer to timeout and abort software download, if no 
		more block is received. On read the AGENT returns a zero length
		(NULL) string."
	::= { rc3Download 2 }

rc3SoftwareBlockNumber OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
        "A sequential counter to keep track of the block number of the 
        image that is sent by the SNMP Manager."
	::= { rc3Download 3 }

-- =======================
-- Misc Group
-- =======================

rc3RandomNumber OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
		"A new Random number is used for every set hash computation.
		After the set operation is successful, the Ravlin changes the
		random number value, thus preventing replay."
	::= { rc3Misc 1 }

rc3HashObject OBJECT-TYPE
    SYNTAX OCTET STRING 
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
	    "Image checksum/hash, after the last block of Image download is
		completed. On this indication the AGENT will do a checksum
		verification, and if correct will write the Image to the flash.
		If download Image checksum is incorrect then returns 'Gen Err'."
	::= { rc3Misc 2 }

rc3UlaAuthenticationTimer OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"The duration in minutes to re-authenticate a user for gateway access.
        Minimum is 30 minutes."
	::= { rc3Misc 3 }

rc3UlaAuthenticationPort OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"UDP port used for ULA authentication. By default the Ravlin will send
        to the RADIUS port 1812."
	::= { rc3Misc 4 }

rc3SystemTime OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"This object reads the current system time in GMT."
	::= { rc3Misc 5 }

-- =======================
-- Certificate Group
-- =======================

-- =======================
-- Certificate Authority Table
-- =======================

rc3CACertTable OBJECT-TYPE
	SYNTAX SEQUENCE OF Rc3CACertEntry
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION
		"Table containing the CA certificates and public keys that
		can be used to verify User Certificate. The index value of
        1 and 2 is reserved for RedCreek CA certificate that is manufactured
        into the box.  Attempts to modify this entry will return generic error.
        A total of upto 6 CA certificates can be added. "
	::= { rc3Cert 1 }

rc3CACertEntry OBJECT-TYPE
	SYNTAX Rc3CACertEntry
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION
		"The entries in this table indicate that trust has been
		established for this root CA.  Entries in the Policy Database
		will list an issuer name that is trusted, and the certificate
		for that issuer should reside in this table."
	INDEX { rc3CACertIndex }
	::= { rc3CACertTable 1 }

Rc3CACertEntry ::= SEQUENCE {
	rc3CACertIndex	INTEGER,
	rc3CACertName	DisplayString,
	rc3CACertData	OCTET STRING,
	rc3CACertStatus	RowStatus
	}

rc3CACertIndex OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS not-accessible
--	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"A unique identifier for this certificate in this table."
	::= { rc3CACertEntry 1 }

rc3CACertName OBJECT-TYPE
    SYNTAX DisplayString (SIZE(1..16))
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"Textual string used to identify a CA certificate."
	::= { rc3CACertEntry 2 }

rc3CACertData OBJECT-TYPE
	SYNTAX OCTET STRING
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"The x509v3 DER encoded CA certificate."
	::= { rc3CACertEntry 3 }

rc3CACertStatus OBJECT-TYPE
	SYNTAX RowStatus
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"This object is used to create/delete rows from the CA
		Certificate Table. See the definition for RowStatus for a
		range of valid values used."
	::= { rc3CACertEntry 4 }

-- =======================
-- User Certificate Table
-- =======================

rc3UserCertTable OBJECT-TYPE
	SYNTAX SEQUENCE OF Rc3UserCertEntry
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION
		"Table containing the User certificates and public keys that
		can be used to identify this device to its peers.  A public/private
		key pair will be generated for each entry as it is added to the
		table.  The private keys are stored on the device, but are not
		readable via SNMP.  The index value of 1 is reserved for the
		User certificate signed by RedCreek CA and is manufactured into
        the box.  Attempts to modify that entry will cause an error."
	::= { rc3Cert 2 }

rc3UserCertEntry OBJECT-TYPE
	SYNTAX Rc3UserCertEntry
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION
		"The entries in this table can be used as a cache for peer
		certificates.  An attempt to verify the certificate is made
		when the entry is added to the table."
	INDEX { rc3UserCertIndex }
	::= { rc3UserCertTable 1 }

Rc3UserCertEntry ::= SEQUENCE {
	rc3UserCertIndex    INTEGER,
	rc3UserCertName     OCTET STRING,
	rc3UserCertData     OCTET STRING,
	rc3UserCertStatus	RowStatus
	}

rc3UserCertIndex OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS not-accessible
--	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"A unique identifier for this certificate in this table."
	::= { rc3UserCertEntry 1 }

rc3UserCertName OBJECT-TYPE
	SYNTAX OCTET STRING
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"Textual string used to identify a User certificate."
	::= { rc3UserCertEntry 2 }

rc3UserCertData OBJECT-TYPE
	SYNTAX OCTET STRING
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"The x509v3 DER encoded user certificate."
	::= { rc3UserCertEntry 3 }

rc3UserCertStatus OBJECT-TYPE
	SYNTAX RowStatus
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"This object is used to create/delete rows from the User 
		Certificate Table. See the definition for RowStatus for a
		range of valid values used."
	::= { rc3UserCertEntry 4 }

rc3UserCertRDN OBJECT-TYPE
	SYNTAX OCTET STRING
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"User Certificate info. used to generate a PKCS10 info.
        This info. a public/private key pair."
	::= { rc3Cert 3 }

rc3UserCertAlgoId OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"Algorithm used to sign the hash."
	::= { rc3Cert 4 }

rc3GenerateKeyPair OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"If the two items above are set then is used to actually to 
        start generating the key pair."
	::= { rc3Cert 5 }

rc3UserCertGetPkcs10 OBJECT-TYPE
	SYNTAX OCTET STRING
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
    " "
	::= { rc3Cert 6 }

