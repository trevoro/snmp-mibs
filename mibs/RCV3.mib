	DESCRIPTION
		"An entry in the syslog server table.  This table can have
        zero to five entries."
	INDEX { rc3SysLogServerIp, rc3SysLogServerPort }
	::= { rc3SysLogServerTable 1 }

Rc3SysLogServerEntry ::= SEQUENCE {
	rc3SysLogServerIp		IpAddress,
	rc3SysLogServerPort		INTEGER,
    rc3SysLogPriorityLevel  INTEGER,
    rc3SysLogMsgStatus      RowStatus
	}

rc3SysLogServerIp OBJECT-TYPE
    SYNTAX IpAddress
	ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
		"This value is used as a unique identifier for each entry."
	::= { rc3SysLogServerEntry 1 }

rc3SysLogServerPort OBJECT-TYPE
    SYNTAX INTEGER
	ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
		"This value is used as a unique identifier for each entry."
	::= { rc3SysLogServerEntry 2 }

rc3SysLogPriorityLevel OBJECT-TYPE
    SYNTAX INTEGER {
    critical(1),
    severe(2),
    error(3),
    warning(4),
    normal(5),
    debug(6)
    }
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
		"Specify how verbose the message logging should be."
	::= { rc3SysLogServerEntry 3 }

rc3SysLogMsgStatus OBJECT-TYPE
    SYNTAX RowStatus
	ACCESS read-write
    STATUS mandatory
    DESCRIPTION
		"This object is used to create/delete rows from the SYSLOG
		server Table. See the definition for RowStatus for a
		range of valid values used. On read status indicates ACTIVE."
	::= { rc3SysLogServerEntry 4 }

-- =======================
-- SNMP Group
-- =======================

rc3ReadCommunityString OBJECT-TYPE
    SYNTAX DisplayString (SIZE(1..20))
    ACCESS read-write
    STATUS deprecated
    DESCRIPTION
