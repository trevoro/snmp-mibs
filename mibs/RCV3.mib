rc3PdeLocalNetworkTable OBJECT-TYPE
	SYNTAX SEQUENCE OF Rc3PdeLocalNetworkEntry
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION
		"This table provides the selector list containing 1..n
		entry pair(s) of network number and mask for the local
		interface. Entries in this table can be added or deleted."
	::= { rc3Pde 2 }

rc3PdeLocalNetworkEntry OBJECT-TYPE
	SYNTAX Rc3PdeLocalNetworkEntry
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION
		"An entry in the Local Network Selector Table. Rows are created
		by an SNMP SET request seeting the value of rc3PdeLocalNetworkRowStatus
		to 'createAndGo' or 'createAndWait'. Rows are deleted by an SNMP SET
		request setting the value of rc3PdeLocalNetworkRowStatus to 'destroy'."
	INDEX { rc3PdePeerIndx, rc3PdeLocalNetworkNumber, rc3PdeLocalNetworkMask }
	::= { rc3PdeLocalNetworkTable 1 }

Rc3PdeLocalNetworkEntry ::= SEQUENCE {
	rc3PdeLocalNetworkNumber		IpAddress,
	rc3PdeLocalNetworkMask		IpAddress,
	rc3PdeLocalNetworkRowStatus	RowStatus
	}
		
rc3PdeLocalNetworkNumber OBJECT-TYPE
	SYNTAX IpAddress
	ACCESS not-accessible
--	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"The local network address that will be used to match packets for
		this entry in the Policy Database."
	::= { rc3PdeLocalNetworkEntry 1 }

rc3PdeLocalNetworkMask OBJECT-TYPE
	SYNTAX IpAddress
	ACCESS not-accessible
--	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"The local network mask that will be used to match packets for
		this entry in the Policy Database."
	::= { rc3PdeLocalNetworkEntry 2 }

rc3PdeLocalNetworkRowStatus OBJECT-TYPE
	SYNTAX RowStatus
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"This object is used to create/delete rows from the Policy Database
		Entry Local Network Table. See the definition for RowStatus for a range
		of valid values used. On read status indicates ACTIVE."
	::= { rc3PdeLocalNetworkEntry 3 }

