		triple-des-cbc(5),
		des-cbc-40(249)
	}
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"The encryption type.  Currently only des-cbc-56 and 3des-cbc
		and des-cbc-40 are supported."
	::= { rc3IsakmpProposalEntry 2 }

rc3IsakmpProposalHash OBJECT-TYPE
	SYNTAX INTEGER{
		md5(1),
		sha(2)
	}
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"The hash type.  Currently md5 and sha are supported."
	::= { rc3IsakmpProposalEntry 3 }

rc3IsakmpProposalAuthMode OBJECT-TYPE
	SYNTAX INTEGER{
		pre-sharedkey(1),
		dss-signature(2),
		rsa-signature(3),
		rsa-encryption(4)
	}
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"later"
	::= { rc3IsakmpProposalEntry 4 }

rc3IsakmpProposalDhGroup OBJECT-TYPE
	SYNTAX INTEGER{
		group1(1),
		group(2)
	}
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"later"
	::= { rc3IsakmpProposalEntry 5 }

