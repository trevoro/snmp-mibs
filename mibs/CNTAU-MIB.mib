		}

		cnttrapIndex OBJECT-TYPE
			SYNTAX  INTEGER
			ACCESS  read-only
			STATUS  mandatory
			DESCRIPTION
				"Trap table index."
			::= { cnttrapEntry 1 }

		cnttrapAddress OBJECT-TYPE
			SYNTAX  IpAddress
			ACCESS  read-only
			STATUS  mandatory
			DESCRIPTION
				"Trap destination address."
			::= { cnttrapEntry 2 }

		cntsnmpTrapFlags OBJECT-TYPE
			SYNTAX  INTEGER
			ACCESS  read-only
			STATUS  mandatory
			DESCRIPTION
				"Flag setting specifying which types of traps
				to send to this destination.  This value is
