acSonetPathThresholdSESs OBJECT-TYPE
    SYNTAX  INTEGER
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
       "The threshold associated with Severely Errored Seconds."
    DEFVAL { 0 }
    ::= { acSonetPathThresholdEntry 8 }

acSonetPathThresholdCVs OBJECT-TYPE
    SYNTAX  INTEGER
    MAX-ACCESS  read-write
