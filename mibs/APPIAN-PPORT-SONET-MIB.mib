         acSonetVT15MinuteIntervalValidStats TruthValue,
         acSonetVT15MinuteIntervalResetStats TruthValue,
         acSonetVT15MinuteIntervalESs        PerfIntervalCount,
         acSonetVT15MinuteIntervalSESs       PerfIntervalCount,
         acSonetVT15MinuteIntervalCVs        PerfIntervalCount,
         acSonetVT15MinuteIntervalUASs       PerfIntervalCount,
         acSonetVT15MinuteIntervalBER        PerfIntervalCount
    }

