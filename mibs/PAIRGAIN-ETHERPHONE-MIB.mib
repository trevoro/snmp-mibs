pgLocUnitEp10BaseTLinkAlarmMode OBJECT-TYPE
        SYNTAX  INTEGER {
                disable(1),
                enable(2)
                }
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
                "This object is used to enable/disable 10BaseT link alarm."
        ::= { pgLocUnitEpEntry 20 }

pgLocUnitEpEPortInFrames OBJECT-TYPE
        SYNTAX  Counter
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
