   DESCRIPTION
     "A single entry in the table represents a single 
      mscX25Dte component."
   INDEX {
      mscX25DteIndex }
::= { mscX25DteRowStatusTable 1 }

MscX25DteRowStatusEntry ::= SEQUENCE {
   mscX25DteRowStatus
      RowStatus,
   mscX25DteComponentName
      DisplayString,
   mscX25DteStorageType
      StorageType,
   mscX25DteIndex
      Integer32 }

mscX25DteRowStatus OBJECT-TYPE
   SYNTAX     RowStatus 
   ACCESS     read-write
   STATUS     mandatory
