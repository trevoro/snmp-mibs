--  the VNET Voice Channel Control Table 

--  This table provides the VNET voice channel control/action commands.

mcmVNETChControlTable OBJECT-TYPE
	SYNTAX  SEQUENCE OF McmVNETChControlEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ; 
	   DESC = The VNET Voice Channel Control table. \
	   HELP = ;
	   CAPABILITIES = NET_OPER, VPN_OPER ;"
	::= { vnet-control 1 }

mcmVNETChControlEntry OBJECT-TYPE
	SYNTAX  McmVNETChControlEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ; 
	   DESC = An entry in the VNET Voice Channel Control \
		  table.;
	   HELP = ;
	   CAPABILITIES = ;"
	INDEX   { mcmVNETChControlLimID, mcmVNETChControlChannelID }
	::= { mcmVNETChControlTable 1 }

McmVNETChControlEntry ::= SEQUENCE {
	mcmVNETChControlLimID		INTEGER,
	mcmVNETChControlChannelID		INTEGER,
	mcmVNETChControlActionCmd		INTEGER
	}

mcmVNETChControlLimID OBJECT-TYPE
	SYNTAX	INTEGER {
		lim1 (2),
		lim2 (3),
		lim3 (4),
		lim4 (5),
		limB (6),
		limC (7),
		limD (8),
		limE (9)
		}
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The card lim (logical interface module) number of a \
		  particular voice channel.  Used as an index to the \
		  VNET Voice Channel Control table.;
	   HELP = ;
	   CAPABILITIES = ;"
	::= { mcmVNETChControlEntry 1 }

mcmVNETChControlChannelID OBJECT-TYPE
	SYNTAX	INTEGER (1..12)
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The position of the voice channel in the lim. \
		  Used as an index to the VNET Voice \
		  Channel Control table.;
	   HELP = ;
	   CAPABILITIES = ;"
	::= { mcmVNETChControlEntry 2 }

mcmVNETChControlActionCmd OBJECT-TYPE
	SYNTAX	INTEGER {
		resetChannel (1),
		clearChannelCounters (2),
		codeDownload (3),
		disconnect (4),
		loopbackTest (5),
		terminateTests (6),
		forceConnect (7)
		}
	ACCESS  write-only
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ; 
	   DESC = Setting this object will perform various \
		  actions on the specified voice channel. \
		  Note: Starting with software release 3.0, \
		  the enumeration clearChannelCounters (2) is \
		  not supported. The enumeration is included \
		  here only to support backwards compatibility \
		  for software versions prior to release 3.0.; 
	   HELP = 1: perform hardware reset to DSP. \
		  2: clear statistics counter. \
		  3: download code to DSP. \
		  4: disconnect call. \
		  5: set DSP to loopback mode. \
		  6: terminate test. \
		  7: force connect channel by using the \
		  auto call number. NOTE: This option is not \
                  supported by the ISDN BRI voice card.;
	   CAPABILITIES = ;"
	::= { mcmVNETChControlEntry 3 }


--  the VNET Voice System Control Group 

--  This object provides the VNET voice system control/action commands.

mcmVNETSysControlActionCmd OBJECT-TYPE
	SYNTAX	INTEGER {
		clearNetworkCounters (1),
		resetChannels (2),
		clearChannelCounters (3),
		download (4),
		disconnect (5),
		loopbackTests (6),
		terminateTests (7),
		forceConnects (8),
                reRegisterPhoneNum (9)
		}
	ACCESS  write-only
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ; 
	   DESC = Setting this object will perform various \
		  actions on the VNET voice system. \
		  Note: Starting with software release 3.0, \
		  the enumerations clearNetworkCounters (1) and \
		  clearChannelCounters (3) are not supported. \
		  The enumeration is included here only to support \
		  backwards compatibility for software versions \
		  prior to release 3.0.; 
	   HELP = 1: clear network statistics counters. \
		  2: perform hardware reset to all channels. \
		  3: clear all channel statistics counters. \
		  4: download code to all channels. \
		  5: disconnect all channel's calls. \
		  6: set all channels to loopback mode. \
		  7: terminate all channel's tests. \
		  8: force connect all channels by using the 
		  auto call numbers. NOTE: This option is not \
                  supported by the ISDN BRI voice card.; \
                  9: re-register phone number.;
	   CAPABILITIES = NET_OPER, VPN_OPER ;"
	::= { vnet-control 2 }

