--  "Parallel" Configuration Parameter ("nvm") Groups/Tables


--  the VNET Voice Channel Configuration Table 
--  (Configuration Parameters)

--  This table provides the VNET voice channel configuration parameters
--  for identifying a voice channel with its corresponding voice profile. (Configuration)

nvmVNETChCfgTable OBJECT-TYPE
	SYNTAX  SEQUENCE OF NvmVNETChCfgEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ; 
	   DESC = The VNET Voice Channel Configuration table. \
		  (Configuration);
	   HELP = ;
	   CAPABILITIES = NET_CFG, VPN_CFG;"
	::= { vnet-configuration 6 }

nvmVNETChCfgEntry OBJECT-TYPE
	SYNTAX  NvmVNETChCfgEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ; 
	   DESC = An entry in the VNET Voice Channel Configuration \
		  table. (Configuration);
	   HELP = ;
	   CAPABILITIES = ;"
	INDEX   { nvmVNETChCfgLimID, nvmVNETChCfgChannelID }
	::= { nvmVNETChCfgTable 1 }

NvmVNETChCfgEntry ::= SEQUENCE {
	nvmVNETChCfgLimID			INTEGER,
	nvmVNETChCfgChannelID			INTEGER,
	nvmVNETChCfgVoiceProfile		INTEGER,
	nvmVNETChCfgSwitchingProfile		INTEGER,
	nvmVNETChCfgInterfaceType		INTEGER
	}

nvmVNETChCfgLimID OBJECT-TYPE
	SYNTAX	INTEGER {
		lim1 (2),
		lim2 (3),
		lim3 (4),
		lim4 (5),
		limB (6),
		limC (7),
		limD (8),
		limE (9)
		}
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The card lim (logical interface module) number of a \
		  particular voice channel.  Used as an index to the \
		  VNET Voice Channel Configuration table. (Configuration);
	   HELP = ;
	   CAPABILITIES = ;"
	::= { nvmVNETChCfgEntry 1 }

nvmVNETChCfgChannelID OBJECT-TYPE
	SYNTAX	INTEGER (1..12)
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
  "NAME = ;
	   DESC = The position of the voice channel in the lim. \
		  Used as an index to the VNET Voice \
		  Channel Configuration table. (Configuration);
	   HELP = ;
	   CAPABILITIES = ;"
	::= { nvmVNETChCfgEntry 2 }

nvmVNETChCfgVoiceProfile OBJECT-TYPE
	SYNTAX	INTEGER (1..32)
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ; 
	   DESC = The value of this object indicates the profile \
		  to be used to configure the specified voice \
		  channel. (Configuration);
	   HELP = ;
	   CAPABILITIES = ;"
	DEFVAL { 1 }
	::= { nvmVNETChCfgEntry 3 }

nvmVNETChCfgSwitchingProfile OBJECT-TYPE
	SYNTAX	INTEGER (1..32)
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ; 
	   DESC = The value of this object indicates the profile \
		  to be used for a switching call. (Configuration);
	   HELP = ;
	   CAPABILITIES = ;"
	DEFVAL { 1 }
	::= { nvmVNETChCfgEntry 4 }

nvmVNETChCfgInterfaceType OBJECT-TYPE
	SYNTAX	INTEGER {
		not-available (1),
		enhanced-loop-start-FXS (2),
		enhanced-EandM (3),
		enhanced-loop-start-FXO (4),
		low-cost-loop-start-FXS (5),
		low-cost-EandM (6),
		low-cost-loop-start-FXO (7),
		ground-start-FXS (8),
		ground-start-FXO (9),
		three-port (10),
		dvm (11),
		bri (12)
		}
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ; 
	   DESC = The value of this object indicates the interface \
		  type of the specified voice channel. (Configuration);
	   HELP = Upon powerup, the interface type will not be \
		  available until the DSP software is loaded and its \
		  status is reported.;
	   CAPABILITIES = ;"
	::= { nvmVNETChCfgEntry 5 }


