            MRV-IN-REACH-LAT-MIB DEFINITIONS ::= BEGIN

                    IMPORTS
                            Counter, Gauge, TimeTicks
                                    FROM RFC1155-SMI
                            DisplayString
                                    FROM RFC1213-MIB
                            OBJECT-TYPE
                                    FROM RFC-1212
	                    mrvInReachProductDivision
                               FROM MRV-IN-REACH-PRODUCT-DIVISION-MIB;

            xLat            OBJECT IDENTIFIER ::= { mrvInReachProductDivision 3 }



