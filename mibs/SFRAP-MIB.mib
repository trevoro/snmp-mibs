--
--     Table of peers discovered by VNIP
--

sfrapVnipTopologyTable   OBJECT-TYPE
      SYNTAX   SEQUENCE OF SfrapVnipTopologyEntry
      ACCESS   not-accessible
      STATUS   mandatory
      DESCRIPTION
         "Topology of peers discovered via VNIP, organized
          by the interface (DATA1 or DATA2) from which they were
          discovered and an entry number or index. The
          topology is of limited size per interface."
      ::= { sfrapStatus 2 }

      sfrapVnipTopologyEntry OBJECT-TYPE
          SYNTAX   SfrapVnipTopologyEntry
          ACCESS   not-accessible
          STATUS   mandatory
          DESCRIPTION
             "The DLCI, IP address, and number of hops
              for a particular node, discovered via VNIP
              off of an interface (DATA1 or DATA2).  The entry
              may also have transit delay measurements
              for the particular node."
         INDEX { sfrapVnipTopologyInterface, sfrapVnipTopologyIndex }
         ::= { sfrapVnipTopologyTable 1 }

      SfrapVnipTopologyEntry ::=
          SEQUENCE {
              sfrapVnipTopologyInterface          INTEGER,
              sfrapVnipTopologyIndex              INTEGER,
              sfrapVnipTopologyDlci               INTEGER,
              sfrapVnipTopologyIpAddr             IpAddress,
              sfrapVnipTopologyNumHops            INTEGER,
              sfrapVnipTopologyLocalDlci          INTEGER,
              sfrapVnipTopoTDNumSamples           Counter,
              sfrapVnipTopoTDAvgDelay             Counter,
              sfrapVnipTopoTDMaxDelay             Counter,
              sfrapVnipTopoTDMinDelay             Counter,
              sfrapVnipTopoTDLastDelay            Counter,
              sfrapVnipTopoVLOOPStatus            INTEGER,
              sfrapVnipTopoVBERTStatus            INTEGER,
              sfrapVnipTopoVBertTxDESetFrames     Counter,
              sfrapVnipTopoVBertRxDESetFrames     Counter,
              sfrapVnipTopoVBertTxDEClrFrames     Counter,
              sfrapVnipTopoVBertRxDEClrFrames     Counter,
              sfrapVnipTopoVBertTransitDelayMax   Counter,
              sfrapVnipTopoVBertTransitDelayAvg   Counter,
              sfrapVnipTopoVBertTimeElapse        TimeTicks,
              sfrapVnipTopoVBertPerUtilCIR        INTEGER,
              sfrapVnipTopoVBertPerUtilEIR        INTEGER
          }

      sfrapVnipTopologyInterface OBJECT-TYPE
          SYNTAX      INTEGER
          {
               data1-interface (1),
               data2-interface (2)
          }
          ACCESS      read-only
          STATUS      mandatory
          DESCRIPTION
             "The interface off of which the peer was
              discovered.  Topology is discovered by
              sending VNIP messages out each interface.
              Nodes discovered via a particular interface
              are kept in a list associated with that
              interface."
          ::= { sfrapVnipTopologyEntry 1 }

      sfrapVnipTopologyIndex OBJECT-TYPE
          SYNTAX      INTEGER
          ACCESS      read-only
          STATUS      mandatory
          DESCRIPTION
             "The number of this discovered peer in the list
              of nodes for this interface. For each interface,
              the nodes are numbered 1 through n."
          ::= { sfrapVnipTopologyEntry 2 }

      sfrapVnipTopologyDlci    OBJECT-TYPE
          SYNTAX      INTEGER 
          ACCESS      read-only
          STATUS      mandatory
          DESCRIPTION
            "The DLCI of the discovered neighboring peer.
             This may be different from the local DLCI."
         ::= { sfrapVnipTopologyEntry 3 }

      sfrapVnipTopologyIpAddr   OBJECT-TYPE
          SYNTAX      IpAddress
          ACCESS      read-only
          STATUS      mandatory
          DESCRIPTION
             "The IP address for the discovered peer."
          ::= { sfrapVnipTopologyEntry 4 }
 
      sfrapVnipTopologyNumHops OBJECT-TYPE
          SYNTAX      INTEGER
          ACCESS      read-only
          STATUS      mandatory
          DESCRIPTION
             "The discovered peer is this number of hops away."
          ::= { sfrapVnipTopologyEntry 5 }
 
      sfrapVnipTopologyLocalDlci OBJECT-TYPE
          SYNTAX      INTEGER
          ACCESS      read-only
          STATUS      mandatory
          DESCRIPTION
            "The DLCI of this node over which the remote peer
             was discovered."
          ::= { sfrapVnipTopologyEntry 6 }

      sfrapVnipTopoTDNumSamples  OBJECT-TYPE
          SYNTAX      Counter
          ACCESS      read-only
          STATUS      mandatory
          DESCRIPTION
            "The number of transit delay samples collected."
          ::= { sfrapVnipTopologyEntry 10 }

      sfrapVnipTopoTDAvgDelay  OBJECT-TYPE
          SYNTAX      Counter
          ACCESS      read-only
          STATUS      mandatory
          DESCRIPTION
            "The average transit delay between this
             node and the remote peer."
          ::= { sfrapVnipTopologyEntry 11 }

      sfrapVnipTopoTDMaxDelay  OBJECT-TYPE
          SYNTAX      Counter
          ACCESS      read-only
          STATUS      mandatory
          DESCRIPTION
            "The maximum transit delay between this
             node and the remote peer."
          ::= { sfrapVnipTopologyEntry 12 }

      sfrapVnipTopoTDMinDelay  OBJECT-TYPE
          SYNTAX      Counter
          ACCESS      read-only
          STATUS      mandatory
          DESCRIPTION
            "The minimum transit delay between this
             node and the remote peer."
          ::= { sfrapVnipTopologyEntry 13 }

      sfrapVnipTopoTDLastDelay  OBJECT-TYPE
          SYNTAX      Counter
          ACCESS      read-only
          STATUS      mandatory
          DESCRIPTION
            "The most recent transit delay measured between
             this node and the remote peer."
          ::= { sfrapVnipTopologyEntry 14 }



      sfrapVnipTopoVLOOPStatus   OBJECT-TYPE
          SYNTAX      INTEGER
          {
               loopback-enable (1),
               loopback-disable (2)
          }
          ACCESS      read-only
          STATUS      mandatory
          DESCRIPTION
             "This selection displays the status of the VNIP Loopback
              This loopback is initiated by the remote node, causing
              this node to loop data back to the remote node."
          ::= { sfrapVnipTopologyEntry  15 }
      

      sfrapVnipTopoVBERTStatus  OBJECT-TYPE
          SYNTAX      INTEGER
          {
               off            (1),
               testing        (2),
               test-failed    (3),
               test-completed (4),
               in-test        (5)
          }
          ACCESS      read-only
          STATUS      mandatory
          DESCRIPTION
             "Displays the current status of the VBERT test, values are
              off(1): no test has run or the entry has been cleared, 
              testing(2): the entry is generating VBERT packets, 
              in-test(5): the entry is on the receiving end of VBERT packets
              test-completed(4): a test has run and is finished results are complete
              test-failed(3): the request for a test on this entry failed"
          ::= { sfrapVnipTopologyEntry 16 }
  

      sfrapVnipTopoVBertTxDESetFrames   OBJECT-TYPE
          SYNTAX      Counter
          ACCESS      read-only
          STATUS      mandatory
          DESCRIPTION
             "Displays the number of Frames transmitted during VBERT Test that
              had the Discard Eligibility indicator bit set."
          ::= { sfrapVnipTopologyEntry  17 }


     
      sfrapVnipTopoVBertRxDESetFrames OBJECT-TYPE
          SYNTAX      Counter
          ACCESS      read-only
          STATUS      mandatory
          DESCRIPTION
             "Displays the number of Frames received during VBERT Test that 
              had the Discard Eligibility indicator bit set."
          ::= { sfrapVnipTopologyEntry  18 }
      
      
      
      sfrapVnipTopoVBertTxDEClrFrames   OBJECT-TYPE
          SYNTAX      Counter
          ACCESS      read-only
          STATUS      mandatory
          DESCRIPTION
             "Displays the number of Frames transmitted during VBERT Test that
              had the Discard Eligibility indicator bit cleared."
          ::= { sfrapVnipTopologyEntry  19 }
      
      
      sfrapVnipTopoVBertRxDEClrFrames OBJECT-TYPE
          SYNTAX      Counter
          ACCESS      read-only
          STATUS      mandatory
          DESCRIPTION
             "Displays the number of Frames received during VBERT Test that 
              had the Discard Eligibility indicator bit cleared."
          ::= { sfrapVnipTopologyEntry  20 }
      
      sfrapVnipTopoVBertTransitDelayMax OBJECT-TYPE
          SYNTAX      Counter
          ACCESS      read-only
          STATUS      mandatory
          DESCRIPTION
             "The maximum transit delay between this
              node and the remote peer during the VBERT test."
          ::= { sfrapVnipTopologyEntry  21 }
      
       
      sfrapVnipTopoVBertTransitDelayAvg OBJECT-TYPE
          SYNTAX      Counter
          ACCESS      read-only
          STATUS      mandatory
          DESCRIPTION
             "The average transit delay between this
              node and the remote peer during the VBERT test."
          ::= { sfrapVnipTopologyEntry  22 }
      
      sfrapVnipTopoVBertTimeElapse OBJECT-TYPE
          SYNTAX      TimeTicks
          ACCESS      read-only
          STATUS      mandatory
          DESCRIPTION
             "Elapsed time since VBERT test was started or cleared."
          ::= { sfrapVnipTopologyEntry  23 }


      sfrapVnipTopoVBertPerUtilCIR OBJECT-TYPE
          SYNTAX      INTEGER
          ACCESS      read-only
          STATUS      mandatory
          DESCRIPTION
             " The calculated percent of CIR utilization during a VBERT test,
               this value is only valid after a test is complete not during."
          ::= { sfrapVnipTopologyEntry 24 }


      sfrapVnipTopoVBertPerUtilEIR OBJECT-TYPE
          SYNTAX      INTEGER
          ACCESS      read-only
          STATUS      mandatory
          DESCRIPTION
             " The calculated percent of EIR utilization during a VBERT test,
               this value is only valid after a test is complete not during."
          ::= { sfrapVnipTopologyEntry 25 }


sfrapVnipTransitDelayClear    OBJECT-TYPE
          SYNTAX      INTEGER
          {
              clear-transit-delay (1)
          }
          ACCESS      write-only
          STATUS      mandatory
          DESCRIPTION
             "Allows the user to clear all the VNIP Transit Delay
              data collected in the VNIP topology database."
          ::= { sfrapStatus 5 }

sfrapLmiSourcing OBJECT-TYPE
         SYNTAX      INTEGER
         {
              initializing (1),
