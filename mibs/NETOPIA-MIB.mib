    ::= { aurpTunnelEntry 10 }

aurpTunnelRemapErrors OBJECT-TYPE
    SYNTAX    Counter
    ACCESS    read-only
    STATUS    mandatory
    DESCRIPTION
              "The number of remapping-specific errors that have occurred
              on this tunnel."
    ::= { aurpTunnelEntry 11 }

aurpTunnelClusterErrors OBJECT-TYPE
    SYNTAX    Counter
    ACCESS    read-only
    STATUS    mandatory
    DESCRIPTION
              "The number of clustering-specific errors that have occurred
              on this tunnel."
    ::= { aurpTunnelEntry 12 }

aurpTunnelBrokenConnections OBJECT-TYPE
    SYNTAX    Counter
    ACCESS    read-only
    STATUS    mandatory
    DESCRIPTION
              "The number of times a point-to-point connection on this
              tunnel has been broken or dropped."
    ::= { aurpTunnelEntry 13 }

aurpTunnelInvalidVersionErrors OBJECT-TYPE
    SYNTAX    Counter
    ACCESS    read-only
    STATUS    mandatory
    DESCRIPTION
