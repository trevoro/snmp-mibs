    ::= { hwMusaIpAddrRejectEntry 5 }

    hwMusaAtmIpAddr    OBJECT-TYPE
        SYNTAX   IpAddress
        ACCESS   read-write
        STATUS   mandatory
    DESCRIPTION
    "The Musa device's IP address."
    ::= { hwMusaDevice 20}
    
    hwMusaAtmIpMask        OBJECT-TYPE
        SYNTAX   IpAddress
        ACCESS   read-write
        STATUS   mandatory
    DESCRIPTION
    "The Musa IP SubNet Mask."
    ::= { hwMusaDevice 21}
    
    hwMusaMtu    OBJECT-TYPE
