-- PNNI Pc Profile Map Table

pnniPcProfileMapTable OBJECT-TYPE
   SYNTAX SEQUENCE OF PnniPcProfileMapEntry
   MAX-ACCESS not-accessible
   STATUS current               
   DESCRIPTION
        "PNNI Precomputed Paths Map Table."
   ::= { pnniGroup 6 }

pnniPcProfileMapEntry OBJECT-TYPE
    SYNTAX PnniPcProfileMapEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        ""
    INDEX { pnniNodeIndex,
	    pnniPcProfileIndex,
	    pnniPcProfileMapOptIndex,
	    pnniPcProfileMapNodeId }
    ::= { pnniPcProfileMapTable 1 }

PnniPcProfileMapEntry ::= SEQUENCE {
    pnniPcProfileMapOptIndex		Integer32,
    pnniPcProfileMapNodeId		PnniNodeId,
    pnniPcProfileMapLocalPort		PnniPortId,
    pnniPcProfileMapParentNode		PnniNodeId,
    pnniPcProfileMapAdminWt		Unsigned32,
    pnniPcProfileMapMaximumCellRate	Unsigned32,
    pnniPcProfileMapAvailableCellRate	Unsigned32,
