sonusNrsDebugIfTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF SonusNrsDebugIfEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "The collection of Debug Interface information for the
             specified instance."
    ::= { sonusIpInterfaceMIBObjects 99 }

sonusNrsDebugIfEntry OBJECT-TYPE
    SYNTAX          SonusNrsDebugIfEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "The collection of Debug Interface information for the
             specified instance."
    INDEX           { sonusNrsDebugIfShelf, sonusNrsDebugIfSlot }
    ::= { sonusNrsDebugIfTable 1 }

SonusNrsDebugIfEntry ::=
    SEQUENCE {
        sonusNrsDebugIfIndex            Integer32,
        sonusNrsDebugIfShelf            Integer32,
        sonusNrsDebugIfSlot             Integer32,
        sonusNrsDebugIfPort             Integer32,
        sonusNrsDebugIfCircuit          Integer32,
        sonusNrsDebugIfType             IANAifType,
        sonusNrsDebugIfIpAddress        IpAddress,
        sonusNrsDebugIfIpMask           IpAddress,
        sonusNrsDebugIfMtu              Integer32,
        sonusNrsDebugIfGatewayAddress   IpAddress,
        sonusNrsDebugIfAdminStatus      INTEGER,
        sonusNrsDebugIfOperStatus       INTEGER
    }

sonusNrsDebugIfIndex OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The ifIndex assigned to this debug NIF when it was created.
             This will not change until the NIF is deleted."
    ::= { sonusNrsDebugIfEntry 1 }

sonusNrsDebugIfShelf OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "A 32-bit integer identifying the shelf of the Debug
             Interface"
    ::= { sonusNrsDebugIfEntry 2 }

sonusNrsDebugIfSlot OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "A 32-bit integer identifying the slot of the Debug
             Interface"
    ::= { sonusNrsDebugIfEntry 3 }

sonusNrsDebugIfPort OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "A 32-bit integer identifying the port of the Debug
             Interface.  For current hardware this must be 1."
    ::= { sonusNrsDebugIfEntry 4 }

sonusNrsDebugIfCircuit OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "A 32-bit integer identifying the circuit of the Debug
             Interface.  For current hardware this must be 1."
    ::= { sonusNrsDebugIfEntry 5 }

sonusNrsDebugIfType OBJECT-TYPE
    SYNTAX          IANAifType
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The type of interface, distinguished according to the
             physical/link protocol(s) immediately 'below' the network
             layer in the protocol stack.
             
             Values are from RFC1213 for the ifType object.
             
             Current hardware supports only the ethernet-csmacd (6)
             type."
    ::= { sonusNrsDebugIfEntry 6 }

sonusNrsDebugIfIpAddress OBJECT-TYPE
    SYNTAX          IpAddress
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The IP Address of the Debug Interface"
    ::= { sonusNrsDebugIfEntry 7 }

sonusNrsDebugIfIpMask OBJECT-TYPE
    SYNTAX          IpAddress
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The IP subnet mask of this Debug Interface."
    ::= { sonusNrsDebugIfEntry 8 }

sonusNrsDebugIfMtu OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The size of the largest datagram that can be sent/received
             on the interface, specified in octets."
    ::= { sonusNrsDebugIfEntry 9 }

sonusNrsDebugIfGatewayAddress OBJECT-TYPE
    SYNTAX          IpAddress
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The IP address of the default gateway for this Debug
             Interface."
    ::= { sonusNrsDebugIfEntry 10 }

sonusNrsDebugIfAdminStatus OBJECT-TYPE
    SYNTAX          INTEGER { disabled(1), enabled(2) }
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The requested status of the interface."
    ::= { sonusNrsDebugIfEntry 11 }

sonusNrsDebugIfOperStatus OBJECT-TYPE
    SYNTAX          INTEGER { outOfService(1), inService(2), 
                        outOfServiceDisabled(3), outOfServiceLinkDown(4) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The actual status of the debug interface.  A link will
             be InService if the sonusNrsDebugIfAdminStatus is enabled and
             the physical link is up.  It will be OutOfService if the admin
             status is enabled but the physical link is down.  It will be
             OutOfServiceDisabled if the physical link is up but the admin
             status is disabled.  It will be OutOfServiceLinkDown if the
             physical link is down and the admin status is disabled."
    ::= { sonusNrsDebugIfEntry 12 }

