-- *************************************************************************
-- *************************************************************************
-- TapeAlert (1.3.6.1.4.1.11.2.3.9.7.1)
-- Issue: v2.1
-- Date: 16th May 1998
-- Changes from v2.0:
--    Added new flag 19 from v2.1 spec
-- Changes from v1.2:
--    Added new flags for library device support from TapeAlert v2 spec
--    Added new flags for tape drive/autoloader support from v2 spec
--    Updated description for errorTrapNum to reflect library flags 255 offset
--
-- Changes from v1.1: 
--    Changed trap definitions for Managewise compilation
-- Changes from v1.0: 
--    Minor change to errorDate field making it 14 octets long rather than 7
--
-- Overall MIB Structure
--
--      tapeAlert
--      .1      control
--              .1      revMajor                (used for presence checking)
--              .2      revMinor
--              .3      trapEnable              (optional)
--              .4      autoCleanEnable         (optional)
--              .5      autoCopyEnable          (optional)
--      .2      numDevices
--      .3      {devTable}                      (table of device information)
--              .1      devInfo                 (device information entry in table)
--                      .1      devNum          (index)
--                      .2      tapeAlertCapability
--                      .3      hbaName
--                      .4      hbaNumber
--                      .5      hbaChannel
--                      .6      driveScsiID
--                      .7      driveInquiry
--                      .8      driveFirmware
--                      .9      driveType
--                      .10     driveCompress
--                      .11     currentMedia
--                      .12     driveSerialNum  (optional)
--      .4      numErrors
--      .5      {errorHistoryTable}             (table of error history)
--              .1      errorHistoryEntry       (error entry in table)
--                      .1      errorIndex      (index)
--                      .2      errorHbaNumber
--                      .3      errorHbaChannel
--                      .4      errorDriveScsiID
--                      .5      errorDate
--                      .6      errorTrapNum
--                      .7      errorMedia
-- **************************************************************************
-- **************************************************************************

TapeAlert-MIB DEFINITIONS ::= BEGIN										

	IMPORTS
		enterprises    FROM RFC1155-SMI
		OBJECT-TYPE    FROM RFC-1212
		DisplayString  FROM RFC1213-MIB
		TRAP-TYPE      FROM RFC-1215;
		
		hp	       OBJECT IDENTIFIER ::= { enterprises 11 }
		nm	       OBJECT IDENTIFIER ::= { hp 2 }
		system	       OBJECT IDENTIFIER ::= { nm 3 }
		netPeripheral  OBJECT IDENTIFIER ::= { system 9 }
		netTape	       OBJECT IDENTIFIER ::= { netPeripheral 7 }
                tapeAlert      OBJECT IDENTIFIER ::= { netTape 1 }
                control        OBJECT IDENTIFIER ::= { tapeAlert 1 }

-- **************************************************************************
-- control group definitions
-- **************************************************************************

revMajor OBJECT-TYPE
	SYNTAX INTEGER (0..255)
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"The TapeAlert major revision level (left of the decimal).
		Used for presence checking."
        ::= { control 1 }

revMinor OBJECT-TYPE
	SYNTAX INTEGER (0..255)
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"The TapeAlert minor revision level (right of the decimal)."
        ::= { control 2 }

trapEnable OBJECT-TYPE
	SYNTAX INTEGER (0..1)
	ACCESS read-write
	STATUS optional
	DESCRIPTION
		"Indicates whether agent should or should not send TapeAlert traps."
        ::= { control 3 }

autoCleanEnable OBJECT-TYPE
	SYNTAX INTEGER (0..1)
	ACCESS read-write
	STATUS optional
	DESCRIPTION
		"Indicates whether application should or should not perform
		an automatic cleaning operation based on TapeAlert cleaning
		information."
        ::= { control 4 }

autoCopyEnable OBJECT-TYPE
	SYNTAX INTEGER (0..1)
	ACCESS read-write
	STATUS optional
	DESCRIPTION
		"Indicates whether application should or should not perform
		an automatic tape copy or tape discard operation based on
		TapeAlert media information."
        ::= { control 5 }

-- **************************************************************************
-- numDevices definition
-- Total number of tape devices attached to server.
-- **************************************************************************

numDevices OBJECT-TYPE
        SYNTAX INTEGER (0..255)
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
                "Total number of tape devices attached to server and
		managed by TapeAlert agent. Zero means no devices present."
        ::= { tapeAlert 2 }

-- **************************************************************************
-- devTable definition
-- A table of tape device information, consisting of a list of devInfo
-- entries. The number of entries is given by the value of numDevices.
-- **************************************************************************

devTable OBJECT-TYPE
        SYNTAX SEQUENCE OF DevInfo
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION
		"A list of tape device information entries. The number
                of entries is given by the value of numDevices."
        ::= { tapeAlert 3 }

-- **************************************************************************
-- devInfo definition
-- A tape device information entry, containing configuration information for 
-- a particular tape device
-- **************************************************************************

devInfo OBJECT-TYPE
        SYNTAX DevInfo
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION
		"A tape device information entry, containing configuration
		information for a particular tape device."
        INDEX { devNum }
        ::= { devTable 1 }

DevInfo ::= SEQUENCE
{
        devNum                  INTEGER (0..255),
        tapeAlertCapability     INTEGER (0..1),
        hbaName                 DisplayString,
        hbaNumber               INTEGER (0..255),
        hbaChannel              INTEGER (0..255),
        driveScsiID             INTEGER (0..255),
        driveInquiry            DisplayString,
        driveFirmware           DisplayString,
        driveType               DisplayString,
        driveCompress           INTEGER (0..1),
        currentMedia            DisplayString,
        driveSerialNum          DisplayString
}

devNum OBJECT-TYPE
	SYNTAX INTEGER (0..255)
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"A unique index value for each tape device. Its value ranges
                between 1 and the value of numDevices."
        ::= { devInfo 1 }

tapeAlertCapability OBJECT-TYPE
	SYNTAX INTEGER (0..1)
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"Indicates whether tape device supports TapeAlert, with
		zero indicating not supported and one indicating supported"
        ::= { devInfo 2 }

hbaName OBJECT-TYPE
	SYNTAX DisplayString (SIZE (0..255))
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"Name of SCSI Host Bus Adaptor connected to tape drive,
		for example Adaptec AHA2740AT."
        ::= { devInfo 3 }

hbaNumber OBJECT-TYPE
	SYNTAX INTEGER (0..255)
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"The number of SCSI Host Bus Adaptor connected
		to tape drive, for example 0 is first HBA on the system."
        ::= { devInfo 4 }

hbaChannel OBJECT-TYPE
	SYNTAX INTEGER (0..255)
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"SCSI Channel number of SCSI Host Bus Adaptor connected
		to tape drive, for example 0 is first SCSI channel of HBA."
        ::= { devInfo 5 }

driveScsiID OBJECT-TYPE
	SYNTAX INTEGER (0..255)
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"SCSI ID of Tape Device."
        ::= { devInfo 6 }

driveInquiry OBJECT-TYPE
	SYNTAX DisplayString (SIZE (0..255))
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"SCSI Inquiry data of Tape Device, with vendor ID data
		followed by device ID data."
        ::= { devInfo 7 }

driveFirmware OBJECT-TYPE
	SYNTAX DisplayString (SIZE (0..255))
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"Firmware revision of Tape Device."
        ::= { devInfo 8 }

driveType OBJECT-TYPE
	SYNTAX DisplayString (SIZE (0..255))
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"Device type of Tape Device, for example DDS1"
        ::= { devInfo 9 }

driveCompress OBJECT-TYPE
	SYNTAX INTEGER (0..1)
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"Indicates whether Tape Device supports hardware compression,
		with zero indicating not supported and one indicating supported."
        ::= { devInfo 10 }

currentMedia OBJECT-TYPE
        SYNTAX DisplayString (SIZE (0..255))
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
                "Software label of current media loaded in drive"
        ::= { devInfo 11 }

driveSerialNum OBJECT-TYPE
	SYNTAX DisplayString (SIZE (0..255))
	ACCESS read-only
	STATUS optional
	DESCRIPTION
		"Hardware Serial Number of Tape Device, if available"
        ::= { devInfo 12 }

-- **************************************************************************
-- numErrors definition
-- Total number of TapeAlert error traps logged by TapeAlert agent since
-- agent was initialised.
-- **************************************************************************

numErrors OBJECT-TYPE
        SYNTAX INTEGER (0..255)
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
                "Total number of TapeAlert error traps logged by TapeAlert
		agent since agent was initialised. Zero means that no
		TapeAlert trap events occurred since initialisation."
        ::= { tapeAlert 4 }

-- **************************************************************************
-- errorHistoryTable definition
-- This table will provide a historical list of all TapeAlert errors since
-- the TapeAlert agent was initialised. This will provide the ability to show
-- the error history of a specified tape device and identify any trends
-- **************************************************************************

errorHistoryTable OBJECT-TYPE
        SYNTAX SEQUENCE OF ErrorHistoryEntry
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION
		"A list of historical error entries for all tape
		devices on the server. The list should contain 
		an entry for each TapeAlert trap sent since the
		TapeAlert agent was initialised."
        ::= { tapeAlert 5 }

-- **************************************************************************
-- errorHistoryEntry definition
-- An error entry in the errorHistoryTable, containing information on a 
-- historic TapeAlert trap sent by the TapeAlert agent
-- **************************************************************************

errorHistoryEntry OBJECT-TYPE
        SYNTAX ErrorHistoryEntry
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION
                "An error entry in the errorHistoryTable, containing
		information on a historic TapeAlert trap sent by the
		TapeAlert agent"
        INDEX { errorIndex }
        ::= { errorHistoryTable 1 }

ErrorHistoryEntry ::= SEQUENCE
{
        errorIndex              INTEGER (0..255),
        errorHbaNumber          INTEGER (0..255),
        errorHbaChannel         INTEGER (0..255),
        errorDriveScsiID        INTEGER (0..255),
        errorDate               OCTET STRING,
        errorTrapNum            INTEGER (1..64),
        errorMedia              DisplayString
}

errorIndex OBJECT-TYPE
	SYNTAX INTEGER (0..255)
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"A unique index value for each error entry. Its value ranges
                between 1 and the value of numErrors."
        ::= { errorHistoryEntry 1 }

errorHbaNumber OBJECT-TYPE
	SYNTAX INTEGER (0..255)
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"The number of SCSI Host Bus Adaptor connected to
		the tape drive that caused this specific TapeAlert
		error, for example 0 is first HBA on the system."
        ::= { errorHistoryEntry 2 }

errorHbaChannel OBJECT-TYPE
	SYNTAX INTEGER (0..255)
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"SCSI channel number of SCSI Host Bus Adaptor connected
		to the tape drive that caused this specific TapeAlert
		error, for example 0 is first SCSI channel of HBA."
        ::= { errorHistoryEntry 3 }

errorDriveScsiID OBJECT-TYPE
	SYNTAX INTEGER (0..255)
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"SCSI of the tape drive that caused this specific TapeAlert
		error, for example 0 is first SCSI channel of HBA."
        ::= { errorHistoryEntry 4 }

errorDate OBJECT-TYPE
	SYNTAX OCTET STRING (SIZE (14))
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
            "Date and time of the error in yyyymmddHHMMSS format:
             where
              yyyy    year    1980-9999
              mm      month   01-12
              dd      day     01-31
              HH      hour    00-23
              MM      minute  00-59
              SS      second  00-60 (use 60 for leap-second)
             Example  14:35:26 5th Oct 1996 = 19961005143526"
        ::= { errorHistoryEntry 5 }

errorTrapNum OBJECT-TYPE
	SYNTAX INTEGER (1..64)
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"TapeAlert trap number of the error.
                 For tape drive/autoloader traps, range 1-64.
                 For library device traps, range 256-319
                 (Library device traps = TapeAlert flag + 255)"
        ::= { errorHistoryEntry 6 }

errorMedia OBJECT-TYPE
	SYNTAX DisplayString (SIZE (0..255))
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"Software label of the media that was loaded in the
		tape drive at the time of the error."
        ::= { errorHistoryEntry 7 }

-- **************************************************************************
-- Trap Definitions
-- **************************************************************************

tapeAlertTrap1                  TRAP-TYPE       
                                ENTERPRISE      tapeAlert
                                VARIABLES       {
                                                hbaNumber,
                                                hbaChannel,
                                                driveScsiID
                                                }
        			DESCRIPTION
                                        "The tape drive is having problems reading data. No
					data has been lost, but there has been a reduction
					in the performance of the tape."
                                --#TYPE "HP TapeAlert #1"
                                --#SUMMARY "Hba%d:Channel%d:ScsiID%d Tape Device Has Problems Reading Data"
                                --#ARGUMENTS {0,1,2}
                                --#SEVERITY MINOR
                                --#TIMEINDEX 9
				--#HELP "ta_traps.hlp"
				--#HELPTAG 1
                                --#STATE   OPERATIONAL     
        	                ::= 1

tapeAlertTrap2                  TRAP-TYPE       
                                ENTERPRISE      tapeAlert
                                VARIABLES       {
                                                hbaNumber,
                                                hbaChannel,
                                                driveScsiID
                                                }
        			DESCRIPTION
                                        "The tape drive is having problems writing data. No
					data has been lost, but there has been a reduction
					in the capacity of the tape."
                                --#TYPE "HP TapeAlert #2"
                                --#SUMMARY "Hba%d:Channel%d:ScsiID%d Tape Device Has Problems Writing Data"
                                --#ARGUMENTS {0,1,2}
                                --#SEVERITY MINOR
                                --#TIMEINDEX 9
					  --#HELP "ta_traps.hlp"
                                --#HELPTAG 2
                                --#STATE   OPERATIONAL     
        	                ::= 2

tapeAlertTrap3                  TRAP-TYPE       
                                ENTERPRISE      tapeAlert
                                VARIABLES       {
                                                hbaNumber,
                                                hbaChannel,
                                                driveScsiID
                                                }
        			DESCRIPTION
					"The operation has stopped because an error has
					occured while reading or writing data which the
					drive cannot correct."
                                --#TYPE "HP TapeAlert #3"
                                --#SUMMARY "Hba%d:Channel%d:ScsiID%d Tape Device Has Detected Read or Write Data Error"
                                --#ARGUMENTS {0,1,2}
                                --#SEVERITY MINOR
                                --#TIMEINDEX 9
				--#HELP "ta_traps.hlp"
				--#HELPTAG 3
                                --#STATE   DEGRADED
        	                ::= 3

tapeAlertTrap4                  TRAP-TYPE       
                                ENTERPRISE      tapeAlert
                                VARIABLES       {
                                                hbaNumber,
                                                hbaChannel,
                                                driveScsiID,
                                                currentMedia
                                                }
        			DESCRIPTION
					"Your data is at risk:
					1. Copy any data you require from this tape
					2. Do not use this tape again
					3. Restart the operation with a different tape"
                                --#TYPE "HP TapeAlert #4"
                                --#SUMMARY "Hba%d:Channel%d:ScsiID%d Tape Device Has Detected Faulty Media [%s]"
                                --#ARGUMENTS {0,1,2,3}
                                --#SEVERITY CRITICAL
                                --#TIMEINDEX 9
				--#HELP "ta_traps.hlp"
				--#HELPTAG 4
                                --#STATE   DEGRADED
        	                ::= 4

tapeAlertTrap5                  TRAP-TYPE       
                                ENTERPRISE      tapeAlert
                                VARIABLES       {
                                                hbaNumber,
                                                hbaChannel,
                                                driveScsiID
                                                }
        			DESCRIPTION
					"The tape is damaged or the drive is faulty.
					Call the tape drive supplier helpline."
                                --#TYPE "HP TapeAlert #5"
                                --#SUMMARY "Hba%d:Channel%d:ScsiID%d Tape Device May Have Read Fault - Call Helpline"
                                --#ARGUMENTS {0,1,2}
                                --#SEVERITY CRITICAL
                                --#TIMEINDEX 9
				--#HELP "ta_traps.hlp"
				--#HELPTAG 5
                                --#STATE   DEGRADED
        	                ::= 5

tapeAlertTrap6                  TRAP-TYPE       
                                ENTERPRISE      tapeAlert
                                VARIABLES       {
                                                hbaNumber,
                                                hbaChannel,
                                                driveScsiID
                                                }
        			DESCRIPTION
					"The tape is from a faulty batch or the tape drive
					is faulty:
					1. Use a good tape to test the drive.
					2. If the problem persists, call the tape drive 
					   supplier helpline."
                                --#TYPE "HP TapeAlert #6"
                                --#SUMMARY "Hba%d:Channel%d:ScsiID%d Tape Device May Have Write Fault - Call Helpline"
                                --#ARGUMENTS {0,1,2}
                                --#SEVERITY CRITICAL
                                --#TIMEINDEX 9
				--#HELP "ta_traps.hlp"
				--#HELPTAG 6
                                --#STATE   DEGRADED
        	                ::= 6

tapeAlertTrap7                  TRAP-TYPE       
                                ENTERPRISE      tapeAlert
                                VARIABLES       {
                                                hbaNumber,
                                                hbaChannel,
                                                driveScsiID,
                                                currentMedia
                                                }
                              DESCRIPTION
					"The tape cartridge has reached the end of its 
					calculated useful life:
					1. Copy any data you need to another tape
					2. Discard the old tape."
                                --#TYPE "HP TapeAlert #7"
                                --#SUMMARY "Hba%d:Channel%d:ScsiID%d Tape Device Has Detected Worn Out Media [%s]"
                                --#ARGUMENTS {0,1,2,3}
                                --#SEVERITY MINOR
                                --#TIMEINDEX 9
				--#HELP "ta_traps.hlp"
				--#HELPTAG 7
                                --#STATE   OPERATIONAL
        	                ::= 7

tapeAlertTrap8                  TRAP-TYPE       
                                ENTERPRISE      tapeAlert
                                VARIABLES       {
                                                hbaNumber,
                                                hbaChannel,
                                                driveScsiID
                                                }
        			DESCRIPTION
					"The tape cartridge is not data-grade. 
					Any data you back up to the tape is at risk. 
					Replace the cartridge with a data-grade tape."
                                --#TYPE "HP TapeAlert #8"
                                --#SUMMARY "Hba%d:Channel%d:ScsiID%d Media Loaded In Tape Device Is Not Data Grade"
                                --#ARGUMENTS {0,1,2}
                                --#SEVERITY MINOR
                                --#TIMEINDEX 9
				--#HELP "ta_traps.hlp"
				--#HELPTAG 8
                                --#STATE   OPERATIONAL
        	                ::= 8

tapeAlertTrap9                  TRAP-TYPE       
                                ENTERPRISE      tapeAlert
                                VARIABLES       {
                                                hbaNumber,
                                                hbaChannel,
                                                driveScsiID
                                                }
        			DESCRIPTION
					"You are trying to write to a write-protected 
					cartridge. Remove the write-protection or use 
					another tape."
                                --#TYPE "HP TapeAlert #9"
                                --#SUMMARY "Hba%d:Channel%d:ScsiID%d Tape Device Attempted Backup to Write-Protected Tape"
                                --#ARGUMENTS {0,1,2}
                                --#SEVERITY CRITICAL
                                --#TIMEINDEX 9
				--#HELP "ta_traps.hlp"
				--#HELPTAG 9
                                --#STATE   DEGRADED
        	                ::= 9

tapeAlertTrap10                         TRAP-TYPE       
                                ENTERPRISE      tapeAlert
                                VARIABLES       {
                                                hbaNumber,
                                                hbaChannel,
                                                driveScsiID
                                                }
        			DESCRIPTION
					"You cannot eject the cartridge because the tape 
					drive is in use. Wait until the operation is 
					complete before ejecting the cartridge."
                                --#TYPE "HP TapeAlert #10"
                                --#SUMMARY "Hba%d:Channel%d:ScsiID%d Tape Device Eject Attempted When Media Locked"
                                --#ARGUMENTS {0,1,2}
                                --#SEVERITY INFORMATIONAL
                                --#TIMEINDEX 9
				--#HELP "ta_traps.hlp"
				--#HELPTAG 10
                                --#STATE   OPERATIONAL
        	                ::= 10

tapeAlertTrap11                 TRAP-TYPE       
                                ENTERPRISE      tapeAlert
                                VARIABLES       {
                                                hbaNumber,
                                                hbaChannel,
                                                driveScsiID
                                                }
        			DESCRIPTION
					"The tape in the drive is a cleaning cartridge.
					If you want to back up or restore, insert a 
					data-grade tape."
                                --#TYPE "HP TapeAlert #11"
                                --#SUMMARY "Hba%d:Channel%d:ScsiID%d Tape Device Attempted Backup/Restore To Cleaning Tape"
                                --#ARGUMENTS {0,1,2}
                                --#SEVERITY INFORMATIONAL
                                --#TIMEINDEX 9
				--#HELP "ta_traps.hlp"
				--#HELPTAG 11
                                --#STATE   OPERATIONAL
        	                ::= 11

tapeAlertTrap12                 TRAP-TYPE       
                                ENTERPRISE      tapeAlert
                                VARIABLES       {
                                                hbaNumber,
                                                hbaChannel,
                                                driveScsiID
                                                }
        			DESCRIPTION
					"You have tried to load a cartridge of a type 
					which is not supported by this drive."
                                --#TYPE "HP TapeAlert #12"
                                --#SUMMARY "Hba%d:Channel%d:ScsiID%d Media Loaded In Tape Device Is Unsupported Type"
                                --#ARGUMENTS {0,1,2}
                                --#SEVERITY INFORMATIONAL
                                --#TIMEINDEX 9
				--#HELP "ta_traps.hlp"
				--#HELPTAG 12
                                --#STATE   OPERATIONAL
        	                ::= 12

tapeAlertTrap13                 TRAP-TYPE       
                                ENTERPRISE      tapeAlert
                                VARIABLES       {
                                                hbaNumber,
                                                hbaChannel,
                                                driveScsiID
                                                }
        			DESCRIPTION
					"The operation has failed because the tape in the 
					drive has snapped:
					1. Discard the old tape.
					2. Restart the operation with a different tape."
                                --#TYPE "HP TapeAlert #13"
                                --#SUMMARY "Hba%d:Channel%d:ScsiID%d Tape Device Has Detected Snapped Tape"
                                --#ARGUMENTS {0,1,2}
                                --#SEVERITY CRITICAL
                                --#TIMEINDEX 9
				--#HELP "ta_traps.hlp"
				--#HELPTAG 13
                                --#STATE   DEGRADED
        	                ::= 13

tapeAlertTrap14                 TRAP-TYPE       
                                ENTERPRISE      tapeAlert
                                VARIABLES       {
                                                hbaNumber,
                                                hbaChannel,
                                                driveScsiID
                                                }
        			DESCRIPTION
					"The operation has failed because the tape in the 
					drive has snapped:
					1. Do not attempt to extract the tape cartridge.
                                        2. Call the tape drive supplier helpline."
                                --#TYPE "HP TapeAlert #14"
                                --#SUMMARY "Hba%d:Channel%d:ScsiID%d Tape Device Has Detected Snapped Tape"
                                --#ARGUMENTS {0,1,2}
                                --#SEVERITY CRITICAL
                                --#TIMEINDEX 9
				--#HELP "ta_traps.hlp"
				--#HELPTAG 14
                                --#STATE   DEGRADED
        	                ::= 14

tapeAlertTrap15                 TRAP-TYPE       
                                ENTERPRISE      tapeAlert
                                VARIABLES       {
                                                hbaNumber,
                                                hbaChannel,
                                                driveScsiID,
                                                currentMedia
                                                }
        			DESCRIPTION
					"The memory in the tape cartridge has failed, which reduces performance. 
                                         Do not use the cartridge for further backup operations."
                                --#TYPE "HP TapeAlert #15"
                                --#SUMMARY "Hba%d:Channel%d:ScsiID%d Tape Device Has Memory Chip in Cartridge Failure [%s]"
                                --#ARGUMENTS {0,1,2,3}
                                --#SEVERITY MINOR
                                --#TIMEINDEX 9
				--#HELP "ta_traps.hlp"
				--#HELPTAG 15
                                --#STATE   DEGRADED
        	                ::= 15

tapeAlertTrap16                 TRAP-TYPE
                                ENTERPRISE      tapeAlert
                                VARIABLES       {
                                                hbaNumber,
                                                hbaChannel,
                                                driveScsiID
                                                }
        			DESCRIPTION
					"The operation has failed because the tape cartridge was 
                                         manually ejected while the tape drive was actively writing 
                                         or reading."
                                --#TYPE "HP TapeAlert #16"
                                --#SUMMARY "Hba%d:Channel%d:ScsiID%d Tape Cartridge Ejected While Actively Writing/Reading"
                                --#ARGUMENTS {0,1,2}
                                --#SEVERITY CRITICAL
                                --#TIMEINDEX 9
				--#HELP "ta_traps.hlp"
				--#HELPTAG 16
                                --#STATE   DEGRADED
        	                ::= 16

tapeAlertTrap17                 TRAP-TYPE
                                ENTERPRISE      tapeAlert
                                VARIABLES       {
                                                hbaNumber,
                                                hbaChannel,
                                                driveScsiID
                                                }
        			DESCRIPTION
					"You have loaded a cartridge of a type that is read-only in this drive.
                                         The cartridge will appear as write-protected"
                                --#TYPE "HP TapeAlert #17"
                                --#SUMMARY "Hba%d:Channel%d:ScsiID%d Tape Device Has Detected Read Only Media"
                                --#ARGUMENTS {0,1,2}
                                --#SEVERITY MINOR
                                --#TIMEINDEX 9
				--#HELP "ta_traps.hlp"
				--#HELPTAG 17
                                --#STATE   OPERATIONAL
        	                ::= 17

tapeAlertTrap18                 TRAP-TYPE
                                ENTERPRISE      tapeAlert
                                VARIABLES       {
                                                hbaNumber,
                                                hbaChannel,
                                                driveScsiID
                                                }
        			DESCRIPTION
