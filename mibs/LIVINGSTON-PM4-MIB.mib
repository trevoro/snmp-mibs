lucentPM4FMChasTrapStatus	OBJECT-TYPE
	SYNTAX	INTEGER {
		other(1),
		online(2),
		offline(3),
		maintenance(4),
		fault(5),
		notinstalled(6)
	}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	"Trap status specifies the associated object in the Trap is online(2), 
	offline(3), maintenance(4) or fault(5)."
	::= { lucentPM4FMChasTrapEntry 5 }

lucentPM4FMChasTrapSeverity	OBJECT-TYPE
	SYNTAX	PMAlarmType
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	"Trap severity specifies the severity of the Trap for the associated
	object."
	::= { lucentPM4FMChasTrapEntry 6 }

lucentPM4FMChasTrapTimeStamp	OBJECT-TYPE
	SYNTAX	TimeTicks
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	"This object stores the timestamp of this trap."
	::= { lucentPM4FMChasTrapEntry 7 }

lucentPM4FMChasTrapState	OBJECT-TYPE
	SYNTAX	INTEGER {
		trapsent(1),
		ackdue(2),
		acked(3)
	}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	"This object stores the Trap state of this trap."
	::= { lucentPM4FMChasTrapEntry 8 }

-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
