lucentPM4T1E1PMCurEntry	OBJECT-TYPE
	SYNTAX	LucentPM4T1E1PMCurEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION
	"Performance management table entries for all the T1/E1 interfaces 
	in the box. This table represents the current 15 mins statistics. 
	This object is not accessible and present for clarity purpose."
	INDEX	{ lucentPM4T1E1PMBoardID,
		  lucentPM4T1E1PMUnitType,
		  lucentPM4T1E1PMLineNum }
	::= { lucentPM4T1E1PMCur 1 }

LucentPM4T1E1PMCurEntry ::=
	SEQUENCE {
		lucentPM4T1E1PMCurBoardID
			INTEGER,
		lucentPM4T1E1PMCurUnitType
			PMUnitType,
		lucentPM4T1E1PMCurLineNum
			INTEGER,
		lucentPM4T1E1PMCurIfIndex
			INTEGER,
		lucentPM4T1E1PMCurESs
			Gauge,
		lucentPM4T1E1PMCurSESs
			Gauge,
		lucentPM4T1E1PMCurSEFs
			Gauge,
		lucentPM4T1E1PMCurUASs
			Gauge,
		lucentPM4T1E1PMCurCSSs
			Gauge,
		lucentPM4T1E1PMCurPCVs
			Gauge,
		lucentPM4T1E1PMCurLESs
			Gauge,
		lucentPM4T1E1PMCurBESs
			Gauge,
		lucentPM4T1E1PMCurDMs
			Gauge,
		lucentPM4T1E1PMCurLCVs
			Gauge
	}

lucentPM4T1E1PMCurBoard	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	"Board number of the interface. The global interface number is 
	computed by using the Most Significant byte of the ifIndex 
	and the Least Significant 2 bytes represents the interface index.
	Byte 3 will represent the unit type which would be a T1 or E1."
	::= { lucentPM4T1E1PMCurEntry 1 }

lucentPM4T1E1PMCurUnitType	OBJECT-TYPE
	SYNTAX	PMUnitType
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	"Unit type indicates the type of interface as T1/E1 or T3/E3
