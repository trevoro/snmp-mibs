LIVINGSTON-PM4-MIB DEFINITIONS ::= BEGIN

IMPORTS
	IpAddress, Counter, Gauge, TimeTicks
		FROM RFC1155-SMI
	OBJECT-TYPE
		FROM RFC-1212
	TRAP-TYPE
		FROM RFC-1215
	DisplayString, PhysAddress, sysName
		FROM RFC1213-MIB
	lucentPM4
		FROM LIVINGSTON-ROOT-MIB;


-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
-- Lucent PortMaster private MIB Begin Textual Conventions
-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++

-- Lucent PortMaster Equipment Types

PMUnitType ::= INTEGER {
	mrgmodule(1),
	quadt1(2),
	trie1(3),
	modem(4),
	serialport(5),
	ether0(6),
	ether1(7),
	console(8),
	acpwrsup(9),
	fan(10),
	dcpwrsup(11),
	allunits(255)
	}

PMEquipPRIStatus ::= INTEGER {
	up(1),
	down(2),
	loopback(3),
	fault(4)
	}

PMEquipStatus ::= INTEGER {
	up(1),
	down(2),
	maintenance(3),
	fault(4),
	other(5)
	}

PMDiagCmdStatus ::= INTEGER {
	success(1),
	fail(2),
	inprogress(3),
	notsupported(4),
	aborted(5),
	other(6)
	}

PMDiagTestCntrl ::= INTEGER {
	normal(1),
	start(2),
	stop(3),
	abort(4)
	}

PMAlarmType ::= INTEGER {
	informational(1),
	warning(2),
	minor(3),
	major(4),
	critical(5)
	}

-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
-- Lucent PortMaster private MIB End of Textual Conventions
-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++


-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
--  Lucent PortMaster 4 Enterprise MIB
-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
lucentPM4Mib	OBJECT IDENTIFIER ::= { lucentPM4 1 }
lucentPM4Traps	OBJECT IDENTIFIER ::= { lucentPM4 2 }

-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
-- Lucent MIB Information Group
-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
lucentPM4MibRev	OBJECT-TYPE
	SYNTAX	DisplayString (SIZE(1..6))
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	"This object specifies the current MIB revision number. Example of the
	MIB revision can be PM4xxx for PM4 product and PM3xxx for PM3 products etc.
	Where xxx can be any combination of alpha-numeric characters."
	::= { lucentPM4Mib 1 }

lucentPM4SWRev	OBJECT-TYPE
	SYNTAX	DisplayString (SIZE(1..10))
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	"This object specifies the ComOS revision number. Example of the
	ComOS revision can be ComOS4.xx. Where xxx can be any combination
	of alpha-numeric characters."
	::= { lucentPM4Mib 2 }

-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
-- Lucent chassis Group
-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
lucentPM4Chassis	OBJECT IDENTIFIER ::= { lucentPM4Mib     3 }

