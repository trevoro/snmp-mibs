lucentPM4EtherIpAddress OBJECT-TYPE
	SYNTAX  IpAddress
	ACCESS  read-write
	STATUS  mandatory
	DESCRIPTION
	"IP address of the interface."
	::= { lucentPM4EtherEntry 7 }

lucentPM4EtherIpGateway OBJECT-TYPE
