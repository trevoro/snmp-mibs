   -- Traps 
   mplsCemNotifications OBJECT IDENTIFIER ::= { mplsCemMIB 0 } 
   mplsCemNotifyPrefix  OBJECT IDENTIFIER ::= { mplsCemNotifications 0 
   } 
    
     
    
   -- Tables, Scalars 
   mplsCemObjects       OBJECT IDENTIFIER ::= { mplsCemMIB 1 } 
    
   -- Conformance 
