OCMP311TRAP-MIB DEFINITIONS ::= BEGIN

    --
    --Hewlett Packard Company
    --MIB Definition for the OpenCall Media Platform traps coming from log2trap

    IMPORTS
    OBJECT-TYPE
        FROM RFC-1212
    TRAP-TYPE
        FROM RFC-1215
    DisplayString
        FROM RFC1213-MIB
    enterprises
        FROM RFC1155-SMI;

--   private OBJECT IDENTIFIER ::= { iso(1) org(3) dod(6) internet(1) 4 }
--   enterprises OBJECT IDENTIFIER ::= { private 1 }

   hp                OBJECT IDENTIFIER ::= { enterprises 11 }
   nm                OBJECT IDENTIFIER ::= { hp 2 }
   inNetElem         OBJECT IDENTIFIER ::= { nm 29 }
   openCall          OBJECT IDENTIFIER ::= { inNetElem 2 }
   openCallTraps     OBJECT IDENTIFIER ::= { openCall 0 }
    
--==========================================================

sourcePlatformEntity OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION " This is the source entity that sends the trap, typically this
is a combination of platform and machine name. (ex: oc_platform, nta2225.india.hp.com"
	::= { openCallTraps 1 }

sourceObjectID OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION " This identifies the objectID of the instance that sends the trap. (ex: Process Name)"
	::= { openCallTraps 2 }

perceivedSeverity OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION " This identifies the severity of the trap. Possible severity are
		Cleared
		Informative
		Warning
		Minor
		Major
		Critical"
	::= { openCallTraps 3 }

alarmText OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION " This describes the alarm generating the trap."
	::= { openCallTraps 4 }

probableCause OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION " This identifies the probable cause of the trap."
	::= { openCallTraps 5 }

correctiveAction OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION " This describes the actions to apply for correcting the
problem that generated this alarm."
	::= { openCallTraps 6 }

timeStamp OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION " Time-Stamp recorded in the log generated by this alarm."
	::= { openCallTraps 7 }

-- nécessaire de remplacer remplacer HPOC- par hHPOC-
hHPOC-20000006 TRAP-TYPE
	ENTERPRISE openCall
	VARIABLES { sourcePlatformEntity, sourceObjectID, perceivedSeverity, alarmText, probableCause, correctiveAction, timeStamp }
DESCRIPTION "Host [<string>] OCMP [<string>] launched"
	::= 20000006

