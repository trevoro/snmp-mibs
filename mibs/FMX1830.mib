             INDEX   { vSecIndex }  
             ::= { vSecTable 1 }  
  
          VSecEntry ::=  
              SEQUENCE {  
                  vSecIndex  
                      INTEGER,  
                  vSecAddress  
                      IpAddress,  
                  vSecMask  
                      IpAddress,  
                  vSecGroups  
