                  vPortCurrSessNumber  
                      INTEGER,  
                  vPortCurrSessProt  
                      OBJECT IDENTIFIER,  
                  vPortAccess  
                      INTEGER,  
                  vPortVirtualEnable  
                      INTEGER,  
                  vPortVirtualString  
                      OCTET STRING,  
                  vPortSessionLimit  
                      INTEGER,  
                  vPortProfile  
                      DisplayString,  
                  vPortQueueing  
                      INTEGER,  
                  vPortPasswordEnable  
                      INTEGER,  
                  vPortTacacsEnable  
                      INTEGER,  
                  vPortSecurityEnable  
                      INTEGER,  
                  vPortGroups  
                      LtGroupList,  
                  vPortBreakMode  
                      INTEGER,  
                  vPortBackSwitch  
