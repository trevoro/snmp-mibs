   mscDclVsLCoSetupPriority
      Unsigned32,
   mscDclVsLCoHoldingPriority
      Unsigned32,
   mscDclVsLCoRequiredTxBandwidth
      Gauge32,
   mscDclVsLCoRequiredRxBandwidth
      Gauge32,
   mscDclVsLCoRequiredTrafficType
      INTEGER,
   mscDclVsLCoPermittedTrunkTypes
      OCTET STRING,
   mscDclVsLCoRequiredSecurity
      Unsigned32,
