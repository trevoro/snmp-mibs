		      "
	::= { wfApplePortEntry 15 }

wfApplePortCurMacAddress OBJECT-TYPE
	SYNTAX	OCTET STRING
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
		" Mac Address used "
	::= { wfApplePortEntry 16 }

wfApplePortCurNodeId OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
		" The AppleTalk node id of this port "
	::= { wfApplePortEntry 17 }

wfApplePortCurNetwork OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
		" The network number for this port.  "
