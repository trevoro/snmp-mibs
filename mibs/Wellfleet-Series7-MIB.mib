	SYNTAX	INTEGER {
		none(1),
		lmi(2),
		t1617d(3),
		t1617b(4),
		annexa(5),
		lmiswitch(6),
		annexdswitch(7),
		annexaswitch(8)
	}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
