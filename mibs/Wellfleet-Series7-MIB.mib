		default(1500),
		maximum(3000)
	}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
		" The value that all bridges use for ForwardDelay when
		  this bridge acting as the root. "
	DEFVAL	{ default }
	::= { wfBrStp 17 }

