wfFrDlcmiFullEnquiryInterval OBJECT-TYPE
	SYNTAX	INTEGER {
		minimum(1),
		default(6),
		maximum(255)
	}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
