      SYNTAX   NvNodeconfEntry
      ACCESS   not-accessible
      STATUS   mandatory
      DESCRIPTION
               "nodeconf record."
      INDEX { nvNodeconfIndex }
      ::= { nvNodeconfTable 1 }

