nvX25callRxtput OBJECT-TYPE
     SYNTAX   INTEGER {
                bps75(4),     -- 75 bps (+1)
                bps150(5),     -- 150 bps
                bps300(6),     -- 300 bps
                bps600(7),     -- 600 bps
