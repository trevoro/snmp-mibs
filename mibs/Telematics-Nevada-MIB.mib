     SYNTAX   DisplayString (SIZE (0..16))
     ACCESS   read-only
     STATUS   mandatory
     DESCRIPTION
"port/link or X.121 address (matches posconf)
"
     ::= { nvPosstatEntry 2 }

nvPosstatNactivchn OBJECT-TYPE
     SYNTAX   INTEGER
     ACCESS   read-only
     STATUS   mandatory
     DESCRIPTION
"number of active channels
Initial status is 0."
     ::= { nvPosstatEntry 3 }

nvPosstatProtocol OBJECT-TYPE
     SYNTAX   INTEGER {
                sdlc(1),     -- simple SDLC subset (+1)
                x25(2),     -- X.25 through nbus
                visa(3),     -- visa terminal protocol
                tcp(4),      -- TCP/IP through nbus
		spdh(5),
		dfono(6),
		dtfspc(7),
		dtfgen(8)
		}
     ACCESS   read-only
     STATUS   mandatory
     DESCRIPTION
"protocol on port
"
     ::= { nvPosstatEntry 4 }

