	}

    sdslTipRingReversal OBJECT-TYPE
        SYNTAX      Byte
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Current TIP/RING state on the SDSL span:
                 0 - TIP/RING is not reversed.
                 1 - TIP/RING is reversed.
            "
        ::= {sdslStatisticsEntry 1}

    sdslUpState  OBJECT-TYPE
        SYNTAX      Byte
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Current loop state of the span:
                 0 - loop is up.
                 1 - loop is acquiring/converging.
                 2 - loop is signaling.
            "
        ::= {sdslStatisticsEntry 2}

