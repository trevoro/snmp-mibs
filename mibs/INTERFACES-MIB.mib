	::= { serialDteConfigTableEntry 19 }

serialDteRL     OBJECT-TYPE
	SYNTAX  INTEGER
		{
		serialDteRLOther(1),
		serialDteRLDisable(2),
		serialDteRLEnable(3)
		}
	ACCESS  read-write
	STATUS  mandatory
	DESCRIPTION
		"Specifies if the unit should respond to remote loop activation by the remote loopback signal (EIA530 or RS-232 pin 21)."
