	}

dbuResetNearIndex               OBJECT-TYPE
	SYNTAX  INTEGER
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
		"Selects the near device."
	::= { dbuResetStringsEntry 1 }

