	SYNTAX  INTEGER
		{
		serialDteFDLAlarmOther(1),
		serialDteFDLAlarmNone(2),
		serialDteFDLAlarmThresholdExceeded(3)
		}
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION     
		"This status indicates if the FDL Errored Seconds threshold is exceeded."
