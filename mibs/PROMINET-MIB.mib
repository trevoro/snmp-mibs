   Prom3ComMappingEntry ::=
      SEQUENCE {
         prom3ComMappingTableIndex
            INTEGER,
         prom3ComMappingTableName
            DisplayString,
         prom3ComMappingTableStatus
            INTEGER
      }

   prom3ComMappingTableIndex OBJECT-TYPE
      SYNTAX  INTEGER
      ACCESS  read-only
