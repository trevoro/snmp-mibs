              ""
      ::= { prom3ComMappingEntry 1 }

   prom3ComMappingTableName OBJECT-TYPE
      SYNTAX  DisplayString (SIZE (1..31))
      ACCESS  read-write
      STATUS  mandatory
      DESCRIPTION
              "A user-readable name associated with this table."
      ::= { prom3ComMappingEntry 2 }

   prom3ComMappingTableStatus OBJECT-TYPE
