               IMPORTS
                       IpAddress
                               FROM RFC1155-SMI
                       X121Address
                               FROM RFC1382-MIB
                       OBJECT-TYPE
                               FROM RFC-1212
                       iTouch
                               FROM ITOUCH-MIB;

       xX25            OBJECT IDENTIFIER ::= { iTouch 20 }


